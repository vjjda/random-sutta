{"thi-ap1":{"thi-ap1:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap1'><header><ul><li class='division'>{}</li>"},"thi-ap1:0.2":{"pli":"Sumedhāvagga ","html":"<li>{}</li></ul>"},"thi-ap1:0.3":{"pli":"Sumedhātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap1:1.1":{"pli":"Namo tassa Bhagavato Arahato Sammāsambuddhassa. ","html":"<p class='namo'>{}</p>"},"thi-ap1:2.1":{"pli":"Atha therikāpadānāni suṇātha—","html":"<p>{}</p>"},"thi-ap1:3.1":{"pli":"“Bhagavati koṇāgamane, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap1:3.2":{"pli":"Saṅghārāmamhi navanivesanamhi; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:3.3":{"pli":"Sakhiyo tisso janiyo, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:3.4":{"pli":"Vihāradānaṃ adāsimha. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:4.1":{"pli":"Dasakkhattuṃ satakkhattuṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:4.2":{"pli":"Dasasatakkhattuṃ satānañca satakkhattuṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:4.3":{"pli":"Devesu upapajjimha, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:4.4":{"pli":"Ko vādo mānuse bhave. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:5.1":{"pli":"Deve mahiddhikā ahumha, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:5.2":{"pli":"mānusakamhi ko vādo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:5.3":{"pli":"Sattaratanamahesī, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:5.4":{"pli":"itthiratanaṃ ahaṃ bhaviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:6.1":{"pli":"Idha sañcitakusalā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:6.2":{"pli":"susamiddhakulappajā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:6.3":{"pli":"Dhanañjānī ca khemā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:6.4":{"pli":"ahampi ca tayo janā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:7.1":{"pli":"Ārāmaṃ sukataṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:7.2":{"pli":"sabbāvayavamaṇḍitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:7.3":{"pli":"Buddhappamukhasaṅghassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:7.4":{"pli":"niyyādetvā samoditā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:8.1":{"pli":"Yattha yatthūpapajjāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:8.2":{"pli":"tassa kammassa vāhasā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:8.3":{"pli":"Devesu aggataṃ pattā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:8.4":{"pli":"manussesu tatheva ca. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:9.1":{"pli":"Imasmiṃyeva kappamhi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:9.2":{"pli":"brahmabandhu mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:9.3":{"pli":"Kassapo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:9.4":{"pli":"uppajji vadataṃ varo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:10.1":{"pli":"Upaṭṭhāko mahesissa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:10.2":{"pli":"tadā āsi narissaro; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:10.3":{"pli":"Kāsirājā kikī nāma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:10.4":{"pli":"bārāṇasipuruttame. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:11.1":{"pli":"Tassāsuṃ satta dhītaro, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:11.2":{"pli":"rājakaññā sukhedhitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:11.3":{"pli":"Buddhopaṭṭhānaniratā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:11.4":{"pli":"brahmacariyaṃ cariṃsu tā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:12.1":{"pli":"Tāsaṃ sahāyikā hutvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:12.2":{"pli":"sīlesu susamāhitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:12.3":{"pli":"Datvā dānāni sakkaccaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:12.4":{"pli":"agāreva vataṃ cariṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:13.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:13.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:13.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:13.4":{"pli":"tāvatiṃsūpagā ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:14.1":{"pli":"Tato cutā yāmamagaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:14.2":{"pli":"tatohaṃ tusitaṃ gatā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:14.3":{"pli":"Tato ca nimmānaratiṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:14.4":{"pli":"vasavattipuraṃ tato. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:15.1":{"pli":"Yattha yatthūpapajjāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:15.2":{"pli":"puññakammasamohitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:15.3":{"pli":"Tattha tattheva rājūnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:15.4":{"pli":"mahesittamakārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:16.1":{"pli":"Tato cutā manussatte, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:16.2":{"pli":"rājūnaṃ cakkavattinaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:16.3":{"pli":"Maṇḍalīnañca rājūnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:16.4":{"pli":"mahesittamakārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:17.1":{"pli":"Sampattimanubhotvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:17.2":{"pli":"devesu mānusesu ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:17.3":{"pli":"Sabbattha sukhitā hutvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:17.4":{"pli":"nekajātīsu saṃsariṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:18.1":{"pli":"So hetu ca so pabhavo, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:18.2":{"pli":"Tammūlaṃ sāsane khamaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:18.3":{"pli":"Paṭhamaṃ taṃ samodhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:18.4":{"pli":"Taṃ dhammaratāya nibbānaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:19.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:19.2":{"pli":"bhavā sabbe samūhatā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:19.3":{"pli":"Nāgīva bandhanaṃ chetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:19.4":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:20.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:20.2":{"pli":"buddhaseṭṭhassa santike; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:20.3":{"pli":"Tisso vijjā anuppattā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:20.4":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap1:21.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap1:21.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:21.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap1:21.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap1:22.1":{"pli":"Itthaṃ sudaṃ sumedhā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap1:23.1":{"pli":"Sumedhātheriyāpadānaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"thi-ap10":{"thi-ap10:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap10'><header><ul><li class='division'>{}</li>"},"thi-ap10:0.2":{"pli":"Sumedhāvagga ","html":"<li>{}</li></ul>"},"thi-ap10:0.3":{"pli":"Udakadāyikātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap10:1.1":{"pli":"“Nagare bandhumatiyā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap10:1.2":{"pli":"ahosiṃ udahārikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:1.3":{"pli":"Udahārena jīvāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:1.4":{"pli":"tena posemi dārake. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:2.1":{"pli":"Deyyadhammo ca me natthi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:2.2":{"pli":"puññakkhette anuttare; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:2.3":{"pli":"Koṭṭhakaṃ upasaṅkamma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:2.4":{"pli":"udakaṃ paṭṭhapesahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:3.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:3.2":{"pli":"tāvatiṃsamagacchahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:3.3":{"pli":"Tattha me sukataṃ byamhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:3.4":{"pli":"udahārena nimmitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:4.1":{"pli":"Accharānaṃ sahassassa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:4.2":{"pli":"ahañhi pavarā tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:4.3":{"pli":"Dasaṭṭhānehi tā sabbā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:4.4":{"pli":"abhibhomi sadā ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:5.1":{"pli":"Paññāsaṃ devarājūnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:5.2":{"pli":"mahesittamakārayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:5.3":{"pli":"Vīsaticakkavattīnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:5.4":{"pli":"mahesittamakārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:6.1":{"pli":"Duve bhave saṃsarāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:6.2":{"pli":"devatte atha mānuse; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:6.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:6.4":{"pli":"dakadānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:7.1":{"pli":"Pabbatagge dumagge vā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:7.2":{"pli":"antalikkhe ca bhūmiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:7.3":{"pli":"Yadā udakamicchāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:7.4":{"pli":"khippaṃ paṭilabhāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:8.1":{"pli":"Avuṭṭhikā disā natthi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:8.2":{"pli":"santattā kuthitāpi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:8.3":{"pli":"Mama saṅkappamaññāya, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:8.4":{"pli":"mahāmegho pavassati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:9.1":{"pli":"Kadāci nīyamānāya, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:9.2":{"pli":"ñātisaṅghena me tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:9.3":{"pli":"Yadā icchāmahaṃ vassaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:9.4":{"pli":"mahāmegho ajāyatha. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:10.1":{"pli":"Uṇhaṃ vā pariḷāho vā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:10.2":{"pli":"sarīre me na vijjati; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:10.3":{"pli":"Kāye ca me rajo natthi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:10.4":{"pli":"dakadānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:11.1":{"pli":"Visuddhamanasā ajja, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:11.2":{"pli":"apetamanapāpikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:11.3":{"pli":"Sabbāsavaparikkhīṇā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:11.4":{"pli":"natthi dāni punabbhavo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:12.1":{"pli":"Ekanavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:12.2":{"pli":"yaṃ dakaṃ adadiṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:12.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:12.4":{"pli":"dakadānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:13.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:13.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:13.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:14.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:14.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:14.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:15.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:15.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:15.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap10:16.1":{"pli":"Itthaṃ sudaṃ udakadāyikā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap10:17.1":{"pli":"Udakadāyikātheriyāpadānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"thi-ap10:18.1":{"pli":"Sumedhāvaggo paṭhamo. ","html":"<p class='endsection'>{}</p>"},"thi-ap10:19.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"thi-ap10:20.1":{"pli":"Sumedhā mekhalādāyī, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"thi-ap10:20.2":{"pli":"maṇḍapaṃ saṅkamaṃ dadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:20.3":{"pli":"Naḷamālī piṇḍadadā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:20.4":{"pli":"kaṭacchu uppalappadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap10:21.1":{"pli":"Dīpadā dakadā ceva, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap10:21.2":{"pli":"gāthāyo gaṇitā iha; ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:21.3":{"pli":"Ekagāthāsatañceva, ","html":"<span class='verse-line'>{}</span>"},"thi-ap10:21.4":{"pli":"tiṃsati ca taduttari. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}},"thi-ap11":{"thi-ap11:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap11'><header><ul><li class='division'>{}</li>"},"thi-ap11:0.2":{"pli":"Ekūposathikavagga ","html":"<li>{}</li></ul>"},"thi-ap11:0.3":{"pli":"1 Ekūposathikātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap11:1.1":{"pli":"“Nagare bandhumatiyā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap11:1.2":{"pli":"bandhumā nāma khattiyo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:1.3":{"pli":"Divase puṇṇamāya so, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:1.4":{"pli":"upavasi uposathaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:2.1":{"pli":"Ahaṃ tena samayena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:2.2":{"pli":"kumbhadāsī ahaṃ tahiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:2.3":{"pli":"Disvā sarājakaṃ senaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:2.4":{"pli":"evāhaṃ cintayiṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:3.1":{"pli":"‘Rājāpi rajjaṃ chaḍḍetvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:3.2":{"pli":"upavasi uposathaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:3.3":{"pli":"Saphalaṃ nūna taṃ kammaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:3.4":{"pli":"janakāyo pamodito’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:4.1":{"pli":"Yoniso paccavekkhitvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:4.2":{"pli":"duggaccañca daliddataṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:4.3":{"pli":"Mānasaṃ sampahaṃsitvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:4.4":{"pli":"upavasiṃ uposathaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:5.1":{"pli":"Ahaṃ uposathaṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:5.2":{"pli":"sammāsambuddhasāsane; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:5.3":{"pli":"Tena kammena sukatena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:5.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:6.1":{"pli":"Tattha me sukataṃ byamhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:6.2":{"pli":"ubbhayojanamuggataṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:6.3":{"pli":"Kūṭāgāravarūpetaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:6.4":{"pli":"mahāsanasubhūsitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:7.1":{"pli":"Accharā satasahassā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:7.2":{"pli":"upatiṭṭhanti maṃ sadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:7.3":{"pli":"Aññe deve atikkamma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:7.4":{"pli":"atirocāmi sabbadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:8.1":{"pli":"Catusaṭṭhi devarājūnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:8.2":{"pli":"mahesittamakārayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:8.3":{"pli":"Tesaṭṭhi cakkavattīnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:8.4":{"pli":"mahesittamakārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:9.1":{"pli":"Suvaṇṇavaṇṇā hutvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:9.2":{"pli":"bhavesu saṃsarāmahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:9.3":{"pli":"Sabbattha pavarā homi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:9.4":{"pli":"uposathassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:10.1":{"pli":"Hatthiyānaṃ assayānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:10.2":{"pli":"rathayānañca sīvikaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:10.3":{"pli":"Labhāmi sabbamevetaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:10.4":{"pli":"uposathassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:11.1":{"pli":"Soṇṇamayaṃ rūpimayaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:11.2":{"pli":"athopi phalikāmayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:11.3":{"pli":"Lohitaṅgamayañceva, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:11.4":{"pli":"sabbaṃ paṭilabhāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:12.1":{"pli":"Koseyyakambaliyāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:12.2":{"pli":"khomakappāsikāni ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:12.3":{"pli":"Mahagghāni ca vatthāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:12.4":{"pli":"sabbaṃ paṭilabhāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:13.1":{"pli":"Annaṃ pānaṃ khādanīyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:13.2":{"pli":"vatthasenāsanāni ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:13.3":{"pli":"Sabbametaṃ paṭilabhe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:13.4":{"pli":"uposathassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:14.1":{"pli":"Varagandhañca mālañca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:14.2":{"pli":"cuṇṇakañca vilepanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:14.3":{"pli":"Sabbametaṃ paṭilabhe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:14.4":{"pli":"uposathassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:15.1":{"pli":"Kūṭāgārañca pāsādaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:15.2":{"pli":"maṇḍapaṃ hammiyaṃ guhaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:15.3":{"pli":"Sabbametaṃ paṭilabhe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:15.4":{"pli":"uposathassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:16.1":{"pli":"Jātiyā sattavassāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:16.2":{"pli":"pabbajiṃ anagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:16.3":{"pli":"Aḍḍhamāse asampatte, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:16.4":{"pli":"arahattamapāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:17.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:17.2":{"pli":"bhavā sabbe samūhatā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:17.3":{"pli":"Sabbāsavā parikkhīṇā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:17.4":{"pli":"natthi dāni punabbhavo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:18.1":{"pli":"Ekanavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:18.2":{"pli":"yaṃ kammamakariṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:18.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:18.4":{"pli":"uposathassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:19.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:19.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:19.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:20.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:20.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:20.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap11:21.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap11:21.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap11:21.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap11:22.1":{"pli":"Itthaṃ sudaṃ ekūposathikā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap11:23.1":{"pli":"Ekūposathikātheriyāpadānaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"thi-ap12":{"thi-ap12:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap12'><header><ul><li class='division'>{}</li>"},"thi-ap12:0.2":{"pli":"Ekūposathikavagga ","html":"<li>{}</li></ul>"},"thi-ap12:0.3":{"pli":"2 Saḷalapupphikātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap12:1.1":{"pli":"“Candabhāgānadītīre, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap12:1.2":{"pli":"ahosiṃ kinnarī tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:1.3":{"pli":"Addasāhaṃ devadevaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:1.4":{"pli":"caṅkamantaṃ narāsabhaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap12:2.1":{"pli":"Ocinitvāna saḷalaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap12:2.2":{"pli":"buddhaseṭṭhassadāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:2.3":{"pli":"Upasiṅghi mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:2.4":{"pli":"saḷalaṃ devagandhikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap12:3.1":{"pli":"Paṭiggahetvā sambuddho, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap12:3.2":{"pli":"vipassī lokanāyako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:3.3":{"pli":"Upasiṅghi mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:3.4":{"pli":"pekkhamānāya me tadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap12:4.1":{"pli":"Añjaliṃ paggahetvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap12:4.2":{"pli":"vanditvā dvipaduttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:4.3":{"pli":"Sakaṃ cittaṃ pasādetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:4.4":{"pli":"tato pabbatamāruhiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap12:5.1":{"pli":"Ekanavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap12:5.2":{"pli":"yaṃ pupphamadadiṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:5.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:5.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap12:6.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap12:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:6.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap12:7.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap12:7.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:7.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap12:8.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap12:8.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap12:8.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap12:9.1":{"pli":"Itthaṃ sudaṃ saḷalapupphikā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap12:10.1":{"pli":"Saḷalapupphikātheriyāpadānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"thi-ap13":{"thi-ap13:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap13'><header><ul><li class='division'>{}</li>"},"thi-ap13:0.2":{"pli":"Ekūposathikavagga ","html":"<li>{}</li></ul>"},"thi-ap13:0.3":{"pli":"Modakadāyikātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap13:1.1":{"pli":"“Nagare bandhumatiyā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap13:1.2":{"pli":"kumbhadāsī ahosahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap13:1.3":{"pli":"Mama bhāgaṃ gahetvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap13:1.4":{"pli":"gacchaṃ udakahārikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap13:2.1":{"pli":"Panthamhi samaṇaṃ disvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap13:2.2":{"pli":"santacittaṃ samāhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap13:2.3":{"pli":"Pasannacittā sumanā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap13:2.4":{"pli":"modake tīṇidāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap13:3.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap13:3.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap13:3.3":{"pli":"Ekanavutikappāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap13:3.4":{"pli":"vinipātaṃ nagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap13:4.1":{"pli":"Sampatti taṃ karitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap13:4.2":{"pli":"sabbaṃ anubhaviṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap13:4.3":{"pli":"Modake tīṇi datvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap13:4.4":{"pli":"pattāhaṃ acalaṃ padaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap13:5.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap13:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap13:5.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap13:6.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap13:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap13:6.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap13:7.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap13:7.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap13:7.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap13:8.1":{"pli":"Itthaṃ sudaṃ modakadāyikā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap13:9.1":{"pli":"Modakadāyikātheriyāpadānaṃ tatiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"thi-ap14":{"thi-ap14:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap14'><header><ul><li class='division'>{}</li>"},"thi-ap14:0.2":{"pli":"Ekūposathikavagga ","html":"<li>{}</li></ul>"},"thi-ap14:0.3":{"pli":"Ekāsanadāyikātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap14:1.1":{"pli":"“Nagare haṃsavatiyā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap14:1.2":{"pli":"ahosiṃ bālikā tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:1.3":{"pli":"Mātā ca me pitā ceva, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:1.4":{"pli":"kammantaṃ agamaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:2.1":{"pli":"Majjhanhikamhi sūriye, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:2.2":{"pli":"addasaṃ samaṇaṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:2.3":{"pli":"Vīthiyā anugacchantaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:2.4":{"pli":"āsanaṃ paññapesahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:3.1":{"pli":"Gonakāvikatikāhi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:3.2":{"pli":"paññapetvā mamāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:3.3":{"pli":"Pasannacittā sumanā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:3.4":{"pli":"idaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:4.1":{"pli":"‘Santattā kuthitā bhūmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:4.2":{"pli":"sūro majjhanhike ṭhito; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:4.3":{"pli":"Mālutā ca na vāyanti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:4.4":{"pli":"kālo cevettha mehiti. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:5.1":{"pli":"Paññattamāsanamidaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:5.2":{"pli":"tavatthāya mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:5.3":{"pli":"Anukampaṃ upādāya, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:5.4":{"pli":"nisīda mama āsane’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:6.1":{"pli":"Nisīdi tattha samaṇo, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:6.2":{"pli":"sudanto suddhamānaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:6.3":{"pli":"Tassa pattaṃ gahetvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:6.4":{"pli":"yathārandhaṃ adāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:7.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:7.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:7.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:7.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:8.1":{"pli":"Tattha me sukataṃ byamhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:8.2":{"pli":"āsanena sunimmitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:8.3":{"pli":"Saṭṭhiyojanamubbedhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:8.4":{"pli":"tiṃsayojanavitthataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:9.1":{"pli":"Soṇṇamayā maṇimayā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:9.2":{"pli":"athopi phalikāmayā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:9.3":{"pli":"Lohitaṅgamayā ceva, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:9.4":{"pli":"pallaṅkā vividhā mama. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:10.1":{"pli":"Tūlikāvikatikāhi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:10.2":{"pli":"kaṭṭissacittakāhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:10.3":{"pli":"Uddaekantalomī ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:10.4":{"pli":"pallaṅkā me susaṇṭhitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:11.1":{"pli":"Yadā icchāmi gamanaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:11.2":{"pli":"hāsakhiḍḍasamappitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:11.3":{"pli":"Saha pallaṅkaseṭṭhena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:11.4":{"pli":"gacchāmi mama patthitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:12.1":{"pli":"Asīti devarājūnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:12.2":{"pli":"mahesittamakārayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:12.3":{"pli":"Sattati cakkavattīnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:12.4":{"pli":"mahesittamakārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:13.1":{"pli":"Bhavābhave saṃsarantī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:13.2":{"pli":"mahābhogaṃ labhāmahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:13.3":{"pli":"Bhoge me ūnatā natthi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:13.4":{"pli":"ekāsanassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:14.1":{"pli":"Duve bhave saṃsarāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:14.2":{"pli":"devatte atha mānuse; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:14.3":{"pli":"Aññe bhave na jānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:14.4":{"pli":"ekāsanassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:15.1":{"pli":"Duve kule pajāyāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:15.2":{"pli":"khattiye cāpi brāhmaṇe; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:15.3":{"pli":"Uccākulīnā sabbattha, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:15.4":{"pli":"ekāsanassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:16.1":{"pli":"Domanassaṃ na jānāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:16.2":{"pli":"cittasantāpanaṃ mama; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:16.3":{"pli":"Vevaṇṇiyaṃ na jānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:16.4":{"pli":"ekāsanassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:17.1":{"pli":"Dhātiyo maṃ upaṭṭhanti, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:17.2":{"pli":"khujjā celāpikā bahū; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:17.3":{"pli":"Aṅkena aṅkaṃ gacchāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:17.4":{"pli":"ekāsanassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:18.1":{"pli":"Aññā nhāpenti bhojenti, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:18.2":{"pli":"aññā ramenti maṃ sadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:18.3":{"pli":"Aññā gandhaṃ vilimpanti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:18.4":{"pli":"ekāsanassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:19.1":{"pli":"Maṇḍape rukkhamūle vā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:19.2":{"pli":"suññāgāre vasantiyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:19.3":{"pli":"Mama saṅkappamaññāya, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:19.4":{"pli":"pallaṅko upatiṭṭhati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:20.1":{"pli":"Ayaṃ pacchimako mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:20.2":{"pli":"carimo vattate bhavo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:20.3":{"pli":"Ajjāpi rajjaṃ chaḍḍetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:20.4":{"pli":"pabbajiṃ anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:21.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:21.2":{"pli":"yaṃ dānamadadiṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:21.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:21.4":{"pli":"ekāsanassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:22.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:22.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:22.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:23.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:23.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:23.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap14:24.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap14:24.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap14:24.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap14:25.1":{"pli":"Itthaṃ sudaṃ ekāsanadāyikā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap14:26.1":{"pli":"Ekāsanadāyikātheriyāpadānaṃ catutthaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}