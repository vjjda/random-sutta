32:25.1":{"pli":"Itthaṃ sudaṃ āyasmā saparivārachattadāyako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap332:26.1":{"pli":"Saparivārachattadāyakattherassāpadānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"tha-ap332:27.1":{"pli":"Umāpupphiyavaggo tettiṃsatimo. ","html":"<p class='endsection'>{}</p>"},"tha-ap332:28.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"tha-ap332:29.1":{"pli":"Umāpupphañca pulinaṃ, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"tha-ap332:29.2":{"pli":"hāso yañño nimittako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap332:29.3":{"pli":"Saṃsāvako nigguṇḍī ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap332:29.4":{"pli":"sumanaṃ pupphachattako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap332:29.5":{"pli":"Saparivārachatto ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap332:29.6":{"pli":"gāthā sattasatuttarāti. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}},"tha-ap333":{"tha-ap333:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap333'><header><ul><li class='division'>{}</li>"},"tha-ap333:0.2":{"pli":"Gandhodakavagga ","html":"<li>{}</li></ul>"},"tha-ap333:0.3":{"pli":"Gandhadhūpiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap333:1.1":{"pli":"“Siddhatthassa bhagavato, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap333:1.2":{"pli":"gandhadhūpaṃ adāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap333:1.3":{"pli":"Sumanehi paṭicchannaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap333:1.4":{"pli":"buddhānucchavikañca taṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap333:2.1":{"pli":"Kañcanagghiyasaṅkāsaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap333:2.2":{"pli":"buddhaṃ lokagganāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap333:2.3":{"pli":"Indīvaraṃva jalitaṃ, ","html":"<span 