{"tha-ap329":{"tha-ap329:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap329'><header><ul><li class='division'>{}</li>"},"tha-ap329:0.2":{"pli":"Umāpupphiyavagga ","html":"<li>{}</li></ul>"},"tha-ap329:0.3":{"pli":"Nigguṇḍipupphiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap329:1.1":{"pli":"“Yadā devo devakāyā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap329:1.2":{"pli":"cavate āyusaṅkhayā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:1.3":{"pli":"Tayo saddā niccharanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:1.4":{"pli":"devānaṃ anumodataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:2.1":{"pli":"‘Ito bho sugatiṃ gaccha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:2.2":{"pli":"manussānaṃ sahabyataṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:2.3":{"pli":"Manussabhūto saddhamme, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:2.4":{"pli":"labha saddhaṃ anuttaraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:3.1":{"pli":"Sā te saddhā niviṭṭhāssa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:3.2":{"pli":"mūlajātā patiṭṭhitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:3.3":{"pli":"Yāvajīvaṃ asaṃhīrā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:3.4":{"pli":"saddhamme suppavedite. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:4.1":{"pli":"Kāyena kusalaṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:4.2":{"pli":"vācāya kusalaṃ bahuṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:4.3":{"pli":"Manasā kusalaṃ katvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:4.4":{"pli":"abyāpajjaṃ nirūpadhiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:5.1":{"pli":"Tato opadhikaṃ puññaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:5.2":{"pli":"katvā dānena taṃ bahuṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:5.3":{"pli":"Aññepi macce saddhamme, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:5.4":{"pli":"brahmacariye nivesaya’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:6.1":{"pli":"Imāya anukampāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:6.2":{"pli":"devādevaṃ yadā vidū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:6.3":{"pli":"Cavantaṃ anumodanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:6.4":{"pli":"ehi deva punappunaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:7.1":{"pli":"‘Saṃvego me tadā āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:7.2":{"pli":"devasaṅghe samāgate; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:7.3":{"pli":"Kaṃsu nāma ahaṃ yoniṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:7.4":{"pli":"gamissāmi ito cuto’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:8.1":{"pli":"Mama saṃvegamaññāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:8.2":{"pli":"samaṇo bhāvitindriyo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:8.3":{"pli":"Mamuddharitukāmo so, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:8.4":{"pli":"āgacchi mama santikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:9.1":{"pli":"Sumano nāma nāmena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:9.2":{"pli":"padumuttarasāvako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:9.3":{"pli":"Atthadhammānusāsitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:9.4":{"pli":"saṃvejesi mamaṃ tadā. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap329:10.1":{"pli":"Dvādasamaṃ bhāṇavāraṃ. ","html":"<p class='endsutta'>{}</p>"},"tha-ap329:11.1":{"pli":"Tassāhaṃ vacanaṃ sutvā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap329:11.2":{"pli":"buddhe cittaṃ pasādayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:11.3":{"pli":"Taṃ dhīraṃ abhivādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:11.4":{"pli":"tattha kālaṅkato ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:12.1":{"pli":"Upapajjiṃ sa tattheva, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:12.2":{"pli":"sukkamūlena codito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:12.3":{"pli":"Vasanto mātukucchimhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:12.4":{"pli":"puna dhāreti mātuyā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:13.1":{"pli":"Tamhā kāyā cavitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:13.2":{"pli":"tidase upapajjahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:13.3":{"pli":"Etthantare na passāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:13.4":{"pli":"domanassamahaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:14.1":{"pli":"Tāvatiṃsā cavitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:14.2":{"pli":"mātukucchiṃ samokkamiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:14.3":{"pli":"Nikkhamitvāna kucchimhā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:14.4":{"pli":"kaṇhasukkaṃ ajānahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:15.1":{"pli":"Jātiyā sattavassova, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:15.2":{"pli":"ārāmaṃ pāvisiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:15.3":{"pli":"Gotamassa bhagavato, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:15.4":{"pli":"sakyaputtassa tādino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:16.1":{"pli":"Vitthārike pāvacane, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:16.2":{"pli":"bāhujaññamhi sāsane; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:16.3":{"pli":"Addasaṃ sāsanakare, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:16.4":{"pli":"bhikkhavo tattha satthuno. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:17.1":{"pli":"Sāvatthi nāma nagaraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:17.2":{"pli":"rājā tatthāsi kosalo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:17.3":{"pli":"Rathena nāgayuttena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:17.4":{"pli":"upesi bodhimuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:18.1":{"pli":"Tassāhaṃ nāgaṃ disvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:18.2":{"pli":"pubbakammaṃ anussariṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:18.3":{"pli":"Añjaliṃ paggahetvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:18.4":{"pli":"samayaṃ agamāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:19.1":{"pli":"Jātiyā sattavassova, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:19.2":{"pli":"pabbajiṃ anagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:19.3":{"pli":"Yo so buddhaṃ upaṭṭhāsi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:19.4":{"pli":"ānando nāma sāvako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:20.1":{"pli":"Gatimā dhitimā ceva, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:20.2":{"pli":"satimā ca bahussuto; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:20.3":{"pli":"Rañño cittaṃ pasādento, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:20.4":{"pli":"niyyādesi mahājuti. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:21.1":{"pli":"Tassāhaṃ dhammaṃ sutvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:21.2":{"pli":"pubbakammaṃ anussariṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:21.3":{"pli":"Tattheva ṭhitako santo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:21.4":{"pli":"arahattamapāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:22.1":{"pli":"Ekaṃsaṃ cīvaraṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:22.2":{"pli":"sire katvāna añjaliṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:22.3":{"pli":"Sambuddhaṃ abhivādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:22.4":{"pli":"imaṃ vācaṃ udīrayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:23.1":{"pli":"‘Padumuttarabuddhassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:23.2":{"pli":"dvipadindassa satthuno; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:23.3":{"pli":"Nigguṇḍipupphaṃ paggayha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:23.4":{"pli":"sīhāsane ṭhapesahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:24.1":{"pli":"Tena kammena dvipadinda, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:24.2":{"pli":"lokajeṭṭha narāsabha; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:24.3":{"pli":"Pattomhi acalaṃ ṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:24.4":{"pli":"hitvā jayaparājayaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:25.1":{"pli":"Pañcavīsasahassamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:25.2":{"pli":"kappānaṃ manujādhipā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:25.3":{"pli":"Abbudanirabbudāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:25.4":{"pli":"aṭṭhaṭṭhāsiṃsu khattiyā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap329:26.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap329:26.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap329:26.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap329:27.1":{"pli":"Itthaṃ sudaṃ āyasmā nigguṇḍipupphiyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap329:28.1":{"pli":"Nigguṇḍipupphiyattherassāpadānaṃ sattamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap33":{"tha-ap33:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap33'><header><ul><li class='division'>{}</li>"},"tha-ap33:0.2":{"pli":"Kuṇḍadhānavagga ","html":"<li>{}</li></ul>"},"tha-ap33:0.3":{"pli":"Kuṇḍadhānattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap33:1.1":{"pli":"“Sattāhaṃ paṭisallīnaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap33:1.2":{"pli":"sayambhuṃ aggapuggalaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:1.3":{"pli":"Pasannacitto sumano, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:1.4":{"pli":"buddhaseṭṭhaṃ upaṭṭhahiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:2.1":{"pli":"Vuṭṭhitaṃ kālamaññāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:2.2":{"pli":"padumuttaraṃ mahāmuniṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:2.3":{"pli":"Mahantiṃ kadalīkaṇṇiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:2.4":{"pli":"gahetvā upagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:3.1":{"pli":"Paṭiggahetvā bhagavā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:3.2":{"pli":"sabbaññū lokanāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:3.3":{"pli":"Mama cittaṃ pasādento, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:3.4":{"pli":"paribhuñji mahāmuni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:4.1":{"pli":"Paribhuñjitvā sambuddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:4.2":{"pli":"satthavāho anuttaro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:4.3":{"pli":"Sakāsane nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:4.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:5.1":{"pli":"‘Ye ca santi samitāro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:5.2":{"pli":"yakkhā imamhi pabbate; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:5.3":{"pli":"Araññe bhūtabhabyāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:5.4":{"pli":"suṇantu vacanaṃ mama. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:6.1":{"pli":"Yo so buddhaṃ upaṭṭhāsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:6.2":{"pli":"migarājaṃva kesariṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:6.3":{"pli":"Tamahaṃ kittayissāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:6.4":{"pli":"suṇātha mama bhāsato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:7.1":{"pli":"Ekādasañcakkhattuṃ so, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:7.2":{"pli":"devarājā bhavissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:7.3":{"pli":"Catutiṃsatikkhattuñca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:7.4":{"pli":"cakkavattī bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:8.1":{"pli":"Kappasatasahassamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:8.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:8.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:8.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:9.1":{"pli":"Akkositvāna samaṇe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:9.2":{"pli":"sīlavante anāsave; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:9.3":{"pli":"Pāpakammavipākena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:9.4":{"pli":"nāmadheyyaṃ labhissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:10.1":{"pli":"Tassa dhamme sudāyādo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:10.2":{"pli":"oraso dhammanimmito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:10.3":{"pli":"Kuṇḍadhānoti nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:10.4":{"pli":"sāvako so bhavissati’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:11.1":{"pli":"Pavivekamanuyutto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:11.2":{"pli":"jhāyī jhānarato ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:11.3":{"pli":"Tosayitvāna satthāraṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:11.4":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:12.1":{"pli":"Sāvakehi parivuto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:12.2":{"pli":"bhikkhusaṅghapurakkhato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:12.3":{"pli":"Bhikkhusaṅghe nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:12.4":{"pli":"salākaṃ gāhayī jino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:13.1":{"pli":"Ekaṃsaṃ cīvaraṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:13.2":{"pli":"vanditvā lokanāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:13.3":{"pli":"Vadataṃ varassa purato, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:13.4":{"pli":"paṭhamaṃ aggahesahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:14.1":{"pli":"Tena kammena bhagavā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:14.2":{"pli":"dasasahassikampako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:14.3":{"pli":"Bhikkhusaṅghe nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:14.4":{"pli":"aggaṭṭhāne ṭhapesi maṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:15.1":{"pli":"Vīriyaṃ me dhuradhorayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:15.2":{"pli":"yogakkhemādhivāhanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:15.3":{"pli":"Dhāremi antimaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:15.4":{"pli":"sammāsambuddhasāsane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap33:16.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap33:16.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:16.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap33:16.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap33:17.1":{"pli":"Itthaṃ sudaṃ āyasmā kuṇḍadhāno thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap33:18.1":{"pli":"Kuṇḍadhānattherassāpadānaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap330":{"tha-ap330:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap330'><header><ul><li class='division'>{}</li>"},"tha-ap330:0.2":{"pli":"Umāpupphiyavagga ","html":"<li>{}</li></ul>"},"tha-ap330:0.3":{"pli":"8 Sumanāveḷiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap330:1.1":{"pli":"“Vessabhussa bhagavato, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap330:1.2":{"pli":"lokajeṭṭhassa tādino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap330:1.3":{"pli":"Sabbe janā samāgamma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap330:1.4":{"pli":"mahāpūjaṃ karonti te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap330:2.1":{"pli":"Sudhāya piṇḍaṃ katvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap330:2.2":{"pli":"āveḷaṃ sumanāyahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap330:2.3":{"pli":"Sīhāsanassa purato, ","html":"<span class='verse-line'>{}</span>"},"tha-ap330:2.4":{"pli":"abhiropesahaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap330:3.1":{"pli":"Sabbe janā samāgamma, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap330:3.2":{"pli":"pekkhanti pupphamuttamaṃ; ","html":"<spa