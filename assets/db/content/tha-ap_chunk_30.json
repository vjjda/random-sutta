{"tha-ap399":{"tha-ap399:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap399'><header><ul><li class='division'>{}</li>"},"tha-ap399:0.2":{"pli":"Pilindavacchavagga ","html":"<li>{}</li></ul>"},"tha-ap399:0.3":{"pli":"Girimānandattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap399:1.1":{"pli":"“Bhariyā me kālaṅkatā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap399:1.2":{"pli":"putto sīvathikaṃ gato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:1.3":{"pli":"Mātā pitā matā bhātā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:1.4":{"pli":"ekacitamhi ḍayhare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:2.1":{"pli":"Tena sokena santatto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:2.2":{"pli":"kiso paṇḍu ahosahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:2.3":{"pli":"Cittakkhepo ca me āsi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:2.4":{"pli":"tena sokena aṭṭito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:3.1":{"pli":"Sokasallaparetohaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:3.2":{"pli":"vanantamupasaṅkamiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:3.3":{"pli":"Pavattaphalaṃ bhuñjitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:3.4":{"pli":"rukkhamūle vasāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:4.1":{"pli":"Sumedho nāma sambuddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:4.2":{"pli":"dukkhassantakaro jino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:4.3":{"pli":"Mamuddharitukāmo so, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:4.4":{"pli":"āgañchi mama santikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:5.1":{"pli":"Padasaddaṃ suṇitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:5.2":{"pli":"sumedhassa mahesino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:5.3":{"pli":"Paggahetvānahaṃ sīsaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:5.4":{"pli":"ullokesiṃ mahāmuniṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:6.1":{"pli":"Upāgate mahāvīre, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:6.2":{"pli":"pīti me udapajjatha; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:6.3":{"pli":"Tadāsimekaggamano, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:6.4":{"pli":"disvā taṃ lokanāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:7.1":{"pli":"Satiṃ paṭilabhitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:7.2":{"pli":"paṇṇamuṭṭhimadāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:7.3":{"pli":"Nisīdi bhagavā tattha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:7.4":{"pli":"anukampāya cakkhumā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:8.1":{"pli":"Nisajja tattha bhagavā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:8.2":{"pli":"sumedho lokanāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:8.3":{"pli":"Dhammaṃ me kathayī buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:8.4":{"pli":"sokasallavinodanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:9.1":{"pli":"‘Anavhitā tato āguṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:9.2":{"pli":"ananuññātā ito gatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:9.3":{"pli":"Yathāgatā tathā gatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:9.4":{"pli":"tattha kā paridevanā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:10.1":{"pli":"Yathāpi pathikā sattā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:10.2":{"pli":"vassamānāya vuṭṭhiyā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:10.3":{"pli":"Sabhaṇḍā upagacchanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:10.4":{"pli":"vassassāpatanāya te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:11.1":{"pli":"Vasse ca te oramite, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:11.2":{"pli":"sampayanti yadicchakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:11.3":{"pli":"Evaṃ mātā pitā tuyhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:11.4":{"pli":"tattha kā paridevanā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:12.1":{"pli":"Āgantukā pāhunakā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:12.2":{"pli":"caliteritakampitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:12.3":{"pli":"Evaṃ mātā pitā tuyhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:12.4":{"pli":"tattha kā paridevanā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:13.1":{"pli":"Yathāpi urago jiṇṇaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:13.2":{"pli":"hitvā gacchati saṃ tacaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:13.3":{"pli":"Evaṃ mātā pitā tuyhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:13.4":{"pli":"saṃ tanuṃ idha hīyare’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:14.1":{"pli":"Buddhassa giramaññāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:14.2":{"pli":"sokasallaṃ vivajjayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:14.3":{"pli":"Pāmojjaṃ janayitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:14.4":{"pli":"buddhaseṭṭhaṃ avandahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:15.1":{"pli":"Vanditvāna mahānāgaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:15.2":{"pli":"Pūjayiṃ girimañjariṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:15.3":{"pli":"Dibbagandhaṃ sampavantaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:15.4":{"pli":"Sumedhaṃ lokanāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:16.1":{"pli":"Pūjayitvāna sambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:16.2":{"pli":"sire katvāna añjaliṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:16.3":{"pli":"Anussaraṃ guṇaggāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:16.4":{"pli":"santhaviṃ lokanāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:17.1":{"pli":"‘Nittiṇṇosi mahāvīra, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:17.2":{"pli":"sabbaññu lokanāyaka; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:17.3":{"pli":"Sabbe satte uddharasi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:17.4":{"pli":"ñāṇena tvaṃ mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:18.1":{"pli":"Vimatiṃ dveḷhakaṃ vāpi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:18.2":{"pli":"sañchindasi mahāmune; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:18.3":{"pli":"Paṭipādesi me maggaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:18.4":{"pli":"tava ñāṇena cakkhuma. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:19.1":{"pli":"Arahā vasipattā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:19.2":{"pli":"chaḷabhiññā mahiddhikā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:19.3":{"pli":"Antalikkhacarā dhīrā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:19.4":{"pli":"parivārenti tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:20.1":{"pli":"Paṭipannā ca sekhā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:20.2":{"pli":"phalaṭṭhā santi sāvakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:20.3":{"pli":"Sūrodayeva padumā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:20.4":{"pli":"pupphanti tava sāvakā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:21.1":{"pli":"Mahāsamuddovakkhobho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:21.2":{"pli":"atulopi duruttaro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:21.3":{"pli":"Evaṃ ñāṇena sampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:21.4":{"pli":"appameyyosi cakkhuma’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:22.1":{"pli":"Vanditvāhaṃ lokajinaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:22.2":{"pli":"cakkhumantaṃ mahāyasaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:22.3":{"pli":"Puthu disā namassanto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:22.4":{"pli":"paṭikuṭiko agañchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:23.1":{"pli":"Devalokā cavitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:23.2":{"pli":"sampajāno patissato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:23.3":{"pli":"Okkamiṃ mātuyā kucchiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:23.4":{"pli":"sandhāvanto bhavābhave. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:24.1":{"pli":"Agārā abhinikkhamma, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:24.2":{"pli":"pabbajiṃ anagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:24.3":{"pli":"Ātāpī nipako jhāyī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:24.4":{"pli":"paṭisallānagocaro. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:25.1":{"pli":"Padhānaṃ padahitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:25.2":{"pli":"tosayitvā mahāmuniṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:25.3":{"pli":"Candovabbhaghanā mutto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:25.4":{"pli":"vicarāmi ahaṃ sadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:26.1":{"pli":"Vivekamanuyuttomhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:26.2":{"pli":"upasanto nirūpadhi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:26.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:26.4":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:27.1":{"pli":"Tiṃsakappasahassamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:27.2":{"pli":"yaṃ buddhamabhipūjayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:27.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:27.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:28.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:28.2":{"pli":"bhavā sabbe samūhatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:28.3":{"pli":"Sabbāsavā parikkhīṇā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:28.4":{"pli":"natthi dāni punabbhavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:29.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:29.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:29.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap399:30.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap399:30.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap399:30.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap399:31.1":{"pli":"Itthaṃ sudaṃ āyasmā girimānando thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap399:32.1":{"pli":"Girimānandattherassāpadānaṃ sattamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap4":{"tha-ap4:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap4'><header><ul><li class='division'>{}</li>"},"tha-ap4:0.2":{"pli":"Buddhavagga ","html":"<li>{}</li></ul>"},"tha-ap4:0.3":{"pli":"Mahāmoggallānattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap4:1.1":{"pli":"“Anomadassī bhagavā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap4:1.2":{"pli":"lokajeṭṭho narāsabho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:1.3":{"pli":"Vihāsi himavantamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:1.4":{"pli":"devasaṅghapurakkhato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:2.1":{"pli":"Varuṇo nāma nāmena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:2.2":{"pli":"nāgarājā ahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:2.3":{"pli":"Kāmarūpī vikubbāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:2.4":{"pli":"mahodadhinivāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:3.1":{"pli":"Saṅgaṇiyaṃ gaṇaṃ hitvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:3.2":{"pli":"tūriyaṃ paṭṭhapesahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:3.3":{"pli":"Sambuddhaṃ parivāretvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:3.4":{"pli":"vādesuṃ accharā tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:4.1":{"pli":"Vajjamānesu tūresu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:4.2":{"pli":"devā tūrāni vajjayuṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:4.3":{"pli":"Ubhinnaṃ saddaṃ sutvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:4.4":{"pli":"buddhopi sampabujjhatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:5.1":{"pli":"Nimantetvāna sambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:5.2":{"pli":"sakaṃ bhavanupāgamiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:5.3":{"pli":"Āsanaṃ paññapetvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:5.4":{"pli":"kālamārocayiṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:6.1":{"pli":"Khīṇāsavasahassehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:6.2":{"pli":"parivuto lokanāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:6.3":{"pli":"Obhāsento disā sabbā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:6.4":{"pli":"bhavanaṃ me upāgami. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:7.1":{"pli":"Upaviṭṭhaṃ mahāvīraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:7.2":{"pli":"Devadevaṃ narāsabhaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:7.3":{"pli":"Sabhikkhusaṅghaṃ tappesiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:7.4":{"pli":"Annapānenahaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:8.1":{"pli":"Anumodi mahāvīro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:8.2":{"pli":"sayambhū aggapuggalo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:8.3":{"pli":"Bhikkhusaṅghe nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:8.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:9.1":{"pli":"Yo so saṅghaṃ apūjesi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:9.2":{"pli":"buddhañca lokanāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:9.3":{"pli":"Tena cittappasādena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:9.4":{"pli":"devalokaṃ gamissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:10.1":{"pli":"Sattasattatikkhattuñca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:10.2":{"pli":"devarajjaṃ karissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:10.3":{"pli":"Pathabyā rajjaṃ aṭṭhasataṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:10.4":{"pli":"vasudhaṃ āvasissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:11.1":{"pli":"Pañcapaññāsakkhattuñca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:11.2":{"pli":"cakkavattī bhavissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:11.3":{"pli":"Bhogā asaṅkhiyā tassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:11.4":{"pli":"uppajjissanti tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:12.1":{"pli":"Aparimeyye ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:12.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:12.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:12.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:13.1":{"pli":"Nirayā so cavitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:13.2":{"pli":"manussataṃ gamissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:13.3":{"pli":"Kolito nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:13.4":{"pli":"brahmabandhu bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:14.1":{"pli":"So pacchā pabbajitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:14.2":{"pli":"kusalamūlena codito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:14.3":{"pli":"Gotamassa bhagavato, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:14.4":{"pli":"dutiyo hessati sāvako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:15.1":{"pli":"Āraddhavīriyo pahitatto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:15.2":{"pli":"Iddhiyā pāramiṃ gato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:15.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:15.4":{"pli":"Nibbāyissatināsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:16.1":{"pli":"Pāpamittopanissāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:16.2":{"pli":"kāmarāgavasaṃ gato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:16.3":{"pli":"Mātaraṃ pitarañcāpi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:16.4":{"pli":"ghātayiṃ duṭṭhamānaso. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:17.1":{"pli":"Yaṃ yaṃ yonupapajjāmi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:17.2":{"pli":"nirayaṃ atha mānusaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:17.3":{"pli":"Pāpakammasamaṅgitā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:17.4":{"pli":"bhinnasīso marāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:18.1":{"pli":"Idaṃ pacchimakaṃ mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:18.2":{"pli":"carimo vattate bhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:18.3":{"pli":"Idhāpi ediso mayhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:18.4":{"pli":"maraṇakāle bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:19.1":{"pli":"Pavivekamanuyutto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:19.2":{"pli":"samādhibhāvanārato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:19.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:19.4":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:20.1":{"pli":"Dharaṇimpi sugambhīraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:20.2":{"pli":"bahalaṃ duppadhaṃsiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:20.3":{"pli":"Vāmaṅguṭṭhena khobheyyaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:20.4":{"pli":"iddhiyā pāramiṃ gato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:21.1":{"pli":"Asmimānaṃ na passāmi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:21.2":{"pli":"māno mayhaṃ na vijjati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:21.3":{"pli":"Sāmaṇere upādāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:21.4":{"pli":"garucittaṃ karomahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:22.1":{"pli":"Aparimeyye ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:22.2":{"pli":"yaṃ kammamabhinīhariṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:22.3":{"pli":"Tāhaṃ bhūmimanuppatto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:22.4":{"pli":"pattomhi āsavakkhayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap4:23.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap4:23.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:23.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap4:23.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap4:24.1":{"pli":"Itthaṃ sudaṃ āyasmā mahāmoggallāno thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap4:25.1":{"pli":"Mahāmoggallānattherassāpadānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap40":{"tha-ap40:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap40'><header><ul><li class='division'>{}</li>"},"tha-ap40:0.2":{"pli":"Kuṇḍadhānavagga ","html":"<li>{}</li></ul>"},"tha-ap40:0.3":{"pli":"8 Āyāgadāyakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap40:1.1":{"pli":"“Nibbute lokanāthamhi, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap40:1.2":{"pli":"sikhimhi vadataṃ vare; ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:1.3":{"pli":"Haṭṭho haṭṭhena cittena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:1.4":{"pli":"avandiṃ thūpamuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap40:2.1":{"pli":"Vaḍḍhakīhi kathāpetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap40:2.2":{"pli":"mūlaṃ datvānahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:2.3":{"pli":"Haṭṭho haṭṭhena cittena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:2.4":{"pli":"āyāgaṃ kārapesahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap40:3.1":{"pli":"Aṭṭha kappāni devesu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap40:3.2":{"pli":"abbokiṇṇaṃ vasiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:3.3":{"pli":"Avasesesu kappesu, ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:3.4":{"pli":"vokiṇṇaṃ saṃsariṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap40:4.1":{"pli":"Kāye visaṃ na kamati, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap40:4.2":{"pli":"satthāni na ca hanti me; ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:4.3":{"pli":"Udakehaṃ na miyyāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:4.4":{"pli":"āyāgassa idaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap40:5.1":{"pli":"Yadicchāmi ahaṃ vassaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap40:5.2":{"pli":"mahāmegho pavassati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:5.3":{"pli":"Devāpi me vasaṃ enti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:5.4":{"pli":"puññakammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap40:6.1":{"pli":"Sattaratanasampanno, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap40:6.2":{"pli":"tiṃsakkhattuṃ ahosahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:6.3":{"pli":"Na maṃ kecāvajānanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:6.4":{"pli":"puññakammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap40:7.1":{"pli":"Ekattiṃse ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap40:7.2":{"pli":"āyāgaṃ yamakārayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:7.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:7.4":{"pli":"āyāgassa idaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap40:8.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap40:8.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap40:8.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap40:9.1":{"pli":"Itthaṃ sudaṃ āyasmā āyāgadāyako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap40:10.1":{"pli":"Āyāgadāyakattherassāpadānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap400":{"tha-ap400:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap400'><header><ul><li class='division'>{}</li>"},"tha-ap400:0.2":{"pli":"Pilindavacchavagga ","html":"<li>{}</li></ul>"},"tha-ap400:0.3":{"pli":"8 Saḷalamaṇḍapiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap400:1.1":{"pli":"“Nibbute kakusandhamhi, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap400:1.2":{"pli":"brāhmaṇamhi vusīmati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap400:1.3":{"pli":"Gahetvā saḷalaṃ mālaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap400:1.4":{"pli":"maṇḍapaṃ kārayiṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap400:2.1":{"pli":"Tāvatiṃsagato santo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap400:2.2":{"pli":"labhāmi byamhamuttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap400:2.3":{"pli":"Aññe devetirocāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap400:2.4":{"pli":"puññakammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap400:3.1":{"pli":"Divā vā yadi vā rattiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap400:3.2":{"pli":"caṅkamanto ṭhito cahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap400:3.3":{"pli":"Channo saḷalapupphehi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap400:3.4":{"pli":"puññakammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap400:4.1":{"pli":"Imasmiṃyeva kappamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap400:4.2":{"pli":"yaṃ buddhamabhipūjayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap400:4.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap400:4.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap400:5.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap400:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap400:5.3":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap400:6.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap400:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap400:6.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap400:7.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap400:7.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap400:7.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap400:8.1":{"pli":"Itthaṃ sudaṃ āyasmā saḷalamaṇḍapiyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap400:9.1":{"pli":"Saḷalamaṇḍapiyattherassāpadānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap401":{"tha-ap401:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap401'><header><ul><li class='division'>{}</li>"},"tha-ap401:0.2":{"pli":"Pilindavacchavagga ","html":"<li>{}</li></ul>"},"tha-ap401:0.3":{"pli":"Sabbadāyakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap401:1.1":{"pli":"“Mahāsamuddaṃ ogayha, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap401:1.2":{"pli":"bhavanaṃ me sunimmitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:1.3":{"pli":"Sunimmitā pokkharaṇī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:1.4":{"pli":"cakkavākapakūjitā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:2.1":{"pli":"Mandālakehi sañchannā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:2.2":{"pli":"padumuppalakehi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:2.3":{"pli":"Nadī ca sandate tattha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:2.4":{"pli":"supatitthā manoramā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:3.1":{"pli":"Macchakacchapasañchannā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:3.2":{"pli":"nānādijasamotthatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:3.3":{"pli":"Mayūrakoñcābhirudā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:3.4":{"pli":"kokilādīhi vagguhi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:4.1":{"pli":"Pārevatā ravihaṃsā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:4.2":{"pli":"cakkavākā nadīcarā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:4.3":{"pli":"Dindibhā sāḷikā cettha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:4.4":{"pli":"pampakā jīvajīvakā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:5.1":{"pli":"Haṃsā koñcāpi naditā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:5.2":{"pli":"kosiyā piṅgalā bahū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:5.3":{"pli":"Sattaratanasampannā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:5.4":{"pli":"maṇimuttikavālukā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:6.1":{"pli":"Sabbasoṇṇamayā rukkhā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:6.2":{"pli":"nānāgandhasameritā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:6.3":{"pli":"Ujjotenti divārattiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:6.4":{"pli":"bhavanaṃ sabbakālikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:7.1":{"pli":"Saṭṭhi tūriyasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:7.2":{"pli":"sāyaṃ pāto pavajjare; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:7.3":{"pli":"Soḷasitthisahassāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:7.4":{"pli":"parivārenti maṃ sadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:8.1":{"pli":"Abhinikkhamma bhavanā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:8.2":{"pli":"sumedhaṃ lokanāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:8.3":{"pli":"Pasannacitto sumano, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:8.4":{"pli":"vandayiṃ taṃ mahāyasaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:9.1":{"pli":"Sambuddhaṃ abhivādetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:9.2":{"pli":"sasaṅghaṃ taṃ nimantayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:9.3":{"pli":"Adhivāsesi so dhīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:9.4":{"pli":"sumedho lokanāyako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:10.1":{"pli":"Mama dhammakathaṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:10.2":{"pli":"uyyojesi mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:10.3":{"pli":"Sambuddhaṃ abhivādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:10.4":{"pli":"bhavanaṃ me upāgamiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:11.1":{"pli":"Āmantayiṃ parijanaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:11.2":{"pli":"‘sabbe sannipatātha vo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:11.3":{"pli":"Pubbaṇhasamayaṃ buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:11.4":{"pli":"bhavanaṃ āgamissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:12.1":{"pli":"Lābhā amhaṃ suladdhaṃ no, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:12.2":{"pli":"ye vasāma tavantike; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:12.3":{"pli":"Mayampi buddhaseṭṭhassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:12.4":{"pli":"pūjaṃ kassāma satthuno’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:13.1":{"pli":"Annapānaṃ paṭṭhapetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:13.2":{"pli":"kālaṃ ārocayiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:13.3":{"pli":"Vasīsatasahassehi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:13.4":{"pli":"upesi lokanāyako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:14.1":{"pli":"Pañcaṅgikehi tūriyehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:14.2":{"pli":"paccuggamanamakāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:14.3":{"pli":"Sabbasoṇṇamaye pīṭhe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:14.4":{"pli":"nisīdi purisuttamo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:15.1":{"pli":"Uparicchadanaṃ āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:15.2":{"pli":"sabbasoṇṇamayaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:15.3":{"pli":"Bījaniyo pavāyanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:15.4":{"pli":"bhikkhusaṅghassa antare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:16.1":{"pli":"Pahūtenannapānena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:16.2":{"pli":"bhikkhusaṅghamatappayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:16.3":{"pli":"Paccekadussayugaḷe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:16.4":{"pli":"bhikkhusaṅghassadāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:17.1":{"pli":"Yaṃ vadanti sumedhoti, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:17.2":{"pli":"lokāhutipaṭiggahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:17.3":{"pli":"Bhikkhusaṅghe nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:17.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:18.1":{"pli":"‘Yo me annena pānena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:18.2":{"pli":"sabbe ime ca tappayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:18.3":{"pli":"Tamahaṃ kittayissāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:18.4":{"pli":"suṇātha mama bhāsato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:19.1":{"pli":"Aṭṭhārase kappasate, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:19.2":{"pli":"devaloke ramissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:19.3":{"pli":"Sahassakkhattuṃ rājā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:19.4":{"pli":"cakkavattī bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:20.1":{"pli":"Upapajjati yaṃ yoniṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:20.2":{"pli":"devattaṃ atha mānusaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:20.3":{"pli":"Sabbadā sabbasovaṇṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:20.4":{"pli":"chadanaṃ dhārayissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:21.1":{"pli":"Tiṃsakappasahassamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:21.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:21.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:21.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:22.1":{"pli":"Tassa dhammesu dāyādo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:22.2":{"pli":"oraso dhammanimmito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:22.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:22.4":{"pli":"nibbāyissatināsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:23.1":{"pli":"Bhikkhusaṅghe nisīditvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:23.2":{"pli":"sīhanādaṃ nadissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:23.3":{"pli":"Citake chattaṃ dhārenti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:23.4":{"pli":"heṭṭhā chattamhi ḍayhatha’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:24.1":{"pli":"Sāmaññaṃ me anuppattaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:24.2":{"pli":"kilesā jhāpitā mayā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:24.3":{"pli":"Maṇḍape rukkhamūle vā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:24.4":{"pli":"santāpo me na vijjati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:25.1":{"pli":"Tiṃsakappasahassamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:25.2":{"pli":"yaṃ dānamadadiṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:25.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:25.4":{"pli":"sabbadānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:26.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:26.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:26.3":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:27.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:27.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:27.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap401:28.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap401:28.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap401:28.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap401:29.1":{"pli":"Itthaṃ sudaṃ āyasmā sabbadāyako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap401:30.1":{"pli":"Sabbadāyakattherassāpadānaṃ navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}