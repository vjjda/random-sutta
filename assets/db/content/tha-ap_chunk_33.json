{"tha-ap409":{"tha-ap409:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap409'><header><ul><li class='division'>{}</li>"},"tha-ap409:0.2":{"pli":"Metteyyavagga ","html":"<li>{}</li></ul>"},"tha-ap409:0.3":{"pli":"Hemakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap409:1.1":{"pli":"“Pabbhārakūṭaṃ nissāya, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap409:1.2":{"pli":"anomo nāma tāpaso; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:1.3":{"pli":"Assamaṃ sukataṃ katvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:1.4":{"pli":"paṇṇasāle vasī tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:2.1":{"pli":"Siddhaṃ tassa tapo kammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:2.2":{"pli":"siddhipatto sake bale; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:2.3":{"pli":"Sakasāmaññavikkanto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:2.4":{"pli":"ātāpī nipako muni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:3.1":{"pli":"Visārado sasamaye, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:3.2":{"pli":"paravāde ca kovido; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:3.3":{"pli":"Paṭṭho bhūmantalikkhamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:3.4":{"pli":"uppātamhi ca kovido. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:4.1":{"pli":"Vītasoko nirārambho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:4.2":{"pli":"appāhāro alolupo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:4.3":{"pli":"Lābhālābhena santuṭṭho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:4.4":{"pli":"jhāyī jhānarato muni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:5.1":{"pli":"Piyadassī nāma sambuddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:5.2":{"pli":"aggo kāruṇiko muni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:5.3":{"pli":"Satte tāretukāmo so, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:5.4":{"pli":"karuṇāya pharī tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:6.1":{"pli":"Bodhaneyyaṃ janaṃ disvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:6.2":{"pli":"piyadassī mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:6.3":{"pli":"Cakkavāḷasahassampi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:6.4":{"pli":"gantvā ovadate muni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:7.1":{"pli":"Mamuddharitukāmo so, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:7.2":{"pli":"mamassamamupāgami; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:7.3":{"pli":"Na diṭṭho me jino pubbe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:7.4":{"pli":"na sutopi ca kassaci. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:8.1":{"pli":"Uppātā supinā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:8.2":{"pli":"lakkhaṇā suppakāsitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:8.3":{"pli":"Paṭṭho bhūmantalikkhamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:8.4":{"pli":"nakkhattapadakovido. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:9.1":{"pli":"Sohaṃ buddhassa sutvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:9.2":{"pli":"tattha cittaṃ pasādayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:9.3":{"pli":"Tiṭṭhanto vā nisinno vā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:9.4":{"pli":"sarāmi niccakālikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:10.1":{"pli":"Mayi evaṃ sarantamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:10.2":{"pli":"bhagavāpi anussari; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:10.3":{"pli":"Buddhaṃ anussarantassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:10.4":{"pli":"pīti me hoti tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:11.1":{"pli":"Kālañca punarāgamma, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:11.2":{"pli":"upesi maṃ mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:11.3":{"pli":"Sampattepi na jānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:11.4":{"pli":"ayaṃ buddho mahāmuni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:12.1":{"pli":"Anukampako kāruṇiko, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:12.2":{"pli":"piyadassī mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:12.3":{"pli":"Sañjānāpesi attānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:12.4":{"pli":"‘ahaṃ buddho sadevake’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:13.1":{"pli":"Sañjānitvāna sambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:13.2":{"pli":"piyadassiṃ mahāmuniṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:13.3":{"pli":"Sakaṃ cittaṃ pasādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:13.4":{"pli":"idaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:14.1":{"pli":"‘Aññe pīṭhe ca pallaṅke, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:14.2":{"pli":"āsandīsu nisīdare; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:14.3":{"pli":"Tuvampi sabbadassāvī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:14.4":{"pli":"nisīda ratanāsane’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:15.1":{"pli":"Sabbaratanamayaṃ pīṭhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:15.2":{"pli":"nimminitvāna tāvade; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:15.3":{"pli":"Piyadassissa munino, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:15.4":{"pli":"adāsiṃ iddhinimmitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:16.1":{"pli":"Ratane ca nisinnassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:16.2":{"pli":"pīṭhake iddhinimmite; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:16.3":{"pli":"Kumbhamattaṃ jambuphalaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:16.4":{"pli":"adāsiṃ tāvade ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:17.1":{"pli":"Mama hāsaṃ janetvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:17.2":{"pli":"paribhuñji mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:17.3":{"pli":"Tadā cittaṃ pasādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:17.4":{"pli":"satthāraṃ abhivādayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:18.1":{"pli":"Piyadassī tu bhagavā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:18.2":{"pli":"lokajeṭṭho narāsabho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:18.3":{"pli":"Ratanāsanamāsīno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:18.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:19.1":{"pli":"‘Yo me ratanamayaṃ pīṭhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:19.2":{"pli":"amatañca phalaṃ adā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:19.3":{"pli":"Tamahaṃ kittayissāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:19.4":{"pli":"suṇātha mama bhāsato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:20.1":{"pli":"Sattasattati kappāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:20.2":{"pli":"devaloke ramissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:20.3":{"pli":"Pañcasattatikkhattuñca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:20.4":{"pli":"cakkavattī bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:21.1":{"pli":"Dvattiṃsakkhattuṃ devindo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:21.2":{"pli":"devarajjaṃ karissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:21.3":{"pli":"Padesarajjaṃ vipulaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:21.4":{"pli":"gaṇanāto asaṅkhiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:22.1":{"pli":"Soṇṇamayaṃ rūpimayaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:22.2":{"pli":"pallaṅkaṃ sukataṃ bahuṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:22.3":{"pli":"Lohitaṅgamayañceva, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:22.4":{"pli":"lacchati ratanāmayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:23.1":{"pli":"Caṅkamantampi manujaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:23.2":{"pli":"puññakammasamaṅginaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:23.3":{"pli":"Pallaṅkāni anekāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:23.4":{"pli":"parivāressare tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:24.1":{"pli":"Kūṭāgārā ca pāsādā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:24.2":{"pli":"sayanañca mahārahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:24.3":{"pli":"Imassa cittamaññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:24.4":{"pli":"nibbattissanti tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:25.1":{"pli":"Saṭṭhi nāgasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:25.2":{"pli":"sabbālaṅkārabhūsitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:25.3":{"pli":"Suvaṇṇakacchā mātaṅgā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:25.4":{"pli":"hemakappanavāsasā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:26.1":{"pli":"Ārūḷhā gāmaṇīyehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:26.2":{"pli":"tomaraṅkusapāṇibhi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:26.3":{"pli":"Imaṃ paricarissanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:26.4":{"pli":"ratnapīṭhassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:27.1":{"pli":"Saṭṭhi assasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:27.2":{"pli":"sabbālaṅkārabhūsitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:27.3":{"pli":"Ājānīyāva jātiyā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:27.4":{"pli":"sindhavā sīghavāhino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:28.1":{"pli":"Ārūḷhā gāmaṇīyehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:28.2":{"pli":"illiyācāpadhāribhi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:28.3":{"pli":"Tepimaṃ paricarissanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:28.4":{"pli":"ratnapīṭhassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:29.1":{"pli":"Saṭṭhi rathasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:29.2":{"pli":"sabbālaṅkārabhūsitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:29.3":{"pli":"Dīpā athopi veyagghā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:29.4":{"pli":"sannaddhā ussitaddhajā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:30.1":{"pli":"Ārūḷhā gāmaṇīyehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:30.2":{"pli":"cāpahatthehi vammibhi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:30.3":{"pli":"Parivāressantimaṃ niccaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:30.4":{"pli":"ratnapīṭhassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:31.1":{"pli":"Saṭṭhi dhenusahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:31.2":{"pli":"dohaññā puṅgavūsabhe; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:31.3":{"pli":"Vacchake janayissanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:31.4":{"pli":"ratnapīṭhassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:32.1":{"pli":"Soḷasitthisahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:32.2":{"pli":"sabbālaṅkārabhūsitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:32.3":{"pli":"Vicittavatthābharaṇā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:32.4":{"pli":"āmukkamaṇikuṇḍalā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:33.1":{"pli":"Aḷārapamhā hasulā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:33.2":{"pli":"susaññā tanumajjhimā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:33.3":{"pli":"Parivāressantimaṃ niccaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:33.4":{"pli":"ratnapīṭhassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:34.1":{"pli":"Aṭṭhārase kappasate, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:34.2":{"pli":"gotamo nāma cakkhumā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:34.3":{"pli":"Tamandhakāraṃ vidhamitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:34.4":{"pli":"buddho loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:35.1":{"pli":"Tassa dassanamāgamma, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:35.2":{"pli":"pabbajissatikiñcano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:35.3":{"pli":"Tosayitvāna satthāraṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:35.4":{"pli":"sāsanebhiramissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:36.1":{"pli":"Tassa dhammaṃ suṇitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:36.2":{"pli":"kilese ghātayissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:36.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:36.4":{"pli":"nibbāyissatināsavo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:37.1":{"pli":"Vīriyaṃ me dhuradhorayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:37.2":{"pli":"yogakkhemādhivāhanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:37.3":{"pli":"Uttamatthaṃ patthayanto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:37.4":{"pli":"sāsane viharāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:38.1":{"pli":"Idaṃ pacchimakaṃ mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:38.2":{"pli":"carimo vattate bhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:38.3":{"pli":"Sabbāsavā parikkhīṇā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:38.4":{"pli":"natthi dāni punabbhavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:39.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:39.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:39.3":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:40.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:40.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:40.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap409:41.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap409:41.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap409:41.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap409:42.1":{"pli":"Itthaṃ sudaṃ āyasmā hemako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap409:43.1":{"pli":"Hemakattherassāpadānaṃ sattamaṃ. ","html":"<p class='endsutta'>{}<br>"},"tha-ap409:43.2":{"pli":"Sattarasamaṃ bhāṇavāraṃ. ","html":"{}</p></article>"}},"tha-ap41":{"tha-ap41:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap41'><header><ul><li class='division'>{}</li>"},"tha-ap41:0.2":{"pli":"Kuṇḍadhānavagga ","html":"<li>{}</li></ul>"},"tha-ap41:0.3":{"pli":"Dhammacakkikattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap41:1.1":{"pli":"“Siddhatthassa bhagavato, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap41:1.2":{"pli":"sīhāsanassa sammukhā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap41:1.3":{"pli":"Dhammacakkaṃ me ṭhapitaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap41:1.4":{"pli":"sukataṃ viññuvaṇṇitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap41:2.1":{"pli":"Cāruvaṇṇova sobhāmi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap41:2.2":{"pli":"sayoggabalavāhano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap41:2.3":{"pli":"Parivārenti maṃ niccaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap41:2.4":{"pli":"anuyantā bahujjanā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap41:3.1":{"pli":"Saṭṭhitūriyasahassehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap41:3.2":{"pli":"paricāremahaṃ sadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap41:3.3":{"pli":"Parivārena sobhāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap41:3.4":{"pli":"puññakammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap41:4.1":{"pli":"Catunnavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap41:4.2":{"pli":"yaṃ cakkaṃ ṭhapayiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap41:4.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap41:4.4":{"pli":"dhammacakkassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap41:5.1":{"pli":"Ito ekādase kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap41:5.2":{"pli":"aṭṭhāsiṃsu janādhipā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap41:5.3":{"pli":"Sahassarājanāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap41:5.4":{"pli":"cakkavattī mahabbalā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap41:6.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap41:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap41:6.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap41:7.1":{"pli":"Itthaṃ sudaṃ āyasmā dhammacakkiko thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap41:8.1":{"pli":"Dhammacakkikattherassāpadānaṃ navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap410":{"tha-ap410:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap410'><header><ul><li class='division'>{}</li>"},"tha-ap410:0.2":{"pli":"Metteyyavagga ","html":"<li>{}</li></ul>"},"tha-ap410:0.3":{"pli":"Todeyyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap410:1.1":{"pli":"“Rājāsi vijayo nāma, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap410:1.2":{"pli":"ketumatīpuruttame; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:1.3":{"pli":"Sūro vikkamasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:1.4":{"pli":"puramajjhāvasī tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:2.1":{"pli":"Tassa rañño pamattassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:2.2":{"pli":"aṭaviyo samuṭṭhahuṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:2.3":{"pli":"Otārā tuṇḍikā ceva, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:2.4":{"pli":"raṭṭhaṃ viddhaṃsayuṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:3.1":{"pli":"Paccante kupite khippaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:3.2":{"pli":"sannipātesirindamo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:3.3":{"pli":"Bhaṭe ceva balatthe ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:3.4":{"pli":"ariṃ niggāhayi tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:4.1":{"pli":"Hatthārohā anīkaṭṭhā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:4.2":{"pli":"sūrā ca cammayodhino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:4.3":{"pli":"Dhanuggahā ca uggā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:4.4":{"pli":"sabbe sannipatuṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:5.1":{"pli":"Āḷārikā ca kappakā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:5.2":{"pli":"nhāpakā mālakārakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:5.3":{"pli":"Sūrā vijitasaṅgāmā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:5.4":{"pli":"sabbe sannipatuṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:6.1":{"pli":"Khaggahatthā ca purisā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:6.2":{"pli":"cāpahatthā ca vammino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:6.3":{"pli":"Luddā vijitasaṅgāmā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:6.4":{"pli":"sabbe sannipatuṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:7.1":{"pli":"Tidhāpabhinnā mātaṅgā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:7.2":{"pli":"kuñjarā saṭṭhihāyanā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:7.3":{"pli":"Suvaṇṇakacchālaṅkārā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:7.4":{"pli":"sabbe sannipatuṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:8.1":{"pli":"Khamā sītassa uṇhassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:8.2":{"pli":"ukkāruharaṇassa ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:8.3":{"pli":"Yodhājīvā katakammā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:8.4":{"pli":"sabbe sannipatuṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:9.1":{"pli":"Saṅkhasaddaṃ bherisaddaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:9.2":{"pli":"atho utujasaddakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:9.3":{"pli":"Etehi te hāsayantā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:9.4":{"pli":"sabbe sannipatuṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:10.1":{"pli":"Tisūlakontimantehi ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:10.2":{"pli":"kavacatomarehi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:10.3":{"pli":"Koṭṭentānaṃ nipātentā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:10.4":{"pli":"sabbe sannipatuṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:11.1":{"pli":"Kimevātinisāmetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:11.2":{"pli":"sarājā ajitaṃ jino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:11.3":{"pli":"Saṭṭhi pāṇasahassāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:11.4":{"pli":"sūle uttāsayiṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:12.1":{"pli":"Saddaṃ mānusakākaṃsu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:12.2":{"pli":"‘aho rājā adhammiko; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:12.3":{"pli":"Niraye paccamānassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:12.4":{"pli":"kadā anto bhavissati’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:13.1":{"pli":"Sayanehaṃ tuvaṭṭento, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:13.2":{"pli":"passāmi niraye tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:13.3":{"pli":"Na supāmi divārattiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:13.4":{"pli":"sūlena tajjayanti maṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:14.1":{"pli":"Kiṃ pamādena rajjena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:14.2":{"pli":"vāhanena balena ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:14.3":{"pli":"Na te pahonti dhāretuṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:14.4":{"pli":"tāpayanti mamaṃ sadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:15.1":{"pli":"Kiṃ me puttehi dārehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:15.2":{"pli":"rajjena sakalena ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:15.3":{"pli":"Yannūna pabbajeyyāhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:15.4":{"pli":"gatimaggaṃ visodhaye. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:16.1":{"pli":"Saṭṭhi nāgasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:16.2":{"pli":"sabbālaṅkārabhūsite; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:16.3":{"pli":"Suvaṇṇakacche mātaṅge, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:16.4":{"pli":"hemakappanavāsase. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:17.1":{"pli":"Ārūḷhe gāmaṇīyehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:17.2":{"pli":"tomaraṅkusapāṇibhi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:17.3":{"pli":"Saṅgāmāvacare ṭhāne, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:17.4":{"pli":"anapekkho vihāyahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:17.5":{"pli":"Sakakammena santatto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:17.6":{"pli":"nikkhamiṃ anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:18.1":{"pli":"Saṭṭhi assasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:18.2":{"pli":"sabbālaṅkārabhūsite; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:18.3":{"pli":"Ājānīyeva jātiyā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:18.4":{"pli":"sindhave sīghavāhane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:19.1":{"pli":"Ārūḷhe gāmaṇīyehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:19.2":{"pli":"cāpahatthehi vammibhi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:19.3":{"pli":"Pahāretvāna te sabbe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:19.4":{"pli":"nikkhamiṃ anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:20.1":{"pli":"Saṭṭhi rathasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:20.2":{"pli":"sabbālaṅkārabhūsite; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:20.3":{"pli":"Dīpe athopi veyagghe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:20.4":{"pli":"sannaddhe ussitaddhaje; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:20.5":{"pli":"Te sabbe parihāretvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:20.6":{"pli":"pabbajiṃ anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:21.1":{"pli":"Saṭṭhi dhenusahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:21.2":{"pli":"sabbā kaṃsūpadhāraṇā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:21.3":{"pli":"Tāyopi chaḍḍayitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:21.4":{"pli":"pabbajiṃ anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:22.1":{"pli":"Saṭṭhi itthisahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:22.2":{"pli":"sabbālaṅkārabhūsitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:22.3":{"pli":"Vicittavatthābharaṇā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:22.4":{"pli":"āmukkamaṇikuṇḍalā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:23.1":{"pli":"Aḷārapamhā hasulā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:23.2":{"pli":"susaññā tanumajjhimā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:23.3":{"pli":"Tā hitvā kandamānāyo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:23.4":{"pli":"pabbajiṃ anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:24.1":{"pli":"Saṭṭhi gāmasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:24.2":{"pli":"paripuṇṇāni sabbaso; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:24.3":{"pli":"Chaḍḍayitvāna taṃ rajjaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:24.4":{"pli":"pabbajiṃ anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:25.1":{"pli":"Nagarā nikkhamitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:25.2":{"pli":"himavantamupāgamiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:25.3":{"pli":"Bhāgīrathīnadītīre, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:25.4":{"pli":"assamaṃ māpayiṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:26.1":{"pli":"Paṇṇasālaṃ karitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:26.2":{"pli":"Agyāgāraṃ akāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:26.3":{"pli":"Āraddhavīriyo pahitatto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:26.4":{"pli":"Vasāmi assame ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:27.1":{"pli":"Maṇḍape rukkhamūle vā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:27.2":{"pli":"suññāgāre ca jhāyato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:27.3":{"pli":"Na tu vijjati tāso me, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:27.4":{"pli":"na passe bhayabheravaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:28.1":{"pli":"Sumedho nāma sambuddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:28.2":{"pli":"aggo kāruṇiko muni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:28.3":{"pli":"Ñāṇālokena jotanto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:28.4":{"pli":"loke uppajji tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:29.1":{"pli":"Mama assamasāmantā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:29.2":{"pli":"yakkho āsi mahiddhiko; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:29.3":{"pli":"Buddhaseṭṭhamhi uppanne, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:29.4":{"pli":"ārocesi mamaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:30.1":{"pli":"‘Buddho loke samuppanno, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:30.2":{"pli":"sumedho nāma cakkhumā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:30.3":{"pli":"Tāreti janataṃ sabbaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:30.4":{"pli":"tampi so tārayissati’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:31.1":{"pli":"Yakkhassa vacanaṃ sutvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:31.2":{"pli":"saṃviggo āsi tāvade; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:31.3":{"pli":"Buddho buddhoti cintento, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:31.4":{"pli":"assamaṃ paṭisāmayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:32.1":{"pli":"Aggidāruñca chaḍḍetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:32.2":{"pli":"saṃsāmetvāna santhataṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:32.3":{"pli":"Assamaṃ abhivanditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:32.4":{"pli":"nikkhamiṃ vipinā ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:33.1":{"pli":"Tato candanamādāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:33.2":{"pli":"gāmā gāmaṃ purā puraṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:33.3":{"pli":"Devadevaṃ gavesanto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:33.4":{"pli":"upagacchiṃ vināyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:34.1":{"pli":"Bhagavā tamhi samaye, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:34.2":{"pli":"sumedho lokanāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:34.3":{"pli":"Catusaccaṃ pakāsento, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:34.4":{"pli":"bodheti janataṃ bahuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:35.1":{"pli":"Añjaliṃ paggahetvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:35.2":{"pli":"sīse katvāna candanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:35.3":{"pli":"Sambuddhaṃ abhivādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:35.4":{"pli":"imā gāthā abhāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:36.1":{"pli":"‘Vassike pupphamānamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:36.2":{"pli":"santike upavāyati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:36.3":{"pli":"Tvaṃ vīra guṇagandhena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:36.4":{"pli":"disā sabbā pavāyasi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:37.1":{"pli":"Campake nāgavanike, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:37.2":{"pli":"atimuttakaketake; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:37.3":{"pli":"Sālesu pupphamānesu, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:37.4":{"pli":"anuvātaṃ pavāyati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:38.1":{"pli":"Tava gandhaṃ suṇitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:38.2":{"pli":"himavantā idhāgamiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:38.3":{"pli":"Pūjemi taṃ mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:38.4":{"pli":"lokajeṭṭha mahāyasa’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:39.1":{"pli":"Varacandanenānulimpiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:39.2":{"pli":"sumedhaṃ lokanāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:39.3":{"pli":"Sakaṃ cittaṃ pasādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:39.4":{"pli":"tuṇhī aṭṭhāsi tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:40.1":{"pli":"Sumedho nāma bhagavā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:40.2":{"pli":"lokajeṭṭho narāsabho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:40.3":{"pli":"Bhikkhusaṅghe nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:40.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:41.1":{"pli":"‘Yo me guṇe pakittesi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:41.2":{"pli":"candanañca apūjayi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:41.3":{"pli":"Tamahaṃ kittayissāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:41.4":{"pli":"suṇātha mama bhāsato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:42.1":{"pli":"Ādeyyavākyavacano, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:42.2":{"pli":"brahmā uju patāpavā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:42.3":{"pli":"Pañcavīsatikappāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:42.4":{"pli":"sappabhāso bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:43.1":{"pli":"Chabbīsatikappasate, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:43.2":{"pli":"devaloke ramissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:43.3":{"pli":"Sahassakkhattuṃ rājā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:43.4":{"pli":"cakkavattī bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:44.1":{"pli":"Tettiṃsakkhattuṃ devindo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:44.2":{"pli":"devarajjaṃ karissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:44.3":{"pli":"Padesarajjaṃ vipulaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:44.4":{"pli":"gaṇanāto asaṅkhiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:45.1":{"pli":"Tato cutoyaṃ manujo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:45.2":{"pli":"manussattaṃ gamissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:45.3":{"pli":"Puññakammena saṃyutto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:45.4":{"pli":"brahmabandhu bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:46.1":{"pli":"Ajjhāyako mantadharo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:46.2":{"pli":"tiṇṇaṃ vedāna pāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:46.3":{"pli":"Tilakkhaṇena sampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:46.4":{"pli":"bāvarī nāma brāhmaṇo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:47.1":{"pli":"Tassa sisso bhavitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:47.2":{"pli":"hessati mantapāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:47.3":{"pli":"Upagantvāna sambuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:47.4":{"pli":"gotamaṃ sakyapuṅgavaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:48.1":{"pli":"Pucchitvā nipuṇe pañhe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:48.2":{"pli":"bhāvayitvāna añjasaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:48.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:48.4":{"pli":"viharissatināsavo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:49.1":{"pli":"Tividhaggi nibbutā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:49.2":{"pli":"bhavā sabbe samūhatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:49.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:49.4":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:50.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:50.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:50.3":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:51.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:51.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:51.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap410:52.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap410:52.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap410:52.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap410:53.1":{"pli":"Itthaṃ sudaṃ āyasmā todeyyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap410:54.1":{"pli":"Todeyyattherassāpadānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}