{"vv50":{"vv50:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv50'><header><ul><li class='division'>{}</li>"},"vv50:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv50:0.3":{"pli":"Mañjiṭṭhakavagga ","html":"<li>{}</li></ul>"},"vv50:0.4":{"pli":"12. Rajjumālāvimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv50:1.1":{"pli":"“Abhikkantena vaṇṇena, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv50:1.2":{"pli":"yā tvaṃ tiṭṭhasi devate; ","html":"<span class='verse-line'>{}</span>"},"vv50:1.3":{"pli":"Hatthapāde ca viggayha, ","html":"<span class='verse-line'>{}</span>"},"vv50:1.4":{"pli":"naccasi suppavādite. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:2.1":{"pli":"Tassā te naccamānāya, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:2.2":{"pli":"aṅgamaṅgehi sabbaso; ","html":"<span class='verse-line'>{}</span>"},"vv50:2.3":{"pli":"Dibbā saddā niccharanti, ","html":"<span class='verse-line'>{}</span>"},"vv50:2.4":{"pli":"savanīyā manoramā. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:3.1":{"pli":"Tassā te naccamānāya, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:3.2":{"pli":"aṅgamaṅgehi sabbaso; ","html":"<span class='verse-line'>{}</span>"},"vv50:3.3":{"pli":"Dibbā gandhā pavāyanti, ","html":"<span class='verse-line'>{}</span>"},"vv50:3.4":{"pli":"sucigandhā manoramā. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:4.1":{"pli":"Vivattamānā kāyena, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:4.2":{"pli":"yā veṇīsu piḷandhanā; ","html":"<span class='verse-line'>{}</span>"},"vv50:4.3":{"pli":"Tesaṃ suyyati nigghoso, ","html":"<span class='verse-line'>{}</span>"},"vv50:4.4":{"pli":"tūriye pañcaṅgike yathā. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:5.1":{"pli":"Vaṭaṃsakā vātadhutā, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:5.2":{"pli":"vātena sampakampitā; ","html":"<span class='verse-line'>{}</span>"},"vv50:5.3":{"pli":"Tesaṃ suyyati nigghoso, ","html":"<span class='verse-line'>{}</span>"},"vv50:5.4":{"pli":"tūriye pañcaṅgike yathā. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:6.1":{"pli":"Yāpi te sirasmiṃ mālā, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:6.2":{"pli":"sucigandhā manoramā; ","html":"<span class='verse-line'>{}</span>"},"vv50:6.3":{"pli":"Vāti gandho disā sabbā, ","html":"<span class='verse-line'>{}</span>"},"vv50:6.4":{"pli":"rukkho mañjūsako yathā. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:7.1":{"pli":"Ghāyase taṃ sucigandhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:7.2":{"pli":"rūpaṃ passasi amānusaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv50:7.3":{"pli":"Devate pucchitācikkha, ","html":"<span class='verse-line'>{}</span>"},"vv50:7.4":{"pli":"kissa kammassidaṃ phalan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:8.1":{"pli":"“Dāsī ahaṃ pure āsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:8.2":{"pli":"gayāyaṃ brāhmaṇassahaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv50:8.3":{"pli":"Appapuññā alakkhikā, ","html":"<span class='verse-line'>{}</span>"},"vv50:8.4":{"pli":"rajjumālāti maṃ viduṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:9.1":{"pli":"Akkosānaṃ vadhānañca, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:9.2":{"pli":"tajjanāya ca uggatā; ","html":"<span class='verse-line'>{}</span>"},"vv50:9.3":{"pli":"Kuṭaṃ gahetvā nikkhamma, ","html":"<span class='verse-line'>{}</span>"},"vv50:9.4":{"pli":"agañchiṃ udahāriyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:10.1":{"pli":"Vipathe kuṭaṃ nikkhipitvā, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:10.2":{"pli":"vanasaṇḍaṃ upāgamiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv50:10.3":{"pli":"Idhevāhaṃ marissāmi, ","html":"<span class='verse-line'>{}</span>"},"vv50:10.4":{"pli":"ko attho jīvitena me. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:11.1":{"pli":"Daḷhaṃ pāsaṃ karitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:11.2":{"pli":"āsumbhitvāna pādape; ","html":"<span class='verse-line'>{}</span>"},"vv50:11.3":{"pli":"Tato disā vilokesiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv50:11.4":{"pli":"‘ko nu kho vanamassito’. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:12.1":{"pli":"Tatthaddasāsiṃ sambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:12.2":{"pli":"sabbalokahitaṃ muniṃ; ","html":"<span class='verse-line'>{}</span>"},"vv50:12.3":{"pli":"Nisinnaṃ rukkhamūlasmiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv50:12.4":{"pli":"jhāyantaṃ akutobhayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:13.1":{"pli":"Tassā me ahu saṃvego, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:13.2":{"pli":"abbhuto lomahaṃsano; ","html":"<span class='verse-line'>{}</span>"},"vv50:13.3":{"pli":"Ko nu kho vanamassito, ","html":"<span class='verse-line'>{}</span>"},"vv50:13.4":{"pli":"manusso udāhu devatā. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:14.1":{"pli":"Pāsādikaṃ pasādanīyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:14.2":{"pli":"vanā nibbanamāgataṃ; ","html":"<span class='verse-line'>{}</span>"},"vv50:14.3":{"pli":"Disvā mano me pasīdi, ","html":"<span class='verse-line'>{}</span>"},"vv50:14.4":{"pli":"nāyaṃ yādisakīdiso. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:15.1":{"pli":"Guttindriyo jhānarato, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:15.2":{"pli":"abahiggatamānaso; ","html":"<span class='verse-line'>{}</span>"},"vv50:15.3":{"pli":"Hito sabbassa lokassa, ","html":"<span class='verse-line'>{}</span>"},"vv50:15.4":{"pli":"buddho ayaṃ bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:16.1":{"pli":"Bhayabheravo durāsado, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:16.2":{"pli":"sīhova guhamassito; ","html":"<span class='verse-line'>{}</span>"},"vv50:16.3":{"pli":"Dullabhāyaṃ dassanāya, ","html":"<span class='verse-line'>{}</span>"},"vv50:16.4":{"pli":"pupphaṃ odumbaraṃ yathā. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:17.1":{"pli":"So maṃ mudūhi vācāhi, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:17.2":{"pli":"ālapitvā tathāgato; ","html":"<span class='verse-line'>{}</span>"},"vv50:17.3":{"pli":"Rajjumāleti maṃvoca, ","html":"<span class='verse-line'>{}</span>"},"vv50:17.4":{"pli":"saraṇaṃ gaccha tathāgataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:18.1":{"pli":"Tāhaṃ giraṃ suṇitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:18.2":{"pli":"nelaṃ atthavatiṃ suciṃ; ","html":"<span class='verse-line'>{}</span>"},"vv50:18.3":{"pli":"Saṇhaṃ muduñca vagguñca, ","html":"<span class='verse-line'>{}</span>"},"vv50:18.4":{"pli":"sabbasokāpanūdanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:19.1":{"pli":"Kallacittañca maṃ ñatvā, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:19.2":{"pli":"pasannaṃ suddhamānasaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv50:19.3":{"pli":"Hito sabbassa lokassa, ","html":"<span class='verse-line'>{}</span>"},"vv50:19.4":{"pli":"anusāsi tathāgato. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:20.1":{"pli":"Idaṃ dukkhanti maṃvoca, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:20.2":{"pli":"Ayaṃ dukkhassa sambhavo; ","html":"<span class='verse-line'>{}</span>"},"vv50:20.3":{"pli":"Dukkha nirodho maggo ca, ","html":"<span class='verse-line'>{}</span>"},"vv50:20.4":{"pli":"Añjaso amatogadho. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:21.1":{"pli":"Anukampakassa kusalassa, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:21.2":{"pli":"Ovādamhi ahaṃ ṭhitā; ","html":"<span class='verse-line'>{}</span>"},"vv50:21.3":{"pli":"Ajjhagā amataṃ santiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv50:21.4":{"pli":"Nibbānaṃ padamaccutaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:22.1":{"pli":"Sāhaṃ avaṭṭhitāpemā, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:22.2":{"pli":"dassane avikampinī; ","html":"<span class='verse-line'>{}</span>"},"vv50:22.3":{"pli":"Mūlajātāya saddhāya, ","html":"<span class='verse-line'>{}</span>"},"vv50:22.4":{"pli":"dhītā buddhassa orasā. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:23.1":{"pli":"Sāhaṃ ramāmi kīḷāmi, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:23.2":{"pli":"modāmi akutobhayā; ","html":"<span class='verse-line'>{}</span>"},"vv50:23.3":{"pli":"Dibbamālaṃ dhārayāmi, ","html":"<span class='verse-line'>{}</span>"},"vv50:23.4":{"pli":"pivāmi madhumaddavaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:24.1":{"pli":"Saṭṭhitūriyasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:24.2":{"pli":"paṭibodhaṃ karonti me; ","html":"<span class='verse-line'>{}</span>"},"vv50:24.3":{"pli":"Āḷambo gaggaro bhīmo, ","html":"<span class='verse-line'>{}</span>"},"vv50:24.4":{"pli":"sādhuvādī ca saṃsayo. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:25.1":{"pli":"Pokkharo ca suphasso ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:25.2":{"pli":"vīṇāmokkhā ca nāriyo; ","html":"<span class='verse-line'>{}</span>"},"vv50:25.3":{"pli":"Nandā ceva sunandā ca, ","html":"<span class='verse-line'>{}</span>"},"vv50:25.4":{"pli":"soṇadinnā sucimhitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:26.1":{"pli":"Alambusā missakesī ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:26.2":{"pli":"puṇḍarīkātidāruṇī; ","html":"<span class='verse-line'>{}</span>"},"vv50:26.3":{"pli":"Eṇīphassā suphassā ca, ","html":"<span class='verse-line'>{}</span>"},"vv50:26.4":{"pli":"subhaddā muduvādinī. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:27.1":{"pli":"Etā caññā ca seyyāse, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:27.2":{"pli":"accharānaṃ pabodhikā; ","html":"<span class='verse-line'>{}</span>"},"vv50:27.3":{"pli":"Tā maṃ kālenupāgantvā, ","html":"<span class='verse-line'>{}</span>"},"vv50:27.4":{"pli":"abhibhāsanti devatā. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:28.1":{"pli":"Handa naccāma gāyāma, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:28.2":{"pli":"handa taṃ ramayāmase; ","html":"<span class='verse-line'>{}</span>"},"vv50:28.3":{"pli":"Nayidaṃ akatapuññānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv50:28.4":{"pli":"katapuññānamevidaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:29.1":{"pli":"Asokaṃ nandanaṃ rammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:29.2":{"pli":"tidasānaṃ mahāvanaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv50:29.3":{"pli":"Sukhaṃ akatapuññānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv50:29.4":{"pli":"idha natthi parattha ca. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:30.1":{"pli":"Sukhañca katapuññānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:30.2":{"pli":"idha ceva parattha ca; ","html":"<span class='verse-line'>{}</span>"},"vv50:30.3":{"pli":"Tesaṃ sahabyakāmānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv50:30.4":{"pli":"kattabbaṃ kusalaṃ bahuṃ; ","html":"<span class='verse-line'>{}</span>"},"vv50:30.5":{"pli":"Katapuññā hi modanti, ","html":"<span class='verse-line'>{}</span>"},"vv50:30.6":{"pli":"sagge bhogasamaṅgino. ","html":"<span class='verse-line'>{}</span></p>"},"vv50:31.1":{"pli":"Bahūnaṃ vata atthāya, ","html":"<p><span class='verse-line'>{}</span>"},"vv50:31.2":{"pli":"uppajjanti tathāgatā; ","html":"<span class='verse-line'>{}</span>"},"vv50:31.3":{"pli":"Dakkhiṇeyyā manussānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv50:31.4":{"pli":"puññakhettānamākarā; ","html":"<span class='verse-line'>{}</span>"},"vv50:31.5":{"pli":"Yattha kāraṃ karitvāna, ","html":"<span class='verse-line'>{}</span>"},"vv50:31.6":{"pli":"sagge modanti dāyakā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv50:32.1":{"pli":"Rajjumālāvimānaṃ dvādasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"vv50:33.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"vv50:34.1":{"pli":"Mañjiṭṭhā pabhassarā nāgā, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"vv50:34.2":{"pli":"Alomākañjikadāyikā; ","html":"<span class='verse-line'>{}</span>"},"vv50:34.3":{"pli":"Vihāracaturitthambā, ","html":"<span class='verse-line'>{}</span>"},"vv50:34.4":{"pli":"Pītā ucchuvandanarajjumālā ca; ","html":"<span class='verse-line'>{}</span>"},"vv50:34.5":{"pli":"Vaggo tena pavuccatīti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv50:35.1":{"pli":"Mañjiṭṭhakavaggo catuttho. ","html":"<p class='endsutta'>{}</p>"},"vv50:36.1":{"pli":"Itthivimānaṃ samattaṃ. ","html":"<p class='endsection'>{}</p></article>"}},"vv51":{"vv51:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv51'><header><ul><li class='division'>{}</li>"},"vv51:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv51:0.3":{"pli":"Mahārathavagga ","html":"<li>{}</li></ul>"},"vv51:0.4":{"pli":"1. Maṇḍūkadevaputtavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv51:1.1":{"pli":"“Ko me vandati pādāni, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv51:1.2":{"pli":"iddhiyā yasasā jalaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv51:1.3":{"pli":"Abhikkantena vaṇṇena, ","html":"<span class='verse-line'>{}</span>"},"vv51:1.4":{"pli":"sabbā obhāsayaṃ disā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv51:2.1":{"pli":"“Maṇḍūkohaṃ pure āsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv51:2.2":{"pli":"udake vārigocaro; ","html":"<span class='verse-line'>{}</span>"},"vv51:2.3":{"pli":"Tava dhammaṃ suṇantassa, ","html":"<span class='verse-line'>{}</span>"},"vv51:2.4":{"pli":"avadhī vacchapālako. ","html":"<span class='verse-line'>{}</span></p>"},"vv51:3.1":{"pli":"Muhuttaṃ cittapasādassa, ","html":"<p><span class='verse-line'>{}</span>"},"vv51:3.2":{"pli":"iddhiṃ passa yasañca me; ","html":"<span class='verse-line'>{}</span>"},"vv51:3.3":{"pli":"Ānubhāvañca me passa, ","html":"<span class='verse-line'>{}</span>"},"vv51:3.4":{"pli":"vaṇṇaṃ passa jutiñca me. ","html":"<span class='verse-line'>{}</span></p>"},"vv51:4.1":{"pli":"Ye ca te dīghamaddhānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv51:4.2":{"pli":"dhammaṃ assosuṃ gotama; ","html":"<span class='verse-line'>{}</span>"},"vv51:4.3":{"pli":"Pattā te acalaṭṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv51:4.4":{"pli":"yattha gantvā na socare”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv51:5.1":{"pli":"Maṇḍūkadevaputtavimānaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv52":{"vv52:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv52'><header><ul><li class='division'>{}</li>"},"vv52:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv52:0.3":{"pli":"Mahārathavagga ","html":"<li>{}</li></ul>"},"vv52:0.4":{"pli":"2. Revatīvimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv52:1.1":{"pli":"“Cirappavāsiṃ purisaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv52:1.2":{"pli":"dūrato sotthimāgataṃ; ","html":"<span class='verse-line'>{}</span>"},"vv52:1.3":{"pli":"Ñātimittā suhajjā ca, ","html":"<span class='verse-line'>{}</span>"},"vv52:1.4":{"pli":"abhinandanti āgataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:2.1":{"pli":"Tatheva katapuññampi, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:2.2":{"pli":"asmā lokā paraṃ gataṃ; ","html":"<span class='verse-line'>{}</span>"},"vv52:2.3":{"pli":"Puññāni paṭigaṇhanti, ","html":"<span class='verse-line'>{}</span>"},"vv52:2.4":{"pli":"piyaṃ ñātīva āgataṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:3.1":{"pli":"“Uṭṭhehi revate supāpadhamme, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:3.2":{"pli":"Apārutadvāre adānasīle; ","html":"<span class='verse-line'>{}</span>"},"vv52:3.3":{"pli":"Nessāma taṃ yattha thunanti duggatā, ","html":"<span class='verse-line'>{}</span>"},"vv52:3.4":{"pli":"Samappitā nerayikā dukkhenā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:4.1":{"pli":"Icceva vatvāna yamassa dūtā, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:4.2":{"pli":"Te dve yakkhā lohitakkhā brahantā; ","html":"<span class='verse-line'>{}</span>"},"vv52:4.3":{"pli":"Paccekabāhāsu gahetvā revataṃ, ","html":"<span class='verse-line'>{}</span>"},"vv52:4.4":{"pli":"Pakkāmayuṃ devagaṇassa santike. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:5.1":{"pli":"“Ādiccavaṇṇaṃ ruciraṃ pabhassaraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:5.2":{"pli":"Byamhaṃ subhaṃ kañcanajālachannaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv52:5.3":{"pli":"Kassetamākiṇṇajanaṃ vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv52:5.4":{"pli":"Suriyassa raṃsīriva jotamānaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:6.1":{"pli":"Nārīgaṇā candanasāralittā, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:6.2":{"pli":"Ubhato vimānaṃ upasobhayanti; ","html":"<span class='verse-line'>{}</span>"},"vv52:6.3":{"pli":"Taṃ dissati suriyasamānavaṇṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv52:6.4":{"pli":"Ko modati saggapatto vimāne”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:7.1":{"pli":"“Bārāṇasiyaṃ nandiyo nāmāsi, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:7.2":{"pli":"Upāsako amaccharī dānapati vadaññū; ","html":"<span class='verse-line'>{}</span>"},"vv52:7.3":{"pli":"Tassetamākiṇṇajanaṃ vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv52:7.4":{"pli":"Suriyassa raṃsīriva jotamānaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:8.1":{"pli":"Nārīgaṇā candanasāralittā, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:8.2":{"pli":"Ubhato vimānaṃ upasobhayanti; ","html":"<span class='verse-line'>{}</span>"},"vv52:8.3":{"pli":"Taṃ dissati suriyasamānavaṇṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv52:8.4":{"pli":"So modati saggapatto vimāne”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:9.1":{"pli":"“Nandiyassāhaṃ bhariyā, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:9.2":{"pli":"Agārinī sabbakulassa issarā; ","html":"<span class='verse-line'>{}</span>"},"vv52:9.3":{"pli":"Bhattu vimāne ramissāmi dānahaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv52:9.4":{"pli":"Na patthaye nirayaṃ dassanāyā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:10.1":{"pli":"“Eso te nirayo supāpadhamme, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:10.2":{"pli":"Puññaṃ tayā akataṃ jīvaloke; ","html":"<span class='verse-line'>{}</span>"},"vv52:10.3":{"pli":"Na hi maccharī rosako pāpadhammo, ","html":"<span class='verse-line'>{}</span>"},"vv52:10.4":{"pli":"Saggūpagānaṃ labhati sahabyatan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:11.1":{"pli":"“Kiṃ nu gūthañca muttañca, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:11.2":{"pli":"asucī paṭidissati; ","html":"<span class='verse-line'>{}</span>"},"vv52:11.3":{"pli":"Duggandhaṃ kimidaṃ mīḷhaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv52:11.4":{"pli":"kimetaṃ upavāyatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:12.1":{"pli":"“Esa saṃsavako nāma, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:12.2":{"pli":"gambhīro sataporiso; ","html":"<span class='verse-line'>{}</span>"},"vv52:12.3":{"pli":"Yattha vassasahassāni, ","html":"<span class='verse-line'>{}</span>"},"vv52:12.4":{"pli":"tuvaṃ paccasi revate”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:13.1":{"pli":"“Kiṃ nu kāyena vācāya, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:13.2":{"pli":"manasā dukkaṭaṃ kataṃ; ","html":"<span class='verse-line'>{}</span>"},"vv52:13.3":{"pli":"Kena saṃsavako laddho, ","html":"<span class='verse-line'>{}</span>"},"vv52:13.4":{"pli":"gambhīro sataporiso”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:14.1":{"pli":"“Samaṇe brāhmaṇe cāpi, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:14.2":{"pli":"aññe vāpi vanibbake; ","html":"<span class='verse-line'>{}</span>"},"vv52:14.3":{"pli":"Musāvādena vañcesi, ","html":"<span class='verse-line'>{}</span>"},"vv52:14.4":{"pli":"taṃ pāpaṃ pakataṃ tayā. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:15.1":{"pli":"Tena saṃsavako laddho, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:15.2":{"pli":"gambhīro sataporiso; ","html":"<span class='verse-line'>{}</span>"},"vv52:15.3":{"pli":"Tattha vassasahassāni, ","html":"<span class='verse-line'>{}</span>"},"vv52:15.4":{"pli":"tuvaṃ paccasi revate. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:16.1":{"pli":"Hatthepi chindanti athopi pāde, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:16.2":{"pli":"Kaṇṇepi chindanti athopi nāsaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv52:16.3":{"pli":"Athopi kākoḷagaṇā samecca, ","html":"<span class='verse-line'>{}</span>"},"vv52:16.4":{"pli":"Saṅgamma khādanti viphandamānan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:17.1":{"pli":"“Sādhu kho maṃ paṭinetha, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:17.2":{"pli":"kāhāmi kusalaṃ bahuṃ; ","html":"<span class='verse-line'>{}</span>"},"vv52:17.3":{"pli":"Dānena samacariyāya, ","html":"<span class='verse-line'>{}</span>"},"vv52:17.4":{"pli":"saṃyamena damena ca; ","html":"<span class='verse-line'>{}</span>"},"vv52:17.5":{"pli":"Yaṃ katvā sukhitā honti, ","html":"<span class='verse-line'>{}</span>"},"vv52:17.6":{"pli":"na ca pacchānutappare”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:18.1":{"pli":"“Pure tuvaṃ pamajjitvā, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:18.2":{"pli":"idāni paridevasi; ","html":"<span class='verse-line'>{}</span>"},"vv52:18.3":{"pli":"Sayaṃ katānaṃ kammānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv52:18.4":{"pli":"vipākaṃ anubhossasī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:19.1":{"pli":"“Ko devalokato manussalokaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:19.2":{"pli":"Gantvāna puṭṭho me evaṃ vadeyya; ","html":"<span class='verse-line'>{}</span>"},"vv52:19.3":{"pli":"‘Nikkhittadaṇḍesu dadātha dānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv52:19.4":{"pli":"Acchādanaṃ seyyamathannapānaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv52:19.5":{"pli":"Na hi maccharī rosako pāpadhammo, ","html":"<span class='verse-line'>{}</span>"},"vv52:19.6":{"pli":"Saggūpagānaṃ labhati sahabyataṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:20.1":{"pli":"Sāhaṃ nūna ito gantvā, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:20.2":{"pli":"yoniṃ laddhāna mānusiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv52:20.3":{"pli":"Vadaññū sīlasampannā, ","html":"<span class='verse-line'>{}</span>"},"vv52:20.4":{"pli":"kāhāmi kusalaṃ bahuṃ; ","html":"<span class='verse-line'>{}</span>"},"vv52:20.5":{"pli":"Dānena samacariyāya, ","html":"<span class='verse-line'>{}</span>"},"vv52:20.6":{"pli":"saṃyamena damena ca. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:21.1":{"pli":"Ārāmāni ca ropissaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:21.2":{"pli":"dugge saṅkamanāni ca; ","html":"<span class='verse-line'>{}</span>"},"vv52:21.3":{"pli":"Papañca udapānañca, ","html":"<span class='verse-line'>{}</span>"},"vv52:21.4":{"pli":"vippasannena cetasā. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:22.1":{"pli":"Cātuddasiṃ pañcadasiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:22.2":{"pli":"yā ca pakkhassa aṭṭhamī; ","html":"<span class='verse-line'>{}</span>"},"vv52:22.3":{"pli":"Pāṭihāriyapakkhañca, ","html":"<span class='verse-line'>{}</span>"},"vv52:22.4":{"pli":"aṭṭhaṅgasusamāgataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:23.1":{"pli":"Uposathaṃ upavasissaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:23.2":{"pli":"sadā sīlesu saṃvutā; ","html":"<span class='verse-line'>{}</span>"},"vv52:23.3":{"pli":"Na ca dāne pamajjissaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv52:23.4":{"pli":"sāmaṃ diṭṭhamidaṃ mayā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:24.1":{"pli":"Iccevaṃ vippalapantiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:24.2":{"pli":"phandamānaṃ tato tato; ","html":"<span class='verse-line'>{}</span>"},"vv52:24.3":{"pli":"Khipiṃsu niraye ghore, ","html":"<span class='verse-line'>{}</span>"},"vv52:24.4":{"pli":"uddhampādaṃ avaṃsiraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv52:25.1":{"pli":"“Ahaṃ pure maccharinī ahosiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv52:25.2":{"pli":"Paribhāsikā samaṇabrāhmaṇānaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv52:25.3":{"pli":"Vitathena ca sāmikaṃ vañcayitvā, ","html":"<span class='verse-line'>{}</span>"},"vv52:25.4":{"pli":"Paccāmahaṃ niraye ghorarūpe”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv52:26.1":{"pli":"Revatīvimānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv53":{"vv53:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv53'><header><ul><li class='division'>{}</li>"},"vv53:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv53:0.3":{"pli":"Mahārathavagga ","html":"<li>{}</li></ul>"},"vv53:0.4":{"pli":"3. Chattamāṇavakavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv53:1.1":{"pli":"“Yo vadataṃ pavaro manujesu, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv53:1.2":{"pli":"Sakyamunī bhagavā katakicco; ","html":"<span class='verse-line'>{}</span>"},"vv53:1.3":{"pli":"Pāragato balaviriyasamaṅgī, ","html":"<span class='verse-line'>{}</span>"},"vv53:1.4":{"pli":"Taṃ sugataṃ saraṇatthamupehi. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:2.1":{"pli":"Rāgavirāgamanejamasokaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:2.2":{"pli":"Dhammamasaṅkhatamappaṭikūlaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv53:2.3":{"pli":"Madhuramimaṃ paguṇaṃ suvibhattaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv53:2.4":{"pli":"Dhammamimaṃ saraṇatthamupehi. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:3.1":{"pli":"Yattha ca dinna mahapphalamāhu, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:3.2":{"pli":"Catūsu sucīsu purisayugesu; ","html":"<span class='verse-line'>{}</span>"},"vv53:3.3":{"pli":"Aṭṭha ca puggaladhammadasā te, ","html":"<span class='verse-line'>{}</span>"},"vv53:3.4":{"pli":"Saṅghamimaṃ saraṇatthamupehi. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:4.1":{"pli":"Na tathā tapati nabhe sūriyo, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:4.2":{"pli":"Cando ca na bhāsati na phusso; ","html":"<span class='verse-line'>{}</span>"},"vv53:4.3":{"pli":"Yathā atulamidaṃ mahappabhāsaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv53:4.4":{"pli":"Ko nu tvaṃ tidivā mahiṃ upāgā. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:5.1":{"pli":"Chindati raṃsī pabhaṅkarassa, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:5.2":{"pli":"Sādhikavīsatiyojanāni ābhā; ","html":"<span class='verse-line'>{}</span>"},"vv53:5.3":{"pli":"Rattimapi yathā divaṃ karoti, ","html":"<span class='verse-line'>{}</span>"},"vv53:5.4":{"pli":"Parisuddhaṃ vimalaṃ subhaṃ vimānaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:6.1":{"pli":"Bahupadumavicitrapuṇḍarīkaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:6.2":{"pli":"Vokiṇṇaṃ kusumehi nekacittaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv53:6.3":{"pli":"Arajavirajahemajālachannaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv53:6.4":{"pli":"Ākāse tapati yathāpi sūriyo. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:7.1":{"pli":"Rattambarapītavāsasāhi, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:7.2":{"pli":"Agarupiyaṅgucandanussadāhi; ","html":"<span class='verse-line'>{}</span>"},"vv53:7.3":{"pli":"Kañcanatanusannibhattacāhi, ","html":"<span class='verse-line'>{}</span>"},"vv53:7.4":{"pli":"Paripūraṃ gaganaṃva tārakāhi. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:8.1":{"pli":"Naranāriyo bahuketthanekavaṇṇā, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:8.2":{"pli":"Kusumavibhūsitābharaṇettha sumanā; ","html":"<span class='verse-line'>{}</span>"},"vv53:8.3":{"pli":"Anilapamuñcitā pavanti surabhiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv53:8.4":{"pli":"Tapaniyavitatā suvaṇṇachannā. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:9.1":{"pli":"Kissa saṃyamassa ayaṃ vipāko, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:9.2":{"pli":"Kenāsi kammaphalenidhūpapanno; ","html":"<span class='verse-line'>{}</span>"},"vv53:9.3":{"pli":"Yathā ca te adhigatamidaṃ vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv53:9.4":{"pli":"Tadanupadaṃ avacāsi iṅgha puṭṭho”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:10.1":{"pli":"“Sayamidha pathe samecca māṇavena, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:10.2":{"pli":"Satthānusāsi anukampamāno; ","html":"<span class='verse-line'>{}</span>"},"vv53:10.3":{"pli":"Tava ratanavarassa dhammaṃ sutvā, ","html":"<span class='verse-line'>{}</span>"},"vv53:10.4":{"pli":"Karissāmīti ca bravittha chatto. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:11.1":{"pli":"Jinavarapavaraṃ upehi saraṇaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:11.2":{"pli":"Dhammañcāpi tatheva bhikkhusaṅghaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv53:11.3":{"pli":"Noti paṭhamaṃ avocahaṃ bhante, ","html":"<span class='verse-line'>{}</span>"},"vv53:11.4":{"pli":"Pacchā te vacanaṃ tathevakāsiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:12.1":{"pli":"Mā ca pāṇavadhaṃ vividhaṃ carassu asuciṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:12.2":{"pli":"Na hi pāṇesu asaññataṃ avaṇṇayiṃsu sappaññā; ","html":"<span class='verse-line'>{}</span>"},"vv53:12.3":{"pli":"Noti paṭhamaṃ avocahaṃ bhante, ","html":"<span class='verse-line'>{}</span>"},"vv53:12.4":{"pli":"Pacchā te vacanaṃ tathevakāsiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:13.1":{"pli":"Mā ca parajanassa rakkhitampi, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:13.2":{"pli":"Ādātabbamamaññitho adinnaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv53:13.3":{"pli":"Noti paṭhamaṃ avocahaṃ bhante, ","html":"<span class='verse-line'>{}</span>"},"vv53:13.4":{"pli":"Pacchā te vacanaṃ tathevakāsiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:14.1":{"pli":"Mā ca parajanassa rakkhitāyo, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:14.2":{"pli":"Parabhariyā agamā anariyametaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv53:14.3":{"pli":"Noti paṭhamaṃ avocahaṃ bhante, ","html":"<span class='verse-line'>{}</span>"},"vv53:14.4":{"pli":"Pacchā te vacanaṃ tathevakāsiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:15.1":{"pli":"Mā ca vitathaṃ aññathā abhāṇi, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:15.2":{"pli":"Na hi musāvādaṃ avaṇṇayiṃsu sappaññā; ","html":"<span class='verse-line'>{}</span>"},"vv53:15.3":{"pli":"Noti paṭhamaṃ avocahaṃ bhante, ","html":"<span class='verse-line'>{}</span>"},"vv53:15.4":{"pli":"Pacchā te vacanaṃ tathevakāsiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:16.1":{"pli":"Yena ca purisassa apeti saññā, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:16.2":{"pli":"Taṃ majjaṃ parivajjayassu sabbaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv53:16.3":{"pli":"Noti paṭhamaṃ avocahaṃ bhante, ","html":"<span class='verse-line'>{}</span>"},"vv53:16.4":{"pli":"Pacchā te vacanaṃ tathevakāsiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:17.1":{"pli":"Svāhaṃ idha pañca sikkhā karitvā, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:17.2":{"pli":"Paṭipajjitvā tathāgatassa dhamme; ","html":"<span class='verse-line'>{}</span>"},"vv53:17.3":{"pli":"Dvepathamagamāsiṃ coramajjhe, ","html":"<span class='verse-line'>{}</span>"},"vv53:17.4":{"pli":"Te maṃ tattha vadhiṃsu bhogahetu. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:18.1":{"pli":"Ettakamidaṃ anussarāmi kusalaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:18.2":{"pli":"Tato paraṃ na me vijjati aññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv53:18.3":{"pli":"Tena sucaritena kammunāhaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv53:18.4":{"pli":"Uppanno tidivesu kāmakāmī. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:19.1":{"pli":"Passa khaṇamuhuttasaññamassa, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:19.2":{"pli":"Anudhammappaṭipattiyā vipākaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv53:19.3":{"pli":"Jalamiva yasasā samekkhamānā, ","html":"<span class='verse-line'>{}</span>"},"vv53:19.4":{"pli":"Bahukā maṃ pihayanti hīnakammā. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:20.1":{"pli":"Passa katipayāya desanāya, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:20.2":{"pli":"Sugatiñcamhi gato sukhañca patto; ","html":"<span class='verse-line'>{}</span>"},"vv53:20.3":{"pli":"Ye ca te satataṃ suṇanti dhammaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv53:20.4":{"pli":"Maññe te amataṃ phusanti khemaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:21.1":{"pli":"Appampi kataṃ mahāvipākaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:21.2":{"pli":"Vipulaṃ hoti tathāgatassa dhamme; ","html":"<span class='verse-line'>{}</span>"},"vv53:21.3":{"pli":"Passa katapuññatāya chatto, ","html":"<span class='verse-line'>{}</span>"},"vv53:21.4":{"pli":"Obhāseti pathaviṃ yathāpi sūriyo. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:22.1":{"pli":"Kimidaṃ kusalaṃ kimācarema, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:22.2":{"pli":"Icceke hi samecca mantayanti; ","html":"<span class='verse-line'>{}</span>"},"vv53:22.3":{"pli":"Te mayaṃ punareva laddha mānusattaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv53:22.4":{"pli":"Paṭipannā viharemu sīlavanto. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:23.1":{"pli":"Bahukāro anukampako ca satthā, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:23.2":{"pli":"Iti me sati agamā divā divassa; ","html":"<span class='verse-line'>{}</span>"},"vv53:23.3":{"pli":"Svāhaṃ upagatomhi saccanāmaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv53:23.4":{"pli":"Anukampassu punapi suṇemu dhammaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv53:24.1":{"pli":"Ye cidha pajahanti kāmarāgaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv53:24.2":{"pli":"Bhavarāgānusayañca pahāya mohaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv53:24.3":{"pli":"Na ca te punamupenti gabbhaseyyaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv53:24.4":{"pli":"Parinibbānagatā hi sītibhūtā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv53:25.1":{"pli":"Chattamāṇavakavimānaṃ tatiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv54":{"vv54:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv54'><header><ul><li class='division'>{}</li>"},"vv54:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv54:0.3":{"pli":"Mahārathavagga ","html":"<li>{}</li></ul>"},"vv54:0.4":{"pli":"4. Kakkaṭakarasadāyakavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv54:1.1":{"pli":"“Uccamidaṃ maṇithūṇaṃ vimānaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv54:1.2":{"pli":"Samantato dvādasa yojanāni; ","html":"<span class='verse-line'>{}</span>"},"vv54:1.3":{"pli":"Kūṭāgārā sattasatā uḷārā, ","html":"<span class='verse-line'>{}</span>"},"vv54:1.4":{"pli":"Veḷuriyathambhā rucakatthatā subhā. ","html":"<span class='verse-line'>{}</span></p>"},"vv54:2.1":{"pli":"Tatthacchasi pivasi khādasi ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv54:2.2":{"pli":"Dibbā ca vīṇā pavadanti vagguṃ; ","html":"<span class='verse-line'>{}</span>"},"vv54:2.3":{"pli":"Dibbā rasā kāmaguṇettha pañca, ","html":"<span class='verse-line'>{}</span>"},"vv54:2.4":{"pli":"Nāriyo ca naccanti suvaṇṇachannā. ","html":"<span class='verse-line'>{}</span></p>"},"vv54:3.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv54:3.2":{"pli":"kena te idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv54:3.3":{"pli":"Uppajjanti ca te bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv54:3.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv54:4.1":{"pli":"Pucchāmi taṃ deva mahānubhāva, ","html":"<p><span class='verse-line'>{}</span>"},"vv54:4.2":{"pli":"Manussabhūto kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv54:4.3":{"pli":"Kenāsi evaṃ jalitānubhāvo, ","html":"<span class='verse-line'>{}</span>"},"vv54:4.4":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv54:5.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv54:5.2":{"pli":"moggallānena pucchito; ","html":"<span class='verse-line'>{}</span>"},"vv54:5.3":{"pli":"Pañhaṃ puṭṭho viyākāsi, ","html":"<span class='verse-line'>{}</span>"},"vv54:5.4":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv54:6.1":{"pli":"“Satisamuppādakaro, ","html":"<p><span class='verse-line'>{}</span>"},"vv54:6.2":{"pli":"dvāre kakkaṭako ṭhito; ","html":"<span class='verse-line'>{}</span>"},"vv54:6.3":{"pli":"Niṭṭhito jātarūpassa, ","html":"<span class='verse-line'>{}</span>"},"vv54:6.4":{"pli":"sobhati dasapādako. ","html":"<span class='verse-line'>{}</span></p>"},"vv54:7.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv54:7.2":{"pli":"tena me idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv54:7.3":{"pli":"Uppajjanti ca me bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv54:7.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv54:8.1":{"pli":"Akkhāmi te bhikkhu mahānubhāva, ","html":"<p><span class='verse-line'>{}</span>"},"vv54:8.2":{"pli":"Manussabhūto yamakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv54:8.3":{"pli":"Tenamhi evaṃ jalitānubhāvo, ","html":"<span class='verse-line'>{}</span>"},"vv54:8.4":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv54:9.1":{"pli":"Kakkaṭakarasadāyakavimānaṃ catutthaṃ. ","html":"<p class='endsutta'>{}</p>"},"vv54:10.1":{"pli":"(Anantaraṃ pañcavimānaṃ yathā kakkaṭakarasadāyakavimānaṃ tathā vitthāretabbaṃ.) ","html":"<p>{}</p></article>"}},"vv55":{"vv55:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv55'><header><ul><li class='division'>{}</li>"},"vv55:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv55:0.3":{"pli":"Mahārathavagga ","html":"<li>{}</li></ul>"},"vv55:0.4":{"pli":"5. Dvārapālavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv55:1.1":{"pli":"“Uccamidaṃ maṇithūṇaṃ vimānaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv55:1.2":{"pli":"Samantato dvādasa yojanāni; ","html":"<span class='verse-line'>{}</span>"},"vv55:1.3":{"pli":"Kūṭāgārā sattasatā uḷārā, ","html":"<span class='verse-line'>{}</span>"},"vv55:1.4":{"pli":"Veḷuriyathambhā rucakatthatā subhā. ","html":"<span class='verse-line'>{}</span></p>"},"vv55:2.1":{"pli":"Tatthacchasi pivasi khādasi ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv55:2.2":{"pli":"Dibbā ca vīṇā pavadanti vagguṃ; ","html":"<span class='verse-line'>{}</span>"},"vv55:2.3":{"pli":"Dibbā rasā kāmaguṇettha pañca, ","html":"<span class='verse-line'>{}</span>"},"vv55:2.4":{"pli":"Nāriyo ca naccanti suvaṇṇachannā. ","html":"<span class='verse-line'>{}</span></p>"},"vv55:3.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv55:3.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv55:3.3":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv55:4.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv55:4.2":{"pli":"moggallānena pucchito; ","html":"<span class='verse-line'>{}</span>"},"vv55:4.3":{"pli":"Pañhaṃ puṭṭho viyākāsi, ","html":"<span class='verse-line'>{}</span>"},"vv55:4.4":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv55:5.1":{"pli":"“Dibbaṃ mamaṃ vassasahassamāyu, ","html":"<p><span class='verse-line'>{}</span>"},"vv55:5.2":{"pli":"Vācābhigītaṃ manasā pavattitaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv55:5.3":{"pli":"Ettāvatā ṭhassati puññakammo, ","html":"<span class='verse-line'>{}</span>"},"vv55:5.4":{"pli":"Dibbehi kāmehi samaṅgibhūto. ","html":"<span class='verse-line'>{}</span></p>"},"vv55:6.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv55:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv55:6.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv55:7.1":{"pli":"Dvārapālavimānaṃ pañcamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv56":{"vv56:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv56'><header><ul><li class='division'>{}</li>"},"vv56:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv56:0.3":{"pli":"Mahārathavagga ","html":"<li>{}</li></ul>"},"vv56:0.4":{"pli":"6. Paṭhamakaraṇīyavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv56:1.1":{"pli":"“Uccamidaṃ maṇithūṇaṃ vimānaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv56:1.2":{"pli":"Samantato dvādasa yojanāni; ","html":"<span class='verse-line'>{}</span>"},"vv56:1.3":{"pli":"Kūṭāgārā sattasatā uḷārā, ","html":"<span class='verse-line'>{}</span>"},"vv56:1.4":{"pli":"Veḷuriyathambhā rucakatthatā subhā. ","html":"<span class='verse-line'>{}</span></p>"},"vv56:2.1":{"pli":"Tatthacchasi pivasi khādasi ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv56:2.2":{"pli":"Dibbā ca vīṇā pavadanti vagguṃ; ","html":"<span class='verse-line'>{}</span>"},"vv56:2.3":{"pli":"Dibbā rasā kāmaguṇettha pañca, ","html":"<span class='verse-line'>{}</span>"},"vv56:2.4":{"pli":"Nāriyo ca naccanti suvaṇṇachannā. ","html":"<span class='verse-line'>{}</span></p>"},"vv56:3.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv56:3.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv56:3.3":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv56:4.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv56:4.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv56:4.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv56:5.1":{"pli":"“Karaṇīyāni puññāni, ","html":"<p><span class='verse-line'>{}</span>"},"vv56:5.2":{"pli":"paṇḍitena vijānatā; ","html":"<span class='verse-line'>{}</span>"},"vv56:5.3":{"pli":"Sammaggatesu buddhesu, ","html":"<span class='verse-line'>{}</span>"},"vv56:5.4":{"pli":"yattha dinnaṃ mahapphalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv56:6.1":{"pli":"Atthāya vata me buddho, ","html":"<p><span class='verse-line'>{}</span>"},"vv56:6.2":{"pli":"araññā gāmamāgato; ","html":"<span class='verse-line'>{}</span>"},"vv56:6.3":{"pli":"Tattha cittaṃ pasādetvā, ","html":"<span class='verse-line'>{}</span>"},"vv56:6.4":{"pli":"tāvatiṃsūpago ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv56:7.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv56:7.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv56:7.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv56:8.1":{"pli":"Paṭhamakaraṇīyavimānaṃ chaṭṭhaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}