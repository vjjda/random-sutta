{"tha-ap80":{"tha-ap80:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap80'><header><ul><li class='division'>{}</li>"},"tha-ap80:0.2":{"pli":"Nāgasamālavagga ","html":"<li>{}</li></ul>"},"tha-ap80:0.3":{"pli":"Pāṭalipupphiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap80:1.1":{"pli":"“Suvaṇṇavaṇṇaṃ sambuddhaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap80:1.2":{"pli":"Gacchantaṃ antarāpaṇe; ","html":"<span class='verse-line'>{}</span>"},"tha-ap80:1.3":{"pli":"Kañcanagghiyasaṅkāsaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap80:1.4":{"pli":"Bāttiṃsavaralakkhaṇaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap80:2.1":{"pli":"Seṭṭhiputto tadā āsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap80:2.2":{"pli":"sukhumālo sukhedhito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap80:2.3":{"pli":"Ucchaṅge pāṭalipupphaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap80:2.4":{"pli":"katvāna abhisaṃhariṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap80:3.1":{"pli":"Haṭṭho haṭṭhena cittena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap80:3.2":{"pli":"pupphehi abhipūjayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap80:3.3":{"pli":"Tissaṃ lokaviduṃ nāthaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap80:3.4":{"pli":"naradevaṃ namassahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap80:4.1":{"pli":"Dvenavute ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap80:4.2":{"pli":"yaṃ kammamakariṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap80:4.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap80:4.4":{"pli":"pupphapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap80:5.1":{"pli":"Ito tesaṭṭhikappamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap80:5.2":{"pli":"abhisammatanāmako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap80:5.3":{"pli":"Sattaratanasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap80:5.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap80:6.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap80:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap80:6.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap80:7.1":{"pli":"Itthaṃ sudaṃ āyasmā pāṭalipupphiyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap80:8.1":{"pli":"Pāṭalipupphiyattherassāpadānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap81":{"tha-ap81:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap81'><header><ul><li class='division'>{}</li>"},"tha-ap81:0.2":{"pli":"Nāgasamālavagga ","html":"<li>{}</li></ul>"},"tha-ap81:0.3":{"pli":"Ṭhitañjaliyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap81:1.1":{"pli":"“Migaluddo pure āsiṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap81:1.2":{"pli":"Araññe kānane ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap81:1.3":{"pli":"Tattha addasaṃ sambuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap81:1.4":{"pli":"Bāttiṃsavaralakkhaṇaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap81:2.1":{"pli":"Tatthāhaṃ añjaliṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap81:2.2":{"pli":"pakkāmiṃ pācināmukho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap81:2.3":{"pli":"Avidūre nisinnassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap81:2.4":{"pli":"niyake paṇṇasanthare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap81:3.1":{"pli":"Tato me asanīpāto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap81:3.2":{"pli":"matthake nipatī tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap81:3.3":{"pli":"Sohaṃ maraṇakālamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap81:3.4":{"pli":"akāsiṃ punarañjaliṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap81:4.1":{"pli":"Dvenavute ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap81:4.2":{"pli":"añjaliṃ akariṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap81:4.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap81:4.4":{"pli":"añjalissa idaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap81:5.1":{"pli":"Catupaṇṇāsakappamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap81:5.2":{"pli":"migaketusanāmako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap81:5.3":{"pli":"Sattaratanasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap81:5.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap81:6.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap81:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap81:6.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap81:7.1":{"pli":"Itthaṃ sudaṃ āyasmā ṭhitañjaliyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap81:8.1":{"pli":"Ṭhitañjaliyattherassāpadānaṃ navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap82":{"tha-ap82:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap82'><header><ul><li class='division'>{}</li>"},"tha-ap82:0.2":{"pli":"Nāgasamālavagga ","html":"<li>{}</li></ul>"},"tha-ap82:0.3":{"pli":"10 Tipadumiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap82:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap82:1.2":{"pli":"sabbadhammāna pāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:1.3":{"pli":"Danto dantaparivuto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:1.4":{"pli":"nagarā nikkhamī tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:2.1":{"pli":"Nagare haṃsavatiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:2.2":{"pli":"ahosiṃ māliko tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:2.3":{"pli":"Yaṃ tattha uttamaṃ tīṇi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:2.4":{"pli":"padmapupphāni aggahiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:3.1":{"pli":"‘Addasaṃ virajaṃ buddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:3.2":{"pli":"paṭimaggantarāpaṇe; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:3.3":{"pli":"Saha disvāna sambuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:3.4":{"pli":"evaṃ cintesahaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:4.1":{"pli":"Kiṃ me imehi pupphehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:4.2":{"pli":"rañño upanitehi me; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:4.3":{"pli":"Gāmaṃ vā gāmakhettaṃ vā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:4.4":{"pli":"sahassaṃ vā labheyyahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:5.1":{"pli":"Adantadamanaṃ vīraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:5.2":{"pli":"sabbasattasukhāvahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:5.3":{"pli":"Lokanāthaṃ pūjayitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:5.4":{"pli":"lacchāmi amataṃ dhanaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:6.1":{"pli":"Evāhaṃ cintayitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:6.2":{"pli":"sakaṃ cittaṃ pasādayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:6.3":{"pli":"Tīṇi lohitake gayha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:6.4":{"pli":"ākāse ukkhipiṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:7.1":{"pli":"Mayā ukkhittamattamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:7.2":{"pli":"ākāse patthariṃsu te; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:7.3":{"pli":"Dhāriṃsu matthake tattha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:7.4":{"pli":"uddhaṃvaṇṭā adhomukhā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:8.1":{"pli":"Ye keci manujā disvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:8.2":{"pli":"ukkuṭṭhiṃ sampavattayuṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:8.3":{"pli":"Devatā antalikkhamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:8.4":{"pli":"sādhukāraṃ pavattayuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:9.1":{"pli":"Accheraṃ loke uppannaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:9.2":{"pli":"buddhaseṭṭhassa vāhasā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:9.3":{"pli":"Sabbe dhammaṃ suṇissāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:9.4":{"pli":"pupphānaṃ vāhasā mayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:10.1":{"pli":"Padumuttaro lokavidū, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:10.2":{"pli":"āhutīnaṃ paṭiggaho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:10.3":{"pli":"Vīthiyañhi ṭhito santo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:10.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:11.1":{"pli":"‘Yo so buddhaṃ apūjesi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:11.2":{"pli":"rattapadmehi māṇavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:11.3":{"pli":"Tamahaṃ kittayissāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:11.4":{"pli":"suṇātha mama bhāsato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:12.1":{"pli":"Tiṃsakappasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:12.2":{"pli":"devaloke ramissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:12.3":{"pli":"Tiṃsakappāni devindo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:12.4":{"pli":"devarajjaṃ karissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:13.1":{"pli":"Mahāvitthārikaṃ nāma, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:13.2":{"pli":"byamhaṃ hessati tāvade; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:13.3":{"pli":"Tiyojanasatubbiddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:13.4":{"pli":"diyaḍḍhasatavitthataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:14.1":{"pli":"Cattārisatasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:14.2":{"pli":"niyyūhā ca sumāpitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:14.3":{"pli":"Kūṭāgāravarūpetā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:14.4":{"pli":"mahāsayanamaṇḍitā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:15.1":{"pli":"Koṭisatasahassiyo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:15.2":{"pli":"parivāressanti accharā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:15.3":{"pli":"Kusalā naccagītassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:15.4":{"pli":"vāditepi padakkhiṇā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:16.1":{"pli":"Etādise byamhavare, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:16.2":{"pli":"nārīgaṇasamākule; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:16.3":{"pli":"Vassissati pupphavasso, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:16.4":{"pli":"dibbo lohitako sadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:17.1":{"pli":"Bhittikhīle nāgadante, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:17.2":{"pli":"dvārabāhāya toraṇe; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:17.3":{"pli":"Cakkamattā lohitakā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:17.4":{"pli":"olambissanti tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:18.1":{"pli":"Pattena pattasañchanne, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:18.2":{"pli":"antobyamhavare imaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:18.3":{"pli":"Attharitvā pārupitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:18.4":{"pli":"tuvaṭṭissanti tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:19.1":{"pli":"Bhavanaṃ parivāretvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:19.2":{"pli":"samantā satayojane; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:19.3":{"pli":"Tepi padmā lohitakā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:19.4":{"pli":"dibbagandhaṃ pavāyare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:20.1":{"pli":"Pañcasattatikkhattuñca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:20.2":{"pli":"cakkavattī bhavissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:20.3":{"pli":"Padesarajjaṃ vipulaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:20.4":{"pli":"gaṇanāto asaṅkhiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:21.1":{"pli":"Sampattiyo duve bhutvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:21.2":{"pli":"anīti anupaddavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:21.3":{"pli":"Sampatte pariyosāne, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:21.4":{"pli":"nibbānaṃ pāpuṇissati’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:22.1":{"pli":"Sudiṭṭho vata me buddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:22.2":{"pli":"vāṇijjaṃ supayojitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:22.3":{"pli":"Padmāni tīṇi pūjetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:22.4":{"pli":"anubhosiṃ tisampadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:23.1":{"pli":"Ajja me dhammappattassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:23.2":{"pli":"vippamuttassa sabbaso; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:23.3":{"pli":"Supupphitaṃ lohitakaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:23.4":{"pli":"dhārayissati matthake. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:24.1":{"pli":"Mama kammaṃ kathentassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:24.2":{"pli":"padumuttarasatthuno; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:24.3":{"pli":"Satapāṇasahassānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:24.4":{"pli":"dhammābhisamayo ahu. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:25.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:25.2":{"pli":"yaṃ buddhamabhipūjayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:25.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:25.4":{"pli":"tipadumānidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:26.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:26.2":{"pli":"bhavā sabbe samūhatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:26.3":{"pli":"Sabbāsavā parikkhīṇā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:26.4":{"pli":"natthi dāni punabbhavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap82:27.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap82:27.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:27.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap82:28.1":{"pli":"Itthaṃ sudaṃ āyasmā tipadumiyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap82:29.1":{"pli":"Tipadumiyattherassāpadānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"tha-ap82:30.1":{"pli":"Nāgasamālavaggo aṭṭhamo. ","html":"<p class='endsection'>{}</p>"},"tha-ap82:31.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"tha-ap82:32.1":{"pli":"Nāgasamālo padasaññī, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"tha-ap82:32.2":{"pli":"saññakāluvadāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:32.3":{"pli":"Ekasaññī tiṇasanthāro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:32.4":{"pli":"sūcipāṭalipupphiyo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:32.5":{"pli":"Ṭhitañjalī tipadumī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap82:32.6":{"pli":"gāthāyo pañcasattati. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}},"tha-ap83":{"tha-ap83:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap83'><header><ul><li class='division'>{}</li>"},"tha-ap83:0.2":{"pli":"Timiravagga ","html":"<li>{}</li></ul>"},"tha-ap83:0.3":{"pli":"Timirapupphiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap83:1.1":{"pli":"“Candabhāgānadītīre, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap83:1.2":{"pli":"anusotaṃ vajāmahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap83:1.3":{"pli":"Nisinnaṃ samaṇaṃ disvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap83:1.4":{"pli":"vippasannamanāvilaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap83:2.1":{"pli":"Tattha cittaṃ pasādetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap83:2.2":{"pli":"evaṃ cintesahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap83:2.3":{"pli":"Tārayissati tiṇṇoyaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap83:2.4":{"pli":"dantoyaṃ damayissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap83:3.1":{"pli":"Assāsissati assattho, ","html":"<p><span class='verse-lin