":{"pli":"Therāpadāna ","html":"<article id='tha-ap88'><header><ul><li class='division'>{}</li>"},"tha-ap88:0.2":{"pli":"Timiravagga ","html":"<li>{}</li></ul>"},"tha-ap88:0.3":{"pli":"6 Dutiyaraṃsisaññakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap88:1.1":{"pli":"“Pabbate himavantamhi, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap88:1.2":{"pli":"vākacīradharo ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap88:1.3":{"pli":"Caṅkamañca samārūḷho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap88:1.4":{"pli":"nisīdiṃ pācināmukho. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap88:2.1":{"pli":"Pabbate sugataṃ disvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap88:2.2":{"pli":"phussaṃ jhānarataṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap88:2.3":{"pli":"Añjaliṃ paggahetvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap88:2.4":{"pli":"raṃsyā cittaṃ pasādayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap88:3.1":{"pli":"Dvenavute ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap88:3.2":{"pli":"yaṃ saññamalabhiṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap88:3.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap88:3.4":{"pli":"raṃsisaññāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap88:4.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap88:4.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap88:4.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap88:5.1":{"pli":"Itthaṃ sudaṃ āyasmā raṃsisaññako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap88:6.1":{"pli":"Dutiyaraṃsisaññakattherassāpadānaṃ chaṭṭhaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap89":{"tha-ap89:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap89'><header><ul><li class='division'>{}</li>"},"tha-ap89:0.2":{"pli":"Timiravagga ","html":"<li>{}</li></ul>"},"tha-ap89:0.3":{"pli":"Phaladāyakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap89:1.1":{"pli":"“Pabbate himavantamhi, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap89:1.2":{"pli":"kharājinadharo ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap89:1.3":{"pli":"Phussaṃ jinavaraṃ disvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap89:1.4":{"pli":"phalahattho phalaṃ adaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap89:2.1":{"pli":"Yamahaṃ phalamadāsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap89:2.2":{"pli":"vippasannena cetasā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap89:2.3":{"pli":"Bhave nibbattamānamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap89:2.4":{"pli":"phalaṃ nibbattate mama. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap89:3.1":{"pli":"Dvenavute ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap89:3.2":{"pli":"yaṃ phalaṃ adadiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap89:3.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap89:3.4":{"pli":"phaladānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap89:4.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap89:4.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap89:4.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap89:5.1":{"pli":"Itthaṃ sudaṃ āyasmā phaladāyako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap89:6.1":{"pli":"Phaladāyakattherassāpadānaṃ sattamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap9":{"tha-ap9:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap9'><header><ul><li class='division'>{}</li>"},"tha-ap9:0.2":{"pli":"Buddhavagga ","html":"<li>{}</li></ul>"},"tha-ap9:0.3":{"pli":"9 Aññāsikoṇḍaññattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap9:1.1":{"pli":"“Padumuttarasambuddhaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap9:1.2":{"pli":"lokajeṭṭhaṃ vināyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap9:1.3":{"pli":"Buddhabhūmimanuppattaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap9:1.4":{"pli":"paṭhamaṃ addasaṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap9:2.1":{"pli":"Yāvatā bodhiyā mūle, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap9:2.2":{"pli":"yakkhā sabbe samāgatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap9:2.3":{"pli":"Sambuddhaṃ parivāretvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap9:2.4":{"pli":"vandanti pañjalīkatā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap9:3.1":{"pli":"Sabbe devā tuṭṭhamanā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap9:3.2":{"pli":"ākāse sañcaranti te; ","html":"<span class='verse-line'>{}</span>"},"tha-ap9:3.3":{"pli":"Buddho ayaṃ anuppatto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap9:3.4":{"pli":"andhakāratamonudo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap9:4.1":{"pli":"Tesaṃ hāsaparetānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap9:4.2":{"pli":"mahānādo avattatha; ","html":"<span class='verse-line'>{}</span>"},"tha-ap9:4.3":{"pli":"Kilese jhāpayissāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap9:4.4":{"pli":"sammāsambuddhasāsane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap9:5.1":{"pli":"Devānaṃ giramaññāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap9:5.2":{"pli":"vācāsabhimudīrihaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap9:5.3":{"pli":"Haṭṭho haṭṭhena cittena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap9:5.4":{"pli":"ādibhik