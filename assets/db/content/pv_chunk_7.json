{"pv39":{"pv39:0.1":{"pli":"Petavatthu ","html":"<article id='pv39'><header><ul><li class='division'>{}</li>"},"pv39:0.2":{"pli":"Mahāvagga ","html":"<li>{}</li></ul>"},"pv39:0.3":{"pli":"4. Revatīpetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv39:1.1":{"pli":"“Uṭṭhehi revate supāpadhamme, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv39:1.2":{"pli":"Apārutadvāre adānasīle; ","html":"<span class='verse-line'>{}</span>"},"pv39:1.3":{"pli":"Nessāma taṃ yattha thunanti duggatā, ","html":"<span class='verse-line'>{}</span>"},"pv39:1.4":{"pli":"Samappitā nerayikā dukhenā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:2.1":{"pli":"Icceva vatvāna yamassa dūtā, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:2.2":{"pli":"Te dve yakkhā lohitakkhā brahantā; ","html":"<span class='verse-line'>{}</span>"},"pv39:2.3":{"pli":"Paccekabāhāsu gahetvā revataṃ, ","html":"<span class='verse-line'>{}</span>"},"pv39:2.4":{"pli":"Pakkāmayuṃ devagaṇassa santike. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:3.1":{"pli":"“Ādiccavaṇṇaṃ ruciraṃ pabhassaraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:3.2":{"pli":"Byamhaṃ subhaṃ kañcanajālachannaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv39:3.3":{"pli":"Kassetamākiṇṇajanaṃ vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv39:3.4":{"pli":"Suriyassa raṃsīriva jotamānaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:4.1":{"pli":"Nārīgaṇā candanasāralittā, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:4.2":{"pli":"Ubhato vimānaṃ upasobhayanti; ","html":"<span class='verse-line'>{}</span>"},"pv39:4.3":{"pli":"Taṃ dissati suriyasamānavaṇṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv39:4.4":{"pli":"Ko modati saggapatto vimāne”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:5.1":{"pli":"“Bārāṇasiyaṃ nandiyo nāmāsi, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:5.2":{"pli":"Upāsako amaccharī dānapati vadaññū; ","html":"<span class='verse-line'>{}</span>"},"pv39:5.3":{"pli":"Tassetamākiṇṇajanaṃ vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv39:5.4":{"pli":"Suriyassa raṃsīriva jotamānaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:6.1":{"pli":"Nārīgaṇā candanasāralittā, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:6.2":{"pli":"Ubhato vimānaṃ upasobhayanti; ","html":"<span class='verse-line'>{}</span>"},"pv39:6.3":{"pli":"Taṃ dissati suriyasamānavaṇṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv39:6.4":{"pli":"So modati saggapatto vimāne”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:7.1":{"pli":"“Nandiyassāhaṃ bhariyā, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:7.2":{"pli":"Agārinī sabbakulassa issarā; ","html":"<span class='verse-line'>{}</span>"},"pv39:7.3":{"pli":"Bhattu vimāne ramissāmi dānahaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv39:7.4":{"pli":"Na patthaye nirayadassanāyā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:8.1":{"pli":"“Eso te nirayo supāpadhamme, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:8.2":{"pli":"Puññaṃ tayā akataṃ jīvaloke; ","html":"<span class='verse-line'>{}</span>"},"pv39:8.3":{"pli":"Na hi maccharī rosako pāpadhammo, ","html":"<span class='verse-line'>{}</span>"},"pv39:8.4":{"pli":"Saggūpagānaṃ labhati sahabyatan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:9.1":{"pli":"“Kiṃ nu gūthañca muttañca, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:9.2":{"pli":"asucī paṭidissati; ","html":"<span class='verse-line'>{}</span>"},"pv39:9.3":{"pli":"Duggandhaṃ kimidaṃ mīḷhaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv39:9.4":{"pli":"kimetaṃ upavāyatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:10.1":{"pli":"“Esa saṃsavako nāma, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:10.2":{"pli":"gambhīro sataporiso; ","html":"<span class='verse-line'>{}</span>"},"pv39:10.3":{"pli":"Yattha vassasahassāni, ","html":"<span class='verse-line'>{}</span>"},"pv39:10.4":{"pli":"tuvaṃ paccasi revate”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:11.1":{"pli":"“Kiṃ nu kāyena vācāya, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:11.2":{"pli":"manasā dukkaṭaṃ kataṃ; ","html":"<span class='verse-line'>{}</span>"},"pv39:11.3":{"pli":"Kena saṃsavako laddho, ","html":"<span class='verse-line'>{}</span>"},"pv39:11.4":{"pli":"gambhīro sataporiso”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:12.1":{"pli":"“Samaṇe brāhmaṇe cāpi, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:12.2":{"pli":"aññe vāpi vanibbake; ","html":"<span class='verse-line'>{}</span>"},"pv39:12.3":{"pli":"Musāvādena vañcesi, ","html":"<span class='verse-line'>{}</span>"},"pv39:12.4":{"pli":"taṃ pāpaṃ pakataṃ tayā. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:13.1":{"pli":"Tena saṃsavako laddho, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:13.2":{"pli":"gambhīro sataporiso; ","html":"<span class='verse-line'>{}</span>"},"pv39:13.3":{"pli":"Tattha vassasahassāni, ","html":"<span class='verse-line'>{}</span>"},"pv39:13.4":{"pli":"tuvaṃ paccasi revate. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:14.1":{"pli":"Hatthepi chindanti athopi pāde, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:14.2":{"pli":"Kaṇṇepi chindanti athopi nāsaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv39:14.3":{"pli":"Athopi kākoḷagaṇā samecca, ","html":"<span class='verse-line'>{}</span>"},"pv39:14.4":{"pli":"Saṅgamma khādanti viphandamānan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:15.1":{"pli":"“Sādhu kho maṃ paṭinetha, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:15.2":{"pli":"kāhāmi kusalaṃ bahuṃ; ","html":"<span class='verse-line'>{}</span>"},"pv39:15.3":{"pli":"Dānena samacariyāya, ","html":"<span class='verse-line'>{}</span>"},"pv39:15.4":{"pli":"saṃyamena damena ca; ","html":"<span class='verse-line'>{}</span>"},"pv39:15.5":{"pli":"Yaṃ katvā sukhitā honti, ","html":"<span class='verse-line'>{}</span>"},"pv39:15.6":{"pli":"na ca pacchānutappare”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:16.1":{"pli":"“Pure tuvaṃ pamajjitvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:16.2":{"pli":"idāni paridevasi; ","html":"<span class='verse-line'>{}</span>"},"pv39:16.3":{"pli":"Sayaṃ katānaṃ kammānaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv39:16.4":{"pli":"vipākaṃ anubhossasī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:17.1":{"pli":"“Ko devalokato manussalokaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:17.2":{"pli":"Gantvāna puṭṭho me evaṃ vadeyya; ","html":"<span class='verse-line'>{}</span>"},"pv39:17.3":{"pli":"‘Nikkhittadaṇḍesu dadātha dānaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv39:17.4":{"pli":"Acchādanaṃ seyya mathannapānaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv39:17.5":{"pli":"Na hi maccharī rosako pāpadhammo, ","html":"<span class='verse-line'>{}</span>"},"pv39:17.6":{"pli":"Saggūpagānaṃ labhati sahabyataṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:18.1":{"pli":"Sāhaṃ nūna ito gantvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:18.2":{"pli":"yoniṃ laddhāna mānusiṃ; ","html":"<span class='verse-line'>{}</span>"},"pv39:18.3":{"pli":"Vadaññū sīlasampannā, ","html":"<span class='verse-line'>{}</span>"},"pv39:18.4":{"pli":"kāhāmi kusalaṃ bahuṃ; ","html":"<span class='verse-line'>{}</span>"},"pv39:18.5":{"pli":"Dānena samacariyāya, ","html":"<span class='verse-line'>{}</span>"},"pv39:18.6":{"pli":"saṃyamena damena ca. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:19.1":{"pli":"Ārāmāni ca ropissaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:19.2":{"pli":"dugge saṅkamanāni ca; ","html":"<span class='verse-line'>{}</span>"},"pv39:19.3":{"pli":"Papañca udapānañca, ","html":"<span class='verse-line'>{}</span>"},"pv39:19.4":{"pli":"vippasannena cetasā. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:20.1":{"pli":"Cātuddasiṃ pañcadasiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:20.2":{"pli":"yā ca pakkhassa aṭṭhamī; ","html":"<span class='verse-line'>{}</span>"},"pv39:20.3":{"pli":"Pāṭihāriyapakkhañca, ","html":"<span class='verse-line'>{}</span>"},"pv39:20.4":{"pli":"aṭṭhaṅgasusamāgataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:21.1":{"pli":"Uposathaṃ upavasissaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:21.2":{"pli":"sadā sīlesu saṃvutā; ","html":"<span class='verse-line'>{}</span>"},"pv39:21.3":{"pli":"Na ca dāne pamajjissaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv39:21.4":{"pli":"sāmaṃ diṭṭhamidaṃ mayā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:22.1":{"pli":"Iccevaṃ vippalapantiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:22.2":{"pli":"phandamānaṃ tato tato; ","html":"<span class='verse-line'>{}</span>"},"pv39:22.3":{"pli":"Khipiṃsu niraye ghore, ","html":"<span class='verse-line'>{}</span>"},"pv39:22.4":{"pli":"uddhampādaṃ avaṃsiraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv39:23.1":{"pli":"“Ahaṃ pure maccharinī ahosiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv39:23.2":{"pli":"Paribhāsikā samaṇabrāhmaṇānaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv39:23.3":{"pli":"Vitathena ca sāmikaṃ vañcayitvā, ","html":"<span class='verse-line'>{}</span>"},"pv39:23.4":{"pli":"Paccāmahaṃ niraye ghorarūpe”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv39:24.1":{"pli":"Revatīpetavatthu catutthaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv4":{"pv4:0.1":{"pli":"Petavatthu ","html":"<article id='pv4'><header><ul><li class='division'>{}</li>"},"pv4:0.2":{"pli":"Uragavagga ","html":"<li>{}</li></ul>"},"pv4:0.3":{"pli":"4. Piṭṭhadhītalikapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv4:1.1":{"pli":"“Yaṃ kiñcārammaṇaṃ katvā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv4:1.2":{"pli":"dajjā dānaṃ amaccharī; ","html":"<span class='verse-line'>{}</span>"},"pv4:1.3":{"pli":"Pubbapete ca ārabbha, ","html":"<span class='verse-line'>{}</span>"},"pv4:1.4":{"pli":"atha vā vatthudevatā. ","html":"<span class='verse-line'>{}</span></p>"},"pv4:2.1":{"pli":"Cattāro ca mahārāje, ","html":"<p><span class='verse-line'>{}</span>"},"pv4:2.2":{"pli":"lokapāle yasassine; ","html":"<span class='verse-line'>{}</span>"},"pv4:2.3":{"pli":"Kuveraṃ dhataraṭṭhañca, ","html":"<span class='verse-line'>{}</span>"},"pv4:2.4":{"pli":"virūpakkhaṃ virūḷhakaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv4:2.5":{"pli":"Te ceva pūjitā honti, ","html":"<span class='verse-line'>{}</span>"},"pv4:2.6":{"pli":"dāyakā ca anipphalā. ","html":"<span class='verse-line'>{}</span></p>"},"pv4:3.1":{"pli":"Na hi ruṇṇaṃ vā soko vā, ","html":"<p><span class='verse-line'>{}</span>"},"pv4:3.2":{"pli":"yā caññā paridevanā; ","html":"<span class='verse-line'>{}</span>"},"pv4:3.3":{"pli":"Na taṃ petassa atthāya, ","html":"<span class='verse-line'>{}</span>"},"pv4:3.4":{"pli":"evaṃ tiṭṭhanti ñātayo. ","html":"<span class='verse-line'>{}</span></p>"},"pv4:4.1":{"pli":"Ayañca kho dakkhiṇā dinnā, ","html":"<p><span class='verse-line'>{}</span>"},"pv4:4.2":{"pli":"saṅghamhi suppatiṭṭhitā; ","html":"<span class='verse-line'>{}</span>"},"pv4:4.3":{"pli":"Dīgharattaṃ hitāyassa, ","html":"<span class='verse-line'>{}</span>"},"pv4:4.4":{"pli":"ṭhānaso upakappatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv4:5.1":{"pli":"Piṭṭhadhītalikapetavatthu catutthaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv40":{"pv40:0.1":{"pli":"Petavatthu ","html":"<article id='pv40'><header><ul><li class='division'>{}</li>"},"pv40:0.2":{"pli":"Mahāvagga ","html":"<li>{}</li></ul>"},"pv40:0.3":{"pli":"5. Ucchupetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv40:1.1":{"pli":"“Idaṃ mama ucchuvanaṃ mahantaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv40:1.2":{"pli":"Nibbattati puññaphalaṃ anappakaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv40:1.3":{"pli":"Taṃ dāni me na paribhogameti, ","html":"<span class='verse-line'>{}</span>"},"pv40:1.4":{"pli":"Ācikkha bhante kissa ayaṃ vipāko. ","html":"<span class='verse-line'>{}</span></p>"},"pv40:2.1":{"pli":"Haññāmi khajjāmi ca vāyamāmi, ","html":"<p><span class='verse-line'>{}</span>"},"pv40:2.2":{"pli":"Parisakkāmi paribhuñjituṃ kiñci; ","html":"<span class='verse-line'>{}</span>"},"pv40:2.3":{"pli":"Svāhaṃ chinnathāmo kapaṇo lālapāmi, ","html":"<span class='verse-line'>{}</span>"},"pv40:2.4":{"pli":"Kissa kammassa ayaṃ vipāko. ","html":"<span class='verse-line'>{}</span></p>"},"pv40:3.1":{"pli":"Vighāto cāhaṃ paripatāmi chamāyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv40:3.2":{"pli":"Parivattāmi vāricarova ghamme; ","html":"<span class='verse-line'>{}</span>"},"pv40:3.3":{"pli":"Rudato ca me assukā niggalanti, ","html":"<span class='verse-line'>{}</span>"},"pv40:3.4":{"pli":"Ācikkha bhante kissa ayaṃ vipāko. ","html":"<span class='verse-line'>{}</span></p>"},"pv40:4.1":{"pli":"Chāto kilanto ca pipāsito ca, ","html":"<p><span class='verse-line'>{}</span>"},"pv40:4.2":{"pli":"Santassito sātasukhaṃ na vinde; ","html":"<span class='verse-line'>{}</span>"},"pv40:4.3":{"pli":"Pucchāmi taṃ etamatthaṃ bhadante, ","html":"<span class='verse-line'>{}</span>"},"pv40:4.4":{"pli":"Kathaṃ nu ucchuparibhogaṃ labheyyan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv40:5.1":{"pli":"“Pure tuvaṃ kammamakāsi attanā, ","html":"<p><span class='verse-line'>{}</span>"},"pv40:5.2":{"pli":"Manussabhūto purimāya jātiyā; ","html":"<span class='verse-line'>{}</span>"},"pv40:5.3":{"pli":"Ahañca taṃ etamatthaṃ vadāmi, ","html":"<span class='verse-line'>{}</span>"},"pv40:5.4":{"pli":"Sutvāna tvaṃ etamatthaṃ vijāna. ","html":"<span class='verse-line'>{}</span></p>"},"pv40:6.1":{"pli":"Ucchuṃ tuvaṃ khādamāno payāto, ","html":"<p><span class='verse-line'>{}</span>"},"pv40:6.2":{"pli":"Puriso ca te piṭṭhito anvagacchi; ","html":"<span class='verse-line'>{}</span>"},"pv40:6.3":{"pli":"So ca taṃ paccāsanto kathesi, ","html":"<span class='verse-line'>{}</span>"},"pv40:6.4":{"pli":"Tassa tuvaṃ na kiñci ālapittha. ","html":"<span class='verse-line'>{}</span></p>"},"pv40:7.1":{"pli":"So ca taṃ abhaṇantaṃ ayāci, ","html":"<p><span class='verse-line'>{}</span>"},"pv40:7.2":{"pli":"‘Dehayya ucchun’ti ca taṃ avoca; ","html":"<span class='verse-line'>{}</span>"},"pv40:7.3":{"pli":"Tassa tuvaṃ piṭṭhito ucchuṃ adāsi, ","html":"<span class='verse-line'>{}</span>"},"pv40:7.4":{"pli":"Tassetaṃ kammassa ayaṃ vipāko. ","html":"<span class='verse-line'>{}</span></p>"},"pv40:8.1":{"pli":"Iṅgha tvaṃ gantvāna piṭṭhito gaṇheyyāsi, ","html":"<p><span class='verse-line'>{}</span>"},"pv40:8.2":{"pli":"Gahetvāna taṃ khādassu yāvadatthaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv40:8.3":{"pli":"Teneva tvaṃ attamano bhavissasi, ","html":"<span class='verse-line'>{}</span>"},"pv40:8.4":{"pli":"Haṭṭho cudaggo ca pamodito cā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv40:9.1":{"pli":"Gantvāna so piṭṭhito aggahesi, ","html":"<p><span class='verse-line'>{}</span>"},"pv40:9.2":{"pli":"Gahetvāna taṃ khādi yāvadatthaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv40:9.3":{"pli":"Teneva so attamano ahosi, ","html":"<span class='verse-line'>{}</span>"},"pv40:9.4":{"pli":"Haṭṭho cudaggo ca pamodito cāti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv40:10.1":{"pli":"Ucchupetavatthu pañcamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv41":{"pv41:0.1":{"pli":"Petavatthu ","html":"<article id='pv41'><header><ul><li class='division'>{}</li>"},"pv41:0.2":{"pli":"Mahāvagga ","html":"<li>{}</li></ul>"},"pv41:0.3":{"pli":"6. Kumārapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv41:1.1":{"pli":"“Sāvatthi nāma nagaraṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv41:1.2":{"pli":"himavantassa passato; ","html":"<span class='verse-line'>{}</span>"},"pv41:1.3":{"pli":"Tattha āsuṃ dve kumārā, ","html":"<span class='verse-line'>{}</span>"},"pv41:1.4":{"pli":"rājaputtāti me sutaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv41:2.1":{"pli":"Sammattā rajanīyesu, ","html":"<p><span class='verse-line'>{}</span>"},"pv41:2.2":{"pli":"kāmassādābhinandino; ","html":"<span class='verse-line'>{}</span>"},"pv41:2.3":{"pli":"Paccuppannasukhe giddhā, ","html":"<span class='verse-line'>{}</span>"},"pv41:2.4":{"pli":"na te passiṃsunāgataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv41:3.1":{"pli":"Te cutā ca manussattā, ","html":"<p><span class='verse-line'>{}</span>"},"pv41:3.2":{"pli":"paralokaṃ ito gatā; ","html":"<span class='verse-line'>{}</span>"},"pv41:3.3":{"pli":"Tedha ghosentyadissantā, ","html":"<span class='verse-line'>{}</span>"},"pv41:3.4":{"pli":"pubbe dukkaṭamattano. ","html":"<span class='verse-line'>{}</span></p>"},"pv41:4.1":{"pli":"Bahūsu vata santesu, ","html":"<p><span class='verse-line'>{}</span>"},"pv41:4.2":{"pli":"deyyadhamme upaṭṭhite; ","html":"<span class='verse-line'>{}</span>"},"pv41:4.3":{"pli":"Nāsakkhimhā ca attānaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv41:4.4":{"pli":"parittaṃ kātuṃ sukhāvahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv41:5.1":{"pli":"Kiṃ tato pāpakaṃ assa, ","html":"<p><span class='verse-line'>{}</span>"},"pv41:5.2":{"pli":"yaṃ no rājakulā cutā; ","html":"<span class='verse-line'>{}</span>"},"pv41:5.3":{"pli":"Upapannā pettivisayaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv41:5.4":{"pli":"khuppipāsasamappitā. ","html":"<span class='verse-line'>{}</span></p>"},"pv41:6.1":{"pli":"Sāmino idha hutvāna, ","html":"<p><span class='verse-line'>{}</span>"},"pv41:6.2":{"pli":"honti asāmino tahiṃ; ","html":"<span class='verse-line'>{}</span>"},"pv41:6.3":{"pli":"Bhamanti khuppipāsāya, ","html":"<span class='verse-line'>{}</span>"},"pv41:6.4":{"pli":"manussā unnatonatā. ","html":"<span class='verse-line'>{}</span></p>"},"pv41:7.1":{"pli":"Etamādīnavaṃ ñatvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv41:7.2":{"pli":"issaramadasambhavaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv41:7.3":{"pli":"Pahāya issaramadaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv41:7.4":{"pli":"bhave saggagato naro; ","html":"<span class='verse-line'>{}</span>"},"pv41:7.5":{"pli":"Kāyassa bhedā sappañño, ","html":"<span class='verse-line'>{}</span>"},"pv41:7.6":{"pli":"saggaṃ so upapajjatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv41:8.1":{"pli":"Kumārapetavatthu chaṭṭhaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv42":{"pv42:0.1":{"pli":"Petavatthu ","html":"<article id='pv42'><header><ul><li class='division'>{}</li>"},"pv42:0.2":{"pli":"Mahāvagga ","html":"<li>{}</li></ul>"},"pv42:0.3":{"pli":"7. Rājaputtapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv42:1.1":{"pli":"Pubbe katānaṃ kammānaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv42:1.2":{"pli":"vipāko mathaye manaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv42:1.3":{"pli":"Rūpe sadde rase gandhe, ","html":"<span class='verse-line'>{}</span>"},"pv42:1.4":{"pli":"phoṭṭhabbe ca manorame. ","html":"<span class='verse-line'>{}</span></p>"},"pv42:2.1":{"pli":"Naccaṃ gītaṃ ratiṃ khiḍḍaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv42:2.2":{"pli":"anubhutvā anappakaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv42:2.3":{"pli":"Uyyāne paricaritvā, ","html":"<span class='verse-line'>{}</span>"},"pv42:2.4":{"pli":"pavisanto giribbajaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv42:3.1":{"pli":"Isiṃ sunetta maddakkhi, ","html":"<p><span class='verse-line'>{}</span>"},"pv42:3.2":{"pli":"attadantaṃ samāhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv42:3.3":{"pli":"Appicchaṃ hirisampannaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv42:3.4":{"pli":"uñche pattagate rataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv42:4.1":{"pli":"Hatthikkhandhato oruyha, ","html":"<p><span class='verse-line'>{}</span>"},"pv42:4.2":{"pli":"laddhā bhanteti cābravi; ","html":"<span class='verse-line'>{}</span>"},"pv42:4.3":{"pli":"Tassa pattaṃ gahetvāna, ","html":"<span class='verse-line'>{}</span>"},"pv42:4.4":{"pli":"uccaṃ paggayha khattiyo. ","html":"<span class='verse-line'>{}</span></p>"},"pv42:5.1":{"pli":"Thaṇḍile pattaṃ bhinditvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv42:5.2":{"pli":"hasamāno apakkami; ","html":"<span class='verse-line'>{}</span>"},"pv42:5.3":{"pli":"“Rañño kitavassāhaṃ putto, ","html":"<span class='verse-line'>{}</span>"},"pv42:5.4":{"pli":"kiṃ maṃ bhikkhu karissasi”. ","html":"<span class='verse-line'>{}</span></p>"},"pv42:6.1":{"pli":"Tassa kammassa pharusassa, ","html":"<p><span class='verse-line'>{}</span>"},"pv42:6.2":{"pli":"vipāko kaṭuko ahu; ","html":"<span class='verse-line'>{}</span>"},"pv42:6.3":{"pli":"Yaṃ rājaputto vedesi, ","html":"<span class='verse-line'>{}</span>"},"pv42:6.4":{"pli":"nirayamhi samappito. ","html":"<span class='verse-line'>{}</span></p>"},"pv42:7.1":{"pli":"Chaḷeva caturāsīti, ","html":"<p><span class='verse-line'>{}</span>"},"pv42:7.2":{"pli":"vassāni navutāni ca; ","html":"<span class='verse-line'>{}</span>"},"pv42:7.3":{"pli":"Bhusaṃ dukkhaṃ nigacchittho, ","html":"<span class='verse-line'>{}</span>"},"pv42:7.4":{"pli":"niraye katakibbiso. ","html":"<span class='verse-line'>{}</span></p>"},"pv42:8.1":{"pli":"Uttānopi ca paccittha, ","html":"<p><span class='verse-line'>{}</span>"},"pv42:8.2":{"pli":"nikujjo vāmadakkhiṇo; ","html":"<span class='verse-line'>{}</span>"},"pv42:8.3":{"pli":"Uddhampādo ṭhito ceva, ","html":"<span class='verse-line'>{}</span>"},"pv42:8.4":{"pli":"ciraṃ bālo apaccatha. ","html":"<span class='verse-line'>{}</span></p>"},"pv42:9.1":{"pli":"Bahūni vassasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"pv42:9.2":{"pli":"pūgāni nahutāni ca; ","html":"<span class='verse-line'>{}</span>"},"pv42:9.3":{"pli":"Bhusaṃ dukkhaṃ nigacchittho, ","html":"<span class='verse-line'>{}</span>"},"pv42:9.4":{"pli":"niraye katakibbiso. ","html":"<span class='verse-line'>{}</span></p>"},"pv42:10.1":{"pli":"Etādisaṃ kho kaṭukaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv42:10.2":{"pli":"appaduṭṭhappadosinaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv42:10.3":{"pli":"Paccanti pāpakammantā, ","html":"<span class='verse-line'>{}</span>"},"pv42:10.4":{"pli":"isimāsajja subbataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv42:11.1":{"pli":"So tattha bahuvassāni, ","html":"<p><span class='verse-line'>{}</span>"},"pv42:11.2":{"pli":"vedayitvā bahuṃ dukhaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv42:11.3":{"pli":"Khuppipāsahato nāma, ","html":"<span class='verse-line'>{}</span>"},"pv42:11.4":{"pli":"peto āsi tato cuto. ","html":"<span class='verse-line'>{}</span></p>"},"pv42:12.1":{"pli":"Etamādīnavaṃ ñatvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv42:12.2":{"pli":"issaramadasambhavaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv42:12.3":{"pli":"Pahāya issaramadaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv42:12.4":{"pli":"nivātamanuvattaye. ","html":"<span class='verse-line'>{}</span></p>"},"pv42:13.1":{"pli":"Diṭṭheva dhamme pāsaṃso, ","html":"<p><span class='verse-line'>{}</span>"},"pv42:13.2":{"pli":"yo buddhesu sagāravo; ","html":"<span class='verse-line'>{}</span>"},"pv42:13.3":{"pli":"Kāyassa bhedā sappañño, ","html":"<span class='verse-line'>{}</span>"},"pv42:13.4":{"pli":"saggaṃ so upapajjatīti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv42:14.1":{"pli":"Rājaputtapetavatthu sattamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv43":{"pv43:0.1":{"pli":"Petavatthu ","html":"<article id='pv43'><header><ul><li class='division'>{}</li>"},"pv43:0.2":{"pli":"Mahāvagga ","html":"<li>{}</li></ul>"},"pv43:0.3":{"pli":"8. Gūthakhādakapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv43:1.1":{"pli":"“Gūthakūpato uggantvā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv43:1.2":{"pli":"ko nu dīno patiṭṭhasi; ","html":"<span class='verse-line'>{}</span>"},"pv43:1.3":{"pli":"Nissaṃsayaṃ pāpakammanto, ","html":"<span class='verse-line'>{}</span>"},"pv43:1.4":{"pli":"kiṃ nu saddahase tuvan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv43:2.1":{"pli":"“Ahaṃ bhadante petomhi, ","html":"<p><span class='verse-line'>{}</span>"},"pv43:2.2":{"pli":"duggato yamalokiko; ","html":"<span class='verse-line'>{}</span>"},"pv43:2.3":{"pli":"Pāpakammaṃ karitvāna, ","html":"<span class='verse-line'>{}</span>"},"pv43:2.4":{"pli":"petalokaṃ ito gato”. ","html":"<span class='verse-line'>{}</span></p>"},"pv43:3.1":{"pli":"“Kiṃ nu kāyena vācāya, ","html":"<p><span class='verse-line'>{}</span>"},"pv43:3.2":{"pli":"manasā dukkaṭaṃ kataṃ; ","html":"<span class='verse-line'>{}</span>"},"pv43:3.3":{"pli":"Kissa kammavipākena, ","html":"<span class='verse-line'>{}</span>"},"pv43:3.4":{"pli":"idaṃ dukkhaṃ nigacchasī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv43:4.1":{"pli":"“Ahu āvāsiko mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv43:4.2":{"pli":"issukī kulamaccharī; ","html":"<span class='verse-line'>{}</span>"},"pv43:4.3":{"pli":"Ajjhāsito mayhaṃ ghare, ","html":"<span class='verse-line'>{}</span>"},"pv43:4.4":{"pli":"kadariyo paribhāsako. ","html":"<span class='verse-line'>{}</span></p>"},"pv43:5.1":{"pli":"Tassāhaṃ vacanaṃ sutvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv43:5.2":{"pli":"bhikkhavo paribhāsisaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv43:5.3":{"pli":"Tassa kammavipākena, ","html":"<span class='verse-line'>{}</span>"},"pv43:5.4":{"pli":"petalokaṃ ito gato”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv43:6.1":{"pli":"“Amitto mittavaṇṇena, ","html":"<p><span class='verse-line'>{}</span>"},"pv43:6.2":{"pli":"yo te āsi kulūpako; ","html":"<span class='verse-line'>{}</span>"},"pv43:6.3":{"pli":"Kāyassa bhedā duppañño, ","html":"<span class='verse-line'>{}</span>"},"pv43:6.4":{"pli":"kiṃ nu pecca gatiṃ gato”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv43:7.1":{"pli":"“Tassevāhaṃ pāpakammassa, ","html":"<p><span class='verse-line'>{}</span>"},"pv43:7.2":{"pli":"sīse tiṭṭhāmi matthake; ","html":"<span class='verse-line'>{}</span>"},"pv43:7.3":{"pli":"So ca paravisayaṃ patto, ","html":"<span class='verse-line'>{}</span>"},"pv43:7.4":{"pli":"mameva paricārako. ","html":"<span class='verse-line'>{}</span></p>"},"pv43:8.1":{"pli":"Yaṃ bhadante hadantaññe, ","html":"<p><span class='verse-line'>{}</span>"},"pv43:8.2":{"pli":"etaṃ me hoti bhojanaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv43:8.3":{"pli":"Ahañca kho yaṃ hadāmi, ","html":"<span class='verse-line'>{}</span>"},"pv43:8.4":{"pli":"etaṃ so upajīvatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv43:9.1":{"pli":"Gūthakhādakapetavatthu aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv44":{"pv44:0.1":{"pli":"Petavatthu ","html":"<article id='pv44'><header><ul><li class='division'>{}</li>"},"pv44:0.2":{"pli":"Mahāvagga ","html":"<li>{}</li></ul>"},"pv44:0.3":{"pli":"9. Gūthakhādakapetivatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv44:1.1":{"pli":"“Gūthakūpato uggantvā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv44:1.2":{"pli":"kā nu dīnā patiṭṭhasi; ","html":"<span class='verse-line'>{}</span>"},"pv44:1.3":{"pli":"Nissaṃsayaṃ pāpakammantā, ","html":"<span class='verse-line'>{}</span>"},"pv44:1.4":{"pli":"kiṃ nu saddahase tuvan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv44:2.1":{"pli":"“Ahaṃ bhadante petīmhi, ","html":"<p><span class='verse-line'>{}</span>"},"pv44:2.2":{"pli":"duggatā yamalokikā; ","html":"<span class='verse-line'>{}</span>"},"pv44:2.3":{"pli":"Pāpakammaṃ karitvāna, ","html":"<span class='verse-line'>{}</span>"},"pv44:2.4":{"pli":"petalokaṃ ito gatā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv44:3.1":{"pli":"“Kiṃ nu kāyena vācāya, ","html":"<p><span class='verse-line'>{}</span>"},"pv44:3.2":{"pli":"manasā dukkaṭaṃ kataṃ; ","html":"<span class='verse-line'>{}</span>"},"pv44:3.3":{"pli":"Kissa kammavipākena, ","html":"<span class='verse-line'>{}</span>"},"pv44:3.4":{"pli":"idaṃ dukkhaṃ nigacchasī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv44:4.1":{"pli":"“Ahu āvāsiko mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv44:4.2":{"pli":"issukī kulamaccharī; ","html":"<span class='verse-line'>{}</span>"},"pv44:4.3":{"pli":"Ajjhāsito mayhaṃ ghare, ","html":"<span class='verse-line'>{}</span>"},"pv44:4.4":{"pli":"kadariyo paribhāsako. ","html":"<span class='verse-line'>{}</span></p>"},"pv44:5.1":{"pli":"Tassāhaṃ vacanaṃ sutvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv44:5.2":{"pli":"bhikkhavo paribhāsisaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv44:5.3":{"pli":"Tassa kammavipākena, ","html":"<span class='verse-line'>{}</span>"},"pv44:5.4":{"pli":"petalokaṃ ito gatā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv44:6.1":{"pli":"“Amitto mittavaṇṇena, ","html":"<p><span class='verse-line'>{}</span>"},"pv44:6.2":{"pli":"yo te āsi kulūpako; ","html":"<span class='verse-line'>{}</span>"},"pv44:6.3":{"pli":"Kāyassa bhedā duppañño, ","html":"<span class='verse-line'>{}</span>"},"pv44:6.4":{"pli":"kiṃ nu pecca gatiṃ gato”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv44:7.1":{"pli":"“Tassevāhaṃ pāpakammassa, ","html":"<p><span class='verse-line'>{}</span>"},"pv44:7.2":{"pli":"sīse tiṭṭhāmi matthake; ","html":"<span class='verse-line'>{}</span>"},"pv44:7.3":{"pli":"So ca paravisayaṃ patto, ","html":"<span class='verse-line'>{}</span>"},"pv44:7.4":{"pli":"mameva paricārako. ","html":"<span class='verse-line'>{}</span></p>"},"pv44:8.1":{"pli":"Yaṃ bhadante hadantaññe, ","html":"<p><span class='verse-line'>{}</span>"},"pv44:8.2":{"pli":"etaṃ me hoti bhojanaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv44:8.3":{"pli":"Ahañca kho yaṃ hadāmi, ","html":"<span class='verse-line'>{}</span>"},"pv44:8.4":{"pli":"etaṃ so upajīvatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv44:9.1":{"pli":"Gūthakhādakapetivatthu navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv45":{"pv45:0.1":{"pli":"Petavatthu ","html":"<article id='pv45'><header><ul><li class='division'>{}</li>"},"pv45:0.2":{"pli":"Mahāvagga ","html":"<li>{}</li></ul>"},"pv45:0.3":{"pli":"10. Gaṇapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv45:1.1":{"pli":"“Naggā dubbaṇṇarūpāttha, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv45:1.2":{"pli":"kisā dhamanisanthatā; ","html":"<span class='verse-line'>{}</span>"},"pv45:1.3":{"pli":"Upphāsulikā kisikā, ","html":"<span class='verse-line'>{}</span>"},"pv45:1.4":{"pli":"ke nu tumhettha mārisā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv45:2.1":{"pli":"“Mayaṃ bhadante petāmhā, ","html":"<p><span class='verse-line'>{}</span>"},"pv45:2.2":{"pli":"duggatā yamalokikā; ","html":"<span class='verse-line'>{}</span>"},"pv45:2.3":{"pli":"Pāpakammaṃ karitvāna, ","html":"<span class='verse-line'>{}</span>"},"pv45:2.4":{"pli":"petalokaṃ ito gatā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv45:3.1":{"pli":"“Kiṃ nu kāyena vācāya, ","html":"<p><span class='verse-line'>{}</span>"},"pv45:3.2":{"pli":"manasā dukkaṭaṃ kataṃ; ","html":"<span class='verse-line'>{}</span>"},"pv45:3.3":{"pli":"Kissa kammavipākena, ","html":"<span class='verse-line'>{}</span>"},"pv45:3.4":{"pli":"petalokaṃ ito gatā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv45:4.1":{"pli":"“Anāvaṭesu titthesu, ","html":"<p><span class='verse-line'>{}</span>"},"pv45:4.2":{"pli":"vicinimhaddhamāsakaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv45:4.3":{"pli":"Santesu deyyadhammesu, ","html":"<span class='verse-line'>{}</span>"},"pv45:4.4":{"pli":"dīpaṃ nākamha attano. ","html":"<span class='verse-line'>{}</span></p>"},"pv45:5.1":{"pli":"Nadiṃ upema tasitā, ","html":"<p><span class='verse-line'>{}</span>"},"pv45:5.2":{"pli":"rittakā parivattati; ","html":"<span class='verse-line'>{}</span>"},"pv45:5.3":{"pli":"Chāyaṃ upema uṇhesu, ","html":"<span class='verse-line'>{}</span>"},"pv45:5.4":{"pli":"ātapo parivattati. ","html":"<span class='verse-line'>{}</span></p>"},"pv45:6.1":{"pli":"Aggivaṇṇo ca no vāto, ","html":"<p><span class='verse-line'>{}</span>"},"pv45:6.2":{"pli":"ḍahanto upavāyati; ","html":"<span class='verse-line'>{}</span>"},"pv45:6.3":{"pli":"Etañca bhante arahāma, ","html":"<span class='verse-line'>{}</span>"},"pv45:6.4":{"pli":"aññañca pāpakaṃ tato. ","html":"<span class='verse-line'>{}</span></p>"},"pv45:7.1":{"pli":"Api yojanāni gacchāma, ","html":"<p><span class='verse-line'>{}</span>"},"pv45:7.2":{"pli":"chātā āhāragedhino; ","html":"<span class='verse-line'>{}</span>"},"pv45:7.3":{"pli":"Aladdhāva nivattāma, ","html":"<span class='verse-line'>{}</span>"},"pv45:7.4":{"pli":"aho no appapuññatā. ","html":"<span class='verse-line'>{}</span></p>"},"pv45:8.1":{"pli":"Chātā pamucchitā bhantā, ","html":"<p><span class='verse-line'>{}</span>"},"pv45:8.2":{"pli":"bhūmiyaṃ paṭisumbhitā; ","html":"<span class='verse-line'>{}</span>"},"pv45:8.3":{"pli":"Uttānā paṭikirāma, ","html":"<span class='verse-line'>{}</span>"},"pv45:8.4":{"pli":"avakujjā patāmase. ","html":"<span class='verse-line'>{}</span></p>"},"pv45:9.1":{"pli":"Te ca tattheva patitā, ","html":"<p><span class='verse-line'>{}</span>"},"pv45:9.2":{"pli":"bhūmiyaṃ paṭisumbhitā; ","html":"<span class='verse-line'>{}</span>"},"pv45:9.3":{"pli":"Uraṃ sīsañca ghaṭṭema, ","html":"<span class='verse-line'>{}</span>"},"pv45:9.4":{"pli":"aho no appapuññatā. ","html":"<span class='verse-line'>{}</span></p>"},"pv45:10.1":{"pli":"Etañca bhante arahāma, ","html":"<p><span class='verse-line'>{}</span>"},"pv45:10.2":{"pli":"aññañca pāpakaṃ tato; ","html":"<span class='verse-line'>{}</span>"},"pv45:10.3":{"pli":"Santesu deyyadhammesu, ","html":"<span class='verse-line'>{}</span>"},"pv45:10.4":{"pli":"dīpaṃ nākamha attano. ","html":"<span class='verse-line'>{}</span></p>"},"pv45:11.1":{"pli":"Te hi nūna ito gantvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv45:11.2":{"pli":"yoniṃ laddhāna mānusiṃ; ","html":"<span class='verse-line'>{}</span>"},"pv45:11.3":{"pli":"Vadaññū sīlasampannā, ","html":"<span class='verse-line'>{}</span>"},"pv45:11.4":{"pli":"kāhāma kusalaṃ bahun”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv45:12.1":{"pli":"Gaṇapetavatthu dasamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv46":{"pv46:0.1":{"pli":"Petavatthu ","html":"<article id='pv46'><header><ul><li class='division'>{}</li>"},"pv46:0.2":{"pli":"Mahāvagga ","html":"<li>{}</li></ul>"},"pv46:0.3":{"pli":"11. Pāṭaliputtapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv46:1.1":{"pli":"“Diṭṭhā tayā nirayā tiracchānayoni, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv46:1.2":{"pli":"Petā asurā athavāpi mānusā devā; ","html":"<span class='verse-line'>{}</span>"},"pv46:1.3":{"pli":"Sayamaddasa kammavipākamattano, ","html":"<span class='verse-line'>{}</span>"},"pv46:1.4":{"pli":"Nessāmi taṃ pāṭaliputtamakkhataṃ; ","html":"<span class='verse-line'>{}</span>"},"pv46:1.5":{"pli":"Tattha gantvā kusalaṃ karohi kammaṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"pv46:2.1":{"pli":"“Atthakāmosi me yakkha, ","html":"<p><span class='verse-line'>{}</span>"},"pv46:2.2":{"pli":"hitakāmosi devate; ","html":"<span class='verse-line'>{}</span>"},"pv46:2.3":{"pli":"Karomi tuyhaṃ vacanaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv46:2.4":{"pli":"tvaṃsi ācariyo mama. ","html":"<span class='verse-line'>{}</span></p>"},"pv46:3.1":{"pli":"Diṭṭhā mayā nirayā tiracchānayoni, ","html":"<p><span class='verse-line'>{}</span>"},"pv46:3.2":{"pli":"Petā asurā athavāpi mānusā devā; ","html":"<span class='verse-line'>{}</span>"},"pv46:3.3":{"pli":"Sayamaddasaṃ kammavipākamattano, ","html":"<span class='verse-line'>{}</span>"},"pv46:3.4":{"pli":"Kāhāmi puññāni anappakānī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv46:4.1":{"pli":"Pāṭaliputtapetavatthu ekādasamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv47":{"pv47:0.1":{"pli":"Petavatthu ","html":"<article id='pv47'><header><ul><li class='division'>{}</li>"},"pv47:0.2":{"pli":"Mahāvagga ","html":"<li>{}</li></ul>"},"pv47:0.3":{"pli":"12. Ambavanapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv47:1.1":{"pli":"“Ayañca te pokkharaṇī surammā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv47:1.2":{"pli":"Samā sutitthā ca mahodakā ca; ","html":"<span class='verse-line'>{}</span>"},"pv47:1.3":{"pli":"Supupphitā bhamaragaṇānukiṇṇā, ","html":"<span class='verse-line'>{}</span>"},"pv47:1.4":{"pli":"Kathaṃ tayā laddhā ayaṃ manuññā. ","html":"<span class='verse-line'>{}</span></p>"},"pv47:2.1":{"pli":"Idañca te ambavanaṃ surammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv47:2.2":{"pli":"Sabbotukaṃ dhārayate phalāni; ","html":"<span class='verse-line'>{}</span>"},"pv47:2.3":{"pli":"Supupphitaṃ bhamaragaṇānukiṇṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv47:2.4":{"pli":"Kathaṃ tayā laddhamidaṃ vimānaṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"pv47:3.1":{"pli":"“Ambapakkaṃ dakaṃ yāgu, ","html":"<p><span class='verse-line'>{}</span>"},"pv47:3.2":{"pli":"sītacchāyā manoramā; ","html":"<span class='verse-line'>{}</span>"},"pv47:3.3":{"pli":"Dhītāya dinnadānena, ","html":"<span class='verse-line'>{}</span>"},"pv47:3.4":{"pli":"tena me idha labbhati”. ","html":"<span class='verse-line'>{}</span></p>"},"pv47:4.1":{"pli":"“Sandiṭṭhikaṃ kammaṃ evaṃ passatha, ","html":"<p><span class='verse-line'>{}</span>"},"pv47:4.2":{"pli":"Dānassa damassa saṃyamassa vipākaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv47:4.3":{"pli":"Dāsī ahaṃ ayyakulesu hutvā, ","html":"<span class='verse-line'>{}</span>"},"pv47:4.4":{"pli":"Suṇisā homi agārassa issarā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv47:5.1":{"pli":"Ambavanapetavatthu dvādasamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv48":{"pv48:0.1":{"pli":"Petavatthu ","html":"<article id='pv48'><header><ul><li class='division'>{}</li>"},"pv48:0.2":{"pli":"Mahāvagga ","html":"<li>{}</li></ul>"},"pv48:0.3":{"pli":"13. Akkharukkhapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv48:1.1":{"pli":"“Yaṃ dadāti na taṃ hoti, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv48:1.2":{"pli":"Detheva dānaṃ datvā ubhayaṃ tarati; ","html":"<span class='verse-line'>{}</span>"},"pv48:1.3":{"pli":"Ubhayaṃ tena dānena gacchati, ","html":"<span class='verse-line'>{}</span>"},"pv48:1.4":{"pli":"Jāgaratha māpamajjathā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv48:2.1":{"pli":"Akkharukkhapetavatthu terasamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv49":{"pv49:0.1":{"pli":"Petavatthu ","html":"<article id='pv49'><header><ul><li class='division'>{}</li>"},"pv49:0.2":{"pli":"Mahāvagga ","html":"<li>{}</li></ul>"},"pv49:0.3":{"pli":"14. Bhogasaṃharaṇapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv49:1.1":{"pli":"“Mayaṃ bhoge saṃharimhā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv49:1.2":{"pli":"samena visamena ca; ","html":"<span class='verse-line'>{}</span>"},"pv49:1.3":{"pli":"Te aññe paribhuñjanti, ","html":"<span class='verse-line'>{}</span>"},"pv49:1.4":{"pli":"mayaṃ dukkhassa bhāginī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv49:2.1":{"pli":"Bhogasaṃharaṇapetavatthu cuddasamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv5":{"pv5:0.1":{"pli":"Petavatthu ","html":"<article id='pv5'><header><ul><li class='division'>{}</li>"},"pv5:0.2":{"pli":"Uragavagga ","html":"<li>{}</li></ul>"},"pv5:0.3":{"pli":"5. Tirokuṭṭapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv5:1.1":{"pli":"“Tirokuṭṭesu tiṭṭhanti, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv5:1.2":{"pli":"sandhisiṅghāṭakesu ca; ","html":"<span class='verse-line'>{}</span>"},"pv5:1.3":{"pli":"Dvārabāhāsu tiṭṭhanti, ","html":"<span class='verse-line'>{}</span>"},"pv5:1.4":{"pli":"āgantvāna sakaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv5:2.1":{"pli":"Pahūte annapānamhi, ","html":"<p><span class='verse-line'>{}</span>"},"pv5:2.2":{"pli":"khajjabhojje upaṭṭhite; ","html":"<span class='verse-line'>{}</span>"},"pv5:2.3":{"pli":"Na tesaṃ koci sarati, ","html":"<span class='verse-line'>{}</span>"},"pv5:2.4":{"pli":"sattānaṃ kammapaccayā. ","html":"<span class='verse-line'>{}</span></p>"},"pv5:3.1":{"pli":"Evaṃ dadanti ñātīnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv5:3.2":{"pli":"ye honti anukampakā; ","html":"<span class='verse-line'>{}</span>"},"pv5:3.3":{"pli":"Suciṃ paṇītaṃ kālena, ","html":"<span class='verse-line'>{}</span>"},"pv5:3.4":{"pli":"kappiyaṃ pānabhojanaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv5:3.5":{"pli":"‘Idaṃ vo ñātīnaṃ hotu, ","html":"<span class='verse-line'>{}</span>"},"pv5:3.6":{"pli":"sukhitā hontu ñātayo’. ","html":"<span class='verse-line'>{}</span></p>"},"pv5:4.1":{"pli":"Te ca tattha samāgantvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv5:4.2":{"pli":"ñātipetā samāgatā; ","html":"<span class='verse-line'>{}</span>"},"pv5:4.3":{"pli":"Pahūte annapānamhi, ","html":"<span class='verse-line'>{}</span>"},"pv5:4.4":{"pli":"sakkaccaṃ anumodare. ","html":"<span class='verse-line'>{}</span></p>"},"pv5:5.1":{"pli":"‘Ciraṃ jīvantu no ñātī, ","html":"<p><span class='verse-line'>{}</span>"},"pv5:5.2":{"pli":"yesaṃ hetu labhāmase; ","html":"<span class='verse-line'>{}</span>"},"pv5:5.3":{"pli":"Amhākañca katā pūjā, ","html":"<span class='verse-line'>{}</span>"},"pv5:5.4":{"pli":"dāyakā ca anipphalā’. ","html":"<span class='verse-line'>{}</span></p>"},"pv5:6.1":{"pli":"Na hi tattha kasī atthi, ","html":"<p><span class='verse-line'>{}</span>"},"pv5:6.2":{"pli":"gorakkhettha na vijjati; ","html":"<span class='verse-line'>{}</span>"},"pv5:6.3":{"pli":"Vaṇijjā tādisī natthi, ","html":"<span class='verse-line'>{}</span>"},"pv5:6.4":{"pli":"hiraññena kayākayaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv5:6.5":{"pli":"Ito dinnena yāpenti, ","html":"<span class='verse-line'>{}</span>"},"pv5:6.6":{"pli":"petā kālagatā tahiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv5:7.1":{"pli":"Unname udakaṃ vuṭṭhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv5:7.2":{"pli":"yathā ninnaṃ pavattati; ","html":"<span class='verse-line'>{}</span>"},"pv5:7.3":{"pli":"Evamevaṃ ito dinnaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv5:7.4":{"pli":"petānaṃ upakappati. ","html":"<span class='verse-line'>{}</span></p>"},"pv5:8.1":{"pli":"Yathā vārivahā pūrā, ","html":"<p><span class='verse-line'>{}</span>"},"pv5:8.2":{"pli":"paripūrenti sāgaraṃ; ","html":"<span class='verse-line'>{}</span>"},"pv5:8.3":{"pli":"Evamevaṃ ito dinnaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv5:8.4":{"pli":"petānaṃ upakappati. ","html":"<span class='verse-line'>{}</span></p>"},"pv5:9.1":{"pli":"‘Adāsi me akāsi me, ","html":"<p><span class='verse-line'>{}</span>"},"pv5:9.2":{"pli":"ñāti mittā sakhā ca me’; ","html":"<span class='verse-line'>{}</span>"},"pv5:9.3":{"pli":"Petānaṃ dakkhiṇaṃ dajjā, ","html":"<span class='verse-line'>{}</span>"},"pv5:9.4":{"pli":"pubbe katamanussaraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv5:10.1":{"pli":"Na hi ruṇṇaṃ vā soko vā, ","html":"<p><span class='verse-line'>{}</span>"},"pv5:10.2":{"pli":"yā caññā paridevanā; ","html":"<span class='verse-line'>{}</span>"},"pv5:10.3":{"pli":"Na taṃ petānamatthāya, ","html":"<span class='verse-line'>{}</span>"},"pv5:10.4":{"pli":"evaṃ tiṭṭhanti ñātayo. ","html":"<span class='verse-line'>{}</span></p>"},"pv5:11.1":{"pli":"Ayañca kho dakkhiṇā dinnā, ","html":"<p><span class='verse-line'>{}</span>"},"pv5:11.2":{"pli":"saṅghamhi suppatiṭṭhitā; ","html":"<span class='verse-line'>{}</span>"},"pv5:11.3":{"pli":"Dīgharattaṃ hitāyassa, ","html":"<span class='verse-line'>{}</span>"},"pv5:11.4":{"pli":"ṭhānaso upakappati. ","html":"<span class='verse-line'>{}</span></p>"},"pv5:12.1":{"pli":"So ñātidhammo ca ayaṃ nidassito, ","html":"<p><span class='verse-line'>{}</span>"},"pv5:12.2":{"pli":"Petāna pūjā ca katā uḷārā; ","html":"<span class='verse-line'>{}</span>"},"pv5:12.3":{"pli":"Balañca bhikkhūnamanuppadinnaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv5:12.4":{"pli":"Tumhehi puññaṃ pasutaṃ anappakan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv5:13.1":{"pli":"Tirokuṭṭapetavatthu pañcamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}