{"tha-ap25":{"tha-ap25:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap25'><header><ul><li class='division'>{}</li>"},"tha-ap25:0.2":{"pli":"Subhūtivagga ","html":"<li>{}</li></ul>"},"tha-ap25:0.3":{"pli":"3 Tisaraṇagamaniyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap25:1.1":{"pli":"“Nagare candavatiyā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap25:1.2":{"pli":"mātuupaṭṭhāko ahuṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:1.3":{"pli":"Andhā mātā pitā mayhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:1.4":{"pli":"te posemi ahaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:2.1":{"pli":"Rahogato nisīditvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:2.2":{"pli":"evaṃ cintesahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:2.3":{"pli":"Posento mātāpitaro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:2.4":{"pli":"pabbajjaṃ na labhāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:3.1":{"pli":"Mahandhakārapihitā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:3.2":{"pli":"tividhaggīhi ḍayhare; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:3.3":{"pli":"Etādise bhave jāte, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:3.4":{"pli":"natthi koci vināyako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:4.1":{"pli":"Buddho loke samuppanno, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:4.2":{"pli":"dippati dāni sāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:4.3":{"pli":"Sakkā uddharituṃ attā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:4.4":{"pli":"puññakāmena jantunā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:5.1":{"pli":"Uggayha tīṇi saraṇe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:5.2":{"pli":"paripuṇṇāni gopayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:5.3":{"pli":"Tena kammena sukatena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:5.4":{"pli":"paṭimokkhāmi duggatiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:6.1":{"pli":"Nisabho nāma samaṇo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:6.2":{"pli":"buddhassa aggasāvako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:6.3":{"pli":"Tamahaṃ upagantvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:6.4":{"pli":"saraṇagamanaṃ gahiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:7.1":{"pli":"Vassasatasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:7.2":{"pli":"āyu vijjati tāvade; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:7.3":{"pli":"Tāvatā saraṇagamanaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:7.4":{"pli":"paripuṇṇaṃ agopayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:8.1":{"pli":"Carime vattamānamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:8.2":{"pli":"saraṇaṃ taṃ anussariṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:8.3":{"pli":"Tena kammena sukatena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:8.4":{"pli":"tāvatiṃsaṃ agacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:9.1":{"pli":"Devalokagato santo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:9.2":{"pli":"puññakammasamāhito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:9.3":{"pli":"Yaṃ desaṃ upapajjāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:9.4":{"pli":"aṭṭha hetū labhāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:10.1":{"pli":"Disāsu pūjito homi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:10.2":{"pli":"tikkhapañño bhavāmahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:10.3":{"pli":"Sabbe devānuvattanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:10.4":{"pli":"amitabhogaṃ labhāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:11.1":{"pli":"Suvaṇṇavaṇṇo sabbattha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:11.2":{"pli":"paṭikanto bhavāmahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:11.3":{"pli":"Mittānaṃ acalo homi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:11.4":{"pli":"yaso abbhuggato mamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:12.1":{"pli":"Asītikkhattu devindo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:12.2":{"pli":"devarajjamakārayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:12.3":{"pli":"Dibbasukhaṃ anubhaviṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:12.4":{"pli":"accharāhi purakkhato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:13.1":{"pli":"Pañcasattatikkhattuñca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:13.2":{"pli":"cakkavattī ahosahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:13.3":{"pli":"Padesarajjaṃ vipulaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:13.4":{"pli":"gaṇanāto asaṅkhiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:14.1":{"pli":"Pacchime bhave sampatte, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:14.2":{"pli":"puññakammasamāhito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:14.3":{"pli":"Pure sāvatthiyaṃ jāto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:14.4":{"pli":"mahāsāle suaḍḍhake. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:15.1":{"pli":"Nagarā nikkhamitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:15.2":{"pli":"dārakehi purakkhato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:15.3":{"pli":"Hasakhiḍḍasamaṅgīhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:15.4":{"pli":"saṅghārāmaṃ upāgamiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:16.1":{"pli":"Tatthaddasāsiṃ samaṇaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:16.2":{"pli":"vippamuttaṃ nirūpadhiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:16.3":{"pli":"So me dhammamadesesi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:16.4":{"pli":"saraṇañca adāsi me. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:17.1":{"pli":"Sohaṃ sutvāna saraṇaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:17.2":{"pli":"saraṇaṃ me anussariṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:17.3":{"pli":"Ekāsane nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:17.4":{"pli":"arahattamapāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:18.1":{"pli":"Jātiyā sattame vasse, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:18.2":{"pli":"arahattamapāpuṇiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:18.3":{"pli":"Upasampādayi buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:18.4":{"pli":"guṇamaññāya cakkhumā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:19.1":{"pli":"Aparimeyye ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:19.2":{"pli":"saraṇāni agacchahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:19.3":{"pli":"Tato me sukataṃ kammaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:19.4":{"pli":"phalaṃ dassesi me idha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:20.1":{"pli":"Sugopitaṃ me saraṇaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:20.2":{"pli":"mānasaṃ suppaṇīhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:20.3":{"pli":"Anubhotvā yasaṃ sabbaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:20.4":{"pli":"pattomhi acalaṃ padaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:21.1":{"pli":"Yesaṃ sotāvadhānatthi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:21.2":{"pli":"suṇotha mama bhāsato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:21.3":{"pli":"Ahaṃ vo kathayissāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:21.4":{"pli":"sāmaṃ diṭṭhaṃ padaṃ mama. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:22.1":{"pli":"‘Buddho loke samuppanno, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:22.2":{"pli":"vattate jinasāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:22.3":{"pli":"Amatā vāditā bherī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:22.4":{"pli":"sokasallavinodanā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:23.1":{"pli":"Yathāsakena thāmena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:23.2":{"pli":"puññakkhette anuttare; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:23.3":{"pli":"Adhikāraṃ kareyyātha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:23.4":{"pli":"passayissatha nibbutiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:24.1":{"pli":"Paggayha tīṇi saraṇe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:24.2":{"pli":"pañcasīlāni gopiya; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:24.3":{"pli":"Buddhe cittaṃ pasādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:24.4":{"pli":"dukkhassantaṃ karissatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:25.1":{"pli":"Sammā dhammaṃ bhāvetvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:25.2":{"pli":"Sīlāni parigopiya; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:25.3":{"pli":"Aciraṃ arahattaṃ vo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:25.4":{"pli":"Sabbepi pāpuṇissatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:26.1":{"pli":"Tevijjo iddhipattomhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:26.2":{"pli":"cetopariyakovido; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:26.3":{"pli":"Sāvako te mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:26.4":{"pli":"saraṇo vandati satthuno’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:27.1":{"pli":"Aparimeyye ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:27.2":{"pli":"saraṇaṃ buddhassa gacchahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:27.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:27.4":{"pli":"saraṇaṃ gamane phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap25:28.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap25:28.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:28.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap25:28.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap25:29.1":{"pli":"Itthaṃ sudaṃ āyasmā tisaraṇagamaniyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap25:30.1":{"pli":"Tisaraṇagamaniyattherassāpadānaṃ tatiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap250":{"tha-ap250:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap250'><header><ul><li class='division'>{}</li>"},"tha-ap250:0.2":{"pli":"Tuvaradāyakavagga ","html":"<li>{}</li></ul>"},"tha-ap250:0.3":{"pli":"8 Sattalipupphapūjakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap250:1.1":{"pli":"“Satta sattalipupphāni, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap250:1.2":{"pli":"sīse katvānahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap250:1.3":{"pli":"Buddhassa abhiropesiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap250:1.4":{"pli":"vessabhumhi naruttame. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap250:2.1":{"pli":"Ekattiṃse ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap250:2.2":{"pli":"yaṃ pupphamabhipūjayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap250:2.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap250:2.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap250:3.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap250:3.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap250:3.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap250:4.1":{"pli":"Itthaṃ sudaṃ āyasmā sattalipupphapūjako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap250:5.1":{"pli":"Sattalipupphapūjakattherassāpadānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap251":{"tha-ap251:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap251'><header><ul><li class='division'>{}</li>"},"tha-ap251:0.2":{"pli":"Tuvaradāyakavagga ","html":"<li>{}</li></ul>"},"tha-ap251:0.3":{"pli":"9 Bimbijāliyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap251:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap251:1.2":{"pli":"sayambhū aggapuggalo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap251:1.3":{"pli":"Catusaccaṃ pakāseti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap251:1.4":{"pli":"dīpeti amataṃ padaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap251:2.1":{"pli":"Bimbijālakapupphāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap251:2.2":{"pli":"puthu katvānahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap251:2.3":{"pli":"Buddhassa abhiropesiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap251:2.4":{"pli":"dvipadindassa tādino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap251:3.1":{"pli":"Aṭṭhasaṭṭhimhito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap251:3.2":{"pli":"caturo kiñjakes