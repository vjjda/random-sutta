{"thi-ap30":{"thi-ap30:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap30'><header><ul><li class='division'>{}</li>"},"thi-ap30:0.2":{"pli":"Kuṇḍalakesīvagga ","html":"<li>{}</li></ul>"},"thi-ap30:0.3":{"pli":"10 Yasodharāpamukhaaṭṭhārasabhikkhunīsahassaapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap30:1.1":{"pli":"Aṭṭhārasasahassāni, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap30:1.2":{"pli":"bhikkhunī sakyasambhavā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:1.3":{"pli":"Yasodharāpamukhāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:1.4":{"pli":"sambuddhaṃ upasaṅkamuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:2.1":{"pli":"Aṭṭhārasasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:2.2":{"pli":"sabbā honti mahiddhikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:2.3":{"pli":"Vandantī munino pāde, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:2.4":{"pli":"ārocenti yathābalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:3.1":{"pli":"“Jāti khīṇā jarā byādhi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:3.2":{"pli":"maraṇañca mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:3.3":{"pli":"Anāsavaṃ padaṃ santaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:3.4":{"pli":"amataṃ yāma nāyaka. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:4.1":{"pli":"Khalitañce pure atthi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:4.2":{"pli":"sabbāsampi mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:4.3":{"pli":"Aparādhamajānantī, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:4.4":{"pli":"khama amhaṃ vināyaka”. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:5.1":{"pli":"“Iddhiñcāpi nidassetha, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:5.2":{"pli":"mama sāsanakārikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:5.3":{"pli":"Parisānañca sabbāsaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:5.4":{"pli":"kaṅkhaṃ chindatha yāvatā”. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:6.1":{"pli":"“Yasodharā mahāvīra, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:6.2":{"pli":"manāpā piyadassanā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:6.3":{"pli":"Sabbā tuyhaṃ mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:6.4":{"pli":"agārasmiṃ pajāpati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:7.1":{"pli":"Thīnaṃ satasahassānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:7.2":{"pli":"navutīnaṃ chaduttari; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:7.3":{"pli":"Agāre te mayaṃ vīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:7.4":{"pli":"pāmokkhā sabbā issarā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:8.1":{"pli":"Rūpācāraguṇūpetā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:8.2":{"pli":"yobbanaṭṭhā piyaṃvadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:8.3":{"pli":"Sabbā no apacāyanti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:8.4":{"pli":"devatā viya mānusā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:9.1":{"pli":"Aṭṭhārasasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:9.2":{"pli":"sabbā sākiyasambhavā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:9.3":{"pli":"Yasodharāsahassāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:9.4":{"pli":"pāmokkhā issarā tadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:10.1":{"pli":"Kāmadhātumatikkamma, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:10.2":{"pli":"saṇṭhitā rūpadhātuyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:10.3":{"pli":"Rūpena sadisā natthi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:10.4":{"pli":"sahassānaṃ mahāmuni”. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:11.1":{"pli":"Sambuddhaṃ abhivādetvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:11.2":{"pli":"iddhiṃ dassaṃsu satthuno; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:11.3":{"pli":"Nekā nānāvidhākārā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:11.4":{"pli":"mahāiddhīpi dassayuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:12.1":{"pli":"Cakkavāḷasamaṃ kāyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:12.2":{"pli":"sīsaṃ uttarato kuru; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:12.3":{"pli":"Ubho pakkhā duve dīpā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:12.4":{"pli":"jambudīpaṃ sarīrato. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:13.1":{"pli":"Dakkhiṇañca saraṃ piñchaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:13.2":{"pli":"nānāsākhā tu pattakā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:13.3":{"pli":"Candañca sūriyañcakkhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:13.4":{"pli":"merupabbatato sikhaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:14.1":{"pli":"Cakkavāḷagiriṃ tuṇḍaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:14.2":{"pli":"jamburukkhaṃ samūlakaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:14.3":{"pli":"Bījamānā upāgantvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:14.4":{"pli":"vandantī lokanāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:15.1":{"pli":"Hatthivaṇṇaṃ tathevassaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:15.2":{"pli":"pabbataṃ jaladhiṃ tathā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:15.3":{"pli":"Candañca sūriyaṃ meruṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:15.4":{"pli":"sakkavaṇṇañca dassayuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:16.1":{"pli":"“Yasodharā mayaṃ vīra, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:16.2":{"pli":"pāde vandāma cakkhuma; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:16.3":{"pli":"Tava cirapabhāvena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:16.4":{"pli":"nipphannā naranāyaka. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:17.1":{"pli":"Iddhīsu ca vasī homa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:17.2":{"pli":"dibbāya sotadhātuyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:17.3":{"pli":"Cetopariyañāṇassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:17.4":{"pli":"vasī homa mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:18.1":{"pli":"Pubbenivāsaṃ jānāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:18.2":{"pli":"dibbacakkhu visodhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:18.3":{"pli":"Sabbāsavā parikkhīṇā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:18.4":{"pli":"natthi dāni punabbhavo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:19.1":{"pli":"Atthadhammaniruttīsu, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:19.2":{"pli":"paṭibhāne tatheva ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:19.3":{"pli":"Ñāṇaṃ amhaṃ mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:19.4":{"pli":"uppannaṃ tava santike. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:20.1":{"pli":"Pubbānaṃ lokanāthānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:20.2":{"pli":"saṅgamaṃ no nidassitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:20.3":{"pli":"Adhikārā bahū amhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:20.4":{"pli":"tuyhatthāya mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:21.1":{"pli":"Yaṃ amhaṃ pūritaṃ kammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:21.2":{"pli":"kusalaṃ sarase mune; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:21.3":{"pli":"Tuyhatthāya mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:21.4":{"pli":"puññānupacitāni no. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:22.1":{"pli":"Abhabbaṭṭhāne vajjetvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:22.2":{"pli":"vārayimha anācaraṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:22.3":{"pli":"Tuyhatthāya mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:22.4":{"pli":"cattāni jīvitāni no. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:23.1":{"pli":"Nekakoṭisahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:23.2":{"pli":"bhariyatthāyadāsi no; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:23.3":{"pli":"Na tattha vimanā homa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:23.4":{"pli":"tuyhatthāya mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:24.1":{"pli":"Nekakoṭisahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:24.2":{"pli":"upakārāyadāsi no; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:24.3":{"pli":"Na tattha vimanā homa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:24.4":{"pli":"tuyhatthāya mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:25.1":{"pli":"Nekakoṭisahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:25.2":{"pli":"bhojanatthāyadāsi no; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:25.3":{"pli":"Na tattha vimanā homa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:25.4":{"pli":"tuyhatthāya mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:26.1":{"pli":"Nekakoṭisahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:26.2":{"pli":"jīvitāni cajimhase; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:26.3":{"pli":"Bhayamokkhaṃ karissāma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:26.4":{"pli":"jīvitāni cajimhase. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:27.1":{"pli":"Aṅgagate alaṅkāre, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:27.2":{"pli":"vatthe nānāvidhe bahū; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:27.3":{"pli":"Itthibhaṇḍe na gūhāma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:27.4":{"pli":"tuyhatthāya mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:28.1":{"pli":"Dhanadhaññapariccāgaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:28.2":{"pli":"gāmāni nigamāni ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:28.3":{"pli":"Khettaṃ puttā ca dhītā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:28.4":{"pli":"pariccattā mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:29.1":{"pli":"Hatthī assā gavā cāpi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:29.2":{"pli":"dāsiyo paricārikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:29.3":{"pli":"Tuyhatthāya mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:29.4":{"pli":"pariccattaṃ asaṅkhiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:30.1":{"pli":"Yaṃ amhe paṭimantesi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:30.2":{"pli":"‘dānaṃ dassāma yācake’; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:30.3":{"pli":"Vimanaṃ no na passāma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:30.4":{"pli":"dadato dānamuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:31.1":{"pli":"Nānāvidhaṃ bahuṃ dukkhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:31.2":{"pli":"saṃsāre ca bahubbidhe; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:31.3":{"pli":"Tuyhatthāya mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:31.4":{"pli":"anubhuttaṃ asaṅkhiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:32.1":{"pli":"Sukhappattānumodāma, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:32.2":{"pli":"na ca dukkhesu dummanā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:32.3":{"pli":"Sabbattha tulitā homa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:32.4":{"pli":"tuyhatthāya mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:33.1":{"pli":"Anumaggena sambuddho, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:33.2":{"pli":"yaṃ dhammaṃ abhinīhari; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:33.3":{"pli":"Anubhotvā sukhaṃ dukkhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:33.4":{"pli":"patto bodhiṃ mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:34.1":{"pli":"Brahmadevañca sambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:34.2":{"pli":"gotamaṃ lokanāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:34.3":{"pli":"Aññesaṃ lokanāthānaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:34.4":{"pli":"saṅgamaṃ tehi no bahū. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:35.1":{"pli":"Adhikāraṃ bahuṃ amhe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:35.2":{"pli":"tuyhatthāya mahāmune; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:35.3":{"pli":"Gavesato buddhadhamme, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:35.4":{"pli":"mayaṃ te paricārikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:36.1":{"pli":"Kappe ca satasahasse, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:36.2":{"pli":"caturo ca asaṅkhiye; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:36.3":{"pli":"Dīpaṅkaro mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:36.4":{"pli":"uppajji lokanāyako. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:37.1":{"pli":"Paccantadesavisaye, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:37.2":{"pli":"nimantetvā tathāgataṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:37.3":{"pli":"Tassa āgamanaṃ maggaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:37.4":{"pli":"sodhenti tuṭṭhamānasā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:38.1":{"pli":"Tena kālena so āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:38.2":{"pli":"sumedho nāma brāhmaṇo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:38.3":{"pli":"Maggañca paṭiyādesi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:38.4":{"pli":"āyato sabbadassino. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:39.1":{"pli":"Tena kālena ahumha, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:39.2":{"pli":"sabbā brāhmaṇasambhavā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:39.3":{"pli":"Thalūdajāni pupphāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:39.4":{"pli":"āharimha samāgamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:40.1":{"pli":"Tasmiṃ so samaye buddho, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:40.2":{"pli":"dīpaṅkaro mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:40.3":{"pli":"Viyākāsi mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:40.4":{"pli":"isimuggatamānasaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:41.1":{"pli":"Calatī ravatī puthavī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:41.2":{"pli":"saṅkampati sadevake; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:41.3":{"pli":"Tassa kammaṃ pakittente, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:41.4":{"pli":"isimuggatamānasaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:42.1":{"pli":"Devakaññā manussā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:42.2":{"pli":"mayañcāpi sadevakā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:42.3":{"pli":"Nānāpūjanīyaṃ bhaṇḍaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:42.4":{"pli":"pūjayitvāna patthayuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:43.1":{"pli":"Tesaṃ buddho viyākāsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:43.2":{"pli":"jotidīpa sanāmako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:43.3":{"pli":"‘Ajja ye patthitā atthi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:43.4":{"pli":"te bhavissanti sammukhā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:44.1":{"pli":"Aparimeyye ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:44.2":{"pli":"yaṃ no buddho viyākari; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:44.3":{"pli":"Taṃ vācamanumodentā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:44.4":{"pli":"evaṅkārī ahumha no. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:45.1":{"pli":"Tassa kammassa sukatassa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:45.2":{"pli":"tassa cittaṃ pasādayuṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:45.3":{"pli":"Devamānusikaṃ yoniṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:45.4":{"pli":"anubhotvā asaṅkhiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:46.1":{"pli":"Sukhadukkhenubhotvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:46.2":{"pli":"devesu mānusesu ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:46.3":{"pli":"Pacchime bhave sampatte, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:46.4":{"pli":"jātāmha sākiye kule. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:47.1":{"pli":"Rūpavatī bhogavatī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:47.2":{"pli":"yasasīlavatī tato; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:47.3":{"pli":"Sabbaṅgasampadā homa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:47.4":{"pli":"kulesu abhisakkatā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:48.1":{"pli":"Lābhaṃ silokaṃ sakkāraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:48.2":{"pli":"lokadhammasamāgamaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:48.3":{"pli":"Cittañca dukkhitaṃ natthi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:48.4":{"pli":"vasāma akutobhayā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:49.1":{"pli":"Vuttañhetaṃ bhagavatā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:49.2":{"pli":"‘rañño antepure tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:49.3":{"pli":"Khattiyānaṃ pure vīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:49.4":{"pli":"upakārañca niddisi. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:50.1":{"pli":"Upakārā ca yā nārī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:50.2":{"pli":"yā ca nārī sukhe dukhe; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:50.3":{"pli":"Atthakkhāyī ca yā nārī, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:50.4":{"pli":"yā ca nārīnukampikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:51.1":{"pli":"Dhammaṃ care sucaritaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:51.2":{"pli":"na naṃ duccaritaṃ care; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:51.3":{"pli":"Dhammacārī sukhaṃ seti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:51.4":{"pli":"asmiṃ loke paramhi ca’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:52.1":{"pli":"Agāraṃ vijahitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:52.2":{"pli":"pabbajimhanagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:52.3":{"pli":"Aḍḍhamāse asampatte, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:52.4":{"pli":"catusaccaṃ phusimha no. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:53.1":{"pli":"Cīvaraṃ piṇḍapātañca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:53.2":{"pli":"paccayaṃ sayanāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:53.3":{"pli":"Upanenti bahū amhe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:53.4":{"pli":"sāgarasseva ūmiyo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:54.1":{"pli":"Kilesā jhāpitā amhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:54.2":{"pli":"bhavā sabbe samūhatā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:54.3":{"pli":"Nāgīva bandhanaṃ chetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:54.4":{"pli":"viharāma anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:55.1":{"pli":"Svāgataṃ vata no āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:55.2":{"pli":"mama buddhassa santike; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:55.3":{"pli":"Tisso vijjā anuppattā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:55.4":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:56.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:56.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:56.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:56.4":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:57.1":{"pli":"Evaṃ bahuvidhaṃ dukkhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:57.2":{"pli":"sampattī ca bahubbidhā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:57.3":{"pli":"Visuddhabhāvaṃ sampattā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:57.4":{"pli":"labhāma sabbasampadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:58.1":{"pli":"Yā dadanti sakattānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:58.2":{"pli":"puññatthāya mahesino; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:58.3":{"pli":"Sahāyasampadā honti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:58.4":{"pli":"nibbānapadamasaṅkhataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:59.1":{"pli":"Parikkhīṇaṃ atītañca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:59.2":{"pli":"paccuppannaṃ anāgataṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:59.3":{"pli":"Sabbakammampi no khīṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:59.4":{"pli":"pāde vandāma cakkhuma”. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:60.1":{"pli":"“Nibbānāya vadantīnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:60.2":{"pli":"Kiṃ vo vakkhāma uttari; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:60.3":{"pli":"Santasaṅkhatadosañhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:60.4":{"pli":"Pappotha amataṃ padaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap30:61.1":{"pli":"Itthaṃ sudaṃ yasodharāpamukhāni aṭṭhārasabhikkhunīsahassāni bhagavato sammukhā imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap30:62.1":{"pli":"Yasodharāpamukhaaṭṭhārasabhikkhunīsahassāpadānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"thi-ap30:63.1":{"pli":"Kuṇḍalakesīvaggo tatiyo. ","html":"<p class='endsection'>{}</p>"},"thi-ap30:64.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"thi-ap30:65.1":{"pli":"Kuṇḍalā gotamī ceva, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"thi-ap30:65.2":{"pli":"dhammadinnā ca sakulā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:65.3":{"pli":"Varanandā ca soṇā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:65.4":{"pli":"kāpilānī yasodharā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap30:66.1":{"pli":"Dasasahassabhikkhunī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap30:66.2":{"pli":"aṭṭhārasasahassakā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:66.3":{"pli":"Gāthāsatāni cattāri, ","html":"<span class='verse-line'>{}</span>"},"thi-ap30:66.4":{"pli":"cha ca sattatimeva ca. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}},"thi-ap31":{"thi-ap31:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap31'><header><ul><li class='division'>{}</li>"},"thi-ap31:0.2":{"pli":"Khattiyāvagga ","html":"<li>{}</li></ul>"},"thi-ap31:0.3":{"pli":"1 Yasavatīpamukhaaṭṭhārasabhikkhunīsahassaapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap31:1.1":{"pli":"“Bhavā sabbe parikkhīṇā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap31:1.2":{"pli":"bhavā santi vimocitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:1.3":{"pli":"Sabbāsavā ca no natthi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:1.4":{"pli":"ārocema mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap31:2.1":{"pli":"Purimaṃ kusalaṃ kammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap31:2.2":{"pli":"yaṃ kiñci sādhupatthitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:2.3":{"pli":"Paribhogamayaṃ dinnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:2.4":{"pli":"tuyhatthāya mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap31:3.1":{"pli":"Buddhapaccekabuddhānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap31:3.2":{"pli":"sāvakānañca patthitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:3.3":{"pli":"Paribhogamayaṃ dinnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:3.4":{"pli":"tuyhatthāya mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap31:4.1":{"pli":"Uccanīcamayaṃ kammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap31:4.2":{"pli":"bhikkhūnaṃ sādhupatthitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:4.3":{"pli":"Uccākulaparikammaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:4.4":{"pli":"katametaṃ mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap31:5.1":{"pli":"Teneva sukkamūlena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap31:5.2":{"pli":"coditā kammasampadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:5.3":{"pli":"Mānusikamatikkantā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:5.4":{"pli":"jāyiṃsu khattiye kule. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap31:6.1":{"pli":"Uppatte ca kate kamme, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap31:6.2":{"pli":"jātiyā vāpi ekato; ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:6.3":{"pli":"Pacchime ekato jātā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:6.4":{"pli":"khattiyā kulasambhavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap31:7.1":{"pli":"Rūpavatī bhogavatī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap31:7.2":{"pli":"lābhasakkārapūjitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:7.3":{"pli":"Antepure mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:7.4":{"pli":"devānaṃ viya nandane. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap31:8.1":{"pli":"Nibbinditvā agāramhā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap31:8.2":{"pli":"pabbajimhanagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:8.3":{"pli":"Katipāhaṃ upādāya, ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:8.4":{"pli":"sabbā pattāmha nibbutiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap31:9.1":{"pli":"Cīvaraṃ piṇḍapātañca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap31:9.2":{"pli":"paccayaṃ sayanāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:9.3":{"pli":"Upanenti bahū amhe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:9.4":{"pli":"sadā sakkatapūjitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap31:10.1":{"pli":"Kilesā jhāpitā amhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap31:10.2":{"pli":"bhavā sabbe samūhatā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:10.3":{"pli":"Nāgīva bandhanaṃ chetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:10.4":{"pli":"viharāma anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap31:11.1":{"pli":"Svāgataṃ vata no āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap31:11.2":{"pli":"mama buddhassa santike; ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:11.3":{"pli":"Tisso vijjā anuppattā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:11.4":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap31:12.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap31:12.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:12.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap31:12.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap31:13.1":{"pli":"Itthaṃ sudaṃ yasavatīpamukhāni khattiyakaññābhikkhuniyo aṭṭhārasasahassāni bhagavato sammukhā imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap31:14.1":{"pli":"Yasavatīpamukhaaṭṭhārasabhikkhunīsahassāpadānaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"thi-ap32":{"thi-ap32:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap32'><header><ul><li class='division'>{}</li>"},"thi-ap32:0.2":{"pli":"Khattiyāvagga ","html":"<li>{}</li></ul>"},"thi-ap32:0.3":{"pli":"2 Caturāsītibhikkhunīsahassaapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap32:1.1":{"pli":"“Cullāsītisahassāni, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap32:1.2":{"pli":"brāhmaññakulasambhavā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:1.3":{"pli":"Sukhumālahatthapādā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:1.4":{"pli":"pure tuyhaṃ mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:2.1":{"pli":"Vessasuddakule jātā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:2.2":{"pli":"devā nāgā ca kinnarā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:2.3":{"pli":"Cātuddīpā bahū kaññā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:2.4":{"pli":"pure tuyhaṃ mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:3.1":{"pli":"Kāci pabbajitā atthi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:3.2":{"pli":"sabbadassāvino bahū; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:3.3":{"pli":"Devā ca kinnarā nāgā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:3.4":{"pli":"phusissanti anāgate. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:4.1":{"pli":"Anubhotvā yasaṃ sabbaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:4.2":{"pli":"Patvāna sabbasampadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:4.3":{"pli":"Tumhaṃ pasādaṃ paṭiladdhā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:4.4":{"pli":"Bujjhissanti anāgate. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:5.1":{"pli":"Amhe brāhmaṇadhītā tu, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:5.2":{"pli":"brāhmaññakulasambhavā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:5.3":{"pli":"Pekkhato no mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:5.4":{"pli":"pāde vandāma cakkhuma. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:6.1":{"pli":"Upāhatā bhavā sabbe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:6.2":{"pli":"mūlataṇhā samūhatā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:6.3":{"pli":"Samucchinnā anusayā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:6.4":{"pli":"puññasaṅkhāradālitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:7.1":{"pli":"Samādhigocarā sabbā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:7.2":{"pli":"samāpattivasī katā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:7.3":{"pli":"Jhānena dhammaratiyā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:7.4":{"pli":"viharissāma no sadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:8.1":{"pli":"Bhavanetti avijjā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:8.2":{"pli":"saṅkhārāpi ca khepitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:8.3":{"pli":"Sududdasaṃ padaṃ gantvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:8.4":{"pli":"anujānātha nāyaka”. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:9.1":{"pli":"“Upakārā mamaṃ tumhe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:9.2":{"pli":"dīgharattaṃ katāvino; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:9.3":{"pli":"Catunnaṃ saṃsayaṃ chetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:9.4":{"pli":"sabbā gacchantu nibbutiṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:10.1":{"pli":"Vanditvā munino pāde, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:10.2":{"pli":"katvā iddhivikubbanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:10.3":{"pli":"Kāci dassenti ālokaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:10.4":{"pli":"andhakāramathāparā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:11.1":{"pli":"Dassenti candasūriye, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:11.2":{"pli":"sāgarañca samacchakaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:11.3":{"pli":"Sineruṃ paribhaṇḍañca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:11.4":{"pli":"dassenti pārichattakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:12.1":{"pli":"Tāvatiṃsañca bhavanaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:12.2":{"pli":"yāmaṃ dassenti iddhiyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:12.3":{"pli":"Tusitaṃ nimmite deve, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:12.4":{"pli":"vasavattī mahissare. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:13.1":{"pli":"Brahmāno kāci dassenti, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:13.2":{"pli":"caṅkamañca mahārahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:13.3":{"pli":"Brahmavaṇṇañca māpetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:13.4":{"pli":"dhammaṃ desenti suññataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:14.1":{"pli":"Nānāvikubbanaṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:14.2":{"pli":"iddhiṃ dassiya satthuno; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:14.3":{"pli":"Dassayiṃsu balaṃ sabbā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:14.4":{"pli":"pāde vandiṃsu satthuno. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:15.1":{"pli":"“Iddhīsu ca vasī homa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:15.2":{"pli":"dibbāya sotadhātuyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:15.3":{"pli":"Cetopariyañāṇassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:15.4":{"pli":"vasī homa mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:16.1":{"pli":"Pubbenivāsaṃ jānāma, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:16.2":{"pli":"dibbacakkhu visodhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:16.3":{"pli":"Sabbāsavā parikkhīṇā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:16.4":{"pli":"natthi dāni punabbhavo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:17.1":{"pli":"Atthadhammaniruttīsu, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:17.2":{"pli":"paṭibhāne tatheva ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:17.3":{"pli":"Ñāṇaṃ amhaṃ mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:17.4":{"pli":"uppannaṃ tava santike. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:18.1":{"pli":"Pubbānaṃ lokanāthānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:18.2":{"pli":"saṅgamaṃ no nidassitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:18.3":{"pli":"Adhikāraṃ bahuṃ amhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:18.4":{"pli":"tuyhatthāya mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:19.1":{"pli":"Yaṃ amhehi kataṃ kammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:19.2":{"pli":"kusalaṃ sara taṃ mune; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:19.3":{"pli":"Tuyhatthāya mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:19.4":{"pli":"puññānupacitāni no. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:20.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:20.2":{"pli":"padumuttaro mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:20.3":{"pli":"Puraṃ haṃsavatī nāma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:20.4":{"pli":"sambuddhassa kulāsayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:21.1":{"pli":"Dvārena haṃsavatiyā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:21.2":{"pli":"gaṅgā sandati sabbadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:21.3":{"pli":"Ubbāḷhā nadiyā bhikkhū, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:21.4":{"pli":"gamanaṃ na labhanti te. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:22.1":{"pli":"Divasaṃ dve tayo ceva, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:22.2":{"pli":"sattāhaṃ māsikaṃ tato; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:22.3":{"pli":"Catumāsampi sampuṇṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:22.4":{"pli":"gamanaṃ na labhanti te. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:23.1":{"pli":"Tadā ahu sattasāro, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:23.2":{"pli":"jaṭilo nāma raṭṭhiko; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:23.3":{"pli":"Oruddhe bhikkhavo disvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:23.4":{"pli":"setuṃ gaṅgāya kārayi. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:24.1":{"pli":"Tadā satasahassehi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:24.2":{"pli":"setuṃ gaṅgāya kārayi; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:24.3":{"pli":"Saṅghassa orime tīre, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:24.4":{"pli":"vihārañca akārayi. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:25.1":{"pli":"Itthiyo purisā ceva, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:25.2":{"pli":"uccanīcakulāni ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:25.3":{"pli":"Tassa setuṃ vihārañca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:25.4":{"pli":"samabhāgaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:26.1":{"pli":"Amhe aññe ca mānujā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:26.2":{"pli":"vippasannena cetasā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:26.3":{"pli":"Tassa dhammesu dāyādā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:26.4":{"pli":"nagare janapadesu ca. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:27.1":{"pli":"Itthī pumā kumārā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:27.2":{"pli":"bahū ceva kumārikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:27.3":{"pli":"Setuno ca vihārassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:27.4":{"pli":"vālukā ākiriṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:28.1":{"pli":"Vīthiṃ sammajjanaṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:28.2":{"pli":"kadalīpuṇṇaghaṭe dhaje; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:28.3":{"pli":"Dhūpaṃ cuṇṇañca mālañca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:28.4":{"pli":"kāraṃ katvāna satthuno. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:29.1":{"pli":"Setuvihāre kāretvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:29.2":{"pli":"nimantetvā vināyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:29.3":{"pli":"Mahādānaṃ daditvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:29.4":{"pli":"sambodhiṃ abhipatthayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:30.1":{"pli":"Padumuttaro mahāvīro, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:30.2":{"pli":"tārako sabbapāṇinaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:30.3":{"pli":"Anumodanīyaṅkāsi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:30.4":{"pli":"jaṭilassa mahāmuni. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:31.1":{"pli":"‘Satasahassātikkante, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:31.2":{"pli":"kappo hessati bhaddako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:31.3":{"pli":"Bhavābhavenubhotvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:31.4":{"pli":"pāpuṇissati bodhiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:32.1":{"pli":"Kāci hatthaparikammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:32.2":{"pli":"katāvī naranāriyo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:32.3":{"pli":"Anāgatamhi addhāne, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:32.4":{"pli":"sabbā hessanti sammukhā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:33.1":{"pli":"Tena kammavipākena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:33.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:33.3":{"pli":"Uppannā devabhavanaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:33.4":{"pli":"tuyhaṃ tā paricārikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:34.1":{"pli":"Dibbasukhaṃ asaṅkhiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:34.2":{"pli":"mānusañca asaṅkhiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:34.3":{"pli":"Tuyhaṃ te paricārema, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:34.4":{"pli":"saṃsarimha bhavābhave. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:35.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:35.2":{"pli":"sukataṃ kammasampadaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:35.3":{"pli":"Sukhumālī manussānaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:35.4":{"pli":"atho devapure vare. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:36.1":{"pli":"Rūpabhogayase ceva, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:36.2":{"pli":"atho kittiñca sakkataṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:36.3":{"pli":"Labhāma satataṃ sabbaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:36.4":{"pli":"sukataṃ kammasampadaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:37.1":{"pli":"Pacchime bhave sampatte, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:37.2":{"pli":"jātāmha brāhmaṇe kule; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:37.3":{"pli":"Sukhumālahatthapādā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:37.4":{"pli":"sakyaputtanivesane. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:38.1":{"pli":"Sabbakālampi pathaviṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:38.2":{"pli":"na passāma na laṅkataṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:38.3":{"pli":"Cikkhallabhūmimasuciṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:38.4":{"pli":"na passāma mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:39.1":{"pli":"Agāraṃ vasante amhe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:39.2":{"pli":"sakkāraṃ sabbakālikaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:39.3":{"pli":"Upanenti sadā sabbaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:39.4":{"pli":"pubbakammaphalena no. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:40.1":{"pli":"Agāraṃ pajahitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:40.2":{"pli":"pabbajitvānagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:40.3":{"pli":"Saṃsārapathanitthiṇṇā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:40.4":{"pli":"vītarāgā bhavāmase. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:41.1":{"pli":"Cīvaraṃ piṇḍapātañca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:41.2":{"pli":"paccayaṃ sayanāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:41.3":{"pli":"Upanenti sadā amhe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:41.4":{"pli":"sahassāni tato tato. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:42.1":{"pli":"Kilesā jhāpitā amhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:42.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:42.3":{"pli":"viharāma anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:43.1":{"pli":"Svāgataṃ vata no āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:43.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:43.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap32:44.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap32:44.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap32:44.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap32:45.1":{"pli":"Itthaṃ sudaṃ caturāsītibrāhmaṇakaññābhikkhunīsahassāni bhagavato sammukhā imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap32:46.1":{"pli":"Caturāsītibhikkhunīsahassāpadānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}