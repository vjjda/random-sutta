{"tha-ap533":{"tha-ap533:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap533'><header><ul><li class='division'>{}</li>"},"tha-ap533:0.2":{"pli":"Kaccāyanavagga ","html":"<li>{}</li></ul>"},"tha-ap533:0.3":{"pli":"1 Mahākaccāyanattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap533:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap533:1.2":{"pli":"anejo ajitaṃ jayo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:1.3":{"pli":"Satasahasse kappānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:1.4":{"pli":"ito uppajji nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:2.1":{"pli":"Vīro kamalapattakkho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:2.2":{"pli":"sasaṅkavimalānano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:2.3":{"pli":"Kanakācalasaṅkāso, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:2.4":{"pli":"ravidittisamappabho. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:3.1":{"pli":"Sattanettamanohārī, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:3.2":{"pli":"varalakkhaṇabhūsito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:3.3":{"pli":"Sabbavākyapathātīto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:3.4":{"pli":"manujāmarasakkato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:4.1":{"pli":"Sambuddho bodhayaṃ satte, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:4.2":{"pli":"vāgīso madhurassaro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:4.3":{"pli":"Karuṇānibandhasantāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:4.4":{"pli":"parisāsu visārado. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:5.1":{"pli":"Deseti madhuraṃ dhammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:5.2":{"pli":"catusaccūpasaṃhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:5.3":{"pli":"Nimugge mohapaṅkamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:5.4":{"pli":"samuddharati pāṇine. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:6.1":{"pli":"Tadā ekacaro hutvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:6.2":{"pli":"tāpaso himavālayo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:6.3":{"pli":"Nabhasā mānusaṃ lokaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:6.4":{"pli":"gacchanto jinamaddasaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:7.1":{"pli":"Upecca santikaṃ tassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:7.2":{"pli":"assosiṃ dhammadesanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:7.3":{"pli":"Vaṇṇayantassa vīrassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:7.4":{"pli":"sāvakassa mahāguṇaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:8.1":{"pli":"‘Saṅkhittena mayā vuttaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:8.2":{"pli":"vitthārena pakāsayaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:8.3":{"pli":"Parisaṃ mañca toseti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:8.4":{"pli":"yathā kaccāyano ayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:9.1":{"pli":"Nāhaṃ evamidhekaccaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:9.2":{"pli":"aññaṃ passāmi sāvakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:9.3":{"pli":"Tasmātadagge esaggo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:9.4":{"pli":"evaṃ dhāretha bhikkhavo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:10.1":{"pli":"Tadāhaṃ vimhito hutvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:10.2":{"pli":"sutvā vākyaṃ manoramaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:10.3":{"pli":"Himavantaṃ gamitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:10.4":{"pli":"āhitvā pupphasañcayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:11.1":{"pli":"Pūjetvā lokasaraṇaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:11.2":{"pli":"taṃ ṭhānamabhipatthayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:11.3":{"pli":"Tadā mamāsayaṃ ñatvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:11.4":{"pli":"byākāsi sa raṇañjaho. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:12.1":{"pli":"‘Passathetaṃ isivaraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:12.2":{"pli":"niddhantakanakattacaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:12.3":{"pli":"Uddhaggalomaṃ pīṇaṃsaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:12.4":{"pli":"acalaṃ pañjaliṃ ṭhitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:13.1":{"pli":"Hāsaṃ supuṇṇanayanaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:13.2":{"pli":"buddhavaṇṇagatāsayaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:13.3":{"pli":"Dhammajaṃ uggahadayaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:13.4":{"pli":"amatāsittasannibhaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:14.1":{"pli":"Kaccānassa guṇaṃ sutvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:14.2":{"pli":"taṃ ṭhānaṃ patthayaṃ ṭhito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:14.3":{"pli":"Anāgatamhi addhāne, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:14.4":{"pli":"gotamassa mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:15.1":{"pli":"Tassa dhammesu dāyādo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:15.2":{"pli":"oraso dhammanimmito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:15.3":{"pli":"Kaccāno nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:15.4":{"pli":"hessati satthu sāvako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:16.1":{"pli":"Bahussuto mahāñāṇī, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:16.2":{"pli":"adhippāyavidū mune; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:16.3":{"pli":"Pāpuṇissati taṃ ṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:16.4":{"pli":"yathāyaṃ byākato mayā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:17.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:17.2":{"pli":"yaṃ kammamakariṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:17.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:17.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:18.1":{"pli":"Duve bhave saṃsarāmi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:18.2":{"pli":"devatte atha mānuse; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:18.3":{"pli":"Aññaṃ gatiṃ na gacchāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:18.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:19.1":{"pli":"Duve kule pajāyāmi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:19.2":{"pli":"khattiye atha brāhmaṇe; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:19.3":{"pli":"Nīce kule na jāyāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:19.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:20.1":{"pli":"Pacchime ca bhave dāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:20.2":{"pli":"jāto ujjeniyaṃ pure; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:20.3":{"pli":"Pajjotassa ca caṇḍassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:20.4":{"pli":"purohitadijādhino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:21.1":{"pli":"Putto tiriṭivacchassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:21.2":{"pli":"nipuṇo vedapāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:21.3":{"pli":"Mātā ca candimā nāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:21.4":{"pli":"kaccānohaṃ varattaco. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:22.1":{"pli":"Vīmaṃsanatthaṃ buddhassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:22.2":{"pli":"bhūmipālena pesito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:22.3":{"pli":"Disvā mokkhapuradvāraṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:22.4":{"pli":"nāyakaṃ guṇasañcayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:23.1":{"pli":"Sutvā ca vimalaṃ vākyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:23.2":{"pli":"gatipaṅkavisosanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:23.3":{"pli":"Pāpuṇiṃ amataṃ santaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:23.4":{"pli":"sesehi saha sattahi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:24.1":{"pli":"Adhippāyavidū jāto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:24.2":{"pli":"sugatassa mahāmate; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:24.3":{"pli":"Ṭhapito etadagge ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:24.4":{"pli":"susamiddhamanoratho. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:25.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:25.2":{"pli":"bhavā sabbe samūhatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:25.3":{"pli":"Nāgova bandhanaṃ chetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:25.4":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:26.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:26.2":{"pli":"mama buddhassa santike; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:26.3":{"pli":"Tisso vijjā anuppattā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:26.4":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap533:27.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap533:27.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:27.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap533:27.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap533:28.1":{"pli":"Itthaṃ sudaṃ āyasmā mahākaccāyano thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap533:29.1":{"pli":"Mahākaccāyanattherassāpadānaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap534":{"tha-ap534:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap534'><header><ul><li class='division'>{}</li>"},"tha-ap534:0.2":{"pli":"Kaccāyanavagga ","html":"<li>{}</li></ul>"},"tha-ap534:0.3":{"pli":"Vakkalittheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap534:1.1":{"pli":"“Ito satasahassamhi, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap534:1.2":{"pli":"kappe uppajji nāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:1.3":{"pli":"Anomanāmo amito, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:1.4":{"pli":"nāmena padumuttaro. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:2.1":{"pli":"Padumākāravadano, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:2.2":{"pli":"padumāmalasucchavī; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:2.3":{"pli":"Lokenānupalittova, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:2.4":{"pli":"toyena padumaṃ yathā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:3.1":{"pli":"Vīro padumapattakkho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:3.2":{"pli":"kanto ca padumaṃ yathā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:3.3":{"pli":"Padumuttaragandhova, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:3.4":{"pli":"tasmā so padumuttaro. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:4.1":{"pli":"Lokajeṭṭho ca nimmāno, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:4.2":{"pli":"andhānaṃ nayanūpamo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:4.3":{"pli":"Santaveso guṇanidhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:4.4":{"pli":"karuṇāmatisāgaro. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:5.1":{"pli":"Sa kadāci mahāvīro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:5.2":{"pli":"brahmāsurasuraccito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:5.3":{"pli":"Sadevamanujākiṇṇe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:5.4":{"pli":"janamajjhe jinuttamo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:6.1":{"pli":"Vadanena sugandhena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:6.2":{"pli":"madhurena rutena ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:6.3":{"pli":"Rañjayaṃ parisaṃ sabbaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:6.4":{"pli":"santhavī sāvakaṃ sakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:7.1":{"pli":"Saddhādhimutto sumati, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:7.2":{"pli":"mama dassanalālaso; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:7.3":{"pli":"Natthi etādiso añño, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:7.4":{"pli":"yathāyaṃ bhikkhu vakkali. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:8.1":{"pli":"Tadāhaṃ haṃsavatiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:8.2":{"pli":"nagare brāhmaṇatrajo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:8.3":{"pli":"Hutvā sutvā ca taṃ vākyaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:8.4":{"pli":"taṃ ṭhānamabhirocayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:9.1":{"pli":"Sasāvakaṃ taṃ vimalaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:9.2":{"pli":"nimantetvā tathāgataṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:9.3":{"pli":"Sattāhaṃ bhojayitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:9.4":{"pli":"dussehacchādayiṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:10.1":{"pli":"Nipacca sirasā tassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:10.2":{"pli":"anantaguṇasāgare; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:10.3":{"pli":"Nimuggo pītisampuṇṇo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:10.4":{"pli":"idaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:11.1":{"pli":"‘Yo so tayā santhavito, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:11.2":{"pli":"ito sattamake muni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:11.3":{"pli":"Bhikkhu saddhāvataṃ aggo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:11.4":{"pli":"tādiso homahaṃ mune’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:12.1":{"pli":"Evaṃ vutte, mahāvīro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:12.2":{"pli":"anāvaraṇadassano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:12.3":{"pli":"Imaṃ vākyaṃ udīresi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:12.4":{"pli":"parisāya mahāmuni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:13.1":{"pli":"‘Passathetaṃ māṇavakaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:13.2":{"pli":"pītamaṭṭhanivāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:13.3":{"pli":"Hemayaññopacitaṅgaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:13.4":{"pli":"jananettamanoharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:14.1":{"pli":"Eso anāgataddhāne, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:14.2":{"pli":"gotamassa mahesino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:14.3":{"pli":"Aggo saddhādhimuttānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:14.4":{"pli":"sāvakoyaṃ bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:15.1":{"pli":"Devabhūto manusso vā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:15.2":{"pli":"sabbasantāpavajjito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:15.3":{"pli":"Sabbabhogaparibyūḷho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:15.4":{"pli":"sukhito saṃsarissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:16.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:16.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:16.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:16.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:17.1":{"pli":"Tassa dhammesu dāyādo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:17.2":{"pli":"oraso dhammanimmito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:17.3":{"pli":"Vakkali nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:17.4":{"pli":"hessati satthu sāvako’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:18.1":{"pli":"Tena kammavisesena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:18.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:18.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:18.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:19.1":{"pli":"Sabbattha sukhito hutvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:19.2":{"pli":"saṃsaranto bhavābhave; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:19.3":{"pli":"Sāvatthiyaṃ pure jāto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:19.4":{"pli":"kule aññatare ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:20.1":{"pli":"Nonītasukhumālaṃ maṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:20.2":{"pli":"jātapallavakomalaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:20.3":{"pli":"Mandaṃ uttānasayanaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:20.4":{"pli":"pisācībhayatajjitā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:21.1":{"pli":"Pādamūle mahesissa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:21.2":{"pli":"sāyesuṃ dīnamānasā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:21.3":{"pli":"‘Imaṃ dadāma te nātha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:21.4":{"pli":"saraṇaṃ hohi nāyaka’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:22.1":{"pli":"Tadā paṭiggahi so maṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:22.2":{"pli":"bhītānaṃ saraṇo muni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:22.3":{"pli":"Jālinā cakkaṅkitena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:22.4":{"pli":"mudukomalapāṇinā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:23.1":{"pli":"Tadā pabhuti tenāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:23.2":{"pli":"arakkheyyena rakkhito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:23.3":{"pli":"Sabbaveravinimutto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:23.4":{"pli":"sukhena parivuddhito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:24.1":{"pli":"Sugatena vinā bhūto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:24.2":{"pli":"ukkaṇṭhāmi muhuttakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:24.3":{"pli":"Jātiyā sattavassohaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:24.4":{"pli":"pabbajiṃ anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:25.1":{"pli":"Sabbapāramisambhūtaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:25.2":{"pli":"nīlakkhinayanaṃ varaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:25.3":{"pli":"Rūpaṃ sabbasubhākiṇṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:25.4":{"pli":"atitto viharāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:26.1":{"pli":"Buddharūparatiṃ ñatvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:26.2":{"pli":"tadā ovadi maṃ jino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:26.3":{"pli":"‘Alaṃ vakkali kiṃ rūpe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:26.4":{"pli":"ramase bālanandite. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:27.1":{"pli":"Yo hi passati saddhammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:27.2":{"pli":"so maṃ passati paṇḍito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:27.3":{"pli":"Apassamāno saddhammaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:27.4":{"pli":"maṃ passampi na passati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:28.1":{"pli":"Anantādīnavo kāyo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:28.2":{"pli":"visarukkhasamūpamo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:28.3":{"pli":"Āvāso sabbarogānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:28.4":{"pli":"puñjo dukkhassa kevalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:29.1":{"pli":"Nibbindiya tato rūpe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:29.2":{"pli":"khandhānaṃ udayabbayaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:29.3":{"pli":"Passa upakkilesānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:29.4":{"pli":"sukhenantaṃ gamissasi’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:30.1":{"pli":"Evaṃ tenānusiṭṭhohaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:30.2":{"pli":"nāyakena hitesinā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:30.3":{"pli":"Gijjhakūṭaṃ samāruyha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:30.4":{"pli":"jhāyāmi girikandare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:31.1":{"pli":"Ṭhito pabbatapādamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:31.2":{"pli":"assāsayi mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:31.3":{"pli":"Vakkalīti jino vācaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:31.4":{"pli":"taṃ sutvā mudito ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:32.1":{"pli":"Pakkhandiṃ selapabbhāre, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:32.2":{"pli":"anekasataporise; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:32.3":{"pli":"Tadā buddhānubhāvena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:32.4":{"pli":"sukheneva mahiṃ gato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:33.1":{"pli":"Punopi dhammaṃ deseti, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:33.2":{"pli":"khandhānaṃ udayabbayaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:33.3":{"pli":"Tamahaṃ dhammamaññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:33.4":{"pli":"arahattamapāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:34.1":{"pli":"Sumahāparisamajjhe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:34.2":{"pli":"tadā maṃ caraṇantago; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:34.3":{"pli":"Aggaṃ saddhādhimuttānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:34.4":{"pli":"paññapesi mahāmati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:35.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:35.2":{"pli":"yaṃ kammamakariṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:35.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:35.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:36.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:36.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:36.3":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:37.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:37.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:37.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap534:38.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap534:38.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap534:38.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap534:39.1":{"pli":"Itthaṃ sudaṃ āyasmā vakkalitthero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap534:40.1":{"pli":"Vakkalittherassāpadānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap535":{"tha-ap535:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap535'><header><ul><li class='division'>{}</li>"},"tha-ap535:0.2":{"pli":"Kaccāyanavagga ","html":"<li>{}</li></ul>"},"tha-ap535:0.3":{"pli":"Mahākappinattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap535:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap535:1.2":{"pli":"sabbadhammāna pāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:1.3":{"pli":"Udito ajaṭākāse, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:1.4":{"pli":"ravīva saradambare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:2.1":{"pli":"Vacanābhāya bodheti, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:2.2":{"pli":"veneyyapadumāni so; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:2.3":{"pli":"Kilesapaṅkaṃ soseti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:2.4":{"pli":"matiraṃsīhi nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:3.1":{"pli":"Titthiyānaṃ yase hanti, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:3.2":{"pli":"khajjotābhā yathā ravi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:3.3":{"pli":"Saccatthābhaṃ pakāseti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:3.4":{"pli":"ratanaṃva divākaro. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:4.1":{"pli":"Guṇānaṃ āyatibhūto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:4.2":{"pli":"ratanānaṃva sāgaro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:4.3":{"pli":"Pajjunnoriva bhūtāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:4.4":{"pli":"dhammameghena vassati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:5.1":{"pli":"Akkhadasso tadā āsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:5.2":{"pli":"nagare haṃsasavhaye; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:5.3":{"pli":"Upecca dhammamassosiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:5.4":{"pli":"jalajuttamanāmino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:6.1":{"pli":"Ovādakassa bhikkhūnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:6.2":{"pli":"sāvakassa katāvino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:6.3":{"pli":"Guṇaṃ pakāsayantassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:6.4":{"pli":"tappayantassa me manaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:7.1":{"pli":"Sutvā patīto sumano, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:7.2":{"pli":"nimantetvā tathāgataṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:7.3":{"pli":"Sasissaṃ bhojayitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:7.4":{"pli":"taṃ ṭhānamabhipatthayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:8.1":{"pli":"Tadā haṃsasamabhāgo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:8.2":{"pli":"haṃsadundubhinissano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:8.3":{"pli":"‘Passathetaṃ mahāmattaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:8.4":{"pli":"vinicchayavisāradaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:9.1":{"pli":"Patitaṃ pādamūle me, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:9.2":{"pli":"samuggatatanūruhaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:9.3":{"pli":"Jīmūtavaṇṇaṃ pīṇaṃsaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:9.4":{"pli":"pasannanayanānanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:10.1":{"pli":"Parivārena mahatā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:10.2":{"pli":"rājayuttaṃ mahāyasaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:10.3":{"pli":"Eso katāvino ṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:10.4":{"pli":"pattheti muditāsayo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:11.1":{"pli":"Iminā paṇipātena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:11.2":{"pli":"cāgena paṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:11.3":{"pli":"Kappasatasahassāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:11.4":{"pli":"nupapajjati duggatiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:12.1":{"pli":"Devesu devasobhaggaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:12.2":{"pli":"manussesu mahantataṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:12.3":{"pli":"Anubhotvāna sesena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:12.4":{"pli":"nibbānaṃ pāpuṇissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:13.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:13.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:13.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:13.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:14.1":{"pli":"Tassa dhammesu dāyādo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:14.2":{"pli":"oraso dhammanimmito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:14.3":{"pli":"Kappino nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:14.4":{"pli":"hessati satthu sāvako’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:15.1":{"pli":"Tatohaṃ sukataṃ kāraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:15.2":{"pli":"katvāna jinasāsane; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:15.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:15.4":{"pli":"tusitaṃ agamāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:16.1":{"pli":"Devamānusarajjāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:16.2":{"pli":"sataso anusāsiya; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:16.3":{"pli":"Bārāṇasiyamāsanne, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:16.4":{"pli":"jāto keniyajātiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:17.1":{"pli":"Sahassaparivārena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:17.2":{"pli":"sapajāpatiko ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:17.3":{"pli":"Pañca paccekabuddhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:17.4":{"pli":"satāni samupaṭṭhahiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:18.1":{"pli":"Temāsaṃ bhojayitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:18.2":{"pli":"pacchādamha ticīvaraṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:18.3":{"pli":"Tato cutā mayaṃ sabbe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:18.4":{"pli":"ahumha tidasūpagā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:19.1":{"pli":"Puno sabbe manussattaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:19.2":{"pli":"agamimha tato cutā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:19.3":{"pli":"Kukkuṭamhi pure jātā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:19.4":{"pli":"himavantassa passato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:20.1":{"pli":"Kappino nāmahaṃ āsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:20.2":{"pli":"rājaputto mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:20.3":{"pli":"Sesāmaccakule jātā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:20.4":{"pli":"mameva parivārayuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:21.1":{"pli":"Mahārajjasukhaṃ patto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:21.2":{"pli":"sabbakāmasamiddhimā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:21.3":{"pli":"Vāṇijehi samakkhātaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:21.4":{"pli":"buddhuppādamahaṃ suṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:22.1":{"pli":"‘Buddho loke samuppanno, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:22.2":{"pli":"asamo ekapuggalo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:22.3":{"pli":"So pakāseti saddhammaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:22.4":{"pli":"amataṃ sukhamuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:23.1":{"pli":"Suyuttā tassa sissā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:23.2":{"pli":"sumuttā ca anāsavā’; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:23.3":{"pli":"Sutvā nesaṃ suvacanaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:23.4":{"pli":"sakkaritvāna vāṇije. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:24.1":{"pli":"Pahāya rajjaṃ sāmacco, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:24.2":{"pli":"nikkhamiṃ buddhamāmako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:24.3":{"pli":"Nadiṃ disvā mahācandaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:24.4":{"pli":"pūritaṃ samatittikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:25.1":{"pli":"Appatiṭṭhaṃ anālambaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:25.2":{"pli":"duttaraṃ sīghavāhiniṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:25.3":{"pli":"Guṇaṃ saritvā buddhassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:25.4":{"pli":"sotthinā samatikkamiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:26.1":{"pli":"‘Bhavasotaṃ sace buddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:26.2":{"pli":"tiṇṇo lokantagū vidū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:26.3":{"pli":"Etena saccavajjena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:26.4":{"pli":"gamanaṃ me samijjhatu. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:27.1":{"pli":"Yadi santigamo maggo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:27.2":{"pli":"mokkho caccantikaṃ sukhaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:27.3":{"pli":"Etena saccavajjena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:27.4":{"pli":"gamanaṃ me samijjhatu. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:28.1":{"pli":"Saṅgho ce tiṇṇakantāro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:28.2":{"pli":"puññakkhetto anuttaro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:28.3":{"pli":"Etena saccavajjena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:28.4":{"pli":"gamanaṃ me samijjhatu’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:29.1":{"pli":"Saha kate saccavare, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:29.2":{"pli":"maggā apagataṃ jalaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:29.3":{"pli":"Tato sukhena uttiṇṇo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:29.4":{"pli":"nadītīre manorame. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:30.1":{"pli":"Nisinnaṃ addasaṃ buddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:30.2":{"pli":"udentaṃva pabhaṅkaraṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:30.3":{"pli":"Jalantaṃ hemaselaṃva, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:30.4":{"pli":"dīparukkhaṃva jotitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:31.1":{"pli":"Sasiṃva tārāsahitaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:31.2":{"pli":"sāvakehi purakkhataṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:31.3":{"pli":"Vāsavaṃ viya vassantaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:31.4":{"pli":"desanājaladantaraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:32.1":{"pli":"Vanditvāna sahāmacco, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:32.2":{"pli":"ekamantamupāvisiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:32.3":{"pli":"Tato no āsayaṃ ñatvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:32.4":{"pli":"buddho dhammamadesayi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:33.1":{"pli":"Sutvāna dhammaṃ vimalaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:33.2":{"pli":"avocumha mayaṃ jinaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:33.3":{"pli":"‘Pabbājehi mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:33.4":{"pli":"nibbindāmha mayaṃ bhave’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:34.1":{"pli":"‘Svākkhāto bhikkhave dhammo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:34.2":{"pli":"dukkhantakaraṇāya vo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:34.3":{"pli":"Caratha brahmacariyaṃ’, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:34.4":{"pli":"iccāha munisattamo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:35.1":{"pli":"Saha vācāya sabbepi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:35.2":{"pli":"bhikkhuvesadharā mayaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:35.3":{"pli":"Ahumha upasampannā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:35.4":{"pli":"sotāpannā ca sāsane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:36.1":{"pli":"Tato jetavanaṃ gantvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:36.2":{"pli":"anusāsi vināyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:36.3":{"pli":"Anusiṭṭho jinenāhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:36.4":{"pli":"arahattamapāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:37.1":{"pli":"Tato bhikkhusahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:37.2":{"pli":"anusāsimahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:37.3":{"pli":"Mamānusāsanakarā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:37.4":{"pli":"tepi āsuṃ anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:38.1":{"pli":"Jino tasmiṃ guṇe tuṭṭho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:38.2":{"pli":"etadagge ṭhapesi maṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:38.3":{"pli":"Bhikkhuovādakānaggo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:38.4":{"pli":"kappinoti mahājane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:39.1":{"pli":"Satasahasse kataṃ kammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:39.2":{"pli":"phalaṃ dassesi me idha; ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:39.3":{"pli":"Pamutto saravegova, ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:39.4":{"pli":"kilese jhāpayiṃ mama. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:40.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:40.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:40.3":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:41.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:41.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:41.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap535:42.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap535:42.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap535:42.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap535:43.1":{"pli":"Itthaṃ sudaṃ āyasmā mahākappino thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap535:44.1":{"pli":"Mahākappinattherassāpadānaṃ tatiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}