{"vv29":{"vv29:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv29'><header><ul><li class='division'>{}</li>"},"vv29:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv29:0.3":{"pli":"Pāricchattakavagga ","html":"<li>{}</li></ul>"},"vv29:0.4":{"pli":"1. Uḷāravimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv29:1.1":{"pli":"“Uḷāro te yaso vaṇṇo, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv29:1.2":{"pli":"sabbā obhāsate disā; ","html":"<span class='verse-line'>{}</span>"},"vv29:1.3":{"pli":"Nāriyo naccanti gāyanti, ","html":"<span class='verse-line'>{}</span>"},"vv29:1.4":{"pli":"devaputtā alaṅkatā. ","html":"<span class='verse-line'>{}</span></p>"},"vv29:2.1":{"pli":"Modenti parivārenti, ","html":"<p><span class='verse-line'>{}</span>"},"vv29:2.2":{"pli":"tava pūjāya devate; ","html":"<span class='verse-line'>{}</span>"},"vv29:2.3":{"pli":"Sovaṇṇāni vimānāni, ","html":"<span class='verse-line'>{}</span>"},"vv29:2.4":{"pli":"tavimāni sudassane. ","html":"<span class='verse-line'>{}</span></p>"},"vv29:3.1":{"pli":"Tuvaṃsi issarā tesaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv29:3.2":{"pli":"sabbakāmasamiddhinī; ","html":"<span class='verse-line'>{}</span>"},"vv29:3.3":{"pli":"Abhijātā mahantāsi, ","html":"<span class='verse-line'>{}</span>"},"vv29:3.4":{"pli":"devakāye pamodasi; ","html":"<span class='verse-line'>{}</span>"},"vv29:3.5":{"pli":"Devate pucchitācikkha, ","html":"<span class='verse-line'>{}</span>"},"vv29:3.6":{"pli":"kissa kammassidaṃ phalan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv29:4.1":{"pli":"“Ahaṃ manussesu manussabhūtā, ","html":"<p><span class='verse-line'>{}</span>"},"vv29:4.2":{"pli":"Purimāya jātiyā manussaloke; ","html":"<span class='verse-line'>{}</span>"},"vv29:4.3":{"pli":"Dussīlakule suṇisā ahosiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv29:4.4":{"pli":"Assaddhesu kadariyesu ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv29:5.1":{"pli":"Saddhā sīlena sampannā, ","html":"<p><span class='verse-line'>{}</span>"},"vv29:5.2":{"pli":"saṃvibhāgaratā sadā; ","html":"<span class='verse-line'>{}</span>"},"vv29:5.3":{"pli":"Piṇḍāya caramānassa, ","html":"<span class='verse-line'>{}</span>"},"vv29:5.4":{"pli":"apūvaṃ te adāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv29:6.1":{"pli":"Tadāhaṃ sassuyācikkhiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv29:6.2":{"pli":"samaṇo āgato idha; ","html":"<span class='verse-line'>{}</span>"},"vv29:6.3":{"pli":"Tassa adāsahaṃ pūvaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv29:6.4":{"pli":"pasannā sehi pāṇibhi. ","html":"<span class='verse-line'>{}</span></p>"},"vv29:7.1":{"pli":"Itissā sassu paribhāsi, ","html":"<p><span class='verse-line'>{}</span>"},"vv29:7.2":{"pli":"avinītāsi tvaṃ vadhu; ","html":"<span class='verse-line'>{}</span>"},"vv29:7.3":{"pli":"Na maṃ sampucchituṃ icchi, ","html":"<span class='verse-line'>{}</span>"},"vv29:7.4":{"pli":"samaṇassa dadāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv29:8.1":{"pli":"Tato me sassu kupitā, ","html":"<p><span class='verse-line'>{}</span>"},"vv29:8.2":{"pli":"pahāsi musalena maṃ; ","html":"<span class='verse-line'>{}</span>"},"vv29:8.3":{"pli":"Kūṭaṅgacchi avadhi maṃ, ","html":"<span class='verse-line'>{}</span>"},"vv29:8.4":{"pli":"nāsakkhiṃ jīvituṃ ciraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv29:9.1":{"pli":"Sā ahaṃ kāyassa bhedā, ","html":"<p><span class='verse-line'>{}</span>"},"vv29:9.2":{"pli":"vippamuttā tato cutā; ","html":"<span class='verse-line'>{}</span>"},"vv29:9.3":{"pli":"Devānaṃ tāvatiṃsānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv29:9.4":{"pli":"upapannā sahabyataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv29:10.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv29:10.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv29:10.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv29:11.1":{"pli":"Uḷāravimānaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv3":{"vv3:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv3'><header><ul><li class='division'>{}</li>"},"vv3:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv3:0.3":{"pli":"Pīṭhavagga ","html":"<li>{}</li></ul>"},"vv3:0.4":{"pli":"3. Tatiyapīṭhavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv3:1.1":{"pli":"“Pīṭhaṃ te sovaṇṇamayaṃ uḷāraṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv3:1.2":{"pli":"Manojavaṃ gacchati yenakāmaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv3:1.3":{"pli":"Alaṅkate malyadhare suvatthe, ","html":"<span class='verse-line'>{}</span>"},"vv3:1.4":{"pli":"Obhāsasi vijjurivabbhakūṭaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv3:2.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv3:2.2":{"pli":"kena te idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv3:2.3":{"pli":"Uppajjanti ca te bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv3:2.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv3:3.1":{"pli":"Pucchāmi taṃ devi mahānubhāve, ","html":"<p><span class='verse-line'>{}</span>"},"vv3:3.2":{"pli":"Manussabhūtā kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv3:3.3":{"pli":"Kenāsi evaṃ jalitānubhāvā, ","html":"<span class='verse-line'>{}</span>"},"vv3:3.4":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv3:4.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv3:4.2":{"pli":"moggallānena pucchitā; ","html":"<span class='verse-line'>{}</span>"},"vv3:4.3":{"pli":"Pañhaṃ puṭṭhā viyākāsi, ","html":"<span class='verse-line'>{}</span>"},"vv3:4.4":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv3:5.1":{"pli":"“Appassa kammassa phalaṃ mamedaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv3:5.2":{"pli":"Yenamhi evaṃ jalitānubhāvā; ","html":"<span class='verse-line'>{}</span>"},"vv3:5.3":{"pli":"Ahaṃ manussesu manussabhūtā, ","html":"<span class='verse-line'>{}</span>"},"vv3:5.4":{"pli":"Purimāya jātiyā manussaloke. ","html":"<span class='verse-line'>{}</span></p>"},"vv3:6.1":{"pli":"Addasaṃ virajaṃ bhikkhuṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv3:6.2":{"pli":"vippasannamanāvilaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv3:6.3":{"pli":"Tassa adāsahaṃ pīṭhaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv3:6.4":{"pli":"pasannā sehi pāṇibhi. ","html":"<span class='verse-line'>{}</span></p>"},"vv3:7.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv3:7.2":{"pli":"tena me idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv3:7.3":{"pli":"Uppajjanti ca me bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv3:7.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv3:8.1":{"pli":"Akkhāmi te bhikkhu mahānubhāva, ","html":"<p><span class='verse-line'>{}</span>"},"vv3:8.2":{"pli":"Manussabhūtā yamakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv3:8.3":{"pli":"Tenamhi evaṃ jalitānubhāvā, ","html":"<span class='verse-line'>{}</span>"},"vv3:8.4":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv3:9.1":{"pli":"Tatiyapīṭhavimānaṃ tatiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv30":{"vv30:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv30'><header><ul><li class='division'>{}</li>"},"vv30:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv30:0.3":{"pli":"Pāricchattakavagga ","html":"<li>{}</li></ul>"},"vv30:0.4":{"pli":"2. Ucchudāyikāvimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv30:1.1":{"pli":"“Obhāsayitvā pathaviṃ sadevakaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv30:1.2":{"pli":"Atirocasi candimasūriyā viya; ","html":"<span class='verse-line'>{}</span>"},"vv30:1.3":{"pli":"Siriyā ca vaṇṇena yasena tejasā, ","html":"<span class='verse-line'>{}</span>"},"vv30:1.4":{"pli":"Brahmāva deve tidase sahindake. ","html":"<span class='verse-line'>{}</span></p>"},"vv30:2.1":{"pli":"Pucchāmi taṃ uppalamāladhārinī, ","html":"<p><span class='verse-line'>{}</span>"},"vv30:2.2":{"pli":"Āveḷinī kañcanasannibhattace; ","html":"<span class='verse-line'>{}</span>"},"vv30:2.3":{"pli":"Alaṅkate uttamavatthadhārinī, ","html":"<span class='verse-line'>{}</span>"},"vv30:2.4":{"pli":"Kā tvaṃ subhe devate vandase mamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv30:3.1":{"pli":"Kiṃ tvaṃ pure kammamakāsi attanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv30:3.2":{"pli":"Manussabhūtā purimāya jātiyā; ","html":"<span class='verse-line'>{}</span>"},"vv30:3.3":{"pli":"Dānaṃ suciṇṇaṃ atha sīlasaṃyamaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv30:3.4":{"pli":"Kenūpapannā sugatiṃ yasassinī; ","html":"<span class='verse-line'>{}</span>"},"vv30:3.5":{"pli":"Devate pucchitācikkha, ","html":"<span class='verse-line'>{}</span>"},"vv30:3.6":{"pli":"Kissa kammassidaṃ phalan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv30:4.1":{"pli":"“Idāni bhante imameva gāmaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv30:4.2":{"pli":"Piṇḍāya amhākaṃ gharaṃ upāgami; ","html":"<span class='verse-line'>{}</span>"},"vv30:4.3":{"pli":"Tato te ucchussa adāsi khaṇḍikaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv30:4.4":{"pli":"Pasannacittā atulāya pītiyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv30:5.1":{"pli":"Sassu ca pacchā anuyuñjate mamaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv30:5.2":{"pli":"Kahaṃ nu ucchuṃ vadhuke avākiri; ","html":"<span class='verse-line'>{}</span>"},"vv30:5.3":{"pli":"Na chaḍḍitaṃ no pana khāditaṃ mayā, ","html":"<span class='verse-line'>{}</span>"},"vv30:5.4":{"pli":"Santassa bhikkhussa sayaṃ adāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv30:6.1":{"pli":"Tuyhaṃ nvidaṃ issariyaṃ atho mama, ","html":"<p><span class='verse-line'>{}</span>"},"vv30:6.2":{"pli":"Itissā sassu paribhāsate mamaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv30:6.3":{"pli":"Pīṭhaṃ gahetvā pahāraṃ adāsi me, ","html":"<span class='verse-line'>{}</span>"},"vv30:6.4":{"pli":"Tato cutā kālakatāmhi devatā. ","html":"<span class='verse-line'>{}</span></p>"},"vv30:7.1":{"pli":"Tadeva kammaṃ kusalaṃ kataṃ mayā, ","html":"<p><span class='verse-line'>{}</span>"},"vv30:7.2":{"pli":"Sukhañca kammaṃ anubhomi attanā; ","html":"<span class='verse-line'>{}</span>"},"vv30:7.3":{"pli":"Devehi saddhiṃ paricārayāmahaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv30:7.4":{"pli":"Modāmahaṃ kāmaguṇehi pañcahi. ","html":"<span class='verse-line'>{}</span></p>"},"vv30:8.1":{"pli":"Tadeva kammaṃ kusalaṃ kataṃ mayā, ","html":"<p><span class='verse-line'>{}</span>"},"vv30:8.2":{"pli":"Sukhañca kammaṃ anubhomi attanā; ","html":"<span class='verse-line'>{}</span>"},"vv30:8.3":{"pli":"Devindaguttā tidasehi rakkhitā, ","html":"<span class='verse-line'>{}</span>"},"vv30:8.4":{"pli":"Samappitā kāmaguṇehi pañcahi. ","html":"<span class='verse-line'>{}</span></p>"},"vv30:9.1":{"pli":"Etādisaṃ puññaphalaṃ anappakaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv30:9.2":{"pli":"Mahāvipākā mama ucchudakkhiṇā; ","html":"<span class='verse-line'>{}</span>"},"vv30:9.3":{"pli":"Devehi saddhiṃ paricārayāmahaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv30:9.4":{"pli":"Modāmahaṃ kāmaguṇehi pañcahi. ","html":"<span class='verse-line'>{}</span></p>"},"vv30:10.1":{"pli":"Etādisaṃ puññaphalaṃ anappakaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv30:10.2":{"pli":"Mahājutikā mama ucchudakkhiṇā; ","html":"<span class='verse-line'>{}</span>"},"vv30:10.3":{"pli":"Devindaguttā tidasehi rakkhitā, ","html":"<span class='verse-line'>{}</span>"},"vv30:10.4":{"pli":"Sahassanettoriva nandane vane. ","html":"<span class='verse-line'>{}</span></p>"},"vv30:11.1":{"pli":"Tuvañca bhante anukampakaṃ viduṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv30:11.2":{"pli":"Upecca vandiṃ kusalañca pucchisaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv30:11.3":{"pli":"Tato te ucchussa adāsi khaṇḍikaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv30:11.4":{"pli":"Pasannacittā atulāya pītiyā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv30:12.1":{"pli":"Ucchudāyikāvimānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv31":{"vv31:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv31'><header><ul><li class='division'>{}</li>"},"vv31:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv31:0.3":{"pli":"Pāricchattakavagga ","html":"<li>{}</li></ul>"},"vv31:0.4":{"pli":"3. Pallaṅkavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv31:1.1":{"pli":"“Pallaṅkaseṭṭhe maṇisoṇṇacitte, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv31:1.2":{"pli":"Pupphābhikiṇṇe sayane uḷāre; ","html":"<span class='verse-line'>{}</span>"},"vv31:1.3":{"pli":"Tatthacchasi devi mahānubhāve, ","html":"<span class='verse-line'>{}</span>"},"vv31:1.4":{"pli":"Uccāvacā iddhi vikubbamānā. ","html":"<span class='verse-line'>{}</span></p>"},"vv31:2.1":{"pli":"Imā ca te accharāyo samantato, ","html":"<p><span class='verse-line'>{}</span>"},"vv31:2.2":{"pli":"Naccanti gāyanti pamodayanti; ","html":"<span class='verse-line'>{}</span>"},"vv31:2.3":{"pli":"Deviddhipattāsi mahānubhāve, ","html":"<span class='verse-line'>{}</span>"},"vv31:2.4":{"pli":"Manussabhūtā kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv31:2.5":{"pli":"Kenāsi evaṃ jalitānubhāvā, ","html":"<span class='verse-line'>{}</span>"},"vv31:2.6":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv31:3.1":{"pli":"“Ahaṃ manussesu manussabhūtā, ","html":"<p><span class='verse-line'>{}</span>"},"vv31:3.2":{"pli":"Aḍḍhe kule suṇisā ahosiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv31:3.3":{"pli":"Akkodhanā bhattuvasānuvattinī, ","html":"<span class='verse-line'>{}</span>"},"vv31:3.4":{"pli":"Uposathe appamattā ahosiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv31:4.1":{"pli":"Manussabhūtā daharā apāpikā, ","html":"<p><span class='verse-line'>{}</span>"},"vv31:4.2":{"pli":"Pasannacittā patimābhirādhayiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv31:4.3":{"pli":"Divā ca ratto ca manāpacārinī, ","html":"<span class='verse-line'>{}</span>"},"vv31:4.4":{"pli":"Ahaṃ pure sīlavatī ahosiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv31:5.1":{"pli":"Pāṇātipātā viratā acorikā, ","html":"<p><span class='verse-line'>{}</span>"},"vv31:5.2":{"pli":"Saṃsuddhakāyā sucibrahmacārinī; ","html":"<span class='verse-line'>{}</span>"},"vv31:5.3":{"pli":"Amajjapā no ca musā abhāṇiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv31:5.4":{"pli":"Sikkhāpadesu paripūrakārinī. ","html":"<span class='verse-line'>{}</span></p>"},"vv31:6.1":{"pli":"Cātuddasiṃ pañcadasiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv31:6.2":{"pli":"yā ca pakkhassa aṭṭhamī; ","html":"<span class='verse-line'>{}</span>"},"vv31:6.3":{"pli":"Pāṭihāriyapakkhañca, ","html":"<span class='verse-line'>{}</span>"},"vv31:6.4":{"pli":"pasannamānasā ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv31:7.1":{"pli":"Aṭṭhaṅgupetaṃ anudhammacārinī, ","html":"<p><span class='verse-line'>{}</span>"},"vv31:7.2":{"pli":"Uposathaṃ pītimanā upāvasiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv31:7.3":{"pli":"Imañca ariyaṃ aṭṭhaṅgavarehupetaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv31:7.4":{"pli":"Samādiyitvā kusalaṃ sukhudrayaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv31:7.5":{"pli":"Patimhi kalyāṇī vasānuvattinī, ","html":"<span class='verse-line'>{}</span>"},"vv31:7.6":{"pli":"Ahosiṃ pubbe sugatassa sāvikā. ","html":"<span class='verse-line'>{}</span></p>"},"vv31:8.1":{"pli":"Etādisaṃ kusalaṃ jīvaloke, ","html":"<p><span class='verse-line'>{}</span>"},"vv31:8.2":{"pli":"Kammaṃ karitvāna visesabhāginī; ","html":"<span class='verse-line'>{}</span>"},"vv31:8.3":{"pli":"Kāyassa bhedā abhisamparāyaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv31:8.4":{"pli":"Deviddhipattā sugatimhi āgatā. ","html":"<span class='verse-line'>{}</span></p>"},"vv31:9.1":{"pli":"Vimānapāsādavare manorame, ","html":"<p><span class='verse-line'>{}</span>"},"vv31:9.2":{"pli":"Parivāritā accharāsaṅgaṇena; ","html":"<span class='verse-line'>{}</span>"},"vv31:9.3":{"pli":"Sayaṃpabhā devagaṇā ramenti maṃ, ","html":"<span class='verse-line'>{}</span>"},"vv31:9.4":{"pli":"Dīghāyukiṃ devavimānamāgatan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv31:10.1":{"pli":"Pallaṅkavimānaṃ tatiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv32":{"vv32:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv32'><header><ul><li class='division'>{}</li>"},"vv32:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv32:0.3":{"pli":"Pāricchattakavagga ","html":"<li>{}</li></ul>"},"vv32:0.4":{"pli":"4. Latāvimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv32:1.1":{"pli":"Latā ca sajjā pavarā ca devatā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv32:1.2":{"pli":"Accimatī rājavarassa sirīmato; ","html":"<span class='verse-line'>{}</span>"},"vv32:1.3":{"pli":"Sutā ca rañño vessavaṇassa dhītā, ","html":"<span class='verse-line'>{}</span>"},"vv32:1.4":{"pli":"Rājīmatī dhammaguṇehi sobhatha. ","html":"<span class='verse-line'>{}</span></p>"},"vv32:2.1":{"pli":"Pañcettha nāriyo āgamaṃsu nhāyituṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv32:2.2":{"pli":"Sītodakaṃ uppaliniṃ sivaṃ nadiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv32:2.3":{"pli":"Tā tattha nhāyitvā rametvā devatā, ","html":"<span class='verse-line'>{}</span>"},"vv32:2.4":{"pli":"Naccitvā gāyitvā sutā lataṃ bravi. ","html":"<span class='verse-line'>{}</span></p>"},"vv32:3.1":{"pli":"“Pucchāmi taṃ uppalamāladhārini, ","html":"<p><span class='verse-line'>{}</span>"},"vv32:3.2":{"pli":"Āveḷini kañcanasannibhattace; ","html":"<span class='verse-line'>{}</span>"},"vv32:3.3":{"pli":"Timiratambakkhi nabheva sobhane, ","html":"<span class='verse-line'>{}</span>"},"vv32:3.4":{"pli":"Dīghāyukī kena kato yaso tava. ","html":"<span class='verse-line'>{}</span></p>"},"vv32:4.1":{"pli":"Kenāsi bhadde patino piyatarā, ","html":"<p><span class='verse-line'>{}</span>"},"vv32:4.2":{"pli":"Visiṭṭhakalyāṇitarassu rūpato; ","html":"<span class='verse-line'>{}</span>"},"vv32:4.3":{"pli":"Padakkhiṇā naccanagītavādite, ","html":"<span class='verse-line'>{}</span>"},"vv32:4.4":{"pli":"Ācikkha no tvaṃ naranāripucchitā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv32:5.1":{"pli":"“Ahaṃ manussesu manussabhūtā, ","html":"<p><span class='verse-line'>{}</span>"},"vv32:5.2":{"pli":"Uḷārabhoge kule suṇisā ahosiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv32:5.3":{"pli":"Akkodhanā bhattuvasānuvattinī, ","html":"<span class='verse-line'>{}</span>"},"vv32:5.4":{"pli":"Uposathe appamattā ahosiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv32:6.1":{"pli":"Manussabhūtā daharā apāpikā, ","html":"<p><span class='verse-line'>{}</span>"},"vv32:6.2":{"pli":"Pasannacittā patimābhirādhayiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv32:6.3":{"pli":"Sadevaraṃ sassasuraṃ sadāsakaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv32:6.4":{"pli":"Abhirādhayiṃ tamhi kato yaso mama. ","html":"<span class='verse-line'>{}</span></p>"},"vv32:7.1":{"pli":"Sāhaṃ tena kusalena kammunā, ","html":"<p><span class='verse-line'>{}</span>"},"vv32:7.2":{"pli":"Catubbhi ṭhānehi visesamajjhagā; ","html":"<span class='verse-line'>{}</span>"},"vv32:7.3":{"pli":"Āyuñca vaṇṇañca sukhaṃ balañca, ","html":"<span class='verse-line'>{}</span>"},"vv32:7.4":{"pli":"Khiḍḍāratiṃ paccanubhomanappakaṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"vv32:8.1":{"pli":"“Sutaṃ nu taṃ bhāsati yaṃ ayaṃ latā, ","html":"<p><span class='verse-line'>{}</span>"},"vv32:8.2":{"pli":"Yaṃ no apucchimha akittayī no; ","html":"<span class='verse-line'>{}</span>"},"vv32:8.3":{"pli":"Patino kiramhākaṃ visiṭṭha nārīnaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv32:8.4":{"pli":"Gatī ca tāsaṃ pavarā ca devatā. ","html":"<span class='verse-line'>{}</span></p>"},"vv32:9.1":{"pli":"Patīsu dhammaṃ pacarāma sabbā, ","html":"<p><span class='verse-line'>{}</span>"},"vv32:9.2":{"pli":"Patibbatā yattha bhavanti itthiyo; ","html":"<span class='verse-line'>{}</span>"},"vv32:9.3":{"pli":"Patīsu dhammaṃ pacaritva sabbā, ","html":"<span class='verse-line'>{}</span>"},"vv32:9.4":{"pli":"Lacchāmase bhāsati yaṃ ayaṃ latā. ","html":"<span class='verse-line'>{}</span></p>"},"vv32:10.1":{"pli":"Sīho yathā pabbatasānugocaro, ","html":"<p><span class='verse-line'>{}</span>"},"vv32:10.2":{"pli":"Mahindharaṃ pabbatamāvasitvā; ","html":"<span class='verse-line'>{}</span>"},"vv32:10.3":{"pli":"Pasayha hantvā itare catuppade, ","html":"<span class='verse-line'>{}</span>"},"vv32:10.4":{"pli":"Khudde mige khādati maṃsabhojano. ","html":"<span class='verse-line'>{}</span></p>"},"vv32:11.1":{"pli":"Tatheva saddhā idha ariyasāvikā, ","html":"<p><span class='verse-line'>{}</span>"},"vv32:11.2":{"pli":"Bhattāraṃ nissāya patiṃ anubbatā; ","html":"<span class='verse-line'>{}</span>"},"vv32:11.3":{"pli":"Kodhaṃ vadhitvā abhibhuyya maccharaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv32:11.4":{"pli":"Saggamhi sā modati dhammacārinī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv32:12.1":{"pli":"Latāvimānaṃ catutthaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}