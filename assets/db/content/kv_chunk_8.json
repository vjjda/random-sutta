kv11.5:7.13":{"pli":"Tena hi na vattabbaṃ—","html":"{}"},"kv11.5:7.14":{"pli":"“iddhibalena samannāgato kappaṃ tiṭṭheyyā”ti. ","html":"{}</p>"},"kv11.5:7.15":{"pli":"Iddhibalakathā niṭṭhitā. ","html":"<p class='endsutta'>{}</p></article>"}}}