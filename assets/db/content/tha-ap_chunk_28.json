{"tha-ap394":{"tha-ap394:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap394'><header><ul><li class='division'>{}</li>"},"tha-ap394:0.2":{"pli":"Pilindavacchavagga ","html":"<li>{}</li></ul>"},"tha-ap394:0.3":{"pli":"Selattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap394:1.1":{"pli":"“Nagare haṃsavatiyā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap394:1.2":{"pli":"vīthisāmī ahosahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:1.3":{"pli":"Mama ñātī samānetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:1.4":{"pli":"idaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:2.1":{"pli":"‘Buddho loke samuppanno, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:2.2":{"pli":"puññakkhetto anuttaro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:2.3":{"pli":"Āsi so sabbalokassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:2.4":{"pli":"āhutīnaṃ paṭiggaho. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:3.1":{"pli":"Khattiyā negamā ceva, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:3.2":{"pli":"mahāsālā ca brāhmaṇā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:3.3":{"pli":"Pasannacittā sumanā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:3.4":{"pli":"pūgadhammaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:4.1":{"pli":"Hatthārohā anīkaṭṭhā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:4.2":{"pli":"rathikā pattikārakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:4.3":{"pli":"Pasannacittā sumanā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:4.4":{"pli":"pūgadhammaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:5.1":{"pli":"Uggā ca rājaputtā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:5.2":{"pli":"vesiyānā ca brāhmaṇā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:5.3":{"pli":"Pasannacittā sumanā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:5.4":{"pli":"pūgadhammaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:6.1":{"pli":"Āḷārikā kappakā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:6.2":{"pli":"nhāpakā mālakārakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:6.3":{"pli":"Pasannacittā sumanā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:6.4":{"pli":"pūgadhammaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:7.1":{"pli":"Rajakā pesakārā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:7.2":{"pli":"cammakārā ca nhāpitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:7.3":{"pli":"Pasannacittā sumanā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:7.4":{"pli":"pūgadhammaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:8.1":{"pli":"Usukārā bhamakārā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:8.2":{"pli":"cammakārā ca tacchakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:8.3":{"pli":"Pasannacittā sumanā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:8.4":{"pli":"pūgadhammaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:9.1":{"pli":"Kammārā soṇṇakārā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:9.2":{"pli":"tipulohakarā tathā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:9.3":{"pli":"Pasannacittā sumanā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:9.4":{"pli":"pūgadhammaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:10.1":{"pli":"Bhatakā ceṭakā ceva, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:10.2":{"pli":"dāsakammakarā bahū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:10.3":{"pli":"Yathāsakena thāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:10.4":{"pli":"pūgadhammaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:11.1":{"pli":"Udahārā kaṭṭhahārā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:11.2":{"pli":"kassakā tiṇahārakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:11.3":{"pli":"Yathāsakena thāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:11.4":{"pli":"pūgadhammaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:12.1":{"pli":"Pupphikā mālikā ceva, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:12.2":{"pli":"paṇṇikā phalahārakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:12.3":{"pli":"Yathāsakena thāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:12.4":{"pli":"pūgadhammaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:13.1":{"pli":"Gaṇikā kumbhadāsī ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:13.2":{"pli":"pūvikā macchikāpi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:13.3":{"pli":"Yathāsakena thāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:13.4":{"pli":"pūgadhammaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:14.1":{"pli":"Etha sabbe samāgantvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:14.2":{"pli":"gaṇaṃ bandhāma ekato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:14.3":{"pli":"Adhikāraṃ karissāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:14.4":{"pli":"puññakkhette anuttare’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:15.1":{"pli":"Te me sutvāna vacanaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:15.2":{"pli":"gaṇaṃ bandhiṃsu tāvade; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:15.3":{"pli":"‘Upaṭṭhānasālaṃ sukataṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:15.4":{"pli":"bhikkhusaṅghassa kārayuṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:16.1":{"pli":"Niṭṭhāpetvāna taṃ sālaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:16.2":{"pli":"udaggo tuṭṭhamānaso; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:16.3":{"pli":"Pareto tehi sabbehi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:16.4":{"pli":"sambuddhamupasaṅkamiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:17.1":{"pli":"Upasaṅkamma sambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:17.2":{"pli":"lokanāthaṃ narāsabhaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:17.3":{"pli":"Vanditvā satthuno pāde, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:17.4":{"pli":"idaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:18.1":{"pli":"‘Ime tīṇi satā vīra, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:18.2":{"pli":"Purisā ekato gaṇā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:18.3":{"pli":"Upaṭṭhānasālaṃ sukataṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:18.4":{"pli":"Niyyādenti tuvaṃ muni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:19.1":{"pli":"Bhikkhusaṅghassa purato, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:19.2":{"pli":"sampaṭicchitva cakkhumā’; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:19.3":{"pli":"Tiṇṇaṃ satānaṃ purato, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:19.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:20.1":{"pli":"‘Tisatāpi ca jeṭṭho ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:20.2":{"pli":"anuvattiṃsu ekato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:20.3":{"pli":"Sampattiñhi karitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:20.4":{"pli":"sabbe anubhavissatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:21.1":{"pli":"Pacchime bhave sampatte, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:21.2":{"pli":"sītibhāvamanuttaraṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:21.3":{"pli":"Ajaraṃ amataṃ santaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:21.4":{"pli":"nibbānaṃ phassayissatha’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:22.1":{"pli":"Evaṃ buddho viyākāsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:22.2":{"pli":"sabbaññū samaṇuttaro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:22.3":{"pli":"Buddhassa vacanaṃ sutvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:22.4":{"pli":"somanassaṃ pavedayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:23.1":{"pli":"Tiṃsa kappasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:23.2":{"pli":"devaloke ramiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:23.3":{"pli":"Devādhipo pañcasataṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:23.4":{"pli":"devarajjamakārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:24.1":{"pli":"Sahassakkhattuṃ rājā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:24.2":{"pli":"cakkavattī ahosahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:24.3":{"pli":"Devarajjaṃ karontassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:24.4":{"pli":"mahādevā avandisuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:25.1":{"pli":"Idha mānusake rajjaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:25.2":{"pli":"parisā honti bandhavā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:25.3":{"pli":"Pacchime bhave sampatte, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:25.4":{"pli":"vāseṭṭho nāma brāhmaṇo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:26.1":{"pli":"Asītikoṭi nicayo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:26.2":{"pli":"tassa putto ahosahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:26.3":{"pli":"Selo iti mamaṃ nāmaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:26.4":{"pli":"chaḷaṅge pāramiṃ gato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:27.1":{"pli":"Jaṅghāvihāraṃ vicaraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:27.2":{"pli":"sasissehi purakkhato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:27.3":{"pli":"Jaṭābhārikabharitaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:27.4":{"pli":"keṇiyaṃ nāma tāpasaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:28.1":{"pli":"Paṭiyattāhutiṃ disvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:28.2":{"pli":"idaṃ vacanamabraviṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:28.3":{"pli":"‘Āvāho vā vivāho vā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:28.4":{"pli":"rājā vā te nimantito’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:29.1":{"pli":"‘Āhutiṃ yiṭṭhukāmohaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:29.2":{"pli":"brāhmaṇe devasammate; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:29.3":{"pli":"Na nimantemi rājānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:29.4":{"pli":"āhutī me na vijjati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:30.1":{"pli":"Na catthi mayhamāvāho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:30.2":{"pli":"vivāho me na vijjati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:30.3":{"pli":"Sakyānaṃ nandijanano, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:30.4":{"pli":"seṭṭho loke sadevake. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:31.1":{"pli":"Sabbalokahitatthāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:31.2":{"pli":"sabbasattasukhāvaho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:31.3":{"pli":"So me nimantito ajja, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:31.4":{"pli":"tassetaṃ paṭiyādanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:32.1":{"pli":"Timbarūsakavaṇṇābho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:32.2":{"pli":"appameyyo anūpamo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:32.3":{"pli":"Rūpenāsadiso buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:32.4":{"pli":"svātanāya nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:33.1":{"pli":"Ukkāmukhapahaṭṭhova, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:33.2":{"pli":"khadiraṅgārasannibho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:33.3":{"pli":"Vijjūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:33.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:34.1":{"pli":"Pabbatagge yathā acci, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:34.2":{"pli":"puṇṇamāyeva candimā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:34.3":{"pli":"Naḷaggivaṇṇasaṅkāso, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:34.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:35.1":{"pli":"Asambhīto bhayātīto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:35.2":{"pli":"bhavantakaraṇo muni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:35.3":{"pli":"Sīhūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:35.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:36.1":{"pli":"Kusalo buddhadhammehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:36.2":{"pli":"apasayho parehi so; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:36.3":{"pli":"Nāgūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:36.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:37.1":{"pli":"Saddhammācārakusalo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:37.2":{"pli":"buddhanāgo asādiso; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:37.3":{"pli":"Usabhūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:37.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:38.1":{"pli":"Anantavaṇṇo amitayaso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:38.2":{"pli":"Vicittasabbalakkhaṇo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:38.3":{"pli":"Sakkūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:38.4":{"pli":"So me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:39.1":{"pli":"Vasī gaṇī patāpī ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:39.2":{"pli":"tejassī ca durāsado; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:39.3":{"pli":"Brahmūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:39.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:40.1":{"pli":"Pattadhammo dasabalo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:40.2":{"pli":"balātibalapārago; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:40.3":{"pli":"Dharaṇūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:40.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:41.1":{"pli":"Sīlavīcisamākiṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:41.2":{"pli":"dhammaviññāṇakhobhito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:41.3":{"pli":"Udadhūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:41.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:42.1":{"pli":"Durāsado duppasaho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:42.2":{"pli":"acalo uggato brahā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:42.3":{"pli":"Nerūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:42.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:43.1":{"pli":"Anantañāṇo asamasamo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:43.2":{"pli":"Atulo aggataṃ gato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:43.3":{"pli":"Gaganūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:43.4":{"pli":"So me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap394:44.1":{"pli":"Pannarasamaṃ bhāṇavāraṃ. ","html":"<p class='endsutta'>{}</p>"},"tha-ap394:45.1":{"pli":"Patiṭṭhā bhayabhītānaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap394:45.2":{"pli":"tāṇo saraṇagāminaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:45.3":{"pli":"Assāsako mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:45.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:46.1":{"pli":"Āsayo buddhimantānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:46.2":{"pli":"puññakkhettaṃ sukhesinaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:46.3":{"pli":"Ratanākaro mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:46.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:47.1":{"pli":"Assāsako vedakaro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:47.2":{"pli":"sāmaññaphaladāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:47.3":{"pli":"Meghūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:47.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:48.1":{"pli":"Lokacakkhu mahātejo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:48.2":{"pli":"sabbatamavinodano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:48.3":{"pli":"Sūriyūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:48.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:49.1":{"pli":"Ārammaṇavimuttīsu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:49.2":{"pli":"sabhāvadassano muni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:49.3":{"pli":"Candūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:49.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:50.1":{"pli":"Buddho samussito loke, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:50.2":{"pli":"lakkhaṇehi alaṅkato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:50.3":{"pli":"Appameyyo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:50.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:51.1":{"pli":"Yassa ñāṇaṃ appameyyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:51.2":{"pli":"sīlaṃ yassa anūpamaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:51.3":{"pli":"Vimutti asadisā yassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:51.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:52.1":{"pli":"Yassa dhīti asadisā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:52.2":{"pli":"thāmo yassa acintiyo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:52.3":{"pli":"Yassa parakkamo jeṭṭho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:52.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:53.1":{"pli":"Rāgo doso ca moho ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:53.2":{"pli":"visā sabbe samūhatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:53.3":{"pli":"Agadūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:53.4":{"pli":"so me buddho nimantito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:54.1":{"pli":"Klesabyādhibahudukkha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:54.2":{"pli":"sabbatamavinodano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:54.3":{"pli":"Vejjūpamo mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:54.4":{"pli":"so me buddho nimantito’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:55.1":{"pli":"Buddhoti bho yaṃ vadesi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:55.2":{"pli":"ghosopeso sudullabho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:55.3":{"pli":"Buddho buddhoti sutvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:55.4":{"pli":"pīti me udapajjatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:56.1":{"pli":"Abbhantaraṃ agaṇhantaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:56.2":{"pli":"pīti me bahi nicchare; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:56.3":{"pli":"Sohaṃ pītimano santo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:56.4":{"pli":"idaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:57.1":{"pli":"‘Kahaṃ nu kho so bhagavā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:57.2":{"pli":"lokajeṭṭho narāsabho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:57.3":{"pli":"Tattha gantvā namassissaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:57.4":{"pli":"sāmaññaphaladāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:58.1":{"pli":"Paggayha dakkhiṇaṃ bāhuṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:58.2":{"pli":"vedajāto katañjalī; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:58.3":{"pli":"Ācikkhi me dhammarājaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:58.4":{"pli":"sokasallavinodanaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:59.1":{"pli":"‘Udentaṃva mahāmeghaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:59.2":{"pli":"nīlaṃ añjanasannibhaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:59.3":{"pli":"Sāgaraṃ viya dissantaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:59.4":{"pli":"passasetaṃ mahāvanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:60.1":{"pli":"Ettha so vasate buddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:60.2":{"pli":"adantadamako muni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:60.3":{"pli":"Vinayanto ca veneyye, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:60.4":{"pli":"bodhento bodhipakkhiye’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:61.1":{"pli":"Pipāsitova udakaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:61.2":{"pli":"bhojanaṃva jighacchito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:61.3":{"pli":"Gāvī yathā vacchagiddhā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:61.4":{"pli":"evāhaṃ viciniṃ jinaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:62.1":{"pli":"Ācāraupacāraññū, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:62.2":{"pli":"dhammānucchavisaṃvaraṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:62.3":{"pli":"Sikkhāpemi sake sisse, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:62.4":{"pli":"gacchante jinasantikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:63.1":{"pli":"‘Durāsadā bhagavanto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:63.2":{"pli":"sīhāva ekacārino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:63.3":{"pli":"Pade padaṃ nikkhipantā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:63.4":{"pli":"āgaccheyyātha māṇavā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:64.1":{"pli":"Āsīviso yathā ghoro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:64.2":{"pli":"migarājāva kesarī; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:64.3":{"pli":"Mattova kuñjaro dantī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:64.4":{"pli":"evaṃ buddhā durāsadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:65.1":{"pli":"Ukkāsitañca khipitaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:65.2":{"pli":"ajjhupekkhiya māṇavā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:65.3":{"pli":"Pade padaṃ nikkhipantā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:65.4":{"pli":"upetha buddhasantikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:66.1":{"pli":"Paṭisallānagarukā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:66.2":{"pli":"appasaddā durāsadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:66.3":{"pli":"Durūpasaṅkamā buddhā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:66.4":{"pli":"garū honti sadevake. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:67.1":{"pli":"Yadāhaṃ pañhaṃ pucchāmi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:67.2":{"pli":"paṭisammodayāmi vā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:67.3":{"pli":"Appasaddā tadā hotha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:67.4":{"pli":"munibhūtāva tiṭṭhatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:68.1":{"pli":"Yaṃ so deseti sambuddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:68.2":{"pli":"khemaṃ nibbānapattiyā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:68.3":{"pli":"Tamevatthaṃ nisāmetha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:68.4":{"pli":"saddhammasavanaṃ sukhaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:69.1":{"pli":"Upasaṅkamma sambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:69.2":{"pli":"sammodiṃ muninā ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:69.3":{"pli":"Taṃ kathaṃ vītisāretvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:69.4":{"pli":"lakkhaṇe upadhārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:70.1":{"pli":"Lakkhaṇe dve ca kaṅkhāmi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:70.2":{"pli":"passāmi tiṃsalakkhaṇe; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:70.3":{"pli":"Kosohitavatthaguyhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:70.4":{"pli":"iddhiyā dassayī muni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:71.1":{"pli":"Jivhaṃ ninnāmayitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:71.2":{"pli":"kaṇṇasote ca nāsike; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:71.3":{"pli":"Paṭimasi nalāṭantaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:71.4":{"pli":"kevalaṃ chādayī jino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:72.1":{"pli":"Tassāhaṃ lakkhaṇe disvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:72.2":{"pli":"paripuṇṇe sabyañjane; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:72.3":{"pli":"Buddhoti niṭṭhaṃ gantvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:72.4":{"pli":"saha sissehi pabbajiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:73.1":{"pli":"Satehi tīhi sahito, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:73.2":{"pli":"pabbajiṃ anagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:73.3":{"pli":"Addhamāse asampatte, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:73.4":{"pli":"sabbe pattāmha nibbutiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:74.1":{"pli":"Ekato kammaṃ katvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:74.2":{"pli":"puññakkhette anuttare; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:74.3":{"pli":"Ekato saṃsaritvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:74.4":{"pli":"ekato vinivattayuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:75.1":{"pli":"Gopānasiyo datvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:75.2":{"pli":"pūgadhamme vasiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:75.3":{"pli":"Tena kammena sukatena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:75.4":{"pli":"aṭṭha hetū labhāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:76.1":{"pli":"Disāsu pūjito homi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:76.2":{"pli":"bhogā ca amitā mama; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:76.3":{"pli":"Patiṭṭhā homi sabbesaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:76.4":{"pli":"tāso mama na vijjati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:77.1":{"pli":"Byādhayo me na vijjanti, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:77.2":{"pli":"dīghāyuṃ pālayāmi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:77.3":{"pli":"Sukhumacchaviko homi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:77.4":{"pli":"āvāse patthite vase. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:78.1":{"pli":"Aṭṭha gopānasī datvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:78.2":{"pli":"pūgadhamme vasiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:78.3":{"pli":"Paṭisambhidārahattañca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:78.4":{"pli":"etaṃ me aparaṭṭhamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:79.1":{"pli":"Sabbavositavosāno, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:79.2":{"pli":"katakicco anāsavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:79.3":{"pli":"Aṭṭhagopānasī nāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:79.4":{"pli":"tava putto mahāmuni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:80.1":{"pli":"Pañca thambhāni datvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:80.2":{"pli":"pūgadhamme vasiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:80.3":{"pli":"Tena kammena sukatena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:80.4":{"pli":"pañca hetū labhāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:81.1":{"pli":"Acalo homi mettāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:81.2":{"pli":"anūnaṅgo bhavāmahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:81.3":{"pli":"Ādeyyavacano homi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:81.4":{"pli":"na dhaṃsemi yathā ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:82.1":{"pli":"Abhantaṃ hoti me cittaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:82.2":{"pli":"akhilo homi kassaci; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:82.3":{"pli":"Tena kammena sukatena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:82.4":{"pli":"vimalo homi sāsane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:83.1":{"pli":"Sagāravo sappatisso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:83.2":{"pli":"katakicco anāsavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:83.3":{"pli":"Sāvako te mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:83.4":{"pli":"bhikkhu taṃ vandate muni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:84.1":{"pli":"Katvā sukatapallaṅkaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:84.2":{"pli":"sālāyaṃ paññapesahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:84.3":{"pli":"Tena kammena sukatena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:84.4":{"pli":"pañca hetū labhāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:85.1":{"pli":"Ucce kule pajāyitvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:85.2":{"pli":"mahābhogo bhavāmahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:85.3":{"pli":"Sabbasampattiko homi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:85.4":{"pli":"maccheraṃ me na vijjati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:86.1":{"pli":"Gamane patthite mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:86.2":{"pli":"pallaṅko upatiṭṭhati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:86.3":{"pli":"Saha pallaṅkaseṭṭhena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:86.4":{"pli":"gacchāmi mama patthitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:87.1":{"pli":"Tena pallaṅkadānena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:87.2":{"pli":"tamaṃ sabbaṃ vinodayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:87.3":{"pli":"Sabbābhiññābalappatto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:87.4":{"pli":"thero vandati taṃ muni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:88.1":{"pli":"Parakiccattakiccāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:88.2":{"pli":"sabbakiccāni sādhayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:88.3":{"pli":"Tena kammena sukatena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:88.4":{"pli":"pāvisiṃ abhayaṃ puraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:89.1":{"pli":"Pariniṭṭhitasālamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:89.2":{"pli":"paribhogamadāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:89.3":{"pli":"Tena kammena sukatena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:89.4":{"pli":"seṭṭhattaṃ ajjhupāgato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:90.1":{"pli":"Ye keci damakā loke, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:90.2":{"pli":"hatthiasse damenti ye; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:90.3":{"pli":"Karitvā kāraṇā nānā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:90.4":{"pli":"dāruṇena damenti te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:91.1":{"pli":"Na hevaṃ tvaṃ mahāvīra, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:91.2":{"pli":"damesi naranāriyo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:91.3":{"pli":"Adaṇḍena asatthena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:91.4":{"pli":"damesi uttame dame. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:92.1":{"pli":"Dānassa vaṇṇe kittento, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:92.2":{"pli":"desanākusalo muni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:92.3":{"pli":"Ekapañhaṃ kathentova, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:92.4":{"pli":"bodhesi tisate muni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:93.1":{"pli":"Dantā mayaṃ sārathinā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:93.2":{"pli":"suvimuttā anāsavā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:93.3":{"pli":"Sabbābhiññābalapattā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:93.4":{"pli":"nibbutā upadhikkhaye. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:94.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:94.2":{"pli":"yaṃ dānamadadiṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:94.3":{"pli":"Atikkantā bhayā sabbe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:94.4":{"pli":"sālādānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:95.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:95.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:95.3":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:96.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:96.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:96.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap394:97.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap394:97.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap394:97.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap394:98.1":{"pli":"Itthaṃ sudaṃ āyasmā selo sapariso bhagavato santike imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap394:99.1":{"pli":"Selattherassāpadānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}