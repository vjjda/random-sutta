{"vv83":{"vv83:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv83'><header><ul><li class='division'>{}</li>"},"vv83:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv83:0.3":{"pli":"Sunikkhittavagga ","html":"<li>{}</li></ul>"},"vv83:0.4":{"pli":"9. Maṭṭhakuṇḍalīvimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv83:1.1":{"pli":"“Alaṅkato maṭṭhakuṇḍalī, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv83:1.2":{"pli":"Māladhārī haricandanussado; ","html":"<span class='verse-line'>{}</span>"},"vv83:1.3":{"pli":"Bāhā paggayha kandasi, ","html":"<span class='verse-line'>{}</span>"},"vv83:1.4":{"pli":"Vanamajjhe kiṃ dukkhito tuvan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:2.1":{"pli":"“Sovaṇṇamayo pabhassaro, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:2.2":{"pli":"Uppanno rathapañjaro mama; ","html":"<span class='verse-line'>{}</span>"},"vv83:2.3":{"pli":"Tassa cakkayugaṃ na vindāmi, ","html":"<span class='verse-line'>{}</span>"},"vv83:2.4":{"pli":"Tena dukkhena jahāmi jīvitan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:3.1":{"pli":"“Sovaṇṇamayaṃ maṇimayaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:3.2":{"pli":"Lohitakamayaṃ atha rūpiyamayaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv83:3.3":{"pli":"Ācikkha me bhaddamāṇava, ","html":"<span class='verse-line'>{}</span>"},"vv83:3.4":{"pli":"Cakkayugaṃ paṭipādayāmi te”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:4.1":{"pli":"So māṇavo tassa pāvadi, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:4.2":{"pli":"“Candasūriyā ubhayettha dissare; ","html":"<span class='verse-line'>{}</span>"},"vv83:4.3":{"pli":"Sovaṇṇamayo ratho mama, ","html":"<span class='verse-line'>{}</span>"},"vv83:4.4":{"pli":"Tena cakkayugena sobhatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:5.1":{"pli":"“Bālo kho tvaṃ asi māṇava, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:5.2":{"pli":"Yo tvaṃ patthayase apatthiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv83:5.3":{"pli":"Maññāmi tuvaṃ marissasi, ","html":"<span class='verse-line'>{}</span>"},"vv83:5.4":{"pli":"Na hi tvaṃ lacchasi candasūriye”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:6.1":{"pli":"“Gamanāgamanampi dissati, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:6.2":{"pli":"Vaṇṇadhātu ubhayattha vīthiyā; ","html":"<span class='verse-line'>{}</span>"},"vv83:6.3":{"pli":"Peto kālakato na dissati, ","html":"<span class='verse-line'>{}</span>"},"vv83:6.4":{"pli":"Ko nidha kandataṃ bālyataro”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:7.1":{"pli":"“Saccaṃ kho vadesi māṇava, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:7.2":{"pli":"Ahameva kandataṃ bālyataro; ","html":"<span class='verse-line'>{}</span>"},"vv83:7.3":{"pli":"Candaṃ viya dārako rudaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv83:7.4":{"pli":"Petaṃ kālakatābhipatthayinti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:8.1":{"pli":"Ādittaṃ vata maṃ santaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:8.2":{"pli":"ghatasittaṃva pāvakaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv83:8.3":{"pli":"Vārinā viya osiñcaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv83:8.4":{"pli":"sabbaṃ nibbāpaye daraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:9.1":{"pli":"Abbahī vata me sallaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:9.2":{"pli":"sokaṃ hadayanissitaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv83:9.3":{"pli":"Yo me sokaparetassa, ","html":"<span class='verse-line'>{}</span>"},"vv83:9.4":{"pli":"puttasokaṃ apānudi. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:10.1":{"pli":"Svāhaṃ abbūḷhasallosmi, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:10.2":{"pli":"sītibhūtosmi nibbuto; ","html":"<span class='verse-line'>{}</span>"},"vv83:10.3":{"pli":"Na socāmi na rodāmi, ","html":"<span class='verse-line'>{}</span>"},"vv83:10.4":{"pli":"tava sutvāna māṇavā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:11.1":{"pli":"“Devatā nusi gandhabbo, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:11.2":{"pli":"adu sakko purindado; ","html":"<span class='verse-line'>{}</span>"},"vv83:11.3":{"pli":"Ko vā tvaṃ kassa vā putto, ","html":"<span class='verse-line'>{}</span>"},"vv83:11.4":{"pli":"kathaṃ jānemu taṃ mayan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:12.1":{"pli":"“Yañca kandasi yañca rodasi, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:12.2":{"pli":"Puttaṃ āḷāhane sayaṃ dahitvā; ","html":"<span class='verse-line'>{}</span>"},"vv83:12.3":{"pli":"Svāhaṃ kusalaṃ karitvā kammaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv83:12.4":{"pli":"Tidasānaṃ sahabyataṃ gato”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:13.1":{"pli":"“Appaṃ vā bahuṃ vā nāddasāma, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:13.2":{"pli":"Dānaṃ dadantassa sake agāre; ","html":"<span class='verse-line'>{}</span>"},"vv83:13.3":{"pli":"Uposathakammaṃ vā tādisaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv83:13.4":{"pli":"Kena kammena gatosi devalokan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:14.1":{"pli":"“Ābādhikohaṃ dukkhito gilāno, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:14.2":{"pli":"Āturarūpomhi sake nivesane; ","html":"<span class='verse-line'>{}</span>"},"vv83:14.3":{"pli":"Buddhaṃ vigatarajaṃ vitiṇṇakaṅkhaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv83:14.4":{"pli":"Addakkhiṃ sugataṃ anomapaññaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:15.1":{"pli":"Svāhaṃ muditamano pasannacitto, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:15.2":{"pli":"Añjaliṃ akariṃ tathāgatassa; ","html":"<span class='verse-line'>{}</span>"},"vv83:15.3":{"pli":"Tāhaṃ kusalaṃ karitvāna kammaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv83:15.4":{"pli":"Tidasānaṃ sahabyataṃ gato”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:16.1":{"pli":"“Acchariyaṃ vata abbhutaṃ vata, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:16.2":{"pli":"Añjalikammassa ayamīdiso vipāko; ","html":"<span class='verse-line'>{}</span>"},"vv83:16.3":{"pli":"Ahampi muditamano pasannacitto, ","html":"<span class='verse-line'>{}</span>"},"vv83:16.4":{"pli":"Ajjeva buddhaṃ saraṇaṃ vajāmī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:17.1":{"pli":"“Ajjeva buddhaṃ saraṇaṃ vajāhi, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:17.2":{"pli":"Dhammañca saṅghañca pasannacitto; ","html":"<span class='verse-line'>{}</span>"},"vv83:17.3":{"pli":"Tatheva sikkhāya padāni pañca, ","html":"<span class='verse-line'>{}</span>"},"vv83:17.4":{"pli":"Akhaṇḍaphullāni samādiyassu. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:18.1":{"pli":"Pāṇātipātā viramassu khippaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:18.2":{"pli":"Loke adinnaṃ parivajjayassu; ","html":"<span class='verse-line'>{}</span>"},"vv83:18.3":{"pli":"Amajjapo mā ca musā bhaṇāhi, ","html":"<span class='verse-line'>{}</span>"},"vv83:18.4":{"pli":"Sakena dārena ca hohi tuṭṭho”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:19.1":{"pli":"“Atthakāmosi me yakkha, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:19.2":{"pli":"hitakāmosi devate; ","html":"<span class='verse-line'>{}</span>"},"vv83:19.3":{"pli":"Karomi tuyhaṃ vacanaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv83:19.4":{"pli":"tvaṃsi ācariyo mamāti. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:20.1":{"pli":"Upemi saraṇaṃ buddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:20.2":{"pli":"dhammañcāpi anuttaraṃ; ","html":"<span class='verse-line'>{}</span>"},"vv83:20.3":{"pli":"Saṅghañca naradevassa, ","html":"<span class='verse-line'>{}</span>"},"vv83:20.4":{"pli":"gacchāmi saraṇaṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv83:21.1":{"pli":"Pāṇātipātā viramāmi khippaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv83:21.2":{"pli":"Loke adinnaṃ parivajjayāmi; ","html":"<span class='verse-line'>{}</span>"},"vv83:21.3":{"pli":"Amajjapo no ca musā bhaṇāmi, ","html":"<span class='verse-line'>{}</span>"},"vv83:21.4":{"pli":"Sakena dārena ca homi tuṭṭho”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv83:22.1":{"pli":"Maṭṭhakuṇḍalīvimānaṃ navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv84":{"vv84:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv84'><header><ul><li class='division'>{}</li>"},"vv84:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv84:0.3":{"pli":"Sunikkhittavagga ","html":"<li>{}</li></ul>"},"vv84:0.4":{"pli":"10. Serīsakavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv84:1.1":{"pli":"“Suṇotha yakkhassa ca vāṇijāna ca, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv84:1.2":{"pli":"Samāgamo yattha tadā ahosi; ","html":"<span class='verse-line'>{}</span>"},"vv84:1.3":{"pli":"Yathā kathaṃ itaritarena cāpi, ","html":"<span class='verse-line'>{}</span>"},"vv84:1.4":{"pli":"Subhāsitaṃ tañca suṇātha sabbe. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:2.1":{"pli":"Yo so ahu rājā pāyāsi nāma, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:2.2":{"pli":"Bhummānaṃ sahabyagato yasassī; ","html":"<span class='verse-line'>{}</span>"},"vv84:2.3":{"pli":"So modamānova sake vimāne, ","html":"<span class='verse-line'>{}</span>"},"vv84:2.4":{"pli":"Amānuso mānuse ajjhabhāsī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:3.1":{"pli":"“Vaṅke araññe amanussaṭṭhāne, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:3.2":{"pli":"Kantāre appodake appabhakkhe; ","html":"<span class='verse-line'>{}</span>"},"vv84:3.3":{"pli":"Suduggame vaṇṇupathassa majjhe, ","html":"<span class='verse-line'>{}</span>"},"vv84:3.4":{"pli":"Vaṅkaṃ bhayā naṭṭhamanā manussā. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:4.1":{"pli":"Nayidha phalā mūlamayā ca santi, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:4.2":{"pli":"Upādānaṃ natthi kutodha bhakkho; ","html":"<span class='verse-line'>{}</span>"},"vv84:4.3":{"pli":"Aññatra paṃsūhi ca vālukāhi ca, ","html":"<span class='verse-line'>{}</span>"},"vv84:4.4":{"pli":"Tatāhi uṇhāhi ca dāruṇāhi ca. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:5.1":{"pli":"Ujjaṅgalaṃ tattamivaṃ kapālaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:5.2":{"pli":"Anāyasaṃ paralokena tulyaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:5.3":{"pli":"Luddānamāvāsamidaṃ purāṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:5.4":{"pli":"Bhūmippadeso abhisattarūpo. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:6.1":{"pli":"Atha tumhe kena vaṇṇena, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:6.2":{"pli":"Kimāsamānā imaṃ padesaṃ hi; ","html":"<span class='verse-line'>{}</span>"},"vv84:6.3":{"pli":"Anupaviṭṭhā sahasā samecca, ","html":"<span class='verse-line'>{}</span>"},"vv84:6.4":{"pli":"Lobhā bhayā atha vā sampamūḷhā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:7.1":{"pli":"“Magadhesu aṅgesu ca satthavāhā, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:7.2":{"pli":"Āropayitvā paṇiyaṃ puthuttaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:7.3":{"pli":"Te yāmase sindhusovīrabhūmiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:7.4":{"pli":"Dhanatthikā uddayaṃ patthayānā. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:8.1":{"pli":"Divā pipāsaṃ nadhivāsayantā, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:8.2":{"pli":"Yoggānukampañca samekkhamānā; ","html":"<span class='verse-line'>{}</span>"},"vv84:8.3":{"pli":"Etena vegena āyāma sabbe, ","html":"<span class='verse-line'>{}</span>"},"vv84:8.4":{"pli":"Rattiṃ maggaṃ paṭipannā vikāle. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:9.1":{"pli":"Te duppayātā aparaddhamaggā, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:9.2":{"pli":"Andhākulā vippanaṭṭhā araññe; ","html":"<span class='verse-line'>{}</span>"},"vv84:9.3":{"pli":"Suduggame vaṇṇupathassa majjhe, ","html":"<span class='verse-line'>{}</span>"},"vv84:9.4":{"pli":"Disaṃ na jānāma pamūḷhacittā. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:10.1":{"pli":"Idañca disvāna adiṭṭhapubbaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:10.2":{"pli":"Vimānaseṭṭhañca tavañca yakkha; ","html":"<span class='verse-line'>{}</span>"},"vv84:10.3":{"pli":"Tatuttariṃ jīvitamāsamānā, ","html":"<span class='verse-line'>{}</span>"},"vv84:10.4":{"pli":"Disvā patītā sumanā udaggā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:11.1":{"pli":"“Pāraṃ samuddassa imañca vaṇṇuṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:11.2":{"pli":"Vettācaraṃ saṅkupathañca maggaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:11.3":{"pli":"Nadiyo pana pabbatānañca duggā, ","html":"<span class='verse-line'>{}</span>"},"vv84:11.4":{"pli":"Puthuddisā gacchatha bhogahetu. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:12.1":{"pli":"Pakkhandiyāna vijitaṃ paresaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:12.2":{"pli":"Verajjake mānuse pekkhamānā; ","html":"<span class='verse-line'>{}</span>"},"vv84:12.3":{"pli":"Yaṃ vo sutaṃ vā atha vāpi diṭṭhaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:12.4":{"pli":"Accherakaṃ taṃ vo suṇoma tātā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:13.1":{"pli":"“Itopi accherataraṃ kumāra, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:13.2":{"pli":"Na no sutaṃ vā atha vāpi diṭṭhaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:13.3":{"pli":"Atītamānussakameva sabbaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:13.4":{"pli":"Disvā na tappāma anomavaṇṇaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:14.1":{"pli":"Vehāyasaṃ pokkharañño savanti, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:14.2":{"pli":"Pahūtamalyā bahupuṇḍarīkā; ","html":"<span class='verse-line'>{}</span>"},"vv84:14.3":{"pli":"Dumā cime niccaphalūpapannā, ","html":"<span class='verse-line'>{}</span>"},"vv84:14.4":{"pli":"Atīva gandhā surabhiṃ pavāyanti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:15.1":{"pli":"Veḷūriyathambhā satamussitāse, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:15.2":{"pli":"Silāpavāḷassa ca āyataṃsā; ","html":"<span class='verse-line'>{}</span>"},"vv84:15.3":{"pli":"Masāragallā sahalohitaṅgā, ","html":"<span class='verse-line'>{}</span>"},"vv84:15.4":{"pli":"Thambhā ime jotirasāmayāse. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:16.1":{"pli":"Sahassathambhaṃ atulānubhāvaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:16.2":{"pli":"Tesūpari sādhumidaṃ vimānaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:16.3":{"pli":"Ratanantaraṃ kañcanavedimissaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:16.4":{"pli":"Tapanīyapaṭṭehi ca sādhuchannaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:17.1":{"pli":"Jambonaduttattamidaṃ sumaṭṭho, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:17.2":{"pli":"Pāsādasopānaphalūpapanno; ","html":"<span class='verse-line'>{}</span>"},"vv84:17.3":{"pli":"Daḷho ca vaggu ca susaṅgato ca, ","html":"<span class='verse-line'>{}</span>"},"vv84:17.4":{"pli":"Atīva nijjhānakhamo manuñño. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:18.1":{"pli":"Ratanantarasmiṃ bahuannapānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:18.2":{"pli":"Parivārito accharāsaṅgaṇena; ","html":"<span class='verse-line'>{}</span>"},"vv84:18.3":{"pli":"Murajaālambaratūriyaghuṭṭho, ","html":"<span class='verse-line'>{}</span>"},"vv84:18.4":{"pli":"Abhivanditosi thutivandanāya. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:19.1":{"pli":"So modasi nārigaṇappabodhano, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:19.2":{"pli":"Vimānapāsādavare manorame; ","html":"<span class='verse-line'>{}</span>"},"vv84:19.3":{"pli":"Acintiyo sabbaguṇūpapanno, ","html":"<span class='verse-line'>{}</span>"},"vv84:19.4":{"pli":"Rājā yathā vessavaṇo naḷinyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:20.1":{"pli":"Devo nu āsi udavāsi yakkho, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:20.2":{"pli":"Udāhu devindo manussabhūto; ","html":"<span class='verse-line'>{}</span>"},"vv84:20.3":{"pli":"Pucchanti taṃ vāṇijā satthavāhā, ","html":"<span class='verse-line'>{}</span>"},"vv84:20.4":{"pli":"Ācikkha ko nāma tuvaṃsi yakkho”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:21.1":{"pli":"“Serīsako nāma ahamhi yakkho, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:21.2":{"pli":"Kantāriyo vaṇṇupathamhi gutto; ","html":"<span class='verse-line'>{}</span>"},"vv84:21.3":{"pli":"Imaṃ padesaṃ abhipālayāmi, ","html":"<span class='verse-line'>{}</span>"},"vv84:21.4":{"pli":"Vacanakaro vessavaṇassa rañño”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:22.1":{"pli":"“Adhiccaladdhaṃ pariṇāmajaṃ te, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:22.2":{"pli":"Sayaṅkataṃ udāhu devehi dinnaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:22.3":{"pli":"Pucchanti taṃ vāṇijā satthavāhā, ","html":"<span class='verse-line'>{}</span>"},"vv84:22.4":{"pli":"Kathaṃ tayā laddhamidaṃ manuññan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:23.1":{"pli":"“Nādhiccaladdhaṃ na pariṇāmajaṃ me, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:23.2":{"pli":"Na sayaṅkataṃ na hi devehi dinnaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:23.3":{"pli":"Sakehi kammehi apāpakehi, ","html":"<span class='verse-line'>{}</span>"},"vv84:23.4":{"pli":"Puññehi me laddhamidaṃ manuññan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:24.1":{"pli":"“Kiṃ te vataṃ kiṃ pana brahmacariyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:24.2":{"pli":"Kissa suciṇṇassa ayaṃ vipāko; ","html":"<span class='verse-line'>{}</span>"},"vv84:24.3":{"pli":"Pucchanti taṃ vāṇijā satthavāhā, ","html":"<span class='verse-line'>{}</span>"},"vv84:24.4":{"pli":"Kathaṃ tayā laddhamidaṃ vimānan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:25.1":{"pli":"“Mamaṃ pāyāsīti ahu samaññā, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:25.2":{"pli":"Rajjaṃ yadā kārayiṃ kosalānaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:25.3":{"pli":"Natthikadiṭṭhi kadariyo pāpadhammo, ","html":"<span class='verse-line'>{}</span>"},"vv84:25.4":{"pli":"Ucchedavādī ca tadā ahosiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:26.1":{"pli":"Samaṇo ca kho āsi kumārakassapo, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:26.2":{"pli":"Bahussuto cittakathī uḷāro; ","html":"<span class='verse-line'>{}</span>"},"vv84:26.3":{"pli":"So me tadā dhammakathaṃ abhāsi, ","html":"<span class='verse-line'>{}</span>"},"vv84:26.4":{"pli":"Diṭṭhivisūkāni vinodayī me. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:27.1":{"pli":"Tāhaṃ tassa dhammakathaṃ suṇitvā, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:27.2":{"pli":"Upāsakattaṃ paṭivedayissaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:27.3":{"pli":"Pāṇātipātā virato ahosiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:27.4":{"pli":"Loke adinnaṃ parivajjayissaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:27.5":{"pli":"Amajjapo no ca musā abhāṇiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:27.6":{"pli":"Sakena dārena ca ahosi tuṭṭho. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:28.1":{"pli":"Taṃ me vataṃ taṃ pana brahmacariyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:28.2":{"pli":"Tassa suciṇṇassa ayaṃ vipāko; ","html":"<span class='verse-line'>{}</span>"},"vv84:28.3":{"pli":"Teheva kammehi apāpakehi, ","html":"<span class='verse-line'>{}</span>"},"vv84:28.4":{"pli":"Puññehi me laddhamidaṃ vimānan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:29.1":{"pli":"“Saccaṃ kirāhaṃsu narā sapaññā, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:29.2":{"pli":"Anaññathā vacanaṃ paṇḍitānaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:29.3":{"pli":"Yahiṃ yahiṃ gacchati puññakammo, ","html":"<span class='verse-line'>{}</span>"},"vv84:29.4":{"pli":"Tahiṃ tahiṃ modati kāmakāmī. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:30.1":{"pli":"Yahiṃ yahiṃ sokapariddavo ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:30.2":{"pli":"Vadho ca bandho ca parikkileso; ","html":"<span class='verse-line'>{}</span>"},"vv84:30.3":{"pli":"Tahiṃ tahiṃ gacchati pāpakammo, ","html":"<span class='verse-line'>{}</span>"},"vv84:30.4":{"pli":"Na muccati duggatiyā kadācīti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:31.1":{"pli":"Sammūḷharūpova jano ahosi, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:31.2":{"pli":"Asmiṃ muhutte kalalīkatova; ","html":"<span class='verse-line'>{}</span>"},"vv84:31.3":{"pli":"Janassimassa tuyhañca kumāra, ","html":"<span class='verse-line'>{}</span>"},"vv84:31.4":{"pli":"Appaccayo kena nu kho ahosī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:32.1":{"pli":"“Ime ca sirīsavanā tātā, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:32.2":{"pli":"Dibbā gandhā surabhī sampavanti; ","html":"<span class='verse-line'>{}</span>"},"vv84:32.3":{"pli":"Te sampavāyanti imaṃ vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:32.4":{"pli":"Divā ca ratto ca tamaṃ nihantvā. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:33.1":{"pli":"Imesañca kho vassasataccayena, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:33.2":{"pli":"Sipāṭikā phalati ekamekā; ","html":"<span class='verse-line'>{}</span>"},"vv84:33.3":{"pli":"Mānussakaṃ vassasataṃ atītaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:33.4":{"pli":"Yadagge kāyamhi idhūpapanno. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:34.1":{"pli":"Disvānahaṃ vassasatāni pañca, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:34.2":{"pli":"Asmiṃ vimāne ṭhatvāna tātā; ","html":"<span class='verse-line'>{}</span>"},"vv84:34.3":{"pli":"Āyukkhayā puññakkhayā cavissaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:34.4":{"pli":"Teneva sokena pamucchitosmī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:35.1":{"pli":"“Kathaṃ nu soceyya tathāvidho so, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:35.2":{"pli":"Laddhā vimānaṃ atulaṃ cirāya; ","html":"<span class='verse-line'>{}</span>"},"vv84:35.3":{"pli":"Ye cāpi kho ittaramupapannā, ","html":"<span class='verse-line'>{}</span>"},"vv84:35.4":{"pli":"Te nūna soceyyuṃ parittapuññā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:36.1":{"pli":"“Anucchaviṃ ovadiyañca me taṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:36.2":{"pli":"Yaṃ maṃ tumhe peyyavācaṃ vadetha; ","html":"<span class='verse-line'>{}</span>"},"vv84:36.3":{"pli":"Tumhe ca kho tātā mayānuguttā, ","html":"<span class='verse-line'>{}</span>"},"vv84:36.4":{"pli":"Yenicchakaṃ tena paletha sotthin”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:37.1":{"pli":"“Gantvā mayaṃ sindhusovīrabhūmiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:37.2":{"pli":"Dhanatthikā uddayaṃ patthayānā; ","html":"<span class='verse-line'>{}</span>"},"vv84:37.3":{"pli":"Yathāpayogā paripuṇṇacāgā, ","html":"<span class='verse-line'>{}</span>"},"vv84:37.4":{"pli":"Kāhāma serīsamahaṃ uḷāran”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:38.1":{"pli":"“Mā ceva serīsamahaṃ akattha, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:38.2":{"pli":"Sabbañca vo bhavissati yaṃ vadetha; ","html":"<span class='verse-line'>{}</span>"},"vv84:38.3":{"pli":"Pāpāni kammāni vivajjayātha, ","html":"<span class='verse-line'>{}</span>"},"vv84:38.4":{"pli":"Dhammānuyogañca adhiṭṭhahātha. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:39.1":{"pli":"Upāsako atthi imamhi saṅghe, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:39.2":{"pli":"Bahussuto sīlavatūpapanno; ","html":"<span class='verse-line'>{}</span>"},"vv84:39.3":{"pli":"Saddho ca cāgī ca supesalo ca, ","html":"<span class='verse-line'>{}</span>"},"vv84:39.4":{"pli":"Vicakkhaṇo santusito mutīmā. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:40.1":{"pli":"Sañjānamāno na musā bhaṇeyya, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:40.2":{"pli":"Parūpaghātāya na cetayeyya; ","html":"<span class='verse-line'>{}</span>"},"vv84:40.3":{"pli":"Vebhūtikaṃ pesuṇaṃ no kareyya, ","html":"<span class='verse-line'>{}</span>"},"vv84:40.4":{"pli":"Saṇhañca vācaṃ sakhilaṃ bhaṇeyya. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:41.1":{"pli":"Sagāravo sappatisso vinīto, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:41.2":{"pli":"Apāpako adhisīle visuddho; ","html":"<span class='verse-line'>{}</span>"},"vv84:41.3":{"pli":"So mātaraṃ pitarañcāpi jantu, ","html":"<span class='verse-line'>{}</span>"},"vv84:41.4":{"pli":"Dhammena poseti ariyavutti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:42.1":{"pli":"Maññe so mātāpitūnaṃ kāraṇā, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:42.2":{"pli":"Bhogāni pariyesati na attahetu; ","html":"<span class='verse-line'>{}</span>"},"vv84:42.3":{"pli":"Mātāpitūnañca yo accayena, ","html":"<span class='verse-line'>{}</span>"},"vv84:42.4":{"pli":"Nekkhammapoṇo carissati brahmacariyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:43.1":{"pli":"Ujū avaṅko asaṭho amāyo, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:43.2":{"pli":"Na lesakappena ca vohareyya; ","html":"<span class='verse-line'>{}</span>"},"vv84:43.3":{"pli":"So tādiso sukatakammakārī, ","html":"<span class='verse-line'>{}</span>"},"vv84:43.4":{"pli":"Dhamme ṭhito kinti labhetha dukkhaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:44.1":{"pli":"Taṃ kāraṇā pātukatomhi attanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:44.2":{"pli":"Tasmā dhammaṃ passatha vāṇijāse; ","html":"<span class='verse-line'>{}</span>"},"vv84:44.3":{"pli":"Aññatra teniha bhasmī bhavetha, ","html":"<span class='verse-line'>{}</span>"},"vv84:44.4":{"pli":"Andhākulā vippanaṭṭhā araññe; ","html":"<span class='verse-line'>{}</span>"},"vv84:44.5":{"pli":"Taṃ khippamānena lahuṃ parena, ","html":"<span class='verse-line'>{}</span>"},"vv84:44.6":{"pli":"Sukho have sappurisena saṅgamo”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:45.1":{"pli":"“Kiṃ nāma so kiñca karoti kammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:45.2":{"pli":"Kiṃ nāmadheyyaṃ kiṃ pana tassa gottaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:45.3":{"pli":"Mayampi naṃ daṭṭhukāmamha yakkha, ","html":"<span class='verse-line'>{}</span>"},"vv84:45.4":{"pli":"Yassānukampāya idhāgatosi; ","html":"<span class='verse-line'>{}</span>"},"vv84:45.5":{"pli":"Lābhā hi tassa yassa tuvaṃ pihesī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:46.1":{"pli":"“Yo kappako sambhavanāmadheyyo, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:46.2":{"pli":"Upāsako kocchaphalūpajīvī; ","html":"<span class='verse-line'>{}</span>"},"vv84:46.3":{"pli":"Jānātha naṃ tumhākaṃ pesiyo so, ","html":"<span class='verse-line'>{}</span>"},"vv84:46.4":{"pli":"Mā kho naṃ hīḷittha supesalo so”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:47.1":{"pli":"“Jānāmase yaṃ tvaṃ pavadesi yakkha, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:47.2":{"pli":"Na kho naṃ jānāma sa edisoti; ","html":"<span class='verse-line'>{}</span>"},"vv84:47.3":{"pli":"Mayampi naṃ pūjayissāma yakkha, ","html":"<span class='verse-line'>{}</span>"},"vv84:47.4":{"pli":"Sutvāna tuyhaṃ vacanaṃ uḷāran”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:48.1":{"pli":"“Ye keci imasmiṃ satthe manussā, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:48.2":{"pli":"Daharā mahantā athavāpi majjhimā; ","html":"<span class='verse-line'>{}</span>"},"vv84:48.3":{"pli":"Sabbeva te ālambantu vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:48.4":{"pli":"Passantu puññānaṃ phalaṃ kadariyā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:49.1":{"pli":"Te tattha sabbeva “ahaṃ pure”ti, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:49.2":{"pli":"Taṃ kappakaṃ tattha purakkhatvā; ","html":"<span class='verse-line'>{}</span>"},"vv84:49.3":{"pli":"Sabbeva te ālambiṃsu vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:49.4":{"pli":"Masakkasāraṃ viya vāsavassa. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:50.1":{"pli":"Te tattha sabbeva “ahaṃ pure”ti, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:50.2":{"pli":"Upāsakattaṃ paṭivedayiṃsu; ","html":"<span class='verse-line'>{}</span>"},"vv84:50.3":{"pli":"Pāṇātipātā viratā ahesuṃ, ","html":"<span class='verse-line'>{}</span>"},"vv84:50.4":{"pli":"Loke adinnaṃ parivajjayiṃsu; ","html":"<span class='verse-line'>{}</span>"},"vv84:50.5":{"pli":"Amajjapā no ca musā bhaṇiṃsu, ","html":"<span class='verse-line'>{}</span>"},"vv84:50.6":{"pli":"Sakena dārena ca ahesuṃ tuṭṭhā. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:51.1":{"pli":"Te tattha sabbeva “ahaṃ pure”ti, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:51.2":{"pli":"Upāsakattaṃ paṭivedayitvā; ","html":"<span class='verse-line'>{}</span>"},"vv84:51.3":{"pli":"Pakkāmi sattho anumodamāno, ","html":"<span class='verse-line'>{}</span>"},"vv84:51.4":{"pli":"Yakkhiddhiyā anumato punappunaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:52.1":{"pli":"Gantvāna te sindhusovīrabhūmiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:52.2":{"pli":"Dhanatthikā uddayaṃ patthayānā; ","html":"<span class='verse-line'>{}</span>"},"vv84:52.3":{"pli":"Yathāpayogā paripuṇṇalābhā, ","html":"<span class='verse-line'>{}</span>"},"vv84:52.4":{"pli":"Paccāgamuṃ pāṭaliputtamakkhataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:53.1":{"pli":"Gantvāna te saṅgharaṃ sotthivanto, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:53.2":{"pli":"Puttehi dārehi samaṅgibhūtā; ","html":"<span class='verse-line'>{}</span>"},"vv84:53.3":{"pli":"Ānandī vittā sumanā patītā, ","html":"<span class='verse-line'>{}</span>"},"vv84:53.4":{"pli":"Akaṃsu serīsamahaṃ uḷāraṃ; ","html":"<span class='verse-line'>{}</span>"},"vv84:53.5":{"pli":"Serīsakaṃ te pariveṇaṃ māpayiṃsu. ","html":"<span class='verse-line'>{}</span></p>"},"vv84:54.1":{"pli":"Etādisā sappurisāna sevanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv84:54.2":{"pli":"Mahatthikā dhammaguṇāna sevanā; ","html":"<span class='verse-line'>{}</span>"},"vv84:54.3":{"pli":"Ekassa atthāya upāsakassa, ","html":"<span class='verse-line'>{}</span>"},"vv84:54.4":{"pli":"Sabbeva sattā sukhitā ahesunti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv84:55.1":{"pli":"Serīsakavimānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv85":{"vv85:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv85'><header><ul><li class='division'>{}</li>"},"vv85:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv85:0.3":{"pli":"Sunikkhittavagga ","html":"<li>{}</li></ul>"},"vv85:0.4":{"pli":"11. Sunikkhittavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv85:1.1":{"pli":"“Uccamidaṃ maṇithūṇaṃ vimānaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv85:1.2":{"pli":"Samantato dvādasa yojanāni; ","html":"<span class='verse-line'>{}</span>"},"vv85:1.3":{"pli":"Kūṭāgārā sattasatā uḷārā, ","html":"<span class='verse-line'>{}</span>"},"vv85:1.4":{"pli":"Veḷuriyathambhā rucakatthatā subhā. ","html":"<span class='verse-line'>{}</span></p>"},"vv85:2.1":{"pli":"Tatthacchasi pivasi khādasi ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv85:2.2":{"pli":"Dibbā ca vīṇā pavadanti vagguṃ; ","html":"<span class='verse-line'>{}</span>"},"vv85:2.3":{"pli":"Dibbā rasā kāmaguṇettha pañca, ","html":"<span class='verse-line'>{}</span>"},"vv85:2.4":{"pli":"Nāriyo ca naccanti suvaṇṇachannā. ","html":"<span class='verse-line'>{}</span></p>"},"vv85:3.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv85:3.2":{"pli":"kena te idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv85:3.3":{"pli":"Uppajjanti ca te bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv85:3.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv85:4.1":{"pli":"Pucchāmi taṃ deva mahānubhāva, ","html":"<p><span class='verse-line'>{}</span>"},"vv85:4.2":{"pli":"Manussabhūto kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv85:4.3":{"pli":"Kenāsi evaṃ jalitānubhāvo, ","html":"<span class='verse-line'>{}</span>"},"vv85:4.4":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv85:5.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv85:5.2":{"pli":"moggallānena pucchito; ","html":"<span class='verse-line'>{}</span>"},"vv85:5.3":{"pli":"Pañhaṃ puṭṭho viyākāsi, ","html":"<span class='verse-line'>{}</span>"},"vv85:5.4":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv85:6.1":{"pli":"“Dunnikkhittaṃ mālaṃ sunikkhipitvā, ","html":"<p><span class='verse-line'>{}</span>"},"vv85:6.2":{"pli":"Patiṭṭhapetvā sugatassa thūpe; ","html":"<span class='verse-line'>{}</span>"},"vv85:6.3":{"pli":"Mahiddhiko camhi mahānubhāvo, ","html":"<span class='verse-line'>{}</span>"},"vv85:6.4":{"pli":"Dibbehi kāmehi samaṅgibhūto. ","html":"<span class='verse-line'>{}</span></p>"},"vv85:7.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv85:7.2":{"pli":"tena me idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv85:7.3":{"pli":"Uppajjanti ca me bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv85:7.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv85:8.1":{"pli":"Akkhāmi te bhikkhu mahānubhāva, ","html":"<p><span class='verse-line'>{}</span>"},"vv85:8.2":{"pli":"Manussabhūto yamahaṃ akāsiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv85:8.3":{"pli":"Tenamhi evaṃ jalitānubhāvo, ","html":"<span class='verse-line'>{}</span>"},"vv85:8.4":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv85:9.1":{"pli":"Sunikkhittavimānaṃ ekādasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"vv85:9.2":{"pli":"Sunikkhittavaggo sattamo. ","html":"<p class='endvagga'>{}</p>"},"vv85:10.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"vv85:11.1":{"pli":"Dve daliddā vanavihārā, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"vv85:11.2":{"pli":"Bhatako gopālakaṇḍako; ","html":"<span class='verse-line'>{}</span>"},"vv85:11.3":{"pli":"Anekavaṇṇamaṭṭhakuṇḍalī, ","html":"<span class='verse-line'>{}</span>"},"vv85:11.4":{"pli":"Serīsako sunikkhittaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv85:11.5":{"pli":"Purisānaṃ tatiyo vaggo pavuccatīti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv85:12.1":{"pli":"Bhāṇavāraṃ catutthaṃ. ","html":"<p class='endsutta'>{}</p>"},"vv85:13.1":{"pli":"Vimānavatthupāḷi niṭṭhitā. ","html":"<p class='endbook'>{}</p></article>"}},"vv9":{"vv9:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv9'><header><ul><li class='division'>{}</li>"},"vv9:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv9:0.3":{"pli":"Pīṭhavagga ","html":"<li>{}</li></ul>"},"vv9:0.4":{"pli":"9. Dīpavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv9:1.1":{"pli":"“Abhikkantena vaṇṇena, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv9:1.2":{"pli":"yā tvaṃ tiṭṭhasi devate; ","html":"<span class='verse-line'>{}</span>"},"vv9:1.3":{"pli":"Obhāsentī disā sabbā, ","html":"<span class='verse-line'>{}</span>"},"vv9:1.4":{"pli":"osadhī viya tārakā. ","html":"<span class='verse-line'>{}</span></p>"},"vv9:2.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv9:2.2":{"pli":"kena te idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv9:2.3":{"pli":"Uppajjanti ca te bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv9:2.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv9:3.1":{"pli":"Kena tvaṃ vimalobhāsā, ","html":"<p><span class='verse-line'>{}</span>"},"vv9:3.2":{"pli":"atirocasi devatā; ","html":"<span class='verse-line'>{}</span>"},"vv9:3.3":{"pli":"Kena te sabbagattehi, ","html":"<span class='verse-line'>{}</span>"},"vv9:3.4":{"pli":"sabbā obhāsate disā. ","html":"<span class='verse-line'>{}</span></p>"},"vv9:4.1":{"pli":"Pucchāmi taṃ devi mahānubhāve, ","html":"<p><span class='verse-line'>{}</span>"},"vv9:4.2":{"pli":"Manussabhūtā kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv9:4.3":{"pli":"Kenāsi evaṃ jalitānubhāvā, ","html":"<span class='verse-line'>{}</span>"},"vv9:4.4":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv9:5.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv9:5.2":{"pli":"moggallānena pucchitā; ","html":"<span class='verse-line'>{}</span>"},"vv9:5.3":{"pli":"Pañhaṃ puṭṭhā viyākāsi, ","html":"<span class='verse-line'>{}</span>"},"vv9:5.4":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv9:6.1":{"pli":"“Ahaṃ manussesu manussabhūtā, ","html":"<p><span class='verse-line'>{}</span>"},"vv9:6.2":{"pli":"Purimāya jātiyā manussaloke; ","html":"<span class='verse-line'>{}</span>"},"vv9:6.3":{"pli":"Tamandhakāramhi timīsikāyaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv9:6.4":{"pli":"Padīpakālamhi adāsi dīpaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv9:7.1":{"pli":"Yo andhakāramhi timīsikāyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv9:7.2":{"pli":"Padīpakālamhi dadāti dīpaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv9:7.3":{"pli":"Upapajjati jotirasaṃ vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv9:7.4":{"pli":"Pahūtamalyaṃ bahupuṇḍarīkaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv9:8.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv9:8.2":{"pli":"tena me idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv9:8.3":{"pli":"Uppajjanti ca me bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv9:8.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv9:9.1":{"pli":"Tenāhaṃ vimalobhāsā, ","html":"<p><span class='verse-line'>{}</span>"},"vv9:9.2":{"pli":"atirocāmi devatā; ","html":"<span class='verse-line'>{}</span>"},"vv9:9.3":{"pli":"Tena me sabbagattehi, ","html":"<span class='verse-line'>{}</span>"},"vv9:9.4":{"pli":"sabbā obhāsate disā. ","html":"<span class='verse-line'>{}</span></p>"},"vv9:10.1":{"pli":"Akkhāmi te bhikkhu mahānubhāva, ","html":"<p><span class='verse-line'>{}</span>"},"vv9:10.2":{"pli":"Manussabhūtā yamakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv9:10.3":{"pli":"Tenamhi evaṃ jalitānubhāvā, ","html":"<span class='verse-line'>{}</span>"},"vv9:10.4":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv9:11.1":{"pli":"Dīpavimānaṃ navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}