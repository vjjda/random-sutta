{"tha-ap552":{"tha-ap552:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap552'><header><ul><li class='division'>{}</li>"},"tha-ap552:0.2":{"pli":"Bhaddiyavagga ","html":"<li>{}</li></ul>"},"tha-ap552:0.3":{"pli":"Cūḷasugandhattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap552:1.1":{"pli":"“Imamhi bhaddake kappe, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap552:1.2":{"pli":"brahmabandhu mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:1.3":{"pli":"Kassapo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:1.4":{"pli":"uppajji vadataṃ varo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:2.1":{"pli":"Anubyañjanasampanno, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:2.2":{"pli":"bāttiṃsavaralakkhaṇo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:2.3":{"pli":"Byāmappabhāparivuto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:2.4":{"pli":"raṃsijālasamotthaṭo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:3.1":{"pli":"Assāsetā yathā cando, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:3.2":{"pli":"sūriyova pabhaṅkaro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:3.3":{"pli":"Nibbāpetā yathā megho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:3.4":{"pli":"sāgarova guṇākaro. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:4.1":{"pli":"Dharaṇīriva sīlena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:4.2":{"pli":"himavāva samādhinā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:4.3":{"pli":"Ākāso viya paññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:4.4":{"pli":"asaṅgo anilo yathā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:5.1":{"pli":"Tadāhaṃ bārāṇasiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:5.2":{"pli":"upapanno mahākule; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:5.3":{"pli":"Pahūtadhanadhaññasmiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:5.4":{"pli":"nānāratanasañcaye. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:6.1":{"pli":"Mahatā parivārena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:6.2":{"pli":"nisinnaṃ lokanāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:6.3":{"pli":"Upecca dhammamassosiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:6.4":{"pli":"amataṃva manoharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:7.1":{"pli":"Dvattiṃsalakkhaṇadharo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:7.2":{"pli":"sanakkhattova candimā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:7.3":{"pli":"Anubyañjanasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:7.4":{"pli":"sālarājāva phullito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:8.1":{"pli":"Raṃsijālaparikkhitto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:8.2":{"pli":"dittova kanakācalo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:8.3":{"pli":"Byāmappabhāparivuto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:8.4":{"pli":"sataraṃsī divākaro. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:9.1":{"pli":"Soṇṇānano jinavaro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:9.2":{"pli":"samaṇīva siluccayo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:9.3":{"pli":"Karuṇā puṇṇahadayo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:9.4":{"pli":"guṇena viya sāgaro. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:10.1":{"pli":"Lokavissutakitti ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:10.2":{"pli":"sinerūva naguttamo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:10.3":{"pli":"Yasasā vitthato vīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:10.4":{"pli":"ākāsasadiso muni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:11.1":{"pli":"Asaṅgacitto sabbattha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:11.2":{"pli":"anilo viya nāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:11.3":{"pli":"Patiṭṭhā sabbabhūtānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:11.4":{"pli":"mahīva munisattamo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:12.1":{"pli":"Anupalitto lokena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:12.2":{"pli":"toyena padumaṃ yathā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:12.3":{"pli":"Kuvādagacchadahano, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:12.4":{"pli":"aggikhandhova sobhasi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:13.1":{"pli":"Agadho viya sabbattha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:13.2":{"pli":"kilesavisanāsako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:13.3":{"pli":"Gandhamādanaselova, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:13.4":{"pli":"guṇagandhavibhūsito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:14.1":{"pli":"Guṇānaṃ ākaro vīro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:14.2":{"pli":"ratanānaṃva sāgaro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:14.3":{"pli":"Sindhūva vanarājīnaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:14.4":{"pli":"kilesamalahārako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:15.1":{"pli":"Vijayīva mahāyodho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap552:15.2":{"pli":"mārasenāvamaddano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:15.3":{"pli":"Cakkavattīva so rājā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap552:15.4":{"pli":"bojjhaṅgaratanissaro. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap552:16.1":{"pli":"Mahābhis