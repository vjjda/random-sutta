{"tha-ap406":{"tha-ap406:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap406'><header><ul><li class='division'>{}</li>"},"tha-ap406:0.2":{"pli":"Metteyyavagga ","html":"<li>{}</li></ul>"},"tha-ap406:0.3":{"pli":"Dhotakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap406:1.1":{"pli":"“Gaṅgā bhāgīrathī nāma, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap406:1.2":{"pli":"himavantā pabhāvitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:1.3":{"pli":"Haṃsavatiyā dvārena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:1.4":{"pli":"anusandati tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:2.1":{"pli":"Sobhito nāma ārāmo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:2.2":{"pli":"gaṅgākūle sumāpito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:2.3":{"pli":"Tattha padumuttaro buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:2.4":{"pli":"vasate lokanāyako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:3.1":{"pli":"Tidasehi yathā indo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:3.2":{"pli":"manujehi purakkhato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:3.3":{"pli":"Nisīdi tattha bhagavā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:3.4":{"pli":"asambhītova kesarī. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:4.1":{"pli":"Nagare haṃsavatiyā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:4.2":{"pli":"vasāmi brāhmaṇo ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:4.3":{"pli":"Chaḷaṅgo nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:4.4":{"pli":"evaṃnāmo mahāmuni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:5.1":{"pli":"Aṭṭhārasa sissasatā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:5.2":{"pli":"parivārenti maṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:5.3":{"pli":"Tehi sissehi samito, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:5.4":{"pli":"gaṅgātīraṃ upāgamiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:6.1":{"pli":"Tatthaddasāsiṃ samaṇe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:6.2":{"pli":"nikkuhe dhotapāpake; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:6.3":{"pli":"Bhāgīrathiṃ tarantehaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:6.4":{"pli":"evaṃ cintesi tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:7.1":{"pli":"‘Sāyaṃ pātaṃ tarantāme, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:7.2":{"pli":"buddhaputtā mahāyasā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:7.3":{"pli":"Vihesayanti attānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:7.4":{"pli":"tesaṃ attā vihaññati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:8.1":{"pli":"Sadevakassa lokassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:8.2":{"pli":"buddho aggo pavuccati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:8.3":{"pli":"Natthi me dakkhiṇe kāraṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:8.4":{"pli":"gatimaggavisodhanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:9.1":{"pli":"Yannūna buddhaseṭṭhassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:9.2":{"pli":"setuṃ gaṅgāya kāraye; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:9.3":{"pli":"Kārāpetvā imaṃ kammaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:9.4":{"pli":"santarāmi imaṃ bhavaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:10.1":{"pli":"Satasahassaṃ datvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:10.2":{"pli":"setuṃ kārāpayiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:10.3":{"pli":"Saddahanto kataṃ kāraṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:10.4":{"pli":"vipulaṃ me bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:11.1":{"pli":"Kārāpetvāna taṃ setuṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:11.2":{"pli":"upesiṃ lokanāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:11.3":{"pli":"Sirasi añjaliṃ katvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:11.4":{"pli":"imaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:12.1":{"pli":"‘Satasahassassa vayaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:12.2":{"pli":"datvā kārāpito mayā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:12.3":{"pli":"Tavatthāya mahāsetu, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:12.4":{"pli":"paṭiggaṇha mahāmune’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:13.1":{"pli":"Padumuttaro lokavidū, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:13.2":{"pli":"āhutīnaṃ paṭiggaho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:13.3":{"pli":"Bhikkhusaṅghe nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:13.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:14.1":{"pli":"‘Yo me setuṃ akāresi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:14.2":{"pli":"pasanno sehi pāṇibhi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:14.3":{"pli":"Tamahaṃ kittayissāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:14.4":{"pli":"suṇātha mama bhāsato. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap406:15.0":{"pli":"4.1. Setudānaānisaṃsa ","html":"<h3>{}</h3>"},"tha-ap406:15.1":{"pli":"Darito pabbatato vā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap406:15.2":{"pli":"rukkhato patitopiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:15.3":{"pli":"Cutopi lacchatī ṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:15.4":{"pli":"setudānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:16.1":{"pli":"Virūḷhamūlasantānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:16.2":{"pli":"nigrodhamiva māluto; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:16.3":{"pli":"Amittā nappasahanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:16.4":{"pli":"setudānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:17.1":{"pli":"Nāssa corā pasahanti, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:17.2":{"pli":"nātimaññanti khattiyā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:17.3":{"pli":"Sabbe tarissatāmitte, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:17.4":{"pli":"setudānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:18.1":{"pli":"Abbhokāsagataṃ santaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:18.2":{"pli":"kaṭhinātapatāpitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:18.3":{"pli":"Puññakammena saṃyuttaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:18.4":{"pli":"na bhavissati vedanā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:19.1":{"pli":"Devaloke manusse vā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:19.2":{"pli":"hatthiyānaṃ sunimmitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:19.3":{"pli":"Tassa saṅkappamaññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:19.4":{"pli":"nibbattissati tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:20.1":{"pli":"Sahassassā vātajavā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:20.2":{"pli":"sindhavā sīghavāhanā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:20.3":{"pli":"Sāyaṃ pātaṃ upessanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:20.4":{"pli":"setudānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:21.1":{"pli":"Āgantvāna manussattaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:21.2":{"pli":"sukhitoyaṃ bhavissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:21.3":{"pli":"Vehāsaṃ manujasseva, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:21.4":{"pli":"hatthiyānaṃ bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:22.1":{"pli":"Kappasatasahassamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:22.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:22.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:22.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:23.1":{"pli":"Tassa dhammesu dāyādo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:23.2":{"pli":"oraso dhammanimmito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:23.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:23.4":{"pli":"nibbāyissatināsavo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:24.1":{"pli":"Aho me sukataṃ kammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:24.2":{"pli":"jalajuttamanāmake; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:24.3":{"pli":"Tattha kāraṃ karitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:24.4":{"pli":"pattohaṃ āsavakkhayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:25.1":{"pli":"Padhānaṃ pahitattomhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:25.2":{"pli":"upasanto nirūpadhi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:25.3":{"pli":"Nāgova bandhanaṃ chetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:25.4":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:26.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:26.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:26.3":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:27.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:27.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:27.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap406:28.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap406:28.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap406:28.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap406:29.1":{"pli":"Itthaṃ sudaṃ āyasmā dhotako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap406:30.1":{"pli":"Dhotakattherassāpadānaṃ catutthaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap407":{"tha-ap407:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap407'><header><ul><li class='division'>{}</li>"},"tha-ap407:0.2":{"pli":"Metteyyavagga ","html":"<li>{}</li></ul>"},"tha-ap407:0.3":{"pli":"5 Upasivattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap407:1.1":{"pli":"“Himavantassāvidūre, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap407:1.2":{"pli":"anomo nāma pabbato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:1.3":{"pli":"Assamo sukato mayhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:1.4":{"pli":"paṇṇasālā sumāpitā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:2.1":{"pli":"Nadī ca sandatī tattha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:2.2":{"pli":"supatitthā manoramā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:2.3":{"pli":"Anūpatitthe jāyanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:2.4":{"pli":"padumuppalakā bahū. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:3.1":{"pli":"Pāṭhīnā pāvusā macchā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:3.2":{"pli":"balajā muñjarohitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:3.3":{"pli":"Macchakacchapasañchannā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:3.4":{"pli":"nadikā sandate tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:4.1":{"pli":"Timirā pupphitā tattha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:4.2":{"pli":"asokā khuddamālakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:4.3":{"pli":"Punnāgā giripunnāgā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:4.4":{"pli":"sampavanti mamassamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:5.1":{"pli":"Kuṭajā pupphitā tattha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:5.2":{"pli":"tiṇasūlavanāni ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:5.3":{"pli":"Sālā ca saḷalā tattha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:5.4":{"pli":"campakā pupphitā bahū. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:6.1":{"pli":"Ajjunā atimuttā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:6.2":{"pli":"mahānāmā ca pupphitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:6.3":{"pli":"Asanā madhugandhī ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:6.4":{"pli":"pupphitā te mamassame. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:7.1":{"pli":"Uddālakā pāṭalikā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:7.2":{"pli":"yūthikā ca piyaṅgukā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:7.3":{"pli":"Bimbijālakasañchannā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:7.4":{"pli":"samantā aḍḍhayojanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:8.1":{"pli":"Mātaggārā sattaliyo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:8.2":{"pli":"pāṭalī sinduvārakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:8.3":{"pli":"Aṅkolakā bahū tattha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:8.4":{"pli":"tālakuṭṭhi ca pupphitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:8.5":{"pli":"Seleyyakā bahū tattha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:8.6":{"pli":"pupphitā mama assame. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:9.1":{"pli":"Etesu pupphajātesu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:9.2":{"pli":"sobhanti pādapā bahū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:9.3":{"pli":"Samantā tena gandhena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:9.4":{"pli":"vāyate mama assamo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:10.1":{"pli":"Harītakā āmalakā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:10.2":{"pli":"ambajambuvibhītakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:10.3":{"pli":"Kolā bhallātakā billā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:10.4":{"pli":"phārusakaphalāni ca. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:11.1":{"pli":"Tindukā ca piyālā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:11.2":{"pli":"madhukā kāsumārayo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:11.3":{"pli":"Labujā panasā tattha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:11.4":{"pli":"kadalī badarīphalā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:12.1":{"pli":"Ambāṭakā bahū tattha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:12.2":{"pli":"vallikāraphalāni ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:12.3":{"pli":"Bījapūrasapāriyo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:12.4":{"pli":"phalitā mama assame. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:13.1":{"pli":"Āḷakā isimuggā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:13.2":{"pli":"tato modaphalā bahū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:13.3":{"pli":"Avaṭā pakkabharitā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:13.4":{"pli":"pilakkhudumbarāni ca. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:14.1":{"pli":"Pipphalī marīcā tattha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:14.2":{"pli":"nigrodhā ca kapitthanā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:14.3":{"pli":"Udumbarakā bahavo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:14.4":{"pli":"kaṇḍupaṇṇā ca hariyo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:15.1":{"pli":"Ete caññe ca bahavo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:15.2":{"pli":"phalitā assame mama; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:15.3":{"pli":"Puppharukkhāpi bahavo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:15.4":{"pli":"pupphitā mama assame. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:16.1":{"pli":"Āluvā ca kaḷambā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:16.2":{"pli":"biḷālī takkalāni ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:16.3":{"pli":"Ālakā tālakā ceva, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:16.4":{"pli":"vijjanti assame mama. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:17.1":{"pli":"Assamassāvidūre me, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap407:17.2":{"pli":"mahājātassaro ahu; ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:17.3":{"pli":"Acchodako sītajalo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap407:17.4":{"pli":"supatittho manoramo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap407:18.1":{"pli":"Padumuppalā bahū tattha, "