{"tha-ap411":{"tha-ap411:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap411'><header><ul><li class='division'>{}</li>"},"tha-ap411:0.2":{"pli":"Metteyyavagga ","html":"<li>{}</li></ul>"},"tha-ap411:0.3":{"pli":"Jatukaṇṇittheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap411:1.1":{"pli":"“Nagare haṃsavatiyā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap411:1.2":{"pli":"seṭṭhiputto ahosahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:1.3":{"pli":"Samappito kāmaguṇe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:1.4":{"pli":"paricāremahaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:2.1":{"pli":"Tato pāsādamāruyha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:2.2":{"pli":"mahābhoge valañjako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:2.3":{"pli":"Tattha naccehi gītehi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:2.4":{"pli":"paricāremahaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:3.1":{"pli":"Tūriyā āhatā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:3.2":{"pli":"sammatāḷasamāhitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:3.3":{"pli":"Naccantā itthiyo sabbā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:3.4":{"pli":"harantiyeva me mano. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:4.1":{"pli":"Celāpikā vāmanikā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:4.2":{"pli":"kuñjavāsī timajjhikā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:4.3":{"pli":"Laṅghikā sokajjhāyī ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:4.4":{"pli":"parivārenti maṃ sadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:5.1":{"pli":"Vetāḷino kumbhathūnī, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:5.2":{"pli":"naṭā ca naccakā bahū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:5.3":{"pli":"Naṭakā nāṭakā ceva, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:5.4":{"pli":"parivārenti maṃ sadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:6.1":{"pli":"Kappakā nhāpakā sūdā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:6.2":{"pli":"mālākārā supāsakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:6.3":{"pli":"Jallā mallā ca te sabbe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:6.4":{"pli":"parivārenti maṃ sadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:7.1":{"pli":"Etesu kīḷamānesu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:7.2":{"pli":"sikkhite katupāsane; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:7.3":{"pli":"Rattindivaṃ na jānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:7.4":{"pli":"indova tidasaṅgaṇe. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:8.1":{"pli":"Addhikā pathikā sabbe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:8.2":{"pli":"yācakā varakā bahū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:8.3":{"pli":"Upagacchanti te niccaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:8.4":{"pli":"bhikkhayantā mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:9.1":{"pli":"Samaṇā brāhmaṇā ceva, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:9.2":{"pli":"puññakkhettā anuttarā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:9.3":{"pli":"Vaḍḍhayantā mamaṃ puññaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:9.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:10.1":{"pli":"Paṭagā laṭukā sabbe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:10.2":{"pli":"nigaṇṭhā pupphasāṭakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:10.3":{"pli":"Tedaṇḍikā ekasikhā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:10.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:11.1":{"pli":"Ājīvakā viluttāvī, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:11.2":{"pli":"godhammā devadhammikā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:11.3":{"pli":"Rajojalladharā ete, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:11.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:12.1":{"pli":"Parittakā santipattā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:12.2":{"pli":"kodhapugganikā bahū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:12.3":{"pli":"Tapassī vanacārī ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:12.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:13.1":{"pli":"Oḍḍakā damiḷā ceva, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:13.2":{"pli":"sākuḷā malavāḷakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:13.3":{"pli":"Savarā yonakā ceva, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:13.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:14.1":{"pli":"Andhakā muṇḍakā sabbe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:14.2":{"pli":"koṭalā hanuvindakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:14.3":{"pli":"Ārāvacīnaraṭṭhā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:14.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:15.1":{"pli":"Alasandakā pallavakā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:15.2":{"pli":"dhammarā niggamānusā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:15.3":{"pli":"Gehikā cetaputtā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:15.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:16.1":{"pli":"Mādhurakā kosalakā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:16.2":{"pli":"kaliṅgā hatthiporikā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:16.3":{"pli":"Isiṇḍā makkalā ceva, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:16.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:17.1":{"pli":"Celāvakā ārabbhā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:17.2":{"pli":"oghuḷhā meghalā bahū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:17.3":{"pli":"Khuddakā suddakā ceva, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:17.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:18.1":{"pli":"Rohaṇā sindhavā ceva, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:18.2":{"pli":"citakā ekakaṇṇikā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:18.3":{"pli":"Suraṭṭhā aparantā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:18.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:19.1":{"pli":"Suppārakā kumārā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:19.2":{"pli":"mallasovaṇṇabhūmikā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:19.3":{"pli":"Vajjītaṅgā ca te sabbe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:19.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:20.1":{"pli":"Naḷakārā pesakārā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:20.2":{"pli":"cammakārā ca tacchakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:20.3":{"pli":"Kammārā kumbhakārā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:20.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:21.1":{"pli":"Maṇikārā lohakārā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:21.2":{"pli":"soṇṇakārā ca dussikā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:21.3":{"pli":"Tipukārā ca te sabbe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:21.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:22.1":{"pli":"Usukārā bhamakārā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:22.2":{"pli":"pesakārā ca gandhikā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:22.3":{"pli":"Rajakā tunnavāyā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:22.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:23.1":{"pli":"Telikā kaṭṭhahārā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:23.2":{"pli":"udahārā ca pessikā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:23.3":{"pli":"Sūpikā sūparakkhā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:23.4":{"pli":"āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:24.1":{"pli":"Dovārikā anīkaṭṭhā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:24.2":{"pli":"Bandhikā pupphachaḍḍakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:24.3":{"pli":"Hatthāruhā hatthipālā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:24.4":{"pli":"Āgacchanti mamaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:25.1":{"pli":"Ānandassa mahārañño, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:25.2":{"pli":"mamatthassa adāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:25.3":{"pli":"Sattavaṇṇena ratanena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:25.4":{"pli":"ūnatthaṃ pūrayāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:26.1":{"pli":"Ye mayā kittitā sabbe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:26.2":{"pli":"nānāvaṇṇā bahū janā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:26.3":{"pli":"Tesāhaṃ cittamaññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:26.4":{"pli":"tappayiṃ ratanenahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:27.1":{"pli":"Vaggūsu bhāsamānāsu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:27.2":{"pli":"vajjamānāsu bherisu; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:27.3":{"pli":"Saṅkhesu dhamayantesu, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:27.4":{"pli":"sakagehe ramāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:28.1":{"pli":"Bhagavā tamhi samaye, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:28.2":{"pli":"padumuttaranāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:28.3":{"pli":"Vasīsatasahassehi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:28.4":{"pli":"parikkhīṇāsavehi so. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:29.1":{"pli":"Bhikkhūhi sahito vīthiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:29.2":{"pli":"paṭipajjittha cakkhumā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:29.3":{"pli":"Obhāsento disā sabbā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:29.4":{"pli":"dīparukkhova jotati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:30.1":{"pli":"Vajjanti bheriyo sabbā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:30.2":{"pli":"gacchante lokanāyake; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:30.3":{"pli":"Pabhā niddhāvate tassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:30.4":{"pli":"sataraṃsīva uggato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:31.1":{"pli":"Kavāṭantarikāyāpi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:31.2":{"pli":"paviṭṭhena ca rasminā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:31.3":{"pli":"Antogharesu vipulo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:31.4":{"pli":"āloko āsi tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:32.1":{"pli":"Pabhaṃ disvāna buddhassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:32.2":{"pli":"pārisajje avocahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:32.3":{"pli":"‘Nissaṃsayaṃ buddhaseṭṭho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:32.4":{"pli":"imaṃ vīthimupāgato’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:33.1":{"pli":"Khippaṃ oruyha pāsādā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:33.2":{"pli":"agamiṃ antarāpaṇaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:33.3":{"pli":"Sambuddhaṃ abhivādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:33.4":{"pli":"idaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:34.1":{"pli":"‘Anukampatu me buddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:34.2":{"pli":"jalajuttamanāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:34.3":{"pli":"Vasīsatasahassehi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:34.4":{"pli":"adhivāsesi so muni’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:35.1":{"pli":"Nimantetvāna sambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:35.2":{"pli":"abhinesiṃ sakaṃ gharaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:35.3":{"pli":"Tattha annena pānena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:35.4":{"pli":"santappesiṃ mahāmuniṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:36.1":{"pli":"Bhuttāviṃ kālamaññāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:36.2":{"pli":"buddhaseṭṭhassa tādino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:36.3":{"pli":"Sataṅgikena tūriyena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:36.4":{"pli":"buddhaseṭṭhaṃ upaṭṭhahiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:37.1":{"pli":"Padumuttaro lokavidū, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:37.2":{"pli":"āhutīnaṃ paṭiggaho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:37.3":{"pli":"Antoghare nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:37.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:38.1":{"pli":"‘Yo maṃ tūriyehupaṭṭhāsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:38.2":{"pli":"annapānañcadāsi me; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:38.3":{"pli":"Tamahaṃ kittayissāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:38.4":{"pli":"suṇātha mama bhāsato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:39.1":{"pli":"Pahūtabhakkho hutvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:39.2":{"pli":"sahirañño sabhojano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:39.3":{"pli":"Catudīpe ekarajjaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:39.4":{"pli":"kārayissatiyaṃ naro. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:40.1":{"pli":"Pañca sīle samādāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:40.2":{"pli":"dasakammapathe tato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:40.3":{"pli":"Samādāya pavattento, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:40.4":{"pli":"parisaṃ sikkhāpayissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:41.1":{"pli":"Tūriyasatasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:41.2":{"pli":"bheriyo samalaṅkatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:41.3":{"pli":"Vajjayissantimaṃ niccaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:41.4":{"pli":"upaṭṭhānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:42.1":{"pli":"Tiṃsakappasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:42.2":{"pli":"devaloke ramissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:42.3":{"pli":"Catusaṭṭhikkhattuṃ devindo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:42.4":{"pli":"devarajjaṃ karissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:43.1":{"pli":"Catusaṭṭhikkhattuṃ rājā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:43.2":{"pli":"cakkavattī bhavissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:43.3":{"pli":"Padesarajjaṃ vipulaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:43.4":{"pli":"gaṇanāto asaṅkhiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:44.1":{"pli":"Kappasatasahassamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:44.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:44.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:44.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:45.1":{"pli":"Upapajjati yaṃ yoniṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:45.2":{"pli":"devattaṃ atha mānusaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:45.3":{"pli":"Anūnabhogo hutvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:45.4":{"pli":"manussattaṃ gamissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:46.1":{"pli":"Ajjhāyako bhavitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap411:46.2":{"pli":"tiṇṇaṃ vedāna pāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:46.3":{"pli":"Uttamatthaṃ gavesanto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap411:46.4":{"pli":"carissati mahiṃ imaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap411:47.1":{"pli":"So pacchā pabbajitvāna, ","html":"<p><span class='verse-line'>{}