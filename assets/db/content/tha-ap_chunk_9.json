{"tha-ap2":{"tha-ap2:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap2'><header><ul><li class='division'>{}</li>"},"tha-ap2:0.2":{"pli":"Buddhavagga ","html":"<li>{}</li></ul>"},"tha-ap2:0.3":{"pli":"Paccekabuddhaapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap2:1.1":{"pli":"Atha paccekabuddhāpadānaṃ suṇātha—","html":"<p>{}</p>"},"tha-ap2:2.1":{"pli":"“Tathāgataṃ jetavane vasantaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap2:2.2":{"pli":"Apucchi vedehamunī nataṅgo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:2.3":{"pli":"‘Paccekabuddhā kira nāma honti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:2.4":{"pli":"Bhavanti te hetubhi kehi vīra’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:3.1":{"pli":"Tadāha sabbaññuvaro mahesi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:3.2":{"pli":"Ānandabhaddaṃ madhurassarena; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:3.3":{"pli":"‘Ye pubbabuddhesu katādhikārā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:3.4":{"pli":"Aladdhamokkhā jinasāsanesu. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:4.1":{"pli":"Teneva saṃvegamukhena dhīrā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:4.2":{"pli":"Vināpi buddhehi sutikkhapaññā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:4.3":{"pli":"Ārammaṇenāpi parittakena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:4.4":{"pli":"Paccekabodhiṃ anupāpuṇanti. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:5.1":{"pli":"Sabbamhi lokamhi mamaṃ ṭhapetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:5.2":{"pli":"Paccekabuddhehi samova natthi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:5.3":{"pli":"Tesaṃ imaṃ vaṇṇapadesamattaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:5.4":{"pli":"Vakkhāmahaṃ sādhu mahāmunīnaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:6.1":{"pli":"Sayameva buddhānaṃ mahāisīnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:6.2":{"pli":"Sādhūni vākyāni madhūva khuddaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:6.3":{"pli":"Anuttaraṃ bhesajaṃ patthayantā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:6.4":{"pli":"Suṇātha sabbesu pasannacittā’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:7.1":{"pli":"Paccekabuddhānaṃ samāgatānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:7.2":{"pli":"Paramparaṃ byākaraṇāni yāni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:7.3":{"pli":"Ādīnavo yañca virāgavatthuṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:7.4":{"pli":"Yathā ca bodhiṃ anupāpuṇiṃsu. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:8.1":{"pli":"Sarāgavatthūsu virāgasaññī, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:8.2":{"pli":"Rattamhi lokamhi virattacittā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:8.3":{"pli":"Hitvā papañce jiya phanditāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:8.4":{"pli":"Tatheva bodhiṃ anupāpuṇiṃsu. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:9.1":{"pli":"Sabbesu bhūtesu nidhāya daṇḍaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:9.2":{"pli":"Aviheṭhayaṃ aññatarampi tesaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:9.3":{"pli":"Mettena cittena hitānukampī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:9.4":{"pli":"Eko care khaggavisāṇakappo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:10.1":{"pli":"Sabbesu bhūtesu nidhāya daṇḍaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:10.2":{"pli":"Aviheṭhayaṃ aññatarampi tesaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:10.3":{"pli":"Na puttamiccheyya kuto sahāyaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:10.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:11.1":{"pli":"‘Saṃsaggajātassa bhavanti snehā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:11.2":{"pli":"Snehanvayaṃ dukkhamidaṃ pahoti; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:11.3":{"pli":"Ādīnavaṃ snehajaṃ pekkhamāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:11.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:12.1":{"pli":"‘Mitte suhajje anukampamāno, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:12.2":{"pli":"Hāpeti atthaṃ paṭibaddhacitto; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:12.3":{"pli":"Etaṃ bhayaṃ santhave pekkhamāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:12.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:13.1":{"pli":"‘Vaṃso visālova yathā visatto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:13.2":{"pli":"Puttesu dāresu ca yā apekkhā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:13.3":{"pli":"Vaṃse kaḷīrova asajjamāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:13.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:14.1":{"pli":"‘Migo araññamhi yathā abaddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:14.2":{"pli":"Yenicchakaṃ gacchati gocarāya; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:14.3":{"pli":"Viññū naro seritaṃ pekkhamāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:14.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:15.1":{"pli":"‘Āmantanā hoti sahāyamajjhe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:15.2":{"pli":"Vāse ca ṭhāne gamane cārikāya; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:15.3":{"pli":"Anabhijjhitaṃ seritaṃ pekkhamāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:15.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:16.1":{"pli":"‘Khiḍḍā ratī hoti sahāyamajjhe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:16.2":{"pli":"Puttesu pemaṃ vipulañca hoti; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:16.3":{"pli":"Piyavippayogaṃ vijigucchamāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:16.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:17.1":{"pli":"‘Cātuddiso appaṭigho ca hoti, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:17.2":{"pli":"Santussamāno itarītarena; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:17.3":{"pli":"Parissayānaṃ sahitā achambhī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:17.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:18.1":{"pli":"‘Dussaṅgahā pabbajitāpi eke, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:18.2":{"pli":"Atho gahaṭṭhā gharamāvasantā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:18.3":{"pli":"Appossukko paraputtesu hutvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:18.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:19.1":{"pli":"‘Oropayitvā gihibyañjanāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:19.2":{"pli":"Sañchinnapatto yathā koviḷāro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:19.3":{"pli":"Chetvāna vīro gihibandhanāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:19.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:20.1":{"pli":"‘Sace labhetha nipakaṃ sahāyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:20.2":{"pli":"Saddhiṃ caraṃ sādhuvihāridhīraṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:20.3":{"pli":"Abhibhuyya sabbāni parissayāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:20.4":{"pli":"Careyya tenattamano satīmā’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:21.1":{"pli":"‘No ce labhetha nipakaṃ sahāyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:21.2":{"pli":"Saddhiṃ caraṃ sādhuvihāri dhīraṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:21.3":{"pli":"Rājāva raṭṭhaṃ vijitaṃ pahāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:21.4":{"pli":"Eko care mātaṅgaraññeva nāgo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:22.1":{"pli":"‘Addhā pasaṃsāma sahāyasampadaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:22.2":{"pli":"Seṭṭhā samā sevitabbā sahāyā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:22.3":{"pli":"Ete aladdhā anavajjabhojī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:22.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:23.1":{"pli":"‘Disvā suvaṇṇassa pabhassarāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:23.2":{"pli":"Kammāraputtena suniṭṭhitāni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:23.3":{"pli":"Saṅghaṭṭamānāni duve bhujasmiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:23.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:24.1":{"pli":"‘Evaṃ dutīyena sahā mamassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:24.2":{"pli":"Vācābhilāpo abhisajjanā vā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:24.3":{"pli":"Etaṃ bhayaṃ āyatiṃ pekkhamāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:24.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:25.1":{"pli":"‘Kāmā hi citrā madhurā manoramā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:25.2":{"pli":"Virūparūpena mathenti cittaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:25.3":{"pli":"Ādīnavaṃ kāmaguṇesu disvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:25.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:26.1":{"pli":"‘Ītī ca gaṇḍo ca upaddavo ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:26.2":{"pli":"Rogo ca sallañca bhayañca metaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:26.3":{"pli":"Etaṃ bhayaṃ kāmaguṇesu disvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:26.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:27.1":{"pli":"‘Sītañca uṇhañca khudaṃ pipāsaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:27.2":{"pli":"Vātātape ḍaṃsasarīsape ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:27.3":{"pli":"Sabbānipetāni abhibbhavitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:27.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:28.1":{"pli":"‘Nāgova yūthāni vivajjayitvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:28.2":{"pli":"Sañjātakhandho padumī uḷāro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:28.3":{"pli":"Yathābhirantaṃ viharaṃ araññe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:28.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:29.1":{"pli":"‘Aṭṭhāna taṃ saṅgaṇikāratassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:29.2":{"pli":"Yaṃ phassaye sāmayikaṃ vimuttiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:29.3":{"pli":"Ādiccabandhussa vaco nisamma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:29.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:30.1":{"pli":"‘Diṭṭhīvisūkāni upātivatto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:30.2":{"pli":"Patto niyāmaṃ paṭiladdhamaggo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:30.3":{"pli":"Uppannañāṇomhi anaññaneyyo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:30.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:31.1":{"pli":"‘Nillolupo nikkuho nippipāso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:31.2":{"pli":"Nimmakkha niddhantakasāvamoho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:31.3":{"pli":"Nirāsayo sabbaloke bhavitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:31.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:32.1":{"pli":"‘Pāpaṃ sahāyaṃ parivajjayetha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:32.2":{"pli":"Anatthadassiṃ visame niviṭṭhaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:32.3":{"pli":"Sayaṃ na seve pasutaṃ pamattaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:32.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:33.1":{"pli":"‘Bahussutaṃ dhammadharaṃ bhajetha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:33.2":{"pli":"Mittaṃ uḷāraṃ paṭibhānavantaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:33.3":{"pli":"Aññāya atthāni vineyya kaṅkhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:33.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:34.1":{"pli":"‘Khiḍḍaṃ ratiṃ kāmasukhañca loke, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:34.2":{"pli":"Analaṅkaritvā anapekkhamāno; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:34.3":{"pli":"Vibhūsaṭṭhānā virato saccavādī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:34.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:35.1":{"pli":"‘Puttañca dāraṃ pitarañca mātaraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:35.2":{"pli":"Dhanāni dhaññāni ca bandhavāni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:35.3":{"pli":"Hitvāna kāmāni yathodhikāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:35.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:36.1":{"pli":"‘Saṅgo eso parittamettha sokhyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:36.2":{"pli":"Appassādo dukkhamevettha bhiyyo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:36.3":{"pli":"Gaḷo eso iti ñatvā matimā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:36.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:37.1":{"pli":"‘Sandālayitvāna saṃyojanāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:37.2":{"pli":"Jālaṃva bhetvā salilambucārī; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:37.3":{"pli":"Aggīva daḍḍhaṃ anivattamāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:37.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:38.1":{"pli":"‘Okkhittacakkhū na ca pādalolo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:38.2":{"pli":"Guttindriyo rakkhitamānasāno; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:38.3":{"pli":"Anavassuto apariḍayhamāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:38.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:39.1":{"pli":"‘Ohārayitvā gihibyañjanāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:39.2":{"pli":"Sañchannapatto yathā pārichatto; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:39.3":{"pli":"Kāsāyavattho abhinikkhamitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:39.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:40.1":{"pli":"‘Rasesu gedhaṃ akaraṃ alolo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:40.2":{"pli":"Anaññaposī sapadānacārī; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:40.3":{"pli":"Kule kule appaṭibaddhacitto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:40.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:41.1":{"pli":"‘Pahāya pañcāvaraṇāni cetaso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:41.2":{"pli":"Upakkilese byapanujja sabbe; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:41.3":{"pli":"Anissito chejja sinehadosaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:41.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:42.1":{"pli":"‘Vipiṭṭhikatvāna sukhañca dukkhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:42.2":{"pli":"Pubbeva somanassadomanassaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:42.3":{"pli":"Laddhānupekkhaṃ samathaṃ visuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:42.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:43.1":{"pli":"‘Āraddhavīriyo paramatthapattiyā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:43.2":{"pli":"Alīnacitto akusītavutti; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:43.3":{"pli":"Daḷhanikkamo thāmabalūpapanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:43.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:44.1":{"pli":"‘Paṭisallānaṃ jhānamariñcamāno, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:44.2":{"pli":"Dhammesu niccaṃ anudhammacārī; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:44.3":{"pli":"Ādīnavaṃ sammasitā bhavesu, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:44.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:45.1":{"pli":"‘Taṇhakkhayaṃ patthayamappamatto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:45.2":{"pli":"Aneḷamūgo sutavā satīmā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:45.3":{"pli":"Saṅkhātadhammo niyato padhānavā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:45.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:46.1":{"pli":"‘Sīhova saddesu asantasanto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:46.2":{"pli":"Vātova jālamhi asajjamāno; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:46.3":{"pli":"Padumaṃva toyena alippamāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:46.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:47.1":{"pli":"‘Sīho yathā dāṭhabalī pasayha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:47.2":{"pli":"Rājā migānaṃ abhibhuyya cārī; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:47.3":{"pli":"Sevetha pantāni senāsanāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:47.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:48.1":{"pli":"‘Mettaṃ upekkhaṃ karuṇaṃ vimuttiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:48.2":{"pli":"Āsevamāno muditañca kāle; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:48.3":{"pli":"Sabbena lokena avirujjhamāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:48.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:49.1":{"pli":"‘Rāgañca dosañca pahāya mohaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:49.2":{"pli":"Sandālayitvāna saṃyojanāni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:49.3":{"pli":"Asantasaṃ jīvitasaṅkhayamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:49.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:50.1":{"pli":"‘Bhajanti sevanti ca kāraṇatthā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:50.2":{"pli":"Nikkāraṇā dullabhā ajja mittā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:50.3":{"pli":"Attatthapaññā asucīmanussā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:50.4":{"pli":"Eko care khaggavisāṇakappo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:51.1":{"pli":"Visuddhasīlā suvisuddhapaññā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:51.2":{"pli":"Samāhitā jāgariyānuyuttā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:51.3":{"pli":"Vipassakā dhammavisesadassī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:51.4":{"pli":"Maggaṅgabojjhaṅgagate vijaññā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:52.1":{"pli":"Suññappaṇidhiñca tathānimittaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:52.2":{"pli":"Āsevayitvā jinasāsanamhi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:52.3":{"pli":"Ye sāvakattaṃ na vajanti dhīrā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:52.4":{"pli":"Bhavanti paccekajinā sayambhū. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:53.1":{"pli":"Mahantadhammā bahudhammakāyā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:53.2":{"pli":"Cittissarā sabbadukkhoghatiṇṇā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:53.3":{"pli":"Udaggacittā paramatthadassī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:53.4":{"pli":"Sīhopamā khaggavisāṇakappā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:54.1":{"pli":"Santindriyā santamanā samādhī, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:54.2":{"pli":"Paccantasattesu patippacārā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:54.3":{"pli":"Dīpā parattha idha vijjalantā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:54.4":{"pli":"Paccekabuddhā satataṃ hitāme. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:55.1":{"pli":"Pahīnasabbāvaraṇā janindā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:55.2":{"pli":"Lokappadīpā ghanakañcanābhā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:55.3":{"pli":"Nissaṃsayaṃ lokasudakkhiṇeyyā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:55.4":{"pli":"Paccekabuddhā satatappitāme. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:56.1":{"pli":"Paccekabuddhānaṃ subhāsitāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:56.2":{"pli":"Caranti lokamhi sadevakamhi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:56.3":{"pli":"Sutvā tathā ye na karonti bālā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:56.4":{"pli":"Caranti dukkhesu punappunaṃ te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:57.1":{"pli":"Paccekabuddhāna subhāsitāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:57.2":{"pli":"Madhuṃ yathā khuddamavassavantaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:57.3":{"pli":"Sutvā tathā ye paṭipattiyuttā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:57.4":{"pli":"Bhavanti te saccadasā sapaññā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:58.1":{"pli":"Paccekabuddhehi jinehi bhāsitā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:58.2":{"pli":"Kathā uḷārā abhinikkhamitvā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:58.3":{"pli":"Tā sakyasīhena naruttamena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:58.4":{"pli":"Pakāsitā dhammavijānanatthaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap2:59.1":{"pli":"Lokānukampāya imāni tesaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap2:59.2":{"pli":"Paccekabuddhāna vikubbitāni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:59.3":{"pli":"Saṃvegasaṅgamativaḍḍhanatthaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap2:59.4":{"pli":"Sayambhusīhena pakāsitānī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap2:60.1":{"pli":"Paccekabuddhāpadānaṃ samattaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap20":{"tha-ap20:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap20'><header><ul><li class='division'>{}</li>"},"tha-ap20:0.2":{"pli":"Sīhāsaniyavagga ","html":"<li>{}</li></ul>"},"tha-ap20:0.3":{"pli":"Raṭṭhapālattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap20:1.1":{"pli":"“Padumuttarassa bhagavato, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap20:1.2":{"pli":"Lokajeṭṭhassa tādino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:1.3":{"pli":"Varanāgo mayā dinno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:1.4":{"pli":"Īsādanto urūḷhavā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap20:2.1":{"pli":"Setacchatto pasobhito, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap20:2.2":{"pli":"sakappano sahatthipo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:2.3":{"pli":"Agghāpetvāna taṃ sabbaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:2.4":{"pli":"saṅghārāmaṃ akārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap20:3.1":{"pli":"Catupaññāsasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap20:3.2":{"pli":"pāsāde kārayiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:3.3":{"pli":"Mahoghadānaṃ karitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:3.4":{"pli":"niyyādesiṃ mahesino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap20:4.1":{"pli":"Anumodi mahāvīro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap20:4.2":{"pli":"sayambhū aggapuggalo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:4.3":{"pli":"Sabbe jane hāsayanto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:4.4":{"pli":"desesi amataṃ padaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap20:5.1":{"pli":"Taṃ me buddho viyākāsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap20:5.2":{"pli":"jalajuttaranāmako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:5.3":{"pli":"Bhikkhusaṅghe nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:5.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap20:6.1":{"pli":"‘Catupaññāsasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap20:6.2":{"pli":"pāsāde kārayī ayaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:6.3":{"pli":"Kathayissāmi vipākaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:6.4":{"pli":"suṇotha mama bhāsato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap20:7.1":{"pli":"Aṭṭhārasasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap20:7.2":{"pli":"kūṭāgārā bhavissare; ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:7.3":{"pli":"Byamhuttamamhi nibbattā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:7.4":{"pli":"sabbasoṇṇamayā ca te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap20:8.1":{"pli":"Paññāsakkhattuṃ devindo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap20:8.2":{"pli":"devarajjaṃ karissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:8.3":{"pli":"Aṭṭhapaññāsakkhattuñca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:8.4":{"pli":"cakkavattī bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap20:9.1":{"pli":"Kappasatasahassamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap20:9.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:9.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:9.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap20:10.1":{"pli":"Devalokā cavitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap20:10.2":{"pli":"sukkamūlena codito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap20:10.3":{"pli":"