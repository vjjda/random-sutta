{"vv76":{"vv76:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv76'><header><ul><li class='division'>{}</li>"},"vv76:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv76:0.3":{"pli":"Sunikkhittavagga ","html":"<li>{}</li></ul>"},"vv76:0.4":{"pli":"2. Nandanavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv76:1.1":{"pli":"“Yathā vanaṃ nandanaṃ pabhāsati, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv76:1.2":{"pli":"Uyyānaseṭṭhaṃ tidasānamuttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv76:1.3":{"pli":"Tathūpamaṃ tuyhamidaṃ vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv76:1.4":{"pli":"Obhāsayaṃ tiṭṭhati antalikkhe. ","html":"<span class='verse-line'>{}</span></p>"},"vv76:2.1":{"pli":"Deviddhipattosi mahānubhāvo, ","html":"<p><span class='verse-line'>{}</span>"},"vv76:2.2":{"pli":"Manussabhūto kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv76:2.3":{"pli":"Kenāsi evaṃ jalitānubhāvo, ","html":"<span class='verse-line'>{}</span>"},"vv76:2.4":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv76:3.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv76:3.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv76:3.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv76:4.1":{"pli":"“Ahaṃ manussesu manussabhūto, ","html":"<p><span class='verse-line'>{}</span>"},"vv76:4.2":{"pli":"Daliddo atāṇo kapaṇo kammakaro ahosiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv76:4.3":{"pli":"Jiṇṇe ca mātāpitaro abhāriṃ, ","html":"<span class='verse-line'>{}</span>"},"vv76:4.4":{"pli":"Piyā ca me sīlavanto ahesuṃ; ","html":"<span class='verse-line'>{}</span>"},"vv76:4.5":{"pli":"Annañca pānañca pasannacitto, ","html":"<span class='verse-line'>{}</span>"},"vv76:4.6":{"pli":"Sakkacca dānaṃ vipulaṃ adāsiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv76:5.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv76:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv76:5.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv76:6.1":{"pli":"Nandanavimānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv77":{"vv77:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv77'><header><ul><li class='division'>{}</li>"},"vv77:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv77:0.3":{"pli":"Sunikkhittavagga ","html":"<li>{}</li></ul>"},"vv77:0.4":{"pli":"3. Maṇithūṇavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv77:1.1":{"pli":"“Uccamidaṃ maṇithūṇaṃ vimānaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv77:1.2":{"pli":"Samantato dvādasa yojanāni; ","html":"<span class='verse-line'>{}</span>"},"vv77:1.3":{"pli":"Kūṭāgārā sattasatā uḷārā, ","html":"<span class='verse-line'>{}</span>"},"vv77:1.4":{"pli":"Veḷuriyathambhā rucakatthatā subhā. ","html":"<span class='verse-line'>{}</span></p>"},"vv77:2.1":{"pli":"Tatthacchasi pivasi khādasi ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv77:2.2":{"pli":"Dibbā ca vīṇā pavadanti vagguṃ; ","html":"<span class='verse-line'>{}</span>"},"vv77:2.3":{"pli":"Dibbā rasā kāmaguṇettha pañca, ","html":"<span class='verse-line'>{}</span>"},"vv77:2.4":{"pli":"Nāriyo ca naccanti suvaṇṇachannā. ","html":"<span class='verse-line'>{}</span></p>"},"vv77:3.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv77:3.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv77:3.3":{"pli":"vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv77:4.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv77:4.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv77:4.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv77:5.1":{"pli":"“Ahaṃ manussesu manussabhūto, ","html":"<p><span class='verse-line'>{}</span>"},"vv77:5.2":{"pli":"Vivane pathe saṅkamanaṃ akāsiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv77:5.3":{"pli":"Ārāmarukkhāni ca ropayissaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv77:5.4":{"pli":"Piyā ca me sīlavanto ahesuṃ; ","html":"<span class='verse-line'>{}</span>"},"vv77:5.5":{"pli":"Annañca pānañca pasannacitto, ","html":"<span class='verse-line'>{}</span>"},"vv77:5.6":{"pli":"Sakkacca dānaṃ vipulaṃ adāsiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv77:6.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv77:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv77:6.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv77:7.1":{"pli":"Maṇithūṇavimānaṃ tatiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv78":{"vv78:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv78'><header><ul><li class='division'>{}</li>"},"vv78:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv78:0.3":{"pli":"Sunikkhittavagga ","html":"<li>{}</li></ul>"},"vv78:0.4":{"pli":"4. Suvaṇṇavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv78:1.1":{"pli":"“Sovaṇṇamaye pabbatasmiṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv78:1.2":{"pli":"vimānaṃ sabbatopabhaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv78:1.3":{"pli":"Hemajālapaṭicchannaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv78:1.4":{"pli":"kiṅkiṇijālakappitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv78:2.1":{"pli":"Aṭṭhaṃsā sukatā thambhā, ","html":"<p><span class='verse-line'>{}</span>"},"vv78:2.2":{"pli":"sabbe veḷuriyāmayā; ","html":"<span class='verse-line'>{}</span>"},"vv78:2.3":{"pli":"Ekamekāya aṃsiyā, ","html":"<span class='verse-line'>{}</span>"},"vv78:2.4":{"pli":"ratanā satta nimmitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv78:3.1":{"pli":"Veḷuriyasuvaṇṇassa, ","html":"<p><span class='verse-line'>{}</span>"},"vv78:3.2":{"pli":"phalikā rūpiyassa ca; ","html":"<span class='verse-line'>{}</span>"},"vv78:3.3":{"pli":"Masāragallamuttāhi, ","html":"<span class='verse-line'>{}</span>"},"vv78:3.4":{"pli":"lohitaṅgamaṇīhi ca. ","html":"<span class='verse-line'>{}</span></p>"},"vv78:4.1":{"pli":"Citrā manoramā bhūmi, ","html":"<p><span class='verse-line'>{}</span>"},"vv78:4.2":{"pli":"na tatthuddhaṃsatī rajo; ","html":"<span class='verse-line'>{}</span>"},"vv78:4.3":{"pli":"Gopānasīgaṇā pītā, ","html":"<span class='verse-line'>{}</span>"},"vv78:4.4":{"pli":"kūṭaṃ dhārenti nimmitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv78:5.1":{"pli":"Sopānāni ca cattāri, ","html":"<p><span class='verse-line'>{}</span>"},"vv78:5.2":{"pli":"nimmitā caturo disā; ","html":"<span class='verse-line'>{}</span>"},"vv78:5.3":{"pli":"Nānāratanagabbhehi, ","html":"<span class='verse-line'>{}</span>"},"vv78:5.4":{"pli":"ādiccova virocati. ","html":"<span class='verse-line'>{}</span></p>"},"vv78:6.1":{"pli":"Vediyā catasso tattha, ","html":"<p><span class='verse-line'>{}</span>"},"vv78:6.2":{"pli":"vibhattā bhāgaso mitā; ","html":"<span class='verse-line'>{}</span>"},"vv78:6.3":{"pli":"Daddallamānā ābhanti, ","html":"<span class='verse-line'>{}</span>"},"vv78:6.4":{"pli":"samantā caturo disā. ","html":"<span class='verse-line'>{}</span></p>"},"vv78:7.1":{"pli":"Tasmiṃ vimāne pavare, ","html":"<p><span class='verse-line'>{}</span>"},"vv78:7.2":{"pli":"devaputto mahappabho; ","html":"<span class='verse-line'>{}</span>"},"vv78:7.3":{"pli":"Atirocasi vaṇṇena, ","html":"<span class='verse-line'>{}</span>"},"vv78:7.4":{"pli":"udayantova bhāṇumā. ","html":"<span class='verse-line'>{}</span></p>"},"vv78:8.1":{"pli":"Dānassa te idaṃ phalaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv78:8.2":{"pli":"atho sīlassa vā pana; ","html":"<span class='verse-line'>{}</span>"},"vv78:8.3":{"pli":"Atho añjalikammassa, ","html":"<span class='verse-line'>{}</span>"},"vv78:8.4":{"pli":"taṃ me akkhāhi pucchito”. ","html":"<span class='verse-line'>{}</span></p>"},"vv78:9.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv78:9.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv78:9.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv78:10.1":{"pli":"“Ahaṃ andhakavindasmiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv78:10.2":{"pli":"buddhassādiccabandhuno; ","html":"<span class='verse-line'>{}</span>"},"vv78:10.3":{"pli":"Vihāraṃ satthu kāresiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv78:10.4":{"pli":"pasanno sehi pāṇibhi. ","html":"<span class='verse-line'>{}</span></p>"},"vv78:11.1":{"pli":"Tattha gandhañca mālañca, ","html":"<p><span class='verse-line'>{}</span>"},"vv78:11.2":{"pli":"paccayañca vilepanaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv78:11.3":{"pli":"Vihāraṃ satthu adāsiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv78:11.4":{"pli":"vippasannena cetasā; ","html":"<span class='verse-line'>{}</span>"},"vv78:11.5":{"pli":"Tena mayhaṃ idaṃ laddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv78:11.6":{"pli":"vasaṃ vattemi nandane. ","html":"<span class='verse-line'>{}</span></p>"},"vv78:12.1":{"pli":"Nandane ca vane ramme, ","html":"<p><span class='verse-line'>{}</span>"},"vv78:12.2":{"pli":"nānādijagaṇāyute; ","html":"<span class='verse-line'>{}</span>"},"vv78:12.3":{"pli":"Ramāmi naccagītehi, ","html":"<span class='verse-line'>{}</span>"},"vv78:12.4":{"pli":"accharāhi purakkhato”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv78:13.1":{"pli":"Suvaṇṇavimānaṃ catutthaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv79":{"vv79:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv79'><header><ul><li class='division'>{}</li>"},"vv79:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv79:0.3":{"pli":"Sunikkhittavagga ","html":"<li>{}</li></ul>"},"vv79:0.4":{"pli":"5. Ambavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv79:1.1":{"pli":"“Uccamidaṃ maṇithūṇaṃ vimānaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv79:1.2":{"pli":"Samantato dvādasa yojanāni; ","html":"<span class='verse-line'>{}</span>"},"vv79:1.3":{"pli":"Kūṭāgārā sattasatā uḷārā, ","html":"<span class='verse-line'>{}</span>"},"vv79:1.4":{"pli":"Veḷuriyathambhā rucakatthatā subhā. ","html":"<span class='verse-line'>{}</span></p>"},"vv79:2.1":{"pli":"Tatthacchasi pivasi khādasi ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv79:2.2":{"pli":"Dibbā ca vīṇā pavadanti vagguṃ; ","html":"<span class='verse-line'>{}</span>"},"vv79:2.3":{"pli":"Dibbā rasā kāmaguṇettha pañca, ","html":"<span class='verse-line'>{}</span>"},"vv79:2.4":{"pli":"Nāriyo ca naccanti suvaṇṇachannā. ","html":"<span class='verse-line'>{}</span></p>"},"vv79:3.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv79:3.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv79:3.3":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv79:4.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv79:4.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv79:4.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv79:5.1":{"pli":"“Gimhānaṃ pacchime māse, ","html":"<p><span class='verse-line'>{}</span>"},"vv79:5.2":{"pli":"patapante divaṅkare; ","html":"<span class='verse-line'>{}</span>"},"vv79:5.3":{"pli":"Paresaṃ bhatako poso, ","html":"<span class='verse-line'>{}</span>"},"vv79:5.4":{"pli":"ambārāmamasiñcati. ","html":"<span class='verse-line'>{}</span></p>"},"vv79:6.1":{"pli":"Atha tenāgamā bhikkhu, ","html":"<p><span class='verse-line'>{}</span>"},"vv79:6.2":{"pli":"sāriputtoti vissuto; ","html":"<span class='verse-line'>{}</span>"},"vv79:6.3":{"pli":"Kilantarūpo kāyena, ","html":"<span class='verse-line'>{}</span>"},"vv79:6.4":{"pli":"akilantova cetasā. ","html":"<span class='verse-line'>{}</span></p>"},"vv79:7.1":{"pli":"Tañca disvāna āyantaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv79:7.2":{"pli":"Avocaṃ ambasiñcako; ","html":"<span class='verse-line'>{}</span>"},"vv79:7.3":{"pli":"Sādhu taṃ bhante nhāpeyyaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv79:7.4":{"pli":"Yaṃ mamassa sukhāvahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv79:8.1":{"pli":"Tassa me anukampāya, ","html":"<p><span class='verse-line'>{}</span>"},"vv79:8.2":{"pli":"nikkhipi pattacīvaraṃ; ","html":"<span class='verse-line'>{}</span>"},"vv79:8.3":{"pli":"Nisīdi rukkhamūlasmiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv79:8.4":{"pli":"chāyāya ekacīvaro. ","html":"<span class='verse-line'>{}</span></p>"},"vv79:9.1":{"pli":"Tañca acchena vārinā, ","html":"<p><span class='verse-line'>{}</span>"},"vv79:9.2":{"pli":"pasannamānaso naro; ","html":"<span class='verse-line'>{}</span>"},"vv79:9.3":{"pli":"Nhāpayī rukkhamūlasmiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv79:9.4":{"pli":"chāyāya ekacīvaraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv79:10.1":{"pli":"Ambo ca sitto samaṇo ca nhāpito, ","html":"<p><span class='verse-line'>{}</span>"},"vv79:10.2":{"pli":"Mayā ca puññaṃ pasutaṃ anappakaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv79:10.3":{"pli":"Iti so pītiyā kāyaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv79:10.4":{"pli":"Sabbaṃ pharati attano. ","html":"<span class='verse-line'>{}</span></p>"},"vv79:11.1":{"pli":"Tadeva ettakaṃ kammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv79:11.2":{"pli":"akāsiṃ tāya jātiyā; ","html":"<span class='verse-line'>{}</span>"},"vv79:11.3":{"pli":"Pahāya mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv79:11.4":{"pli":"upapannomhi nandanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv79:12.1":{"pli":"Nandane ca vane ramme, ","html":"<p><span class='verse-line'>{}</span>"},"vv79:12.2":{"pli":"nānādijagaṇāyute; ","html":"<span class='verse-line'>{}</span>"},"vv79:12.3":{"pli":"Ramāmi naccagītehi, ","html":"<span class='verse-line'>{}</span>"},"vv79:12.4":{"pli":"accharāhi purakkhato”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv79:13.1":{"pli":"Ambavimānaṃ pañcamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv8":{"vv8:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv8'><header><ul><li class='division'>{}</li>"},"vv8:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv8:0.3":{"pli":"Pīṭhavagga ","html":"<li>{}</li></ul>"},"vv8:0.4":{"pli":"8. Tatiyanāvāvimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv8:1.1":{"pli":"“Suvaṇṇacchadanaṃ nāvaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv8:1.2":{"pli":"Nāri āruyha tiṭṭhasi; ","html":"<span class='verse-line'>{}</span>"},"vv8:1.3":{"pli":"Ogāhasi pokkharaṇiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv8:1.4":{"pli":"Padmaṃ chindasi pāṇinā. ","html":"<span class='verse-line'>{}</span></p>"},"vv8:2.1":{"pli":"Kūṭāgārā nivesā te, ","html":"<p><span class='verse-line'>{}</span>"},"vv8:2.2":{"pli":"vibhattā bhāgaso mitā; ","html":"<span class='verse-line'>{}</span>"},"vv8:2.3":{"pli":"Daddallamānā ābhanti, ","html":"<span class='verse-line'>{}</span>"},"vv8:2.4":{"pli":"samantā caturo disā. ","html":"<span class='verse-line'>{}</span></p>"},"vv8:3.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv8:3.2":{"pli":"kena te idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv8:3.3":{"pli":"Uppajjanti ca te bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv8:3.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv8:4.1":{"pli":"Pucchāmi taṃ devi mahānubhāve, ","html":"<p><span class='verse-line'>{}</span>"},"vv8:4.2":{"pli":"Manussabhūtā kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv8:4.3":{"pli":"Kenāsi evaṃ jalitānubhāvā, ","html":"<span class='verse-line'>{}</span>"},"vv8:4.4":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv8:5.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv8:5.2":{"pli":"sambuddheneva pucchitā; ","html":"<span class='verse-line'>{}</span>"},"vv8:5.3":{"pli":"Pañhaṃ puṭṭhā viyākāsi, ","html":"<span class='verse-line'>{}</span>"},"vv8:5.4":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv8:6.1":{"pli":"“Ahaṃ manussesu manussabhūtā, ","html":"<p><span class='verse-line'>{}</span>"},"vv8:6.2":{"pli":"Purimāya jātiyā manussaloke; ","html":"<span class='verse-line'>{}</span>"},"vv8:6.3":{"pli":"Disvāna bhikkhū tasite kilante, ","html":"<span class='verse-line'>{}</span>"},"vv8:6.4":{"pli":"Uṭṭhāya pātuṃ udakaṃ adāsiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv8:7.1":{"pli":"Yo ve kilantāna pipāsitānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv8:7.2":{"pli":"Uṭṭhāya pātuṃ udakaṃ dadāti; ","html":"<span class='verse-line'>{}</span>"},"vv8:7.3":{"pli":"Sītodakā tassa bhavanti najjo, ","html":"<span class='verse-line'>{}</span>"},"vv8:7.4":{"pli":"Pahūtamalyā bahupuṇḍarīkā. ","html":"<span class='verse-line'>{}</span></p>"},"vv8:8.1":{"pli":"Taṃ āpagā anupariyanti sabbadā, ","html":"<p><span class='verse-line'>{}</span>"},"vv8:8.2":{"pli":"Sītodakā vālukasanthatā nadī; ","html":"<span class='verse-line'>{}</span>"},"vv8:8.3":{"pli":"Ambā ca sālā tilakā ca jambuyo, ","html":"<span class='verse-line'>{}</span>"},"vv8:8.4":{"pli":"Uddālakā pāṭaliyo ca phullā. ","html":"<span class='verse-line'>{}</span></p>"},"vv8:9.1":{"pli":"Taṃ bhūmibhāgehi upetarūpaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv8:9.2":{"pli":"Vimānaseṭṭhaṃ bhusa sobhamānaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv8:9.3":{"pli":"Tassīdha kammassa ayaṃ vipāko, ","html":"<span class='verse-line'>{}</span>"},"vv8:9.4":{"pli":"Etādisaṃ puññakatā labhanti. ","html":"<span class='verse-line'>{}</span></p>"},"vv8:10.1":{"pli":"Kūṭāgārā nivesā me, ","html":"<p><span class='verse-line'>{}</span>"},"vv8:10.2":{"pli":"vibhattā bhāgaso mitā; ","html":"<span class='verse-line'>{}</span>"},"vv8:10.3":{"pli":"Daddallamānā ābhanti, ","html":"<span class='verse-line'>{}</span>"},"vv8:10.4":{"pli":"samantā caturo disā. ","html":"<span class='verse-line'>{}</span></p>"},"vv8:11.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv8:11.2":{"pli":"tena me idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv8:11.3":{"pli":"Uppajjanti ca me bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv8:11.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv8:12.1":{"pli":"Akkhāmi te buddha mahānubhāva, ","html":"<p><span class='verse-line'>{}</span>"},"vv8:12.2":{"pli":"Manussabhūtā yamakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv8:12.3":{"pli":"Tenamhi evaṃ jalitānubhāvā, ","html":"<span class='verse-line'>{}</span>"},"vv8:12.4":{"pli":"Vaṇṇo ca me sabbadisā pabhāsati; ","html":"<span class='verse-line'>{}</span>"},"vv8:12.5":{"pli":"Etassa kammassa phalaṃ mamedaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv8:12.6":{"pli":"Atthāya buddho udakaṃ apāyī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv8:13.1":{"pli":"Tatiyanāvāvimānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv80":{"vv80:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv80'><header><ul><li class='division'>{}</li>"},"vv80:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv80:0.3":{"pli":"Sunikkhittavagga ","html":"<li>{}</li></ul>"},"vv80:0.4":{"pli":"6. Gopālavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv80:1.1":{"pli":"“Disvāna devaṃ paṭipucchi bhikkhu, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv80:1.2":{"pli":"Ucce vimānamhi ciraṭṭhitike; ","html":"<span class='verse-line'>{}</span>"},"vv80:1.3":{"pli":"Āmuttahatthābharaṇaṃ yasassiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv80:1.4":{"pli":"Dibbe vimānamhi yathāpi candimā. ","html":"<span class='verse-line'>{}</span></p>"},"vv80:2.1":{"pli":"Alaṅkato malyadharo suvattho, ","html":"<p><span class='verse-line'>{}</span>"},"vv80:2.2":{"pli":"Sukuṇḍalī kappitakesamassu; ","html":"<span class='verse-line'>{}</span>"},"vv80:2.3":{"pli":"Āmuttahatthābharaṇo yasassī, ","html":"<span class='verse-line'>{}</span>"},"vv80:2.4":{"pli":"Dibbe vimānamhi yathāpi candimā. ","html":"<span class='verse-line'>{}</span></p>"},"vv80:3.1":{"pli":"Dibbā ca vīṇā pavadanti vagguṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv80:3.2":{"pli":"Aṭṭhaṭṭhakā sikkhitā sādhurūpā; ","html":"<span class='verse-line'>{}</span>"},"vv80:3.3":{"pli":"Dibbā ca kaññā tidasacarā uḷārā, ","html":"<span class='verse-line'>{}</span>"},"vv80:3.4":{"pli":"Naccanti gāyanti pamodayanti. ","html":"<span class='verse-line'>{}</span></p>"},"vv80:4.1":{"pli":"Deviddhipattosi mahānubhāvo, ","html":"<p><span class='verse-line'>{}</span>"},"vv80:4.2":{"pli":"Manussabhūto kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv80:4.3":{"pli":"Kenāsi evaṃ jalitānubhāvo, ","html":"<span class='verse-line'>{}</span>"},"vv80:4.4":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv80:5.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv80:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv80:5.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv80:6.1":{"pli":"“Ahaṃ manussesu manussabhūto, ","html":"<p><span class='verse-line'>{}</span>"},"vv80:6.2":{"pli":"Saṅgamma rakkhissaṃ paresaṃ dhenuyo; ","html":"<span class='verse-line'>{}</span>"},"vv80:6.3":{"pli":"Tato ca āgā samaṇo mamantike, ","html":"<span class='verse-line'>{}</span>"},"vv80:6.4":{"pli":"Gāvo ca māse agamaṃsu khādituṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv80:7.1":{"pli":"Dvayajja kiccaṃ ubhayañca kāriyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv80:7.2":{"pli":"Iccevahaṃ bhante tadā vicintayiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv80:7.3":{"pli":"Tato ca saññaṃ paṭiladdha yoniso, ","html":"<span class='verse-line'>{}</span>"},"vv80:7.4":{"pli":"Dadāmi bhanteti khipiṃ anantakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv80:8.1":{"pli":"So māsakhettaṃ turito avāsariṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv80:8.2":{"pli":"Purā ayaṃ bhañjati yassidaṃ dhanaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv80:8.3":{"pli":"Tato ca kaṇho urago mahāviso, ","html":"<span class='verse-line'>{}</span>"},"vv80:8.4":{"pli":"Aḍaṃsi pāde turitassa me sato. ","html":"<span class='verse-line'>{}</span></p>"},"vv80:9.1":{"pli":"Svāhaṃ aṭṭomhi dukkhena pīḷito, ","html":"<p><span class='verse-line'>{}</span>"},"vv80:9.2":{"pli":"Bhikkhu ca taṃ sāmaṃ muñcitvānantakaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv80:9.3":{"pli":"Ahāsi kummāsaṃ mamānukampayā, ","html":"<span class='verse-line'>{}</span>"},"vv80:9.4":{"pli":"Tato cuto kālakatomhi devatā. ","html":"<span class='verse-line'>{}</span></p>"},"vv80:10.1":{"pli":"Tadeva kammaṃ kusalaṃ kataṃ mayā, ","html":"<p><span class='verse-line'>{}</span>"},"vv80:10.2":{"pli":"Sukhañca kammaṃ anubhomi attanā; ","html":"<span class='verse-line'>{}</span>"},"vv80:10.3":{"pli":"Tayā hi bhante anukampito bhusaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv80:10.4":{"pli":"Kataññutāya abhivādayāmi taṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv80:11.1":{"pli":"Sadevake loke samārake ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv80:11.2":{"pli":"Añño muni natthi tayānukampako; ","html":"<span class='verse-line'>{}</span>"},"vv80:11.3":{"pli":"Tayā hi bhante anukampito bhusaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv80:11.4":{"pli":"Kataññutāya abhivādayāmi taṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv80:12.1":{"pli":"Imasmiṃ loke parasmiṃ vā pana, ","html":"<p><span class='verse-line'>{}</span>"},"vv80:12.2":{"pli":"Añño munī natthi tayānukampako; ","html":"<span class='verse-line'>{}</span>"},"vv80:12.3":{"pli":"Tayā hi bhante anukampito bhusaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv80:12.4":{"pli":"Kataññutāya abhivādayāmi tan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv80:13.1":{"pli":"Gopālavimānaṃ chaṭṭhaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv81":{"vv81:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv81'><header><ul><li class='division'>{}</li>"},"vv81:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv81:0.3":{"pli":"Sunikkhittavagga ","html":"<li>{}</li></ul>"},"vv81:0.4":{"pli":"7. Kaṇḍakavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv81:1.1":{"pli":"“Puṇṇamāse yathā cando, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv81:1.2":{"pli":"nakkhattaparivārito; ","html":"<span class='verse-line'>{}</span>"},"vv81:1.3":{"pli":"Samantā anupariyāti, ","html":"<span class='verse-line'>{}</span>"},"vv81:1.4":{"pli":"tārakādhipatī sasī. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:2.1":{"pli":"Tathūpamaṃ idaṃ byamhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:2.2":{"pli":"dibbaṃ devapuramhi ca; ","html":"<span class='verse-line'>{}</span>"},"vv81:2.3":{"pli":"Atirocati vaṇṇena, ","html":"<span class='verse-line'>{}</span>"},"vv81:2.4":{"pli":"udayantova raṃsimā. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:3.1":{"pli":"Veḷuriyasuvaṇṇassa, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:3.2":{"pli":"phalikā rūpiyassa ca; ","html":"<span class='verse-line'>{}</span>"},"vv81:3.3":{"pli":"Masāragallamuttāhi, ","html":"<span class='verse-line'>{}</span>"},"vv81:3.4":{"pli":"lohitaṅgamaṇīhi ca. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:4.1":{"pli":"Citrā manoramā bhūmi, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:4.2":{"pli":"veḷūriyassa santhatā; ","html":"<span class='verse-line'>{}</span>"},"vv81:4.3":{"pli":"Kūṭāgārā subhā rammā, ","html":"<span class='verse-line'>{}</span>"},"vv81:4.4":{"pli":"pāsādo te sumāpito. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:5.1":{"pli":"Rammā ca te pokkharaṇī, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:5.2":{"pli":"puthulomanisevitā; ","html":"<span class='verse-line'>{}</span>"},"vv81:5.3":{"pli":"Acchodakā vippasannā, ","html":"<span class='verse-line'>{}</span>"},"vv81:5.4":{"pli":"sovaṇṇavālukasanthatā. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:6.1":{"pli":"Nānāpadumasañchannā, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:6.2":{"pli":"puṇḍarīkasamotatā; ","html":"<span class='verse-line'>{}</span>"},"vv81:6.3":{"pli":"Surabhiṃ sampavāyanti, ","html":"<span class='verse-line'>{}</span>"},"vv81:6.4":{"pli":"manuññā māluteritā. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:7.1":{"pli":"Tassā te ubhato passe, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:7.2":{"pli":"vanagumbā sumāpitā; ","html":"<span class='verse-line'>{}</span>"},"vv81:7.3":{"pli":"Upetā puppharukkhehi, ","html":"<span class='verse-line'>{}</span>"},"vv81:7.4":{"pli":"phalarukkhehi cūbhayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:8.1":{"pli":"Sovaṇṇapāde pallaṅke, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:8.2":{"pli":"muduke gonakatthate; ","html":"<span class='verse-line'>{}</span>"},"vv81:8.3":{"pli":"Nisinnaṃ devarājaṃva, ","html":"<span class='verse-line'>{}</span>"},"vv81:8.4":{"pli":"upatiṭṭhanti accharā. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:9.1":{"pli":"Sabbābharaṇasañchannā, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:9.2":{"pli":"nānāmālāvibhūsitā; ","html":"<span class='verse-line'>{}</span>"},"vv81:9.3":{"pli":"Ramenti taṃ mahiddhikaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv81:9.4":{"pli":"vasavattīva modasi. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:10.1":{"pli":"Bherisaṅkhamudiṅgāhi, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:10.2":{"pli":"vīṇāhi paṇavehi ca; ","html":"<span class='verse-line'>{}</span>"},"vv81:10.3":{"pli":"Ramasi ratisampanno, ","html":"<span class='verse-line'>{}</span>"},"vv81:10.4":{"pli":"naccagīte suvādite. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:11.1":{"pli":"Dibbā te vividhā rūpā, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:11.2":{"pli":"dibbā saddā atho rasā; ","html":"<span class='verse-line'>{}</span>"},"vv81:11.3":{"pli":"Gandhā ca te adhippetā, ","html":"<span class='verse-line'>{}</span>"},"vv81:11.4":{"pli":"phoṭṭhabbā ca manoramā. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:12.1":{"pli":"Tasmiṃ vimāne pavare, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:12.2":{"pli":"devaputta mahappabho; ","html":"<span class='verse-line'>{}</span>"},"vv81:12.3":{"pli":"Atirocasi vaṇṇena, ","html":"<span class='verse-line'>{}</span>"},"vv81:12.4":{"pli":"udayantova bhāṇumā. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:13.1":{"pli":"Dānassa te idaṃ phalaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:13.2":{"pli":"atho sīlassa vā pana; ","html":"<span class='verse-line'>{}</span>"},"vv81:13.3":{"pli":"Atho añjalikammassa, ","html":"<span class='verse-line'>{}</span>"},"vv81:13.4":{"pli":"taṃ me akkhāhi pucchito”. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:14.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:14.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv81:14.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:15.1":{"pli":"“Ahaṃ kapilavatthusmiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:15.2":{"pli":"sākiyānaṃ puruttame; ","html":"<span class='verse-line'>{}</span>"},"vv81:15.3":{"pli":"Suddhodanassa puttassa, ","html":"<span class='verse-line'>{}</span>"},"vv81:15.4":{"pli":"kaṇḍako sahajo ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:16.1":{"pli":"Yadā so aḍḍharattāyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:16.2":{"pli":"bodhāya mabhinikkhami; ","html":"<span class='verse-line'>{}</span>"},"vv81:16.3":{"pli":"So maṃ mudūhi pāṇīhi, ","html":"<span class='verse-line'>{}</span>"},"vv81:16.4":{"pli":"jāli tambanakhehi ca. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:17.1":{"pli":"Satthiṃ ākoṭayitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:17.2":{"pli":"vaha sammāti cabravi; ","html":"<span class='verse-line'>{}</span>"},"vv81:17.3":{"pli":"Ahaṃ lokaṃ tārayissaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv81:17.4":{"pli":"patto sambodhimuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:18.1":{"pli":"Taṃ me giraṃ suṇantassa, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:18.2":{"pli":"hāso me vipulo ahu; ","html":"<span class='verse-line'>{}</span>"},"vv81:18.3":{"pli":"Udaggacitto sumano, ","html":"<span class='verse-line'>{}</span>"},"vv81:18.4":{"pli":"abhisīsiṃ tadā ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:19.1":{"pli":"Abhirūḷhañca maṃ ñatvā, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:19.2":{"pli":"sakyaputtaṃ mahāyasaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv81:19.3":{"pli":"Udaggacitto mudito, ","html":"<span class='verse-line'>{}</span>"},"vv81:19.4":{"pli":"vahissaṃ purisuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:20.1":{"pli":"Paresaṃ vijitaṃ gantvā, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:20.2":{"pli":"uggatasmiṃ divākare; ","html":"<span class='verse-line'>{}</span>"},"vv81:20.3":{"pli":"Mamaṃ channañca ohāya, ","html":"<span class='verse-line'>{}</span>"},"vv81:20.4":{"pli":"anapekkho so apakkami. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:21.1":{"pli":"Tassa tambanakhe pāde, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:21.2":{"pli":"jivhāya parilehisaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv81:21.3":{"pli":"Gacchantañca mahāvīraṃ, ","html":"<span class='verse-line'>{}</span>"},"vv81:21.4":{"pli":"rudamāno udikkhisaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:22.1":{"pli":"Adassanenahaṃ tassa, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:22.2":{"pli":"sakyaputtassa sirīmato; ","html":"<span class='verse-line'>{}</span>"},"vv81:22.3":{"pli":"Alatthaṃ garukābādhaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv81:22.4":{"pli":"khippaṃ me maraṇaṃ ahu. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:23.1":{"pli":"Tasseva ānubhāvena, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:23.2":{"pli":"vimānaṃ āvasāmidaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv81:23.3":{"pli":"Sabbakāmaguṇopetaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv81:23.4":{"pli":"dibbaṃ devapuramhi ca. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:24.1":{"pli":"Yañca me ahuvā hāso, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:24.2":{"pli":"saddaṃ sutvāna bodhiyā; ","html":"<span class='verse-line'>{}</span>"},"vv81:24.3":{"pli":"Teneva kusalamūlena, ","html":"<span class='verse-line'>{}</span>"},"vv81:24.4":{"pli":"phusissaṃ āsavakkhayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:25.1":{"pli":"Sace hi bhante gaccheyyāsi, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:25.2":{"pli":"satthu buddhassa santike; ","html":"<span class='verse-line'>{}</span>"},"vv81:25.3":{"pli":"Mamāpi naṃ vacanena, ","html":"<span class='verse-line'>{}</span>"},"vv81:25.4":{"pli":"sirasā vajjāsi vandanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:26.1":{"pli":"Ahampi daṭṭhuṃ gacchissaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:26.2":{"pli":"jinaṃ appaṭipuggalaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv81:26.3":{"pli":"Dullabhaṃ dassanaṃ hoti, ","html":"<span class='verse-line'>{}</span>"},"vv81:26.4":{"pli":"lokanāthāna tādinan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:27.1":{"pli":"So kataññū katavedī, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:27.2":{"pli":"satthāraṃ upasaṅkami; ","html":"<span class='verse-line'>{}</span>"},"vv81:27.3":{"pli":"Sutvā giraṃ cakkhumato, ","html":"<span class='verse-line'>{}</span>"},"vv81:27.4":{"pli":"dhammacakkhuṃ visodhayi. ","html":"<span class='verse-line'>{}</span></p>"},"vv81:28.1":{"pli":"Visodhetvā diṭṭhigataṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv81:28.2":{"pli":"vicikicchaṃ vatāni ca; ","html":"<span class='verse-line'>{}</span>"},"vv81:28.3":{"pli":"Vanditvā satthuno pāde, ","html":"<span class='verse-line'>{}</span>"},"vv81:28.4":{"pli":"tatthevantaradhāyathāti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv81:29.1":{"pli":"Kaṇḍakavimānaṃ sattamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv82":{"vv82:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv82'><header><ul><li class='division'>{}</li>"},"vv82:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv82:0.3":{"pli":"Sunikkhittavagga ","html":"<li>{}</li></ul>"},"vv82:0.4":{"pli":"8. Anekavaṇṇavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv82:1.1":{"pli":"“Anekavaṇṇaṃ darasokanāsanaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv82:1.2":{"pli":"Vimānamāruyha anekacittaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv82:1.3":{"pli":"Parivārito accharāsaṅgaṇena, ","html":"<span class='verse-line'>{}</span>"},"vv82:1.4":{"pli":"Sunimmito bhūtapatīva modasi. ","html":"<span class='verse-line'>{}</span></p>"},"vv82:2.1":{"pli":"Samassamo natthi kuto panuttaro, ","html":"<p><span class='verse-line'>{}</span>"},"vv82:2.2":{"pli":"Yasena puññena ca iddhiyā ca; ","html":"<span class='verse-line'>{}</span>"},"vv82:2.3":{"pli":"Sabbe ca devā tidasagaṇā samecca, ","html":"<span class='verse-line'>{}</span>"},"vv82:2.4":{"pli":"Taṃ taṃ namassanti sasiṃva devā; ","html":"<span class='verse-line'>{}</span>"},"vv82:2.5":{"pli":"Imā ca te accharāyo samantato, ","html":"<span class='verse-line'>{}</span>"},"vv82:2.6":{"pli":"Naccanti gāyanti pamodayanti. ","html":"<span class='verse-line'>{}</span></p>"},"vv82:3.1":{"pli":"Deviddhipattosi mahānubhāvo, ","html":"<p><span class='verse-line'>{}</span>"},"vv82:3.2":{"pli":"Manussabhūto kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv82:3.3":{"pli":"Kenāsi evaṃ jalitānubhāvo, ","html":"<span class='verse-line'>{}</span>"},"vv82:3.4":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv82:4.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv82:4.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv82:4.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv82:5.1":{"pli":"“Ahaṃ bhadante ahuvāsi pubbe, ","html":"<p><span class='verse-line'>{}</span>"},"vv82:5.2":{"pli":"Sumedhanāmassa jinassa sāvako; ","html":"<span class='verse-line'>{}</span>"},"vv82:5.3":{"pli":"Puthujjano ananubodhohamasmi, ","html":"<span class='verse-line'>{}</span>"},"vv82:5.4":{"pli":"So satta vassāni paribbajissahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv82:6.1":{"pli":"Sohaṃ sumedhassa jinassa satthuno, ","html":"<p><span class='verse-line'>{}</span>"},"vv82:6.2":{"pli":"Parinibbutassoghatiṇṇassa tādino; ","html":"<span class='verse-line'>{}</span>"},"vv82:6.3":{"pli":"Ratanuccayaṃ hemajālena channaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv82:6.4":{"pli":"Vanditvā thūpasmiṃ manaṃ pasādayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv82:7.1":{"pli":"Na māsi dānaṃ na ca matthi dātuṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv82:7.2":{"pli":"Pare ca kho tattha samādapesiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv82:7.3":{"pli":"Pūjetha naṃ pūjanīyassa dhātuṃ, ","html":"<span class='verse-line'>{}</span>"},"vv82:7.4":{"pli":"Evaṃ kira saggamito gamissatha. ","html":"<span class='verse-line'>{}</span></p>"},"vv82:8.1":{"pli":"Tadeva kammaṃ kusalaṃ kataṃ mayā, ","html":"<p><span class='verse-line'>{}</span>"},"vv82:8.2":{"pli":"Sukhañca dibbaṃ anubhomi attanā; ","html":"<span class='verse-line'>{}</span>"},"vv82:8.3":{"pli":"Modāmahaṃ tidasagaṇassa majjhe, ","html":"<span class='verse-line'>{}</span>"},"vv82:8.4":{"pli":"Na tassa puññassa khayampi ajjhagan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv82:9.1":{"pli":"Anekavaṇṇavimānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}