{"vv43":{"vv43:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv43'><header><ul><li class='division'>{}</li>"},"vv43:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv43:0.3":{"pli":"Mañjiṭṭhakavagga ","html":"<li>{}</li></ul>"},"vv43:0.4":{"pli":"5. Kañjikadāyikāvimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv43:1.1":{"pli":"“Abhikkantena vaṇṇena, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv43:1.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv43:1.3":{"pli":"osadhī viya tārakā. ","html":"<span class='verse-line'>{}</span></p>"},"vv43:2.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv43:2.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv43:2.3":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv43:3.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv43:3.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv43:3.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv43:4.1":{"pli":"“Ahaṃ andhakavindamhi, ","html":"<p><span class='verse-line'>{}</span>"},"vv43:4.2":{"pli":"buddhassādiccabandhuno; ","html":"<span class='verse-line'>{}</span>"},"vv43:4.3":{"pli":"Adāsiṃ kolasampākaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv43:4.4":{"pli":"kañjikaṃ teladhūpitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv43:5.1":{"pli":"Pipphalyā lasuṇena ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv43:5.2":{"pli":"missaṃ lāmañjakena ca; ","html":"<span class='verse-line'>{}</span>"},"vv43:5.3":{"pli":"Adāsiṃ ujubhūtasmiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv43:5.4":{"pli":"vippasannena cetasā. ","html":"<span class='verse-line'>{}</span></p>"},"vv43:6.1":{"pli":"Yā mahesittaṃ kāreyya, ","html":"<p><span class='verse-line'>{}</span>"},"vv43:6.2":{"pli":"cakkavattissa rājino; ","html":"<span class='verse-line'>{}</span>"},"vv43:6.3":{"pli":"Nārī sabbaṅgakalyāṇī, ","html":"<span class='verse-line'>{}</span>"},"vv43:6.4":{"pli":"bhattu cānomadassikā; ","html":"<span class='verse-line'>{}</span>"},"vv43:6.5":{"pli":"Ekassa kañjikadānassa, ","html":"<span class='verse-line'>{}</span>"},"vv43:6.6":{"pli":"kalaṃ nāgghati soḷasiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv43:7.1":{"pli":"Sataṃ nikkhā sataṃ assā, ","html":"<p><span class='verse-line'>{}</span>"},"vv43:7.2":{"pli":"sataṃ assatarīrathā; ","html":"<span class='verse-line'>{}</span>"},"vv43:7.3":{"pli":"Sataṃ kaññāsahassāni, ","html":"<span class='verse-line'>{}</span>"},"vv43:7.4":{"pli":"āmuttamaṇikuṇḍalā; ","html":"<span class='verse-line'>{}</span>"},"vv43:7.5":{"pli":"Ekassa kañjikadānassa, ","html":"<span class='verse-line'>{}</span>"},"vv43:7.6":{"pli":"kalaṃ nāgghanti soḷasiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv43:8.1":{"pli":"Sataṃ hemavatā nāgā, ","html":"<p><span class='verse-line'>{}</span>"},"vv43:8.2":{"pli":"īsādantā urūḷhavā; ","html":"<span class='verse-line'>{}</span>"},"vv43:8.3":{"pli":"Suvaṇṇakacchā mātaṅgā, ","html":"<span class='verse-line'>{}</span>"},"vv43:8.4":{"pli":"hemakappanavāsasā; ","html":"<span class='verse-line'>{}</span>"},"vv43:8.5":{"pli":"Ekassa kañjikadānassa, ","html":"<span class='verse-line'>{}</span>"},"vv43:8.6":{"pli":"kalaṃ nāgghanti soḷasiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv43:9.1":{"pli":"Catunnamapi dīpānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv43:9.2":{"pli":"issaraṃ yodha kāraye; ","html":"<span class='verse-line'>{}</span>"},"vv43:9.3":{"pli":"Ekassa kañjikadānassa, ","html":"<span class='verse-line'>{}</span>"},"vv43:9.4":{"pli":"kalaṃ nāgghati soḷasin”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv43:10.1":{"pli":"Kañjikadāyikāvimānaṃ pañcamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv44":{"vv44:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv44'><header><ul><li class='division'>{}</li>"},"vv44:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv44:0.3":{"pli":"Mañjiṭṭhakavagga ","html":"<li>{}</li></ul>"},"vv44:0.4":{"pli":"6. Vihāravimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv44:1.1":{"pli":"“Abhikkantena vaṇṇena, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv44:1.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv44:1.3":{"pli":"osadhī viya tārakā. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:2.1":{"pli":"Tassā te naccamānāya, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:2.2":{"pli":"aṅgamaṅgehi sabbaso; ","html":"<span class='verse-line'>{}</span>"},"vv44:2.3":{"pli":"Dibbā saddā niccharanti, ","html":"<span class='verse-line'>{}</span>"},"vv44:2.4":{"pli":"savanīyā manoramā. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:3.1":{"pli":"Tassā te naccamānāya, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:3.2":{"pli":"aṅgamaṅgehi sabbaso; ","html":"<span class='verse-line'>{}</span>"},"vv44:3.3":{"pli":"Dibbā gandhā pavāyanti, ","html":"<span class='verse-line'>{}</span>"},"vv44:3.4":{"pli":"sucigandhā manoramā. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:4.1":{"pli":"Vivattamānā kāyena, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:4.2":{"pli":"yā veṇīsu piḷandhanā; ","html":"<span class='verse-line'>{}</span>"},"vv44:4.3":{"pli":"Tesaṃ suyyati nigghoso, ","html":"<span class='verse-line'>{}</span>"},"vv44:4.4":{"pli":"tūriye pañcaṅgike yathā. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:5.1":{"pli":"Vaṭaṃsakā vātadhutā, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:5.2":{"pli":"vātena sampakampitā; ","html":"<span class='verse-line'>{}</span>"},"vv44:5.3":{"pli":"Tesaṃ suyyati nigghoso, ","html":"<span class='verse-line'>{}</span>"},"vv44:5.4":{"pli":"tūriye pañcaṅgike yathā. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:6.1":{"pli":"Yāpi te sirasmiṃ mālā, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:6.2":{"pli":"sucigandhā manoramā; ","html":"<span class='verse-line'>{}</span>"},"vv44:6.3":{"pli":"Vāti gandho disā sabbā, ","html":"<span class='verse-line'>{}</span>"},"vv44:6.4":{"pli":"rukkho mañjūsako yathā. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:7.1":{"pli":"Ghāyase taṃ sucigandhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:7.2":{"pli":"rūpaṃ passasi amānusaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv44:7.3":{"pli":"Devate pucchitācikkha, ","html":"<span class='verse-line'>{}</span>"},"vv44:7.4":{"pli":"kissa kammassidaṃ phalan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:8.1":{"pli":"“Sāvatthiyaṃ mayhaṃ sakhī bhadante, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:8.2":{"pli":"Saṅghassa kāresi mahāvihāraṃ; ","html":"<span class='verse-line'>{}</span>"},"vv44:8.3":{"pli":"Tatthappasannā ahamānumodiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv44:8.4":{"pli":"Disvā agārañca piyañca metaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:9.1":{"pli":"Tāyeva me suddhanumodanāya, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:9.2":{"pli":"Laddhaṃ vimānabbhutadassaneyyaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv44:9.3":{"pli":"Samantato soḷasayojanāni, ","html":"<span class='verse-line'>{}</span>"},"vv44:9.4":{"pli":"Vehāyasaṃ gacchati iddhiyā mama. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:10.1":{"pli":"Kūṭāgārā nivesā me, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:10.2":{"pli":"vibhattā bhāgaso mitā; ","html":"<span class='verse-line'>{}</span>"},"vv44:10.3":{"pli":"Daddallamānā ābhanti, ","html":"<span class='verse-line'>{}</span>"},"vv44:10.4":{"pli":"samantā satayojanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:11.1":{"pli":"Pokkharañño ca me ettha, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:11.2":{"pli":"puthulomanisevitā; ","html":"<span class='verse-line'>{}</span>"},"vv44:11.3":{"pli":"Acchodakā vippasannā, ","html":"<span class='verse-line'>{}</span>"},"vv44:11.4":{"pli":"soṇṇavālukasanthatā. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:12.1":{"pli":"Nānāpadumasañchannā, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:12.2":{"pli":"puṇḍarīkasamotatā; ","html":"<span class='verse-line'>{}</span>"},"vv44:12.3":{"pli":"Surabhī sampavāyanti, ","html":"<span class='verse-line'>{}</span>"},"vv44:12.4":{"pli":"manuññā māluteritā. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:13.1":{"pli":"Jambuyo panasā tālā, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:13.2":{"pli":"nāḷikeravanāni ca; ","html":"<span class='verse-line'>{}</span>"},"vv44:13.3":{"pli":"Antonivesane jātā, ","html":"<span class='verse-line'>{}</span>"},"vv44:13.4":{"pli":"nānārukkhā aropimā. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:14.1":{"pli":"Nānātūriyasaṅghuṭṭhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:14.2":{"pli":"accharāgaṇaghositaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv44:14.3":{"pli":"Yopi maṃ supine passe, ","html":"<span class='verse-line'>{}</span>"},"vv44:14.4":{"pli":"sopi vitto siyā naro. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:15.1":{"pli":"Etādisaṃ abbhutadassaneyyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:15.2":{"pli":"Vimānaṃ sabbasopabhaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv44:15.3":{"pli":"Mama kammehi nibbattaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv44:15.4":{"pli":"Alaṃ puññāni kātave”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:16.1":{"pli":"“Tāyeva te suddhanumodanāya, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:16.2":{"pli":"Laddhaṃ vimānabbhutadassaneyyaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv44:16.3":{"pli":"Yā ceva sā dānamadāsi nārī, ","html":"<span class='verse-line'>{}</span>"},"vv44:16.4":{"pli":"Tassā gatiṃ brūhi kuhiṃ uppannā sā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:17.1":{"pli":"“Yā sā ahu mayhaṃ sakhī bhadante, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:17.2":{"pli":"Saṅghassa kāresi mahāvihāraṃ; ","html":"<span class='verse-line'>{}</span>"},"vv44:17.3":{"pli":"Viññātadhammā sā adāsi dānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv44:17.4":{"pli":"Uppannā nimmānaratīsu devesu. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:18.1":{"pli":"Pajāpatī tassa sunimmitassa, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:18.2":{"pli":"Acintiyā kammavipākā tassā; ","html":"<span class='verse-line'>{}</span>"},"vv44:18.3":{"pli":"Yametaṃ pucchasi kuhiṃ uppannā sāti, ","html":"<span class='verse-line'>{}</span>"},"vv44:18.4":{"pli":"Taṃ te viyākāsiṃ anaññathā ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:19.1":{"pli":"Tenahaññepi samādapetha, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:19.2":{"pli":"Saṅghassa dānāni dadātha vittā; ","html":"<span class='verse-line'>{}</span>"},"vv44:19.3":{"pli":"Dhammañca suṇātha pasannamānasā, ","html":"<span class='verse-line'>{}</span>"},"vv44:19.4":{"pli":"Sudullabho laddho manussalābho. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:20.1":{"pli":"Yaṃ maggaṃ maggādhipatī adesayi, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:20.2":{"pli":"Brahmassaro kañcanasannibhattaco; ","html":"<span class='verse-line'>{}</span>"},"vv44:20.3":{"pli":"Saṅghassa dānāni dadātha vittā, ","html":"<span class='verse-line'>{}</span>"},"vv44:20.4":{"pli":"Mahapphalā yattha bhavanti dakkhiṇā. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:21.1":{"pli":"Ye puggalā aṭṭha sataṃ pasatthā, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:21.2":{"pli":"Cattāri etāni yugāni honti; ","html":"<span class='verse-line'>{}</span>"},"vv44:21.3":{"pli":"Te dakkhiṇeyyā sugatassa sāvakā, ","html":"<span class='verse-line'>{}</span>"},"vv44:21.4":{"pli":"Etesu dinnāni mahapphalāni. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:22.1":{"pli":"Cattāro ca paṭipannā, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:22.2":{"pli":"cattāro ca phale ṭhitā; ","html":"<span class='verse-line'>{}</span>"},"vv44:22.3":{"pli":"Esa saṅgho ujubhūto, ","html":"<span class='verse-line'>{}</span>"},"vv44:22.4":{"pli":"paññāsīlasamāhito. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:23.1":{"pli":"Yajamānānaṃ manussānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:23.2":{"pli":"puññapekkhāna pāṇinaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv44:23.3":{"pli":"Karotaṃ opadhikaṃ puññaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv44:23.4":{"pli":"saṅghe dinnaṃ mahapphalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:24.1":{"pli":"Eso hi saṅgho vipulo mahaggato, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:24.2":{"pli":"Esappameyyo udadhīva sāgaro; ","html":"<span class='verse-line'>{}</span>"},"vv44:24.3":{"pli":"Etehi seṭṭhā naravīrasāvakā, ","html":"<span class='verse-line'>{}</span>"},"vv44:24.4":{"pli":"Pabhaṅkarā dhammamudīrayanti. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:25.1":{"pli":"Tesaṃ sudinnaṃ suhutaṃ suyiṭṭhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:25.2":{"pli":"Ye saṅghamuddissa dadanti dānaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv44:25.3":{"pli":"Sā dakkhiṇā saṅghagatā patiṭṭhitā, ","html":"<span class='verse-line'>{}</span>"},"vv44:25.4":{"pli":"Mahapphalā lokavidūna vaṇṇitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv44:26.1":{"pli":"Etādisaṃ yaññamanussarantā, ","html":"<p><span class='verse-line'>{}</span>"},"vv44:26.2":{"pli":"Ye vedajātā vicaranti loke; ","html":"<span class='verse-line'>{}</span>"},"vv44:26.3":{"pli":"Vineyya maccheramalaṃ samūlaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv44:26.4":{"pli":"Aninditā saggamupenti ṭhānan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv44:27.1":{"pli":"Vihāravimānaṃ chaṭṭhaṃ. ","html":"<p class='endsutta'>{}<br>"},"vv44:27.2":{"pli":"Bhāṇavāraṃ dutiyaṃ. ","html":"{}</p></article>"}},"vv45":{"vv45:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv45'><header><ul><li class='division'>{}</li>"},"vv45:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv45:0.3":{"pli":"Mañjiṭṭhakavagga ","html":"<li>{}</li></ul>"},"vv45:0.4":{"pli":"7. Caturitthivimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv45:1.1":{"pli":"“Abhikkantena vaṇṇena, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv45:1.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv45:1.3":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:2.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:2.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv45:2.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:3.1":{"pli":"“Indīvarānaṃ hatthakaṃ ahamadāsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:3.2":{"pli":"Bhikkhuno piṇḍāya carantassa; ","html":"<span class='verse-line'>{}</span>"},"vv45:3.3":{"pli":"Esikānaṃ uṇṇatasmiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv45:3.4":{"pli":"Nagaravare paṇṇakate ramme. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:4.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:4.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv45:4.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:5.1":{"pli":"“Abhikkantena vaṇṇena, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv45:5.3":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:6.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv45:6.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:7.1":{"pli":"“Nīluppalahatthakaṃ ahamadāsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:7.2":{"pli":"Bhikkhuno piṇḍāya carantassa; ","html":"<span class='verse-line'>{}</span>"},"vv45:7.3":{"pli":"Esikānaṃ uṇṇatasmiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv45:7.4":{"pli":"Nagaravare paṇṇakate ramme. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:8.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:8.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv45:8.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:9.1":{"pli":"“Abhikkantena vaṇṇena, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:9.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv45:9.3":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:10.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:10.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv45:10.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:11.1":{"pli":"“Odātamūlakaṃ haritapattaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:11.2":{"pli":"Udakasmiṃ sare jātaṃ ahamadāsiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv45:11.3":{"pli":"Bhikkhuno piṇḍāya carantassa, ","html":"<span class='verse-line'>{}</span>"},"vv45:11.4":{"pli":"Esikānaṃ uṇṇatasmiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv45:11.5":{"pli":"Nagaravare paṇṇakate ramme. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:12.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:12.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv45:12.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:13.1":{"pli":"“Abhikkantena vaṇṇena, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:13.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv45:13.3":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:14.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:14.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv45:14.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:15.1":{"pli":"“Ahaṃ sumanā sumanassa sumanamakuḷāni, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:15.2":{"pli":"Dantavaṇṇāni ahamadāsiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv45:15.3":{"pli":"Bhikkhuno piṇḍāya carantassa, ","html":"<span class='verse-line'>{}</span>"},"vv45:15.4":{"pli":"Esikānaṃ uṇṇatasmiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv45:15.5":{"pli":"Nagaravare paṇṇakate ramme. ","html":"<span class='verse-line'>{}</span></p>"},"vv45:16.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv45:16.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv45:16.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv45:17.1":{"pli":"Caturitthivimānaṃ sattamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv46":{"vv46:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv46'><header><ul><li class='division'>{}</li>"},"vv46:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv46:0.3":{"pli":"Mañjiṭṭhakavagga ","html":"<li>{}</li></ul>"},"vv46:0.4":{"pli":"8. Ambavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv46:1.1":{"pli":"“Dibbaṃ te ambavanaṃ rammaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv46:1.2":{"pli":"Pāsādettha mahallako; ","html":"<span class='verse-line'>{}</span>"},"vv46:1.3":{"pli":"Nānātūriyasaṅghuṭṭho, ","html":"<span class='verse-line'>{}</span>"},"vv46:1.4":{"pli":"Accharāgaṇaghosito. ","html":"<span class='verse-line'>{}</span></p>"},"vv46:2.1":{"pli":"Padīpo cettha jalati, ","html":"<p><span class='verse-line'>{}</span>"},"vv46:2.2":{"pli":"niccaṃ sovaṇṇayo mahā; ","html":"<span class='verse-line'>{}</span>"},"vv46:2.3":{"pli":"Dussaphalehi rukkhehi, ","html":"<span class='verse-line'>{}</span>"},"vv46:2.4":{"pli":"samantā parivārito. ","html":"<span class='verse-line'>{}</span></p>"},"vv46:3.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv46:3.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv46:3.3":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv46:4.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv46:4.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv46:4.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv46:5.1":{"pli":"“Ahaṃ manussesu manussabhūtā, ","html":"<p><span class='verse-line'>{}</span>"},"vv46:5.2":{"pli":"Purimāya jātiyā manussaloke; ","html":"<span class='verse-line'>{}</span>"},"vv46:5.3":{"pli":"Vihāraṃ saṅghassa kāresiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv46:5.4":{"pli":"Ambehi parivāritaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv46:6.1":{"pli":"Pariyosite vihāre, ","html":"<p><span class='verse-line'>{}</span>"},"vv46:6.2":{"pli":"kārente niṭṭhite mahe; ","html":"<span class='verse-line'>{}</span>"},"vv46:6.3":{"pli":"Ambehi chādayitvāna, ","html":"<span class='verse-line'>{}</span>"},"vv46:6.4":{"pli":"katvā dussamaye phale. ","html":"<span class='verse-line'>{}</span></p>"},"vv46:7.1":{"pli":"Padīpaṃ tattha jāletvā, ","html":"<p><span class='verse-line'>{}</span>"},"vv46:7.2":{"pli":"bhojayitvā gaṇuttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv46:7.3":{"pli":"Niyyādesiṃ taṃ saṅghassa, ","html":"<span class='verse-line'>{}</span>"},"vv46:7.4":{"pli":"pasannā sehi pāṇibhi. ","html":"<span class='verse-line'>{}</span></p>"},"vv46:8.1":{"pli":"Tena me ambavanaṃ rammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv46:8.2":{"pli":"pāsādettha mahallako; ","html":"<span class='verse-line'>{}</span>"},"vv46:8.3":{"pli":"Nānātūriyasaṅghuṭṭho, ","html":"<span class='verse-line'>{}</span>"},"vv46:8.4":{"pli":"accharāgaṇaghosito. ","html":"<span class='verse-line'>{}</span></p>"},"vv46:9.1":{"pli":"Padīpo cettha jalati, ","html":"<p><span class='verse-line'>{}</span>"},"vv46:9.2":{"pli":"niccaṃ sovaṇṇayo mahā; ","html":"<span class='verse-line'>{}</span>"},"vv46:9.3":{"pli":"Dussaphalehi rukkhehi, ","html":"<span class='verse-line'>{}</span>"},"vv46:9.4":{"pli":"samantā parivārito. ","html":"<span class='verse-line'>{}</span></p>"},"vv46:10.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv46:10.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv46:10.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv46:11.1":{"pli":"Ambavimānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv47":{"vv47:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv47'><header><ul><li class='division'>{}</li>"},"vv47:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv47:0.3":{"pli":"Mañjiṭṭhakavagga ","html":"<li>{}</li></ul>"},"vv47:0.4":{"pli":"9. Pītavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv47:1.1":{"pli":"“Pītavatthe pītadhaje, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv47:1.2":{"pli":"pītālaṅkārabhūsite; ","html":"<span class='verse-line'>{}</span>"},"vv47:1.3":{"pli":"Pītacandanalittaṅge, ","html":"<span class='verse-line'>{}</span>"},"vv47:1.4":{"pli":"pītauppalamālinī. ","html":"<span class='verse-line'>{}</span></p>"},"vv47:2.1":{"pli":"Pītapāsādasayane, ","html":"<p><span class='verse-line'>{}</span>"},"vv47:2.2":{"pli":"pītāsane pītabhājane; ","html":"<span class='verse-line'>{}</span>"},"vv47:2.3":{"pli":"Pītachatte pītarathe, ","html":"<span class='verse-line'>{}</span>"},"vv47:2.4":{"pli":"pītasse pītabījane. ","html":"<span class='verse-line'>{}</span></p>"},"vv47:3.1":{"pli":"Kiṃ kammamakarī bhadde, ","html":"<p><span class='verse-line'>{}</span>"},"vv47:3.2":{"pli":"pubbe mānusake bhave; ","html":"<span class='verse-line'>{}</span>"},"vv47:3.3":{"pli":"Devate pucchitācikkha, ","html":"<span class='verse-line'>{}</span>"},"vv47:3.4":{"pli":"kissa kammassidaṃ phalan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv47:4.1":{"pli":"“Kosātakī nāma latatthi bhante, ","html":"<p><span class='verse-line'>{}</span>"},"vv47:4.2":{"pli":"Tittikā anabhicchitā; ","html":"<span class='verse-line'>{}</span>"},"vv47:4.3":{"pli":"Tassā cattāri pupphāni, ","html":"<span class='verse-line'>{}</span>"},"vv47:4.4":{"pli":"Thūpaṃ abhihariṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv47:5.1":{"pli":"Satthu sarīramuddissa, ","html":"<p><span class='verse-line'>{}</span>"},"vv47:5.2":{"pli":"vippasannena cetasā; ","html":"<span class='verse-line'>{}</span>"},"vv47:5.3":{"pli":"Nāssa maggaṃ avekkhissaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv47:5.4":{"pli":"na taggamanasā satī. ","html":"<span class='verse-line'>{}</span></p>"},"vv47:6.1":{"pli":"Tato maṃ avadhī gāvī, ","html":"<p><span class='verse-line'>{}</span>"},"vv47:6.2":{"pli":"thūpaṃ apattamānasaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv47:6.3":{"pli":"Tañcāhaṃ abhisañceyyaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv47:6.4":{"pli":"bhiyyo nūna ito siyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv47:7.1":{"pli":"Tena kammena devinda, ","html":"<p><span class='verse-line'>{}</span>"},"vv47:7.2":{"pli":"maghavā devakuñjara; ","html":"<span class='verse-line'>{}</span>"},"vv47:7.3":{"pli":"Pahāya mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv47:7.4":{"pli":"tava sahabyamāgatā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv47:8.1":{"pli":"Idaṃ sutvā tidasādhipati, ","html":"<p><span class='verse-line'>{}</span>"},"vv47:8.2":{"pli":"Maghavā devakuñjaro; ","html":"<span class='verse-line'>{}</span>"},"vv47:8.3":{"pli":"Tāvatiṃse pasādento, ","html":"<span class='verse-line'>{}</span>"},"vv47:8.4":{"pli":"Mātaliṃ etadabravi. ","html":"<span class='verse-line'>{}</span></p>"},"vv47:9.1":{"pli":"“Passa mātali accheraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv47:9.2":{"pli":"cittaṃ kammaphalaṃ idaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv47:9.3":{"pli":"Appakampi kataṃ deyyaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv47:9.4":{"pli":"puññaṃ hoti mahapphalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv47:10.1":{"pli":"Natthi citte pasannamhi, ","html":"<p><span class='verse-line'>{}</span>"},"vv47:10.2":{"pli":"appakā nāma dakkhiṇā; ","html":"<span class='verse-line'>{}</span>"},"vv47:10.3":{"pli":"Tathāgate vā sambuddhe, ","html":"<span class='verse-line'>{}</span>"},"vv47:10.4":{"pli":"atha vā tassa sāvake. ","html":"<span class='verse-line'>{}</span></p>"},"vv47:11.1":{"pli":"Ehi mātali amhepi, ","html":"<p><span class='verse-line'>{}</span>"},"vv47:11.2":{"pli":"bhiyyo bhiyyo mahemase; ","html":"<span class='verse-line'>{}</span>"},"vv47:11.3":{"pli":"Tathāgatassa dhātuyo, ","html":"<span class='verse-line'>{}</span>"},"vv47:11.4":{"pli":"sukho puññāna muccayo. ","html":"<span class='verse-line'>{}</span></p>"},"vv47:12.1":{"pli":"Tiṭṭhante nibbute cāpi, ","html":"<p><span class='verse-line'>{}</span>"},"vv47:12.2":{"pli":"same citte samaṃ phalaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv47:12.3":{"pli":"Cetopaṇidhihetu hi, ","html":"<span class='verse-line'>{}</span>"},"vv47:12.4":{"pli":"sattā gacchanti suggatiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv47:13.1":{"pli":"Bahūnaṃ vata atthāya, ","html":"<p><span class='verse-line'>{}</span>"},"vv47:13.2":{"pli":"uppajjanti tathāgatā; ","html":"<span class='verse-line'>{}</span>"},"vv47:13.3":{"pli":"Yattha kāraṃ karitvāna, ","html":"<span class='verse-line'>{}</span>"},"vv47:13.4":{"pli":"saggaṃ gacchanti dāyakā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv47:14.1":{"pli":"Pītavimānaṃ navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv48":{"vv48:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv48'><header><ul><li class='division'>{}</li>"},"vv48:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv48:0.3":{"pli":"Mañjiṭṭhakavagga ","html":"<li>{}</li></ul>"},"vv48:0.4":{"pli":"10. Ucchuvimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv48:1.1":{"pli":"“Obhāsayitvā pathaviṃ sadevakaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv48:1.2":{"pli":"Atirocasi candimasūriyā viya; ","html":"<span class='verse-line'>{}</span>"},"vv48:1.3":{"pli":"Siriyā ca vaṇṇena yasena tejasā, ","html":"<span class='verse-line'>{}</span>"},"vv48:1.4":{"pli":"Brahmāva deve tidase sahindake. ","html":"<span class='verse-line'>{}</span></p>"},"vv48:2.1":{"pli":"Pucchāmi taṃ uppalamāladhārinī, ","html":"<p><span class='verse-line'>{}</span>"},"vv48:2.2":{"pli":"Āveḷinī kañcanasannibhattace; ","html":"<span class='verse-line'>{}</span>"},"vv48:2.3":{"pli":"Alaṅkate uttamavatthadhārinī, ","html":"<span class='verse-line'>{}</span>"},"vv48:2.4":{"pli":"Kā tvaṃ subhe devate vandase mamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv48:3.1":{"pli":"Kiṃ tvaṃ pure kammamakāsi attanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv48:3.2":{"pli":"Manussabhūtā purimāya jātiyā; ","html":"<span class='verse-line'>{}</span>"},"vv48:3.3":{"pli":"Dānaṃ suciṇṇaṃ atha sīlasaññamaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv48:3.4":{"pli":"Kenupapannā sugatiṃ yasassinī; ","html":"<span class='verse-line'>{}</span>"},"vv48:3.5":{"pli":"Devate pucchitācikkha, ","html":"<span class='verse-line'>{}</span>"},"vv48:3.6":{"pli":"Kissa kammassidaṃ phalan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv48:4.1":{"pli":"“Idāni bhante imameva gāmaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv48:4.2":{"pli":"Piṇḍāya amhāka gharaṃ upāgami; ","html":"<span class='verse-line'>{}</span>"},"vv48:4.3":{"pli":"Tato te ucchussa adāsi khaṇḍikaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv48:4.4":{"pli":"Pasannacittā atulāya pītiyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv48:5.1":{"pli":"Sassu ca pacchā anuyuñjate mamaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv48:5.2":{"pli":"Kahaṃ nu ucchuṃ vadhuke avākiri; ","html":"<span class='verse-line'>{}</span>"},"vv48:5.3":{"pli":"Na chaḍḍitaṃ no pana khāditaṃ mayā, ","html":"<span class='verse-line'>{}</span>"},"vv48:5.4":{"pli":"Santassa bhikkhussa sayaṃ adāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv48:6.1":{"pli":"Tuyhaṃ nvidaṃ issariyaṃ atho mama, ","html":"<p><span class='verse-line'>{}</span>"},"vv48:6.2":{"pli":"Itissā sassu paribhāsate mamaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv48:6.3":{"pli":"Leḍḍuṃ gahetvā pahāraṃ adāsi me, ","html":"<span class='verse-line'>{}</span>"},"vv48:6.4":{"pli":"Tato cutā kālakatāmhi devatā. ","html":"<span class='verse-line'>{}</span></p>"},"vv48:7.1":{"pli":"Tadeva kammaṃ kusalaṃ kataṃ mayā, ","html":"<p><span class='verse-line'>{}</span>"},"vv48:7.2":{"pli":"Sukhañca kammaṃ anubhomi attanā; ","html":"<span class='verse-line'>{}</span>"},"vv48:7.3":{"pli":"Devehi saddhiṃ paricārayāmahaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv48:7.4":{"pli":"Modāmahaṃ kāmaguṇehi pañcahi. ","html":"<span class='verse-line'>{}</span></p>"},"vv48:8.1":{"pli":"Tadeva kammaṃ kusalaṃ kataṃ mayā, ","html":"<p><span class='verse-line'>{}</span>"},"vv48:8.2":{"pli":"Sukhañca kammaṃ anubhomi attanā; ","html":"<span class='verse-line'>{}</span>"},"vv48:8.3":{"pli":"Devindaguttā tidasehi rakkhitā, ","html":"<span class='verse-line'>{}</span>"},"vv48:8.4":{"pli":"Samappitā kāmaguṇehi pañcahi. ","html":"<span class='verse-line'>{}</span></p>"},"vv48:9.1":{"pli":"Etādisaṃ puññaphalaṃ anappakaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv48:9.2":{"pli":"Mahāvipākā mama ucchudakkhiṇā; ","html":"<span class='verse-line'>{}</span>"},"vv48:9.3":{"pli":"Devehi saddhiṃ paricārayāmahaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv48:9.4":{"pli":"Modāmahaṃ kāmaguṇehi pañcahi. ","html":"<span class='verse-line'>{}</span></p>"},"vv48:10.1":{"pli":"Etādisaṃ puññaphalaṃ anappakaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv48:10.2":{"pli":"Mahājutikā mama ucchudakkhiṇā; ","html":"<span class='verse-line'>{}</span>"},"vv48:10.3":{"pli":"Devindaguttā tidasehi rakkhitā, ","html":"<span class='verse-line'>{}</span>"},"vv48:10.4":{"pli":"Sahassanettoriva nandane vane. ","html":"<span class='verse-line'>{}</span></p>"},"vv48:11.1":{"pli":"Tuvañca bhante anukampakaṃ viduṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv48:11.2":{"pli":"Upecca vandiṃ kusalañca pucchisaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv48:11.3":{"pli":"Tato te ucchussa adāsiṃ khaṇḍikaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv48:11.4":{"pli":"Pasannacittā atulāya pītiyā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv48:12.1":{"pli":"Ucchuvimānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv49":{"vv49:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv49'><header><ul><li class='division'>{}</li>"},"vv49:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv49:0.3":{"pli":"Mañjiṭṭhakavagga ","html":"<li>{}</li></ul>"},"vv49:0.4":{"pli":"11. Vandanavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv49:1.1":{"pli":"“Abhikkantena vaṇṇena, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv49:1.2":{"pli":"yā tvaṃ tiṭṭhasi devate; ","html":"<span class='verse-line'>{}</span>"},"vv49:1.3":{"pli":"Obhāsentī disā sabbā, ","html":"<span class='verse-line'>{}</span>"},"vv49:1.4":{"pli":"osadhī viya tārakā. ","html":"<span class='verse-line'>{}</span></p>"},"vv49:2.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv49:2.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv49:2.3":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv49:3.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv49:3.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv49:3.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv49:4.1":{"pli":"“Ahaṃ manussesu manussabhūtā, ","html":"<p><span class='verse-line'>{}</span>"},"vv49:4.2":{"pli":"Disvāna samaṇe sīlavante; ","html":"<span class='verse-line'>{}</span>"},"vv49:4.3":{"pli":"Pādāni vanditvā manaṃ pasādayiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv49:4.4":{"pli":"Vittā cahaṃ añjalikaṃ akāsiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv49:5.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv49:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv49:5.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv49:6.1":{"pli":"Vandanavimānaṃ ekādasamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv5":{"vv5:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv5'><header><ul><li class='division'>{}</li>"},"vv5:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv5:0.3":{"pli":"Pīṭhavagga ","html":"<li>{}</li></ul>"},"vv5:0.4":{"pli":"5. Kuñjaravimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv5:1.1":{"pli":"“Kuñjaro te varāroho, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv5:1.2":{"pli":"Nānāratanakappano; ","html":"<span class='verse-line'>{}</span>"},"vv5:1.3":{"pli":"Ruciro thāmavā javasampanno, ","html":"<span class='verse-line'>{}</span>"},"vv5:1.4":{"pli":"Ākāsamhi samīhati. ","html":"<span class='verse-line'>{}</span></p>"},"vv5:2.1":{"pli":"Padumi padmapattakkhi, ","html":"<p><span class='verse-line'>{}</span>"},"vv5:2.2":{"pli":"padmuppalajutindharo; ","html":"<span class='verse-line'>{}</span>"},"vv5:2.3":{"pli":"Padmacuṇṇābhikiṇṇaṅgo, ","html":"<span class='verse-line'>{}</span>"},"vv5:2.4":{"pli":"soṇṇapokkharamāladhā. ","html":"<span class='verse-line'>{}</span></p>"},"vv5:3.1":{"pli":"Padumānusaṭaṃ maggaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv5:3.2":{"pli":"padmapattavibhūsitaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv5:3.3":{"pli":"Ṭhitaṃ vaggu manugghātī, ","html":"<span class='verse-line'>{}</span>"},"vv5:3.4":{"pli":"mitaṃ gacchati vāraṇo. ","html":"<span class='verse-line'>{}</span></p>"},"vv5:4.1":{"pli":"Tassa pakkamamānassa, ","html":"<p><span class='verse-line'>{}</span>"},"vv5:4.2":{"pli":"soṇṇakaṃsā ratissarā; ","html":"<span class='verse-line'>{}</span>"},"vv5:4.3":{"pli":"Tesaṃ suyyati nigghoso, ","html":"<span class='verse-line'>{}</span>"},"vv5:4.4":{"pli":"tūriye pañcaṅgike yathā. ","html":"<span class='verse-line'>{}</span></p>"},"vv5:5.1":{"pli":"Tassa nāgassa khandhamhi, ","html":"<p><span class='verse-line'>{}</span>"},"vv5:5.2":{"pli":"sucivatthā alaṅkatā; ","html":"<span class='verse-line'>{}</span>"},"vv5:5.3":{"pli":"Mahantaṃ accharāsaṅghaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv5:5.4":{"pli":"vaṇṇena atirocasi. ","html":"<span class='verse-line'>{}</span></p>"},"vv5:6.1":{"pli":"Dānassa te idaṃ phalaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv5:6.2":{"pli":"atho sīlassa vā pana; ","html":"<span class='verse-line'>{}</span>"},"vv5:6.3":{"pli":"Atho añjalikammassa, ","html":"<span class='verse-line'>{}</span>"},"vv5:6.4":{"pli":"taṃ me akkhāhi pucchitā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv5:7.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv5:7.2":{"pli":"moggallānena pucchitā; ","html":"<span class='verse-line'>{}</span>"},"vv5:7.3":{"pli":"Pañhaṃ puṭṭhā viyākāsi, ","html":"<span class='verse-line'>{}</span>"},"vv5:7.4":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv5:8.1":{"pli":"“Disvāna guṇasampannaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv5:8.2":{"pli":"jhāyiṃ jhānarataṃ sataṃ; ","html":"<span class='verse-line'>{}</span>"},"vv5:8.3":{"pli":"Adāsiṃ pupphābhikiṇṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv5:8.4":{"pli":"āsanaṃ dussasanthataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv5:9.1":{"pli":"Upaḍḍhaṃ padmamālāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv5:9.2":{"pli":"āsanassa samantato; ","html":"<span class='verse-line'>{}</span>"},"vv5:9.3":{"pli":"Abbhokirissaṃ pattehi, ","html":"<span class='verse-line'>{}</span>"},"vv5:9.4":{"pli":"pasannā sehi pāṇibhi. ","html":"<span class='verse-line'>{}</span></p>"},"vv5:10.1":{"pli":"Tassa kammakusalassa, ","html":"<p><span class='verse-line'>{}</span>"},"vv5:10.2":{"pli":"idaṃ me īdisaṃ phalaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv5:10.3":{"pli":"Sakkāro garukāro ca, ","html":"<span class='verse-line'>{}</span>"},"vv5:10.4":{"pli":"devānaṃ apacitā ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv5:11.1":{"pli":"Yo ve sammāvimuttānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv5:11.2":{"pli":"santānaṃ brahmacārinaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv5:11.3":{"pli":"Pasanno āsanaṃ dajjā, ","html":"<span class='verse-line'>{}</span>"},"vv5:11.4":{"pli":"evaṃ nande yathā ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv5:12.1":{"pli":"Tasmā hi attakāmena, ","html":"<p><span class='verse-line'>{}</span>"},"vv5:12.2":{"pli":"mahattamabhikaṅkhatā; ","html":"<span class='verse-line'>{}</span>"},"vv5:12.3":{"pli":"Āsanaṃ dātabbaṃ hoti, ","html":"<span class='verse-line'>{}</span>"},"vv5:12.4":{"pli":"sarīrantimadhārinan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv5:13.1":{"pli":"Kuñjaravimānaṃ pañcamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}