{"tha-ap22":{"tha-ap22:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap22'><header><ul><li class='division'>{}</li>"},"tha-ap22:0.2":{"pli":"Sīhāsaniyavagga ","html":"<li>{}</li></ul>"},"tha-ap22:0.3":{"pli":"Sumaṅgalattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap22:1.1":{"pli":"“Āhutiṃ yiṭṭhukāmohaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap22:1.2":{"pli":"paṭiyādetvāna bhojanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:1.3":{"pli":"Brāhmaṇe paṭimānento, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:1.4":{"pli":"visāle māḷake ṭhito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:2.1":{"pli":"Athaddasāsiṃ sambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:2.2":{"pli":"piyadassiṃ mahāyasaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:2.3":{"pli":"Sabbalokavinetāraṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:2.4":{"pli":"sayambhuṃ aggapuggalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:3.1":{"pli":"Bhagavantaṃ jutimantaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:3.2":{"pli":"sāvakehi purakkhataṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:3.3":{"pli":"Ādiccamiva rocantaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:3.4":{"pli":"rathiyaṃ paṭipannakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:4.1":{"pli":"Añjaliṃ paggahetvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:4.2":{"pli":"sakaṃ cittaṃ pasādayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:4.3":{"pli":"Manasāva nimantesiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:4.4":{"pli":"‘āgacchatu mahāmuni’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:5.1":{"pli":"Mama saṅkappamaññāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:5.2":{"pli":"satthā loke anuttaro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:5.3":{"pli":"Khīṇāsavasahassehi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:5.4":{"pli":"mama dvāraṃ upāgami. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:6.1":{"pli":"Namo te purisājañña, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:6.2":{"pli":"namo te purisuttama; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:6.3":{"pli":"Pāsādaṃ abhirūhitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:6.4":{"pli":"sīhāsane nisīdataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:7.1":{"pli":"Danto dantaparivāro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:7.2":{"pli":"tiṇṇo tārayataṃ varo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:7.3":{"pli":"Pāsādaṃ abhirūhitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:7.4":{"pli":"nisīdi pavarāsane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:8.1":{"pli":"Yaṃ me atthi sake gehe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:8.2":{"pli":"āmisaṃ paccupaṭṭhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:8.3":{"pli":"Tāhaṃ buddhassa pādāsiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:8.4":{"pli":"pasanno sehi pāṇibhi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:9.1":{"pli":"Pasannacitto sumano, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:9.2":{"pli":"vedajāto katañjalī; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:9.3":{"pli":"Buddhaseṭṭhaṃ namassāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:9.4":{"pli":"aho buddhassuḷāratā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:10.1":{"pli":"Aṭṭhannaṃ payirūpāsataṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:10.2":{"pli":"bhuñjaṃ khīṇāsavā bahū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:10.3":{"pli":"Tuyheveso ānubhāvo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:10.4":{"pli":"saraṇaṃ taṃ upemahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:11.1":{"pli":"Piyadassī ca bhagavā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:11.2":{"pli":"lokajeṭṭho narāsabho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:11.3":{"pli":"Bhikkhusaṅghe nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:11.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:12.1":{"pli":"‘Yo so saṅghaṃ abhojesi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:12.2":{"pli":"ujubhūtaṃ samāhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:12.3":{"pli":"Tathāgatañca sambuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:12.4":{"pli":"suṇātha mama bhāsato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:13.1":{"pli":"Sattavīsatikkhattuṃ so, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:13.2":{"pli":"devarajjaṃ karissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:13.3":{"pli":"Sakakammābhiraddho so, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:13.4":{"pli":"devaloke ramissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:14.1":{"pli":"Dasa aṭṭha cakkhattuṃ so, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:14.2":{"pli":"cakkavattī bhavissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:14.3":{"pli":"Pathabyā rajjaṃ pañcasataṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:14.4":{"pli":"vasudhaṃ āvasissati’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:15.1":{"pli":"Araññavanamoggayha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:15.2":{"pli":"kānanaṃ byagghasevitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:15.3":{"pli":"Padhānaṃ padahitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:15.4":{"pli":"kilesā jhāpitā mayā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:16.1":{"pli":"Aṭṭhārase kappasate, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:16.2":{"pli":"yaṃ dānamadadiṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:16.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:16.4":{"pli":"bhattadānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:17.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:17.2":{"pli":"Vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:17.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:17.4":{"pli":"Kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap22:18.1":{"pli":"Itthaṃ sudaṃ āyasmā sumaṅgalo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap22:19.1":{"pli":"Sumaṅgalattherassāpadānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"tha-ap22:20.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"tha-ap22:21.1":{"pli":"Sīhāsanī ekathambhī, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"tha-ap22:21.2":{"pli":"nando ca cūḷapanthako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:21.3":{"pli":"Pilindarāhulo ceva, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:21.4":{"pli":"vaṅganto raṭṭhapālako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap22:22.1":{"pli":"Sopāko maṅgalo ceva, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap22:22.2":{"pli":"daseva dutiye vagge; ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:22.3":{"pli":"Satañca aṭṭhatiṃsa ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap22:22.4":{"pli":"gāthā cettha pakāsitā. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap22:23.1":{"pli":"Sīhāsaniyavaggo dutiyo. ","html":"<p class='endsection'>{}</p></article>"}},"tha-ap220":{"tha-ap220:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap220'><header><ul><li class='division'>{}</li>"},"tha-ap220:0.2":{"pli":"Hatthivagga ","html":"<li>{}</li></ul>"},"tha-ap220:0.3":{"pli":"Akkantasaññakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap220:1.1":{"pli":"“Kusāṭakaṃ gahetvāna, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap220:1.2":{"pli":"upajjhāyassahaṃ pure; ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:1.3":{"pli":"Mantañca anusikkhāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:1.4":{"pli":"ganthādosassa pattiyā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap220:2.1":{"pli":"Addasaṃ virajaṃ buddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap220:2.2":{"pli":"Āhutīnaṃ paṭiggahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:2.3":{"pli":"Usabhaṃ pavaraṃ aggaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:2.4":{"pli":"Tissaṃ buddhaṃ gaṇuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap220:3.1":{"pli":"Kusāṭakaṃ pattharitaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap220:3.2":{"pli":"akkamantaṃ naruttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:3.3":{"pli":"Samuggataṃ mahāvīraṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:3.4":{"pli":"lokajeṭṭhaṃ narāsabhaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap220:4.1":{"pli":"Disvā taṃ lokapajjotaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap220:4.2":{"pli":"vimalaṃ candasannibhaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:4.3":{"pli":"Avandiṃ satthuno pāde, ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:4.4":{"pli":"vippasannena cetasā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap220:5.1":{"pli":"Catunnavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap220:5.2":{"pli":"yaṃ adāsiṃ kusāṭakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:5.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:5.4":{"pli":"kusāṭakassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap220:6.1":{"pli":"Sattatiṃse ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap220:6.2":{"pli":"eko āsiṃ janādhipo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:6.3":{"pli":"Sunando nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:6.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap220:7.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap220:7.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap220:7.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap220:8.1":{"pli":"Itthaṃ sudaṃ āyasmā akkantasaññako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap220:9.1":{"pli":"Akkantasaññakattherassāpadānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap221":{"tha-ap221:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap221'><header><ul><li class='division'>{}</li>"},"tha-ap221:0.2":{"pli":"Hatthivagga ","html":"<li>{}</li></ul>"},"tha-ap221:0.3":{"pli":"Sappidāyakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap221:1.1":{"pli":"“Nisinno pāsādavare, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap221:1.2":{"pli":"nārīgaṇapurakkhato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:1.3":{"pli":"Byādhitaṃ samaṇaṃ disvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:1.4":{"pli":"abhināmesahaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap221:2.1":{"pli":"Upaviṭṭhaṃ mahāvīraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap221:2.2":{"pli":"devadevaṃ narāsabhaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:2.3":{"pli":"Sappitelaṃ mayā dinnaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:2.4":{"pli":"siddhatthassa mahesino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap221:3.1":{"pli":"Passaddhadarathaṃ disvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap221:3.2":{"pli":"vippasannamukhindriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:3.3":{"pli":"Vanditvā satthuno pāde, ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:3.4":{"pli":"anusaṃsāvayiṃ pure. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap221:4.1":{"pli":"Disvā maṃ suppasannattaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap221:4.2":{"pli":"iddhiyā pāramiṅgato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:4.3":{"pli":"Nabhaṃ abbhuggamī dhīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:4.4":{"pli":"haṃsarājāva ambare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap221:5.1":{"pli":"Catunnavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap221:5.2":{"pli":"yaṃ dānamadadiṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:5.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:5.4":{"pli":"sappitelassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap221:6.1":{"pli":"Ito sattarase kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap221:6.2":{"pli":"jutideva sanāmako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:6.3":{"pli":"Sattaratanasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:6.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap221:7.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap221:7.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap221:7.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap221:8.1":{"pli":"Itthaṃ sud