{"tha-ap23":{"tha-ap23:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap23'><header><ul><li class='division'>{}</li>"},"tha-ap23:0.2":{"pli":"Subhūtivagga ","html":"<li>{}</li></ul>"},"tha-ap23:0.3":{"pli":"Subhūtittheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap23:1.1":{"pli":"“Himavantassāvidūre, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap23:1.2":{"pli":"nisabho nāma pabbato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:1.3":{"pli":"Assamo sukato mayhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:1.4":{"pli":"paṇṇasālā sumāpitā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:2.1":{"pli":"Kosiyo nāma nāmena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:2.2":{"pli":"jaṭilo uggatāpano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:2.3":{"pli":"Ekākiyo adutiyo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:2.4":{"pli":"vasāmi nisabhe tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:3.1":{"pli":"Phalaṃ mūlañca paṇṇañca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:3.2":{"pli":"na bhuñjāmi ahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:3.3":{"pli":"Pavattaṃva supātāhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:3.4":{"pli":"upajīvāmi tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:4.1":{"pli":"Nāhaṃ kopemi ājīvaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:4.2":{"pli":"cajamānopi jīvitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:4.3":{"pli":"Ārādhemi sakaṃ cittaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:4.4":{"pli":"vivajjemi anesanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:5.1":{"pli":"Rāgūpasaṃhitaṃ cittaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:5.2":{"pli":"yadā uppajjate mama; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:5.3":{"pli":"Sayaṃva paccavekkhāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:5.4":{"pli":"ekaggo taṃ damemahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:6.1":{"pli":"‘Rajjase rajjanīye ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:6.2":{"pli":"dussanīye ca dussase; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:6.3":{"pli":"Muyhase mohanīye ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:6.4":{"pli":"nikkhamassu vanā tuvaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:7.1":{"pli":"Visuddhānaṃ ayaṃ vāso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:7.2":{"pli":"nimmalānaṃ tapassinaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:7.3":{"pli":"Mā kho visuddhaṃ dūsesi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:7.4":{"pli":"nikkhamassu vanā tuvaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:8.1":{"pli":"Agāriko bhavitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:8.2":{"pli":"yadā puttaṃ labhissasi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:8.3":{"pli":"Ubhopi mā virādhesi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:8.4":{"pli":"nikkhamassu vanā tuvaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:9.1":{"pli":"Chavālātaṃ yathā kaṭṭhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:9.2":{"pli":"na kvaci kiccakārakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:9.3":{"pli":"Neva gāme araññe vā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:9.4":{"pli":"na hi taṃ kaṭṭhasammataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:10.1":{"pli":"Chavālātūpamo tvaṃsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:10.2":{"pli":"na gihī nāpi saññato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:10.3":{"pli":"Ubhato muttako ajja, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:10.4":{"pli":"nikkhamassu vanā tuvaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:11.1":{"pli":"Siyā nu kho tava etaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:11.2":{"pli":"ko pajānāti te idaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:11.3":{"pli":"Saddhādhuraṃ vahisi me, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:11.4":{"pli":"kosajjabahulāya ca. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:12.1":{"pli":"Jigucchissanti taṃ viññū, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:12.2":{"pli":"asuciṃ nāgariko yathā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:12.3":{"pli":"Ākaḍḍhitvāna isayo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:12.4":{"pli":"codayissanti taṃ sadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:13.1":{"pli":"Taṃ viññū pavadissanti, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:13.2":{"pli":"samatikkantasāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:13.3":{"pli":"Saṃvāsaṃ alabhanto hi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:13.4":{"pli":"kathaṃ jīvihisi tuvaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:14.1":{"pli":"Tidhā pabhinnaṃ mātaṅgaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:14.2":{"pli":"kuñjaraṃ saṭṭhihāyanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:14.3":{"pli":"Balī nāgo upagantvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:14.4":{"pli":"yūthā nīharate gajaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:15.1":{"pli":"Yūthā vinissaṭo santo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:15.2":{"pli":"sukhaṃ sātaṃ na vindati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:15.3":{"pli":"Dukkhito vimano hoti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:15.4":{"pli":"pajjhāyanto pavedhati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:16.1":{"pli":"Tatheva jaṭilā tampi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:16.2":{"pli":"nīharissanti dummatiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:16.3":{"pli":"Tehi tvaṃ nissaṭo santo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:16.4":{"pli":"sukhaṃ sātaṃ na lacchasi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:17.1":{"pli":"Divā vā yadi vā rattiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:17.2":{"pli":"sokasallasamappito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:17.3":{"pli":"Ḍayhasi pariḷāhena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:17.4":{"pli":"gajo yūthāva nissaṭo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:18.1":{"pli":"Jātarūpaṃ yathā kūṭaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:18.2":{"pli":"neva jhāyati katthaci; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:18.3":{"pli":"Tathā sīlavihīno tvaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:18.4":{"pli":"na jhāyissasi katthaci. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:19.1":{"pli":"Agāraṃ vasamānopi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:19.2":{"pli":"kathaṃ jīvihisi tuvaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:19.3":{"pli":"Mattikaṃ pettikañcāpi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:19.4":{"pli":"natthi te nihitaṃ dhanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:20.1":{"pli":"Sayaṃ kammaṃ karitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:20.2":{"pli":"gatte sedaṃ pamocayaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:20.3":{"pli":"Evaṃ jīvihisi gehe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:20.4":{"pli":"sādhu te taṃ na ruccati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:21.1":{"pli":"Evāhaṃ tattha vāremi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:21.2":{"pli":"saṅkilesagataṃ manaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:21.3":{"pli":"Nānādhammakathaṃ katvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:21.4":{"pli":"pāpā cittaṃ nivārayiṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:22.1":{"pli":"Evaṃ me viharantassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:22.2":{"pli":"appamādavihārino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:22.3":{"pli":"Tiṃsavassasahassāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:22.4":{"pli":"vipine me atikkamuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:23.1":{"pli":"Appamādarataṃ disvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:23.2":{"pli":"uttamatthaṃ gavesakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:23.3":{"pli":"Padumuttarasambuddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:23.4":{"pli":"āgacchi mama santikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:24.1":{"pli":"Timbarūsakavaṇṇābho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:24.2":{"pli":"appameyyo anūpamo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:24.3":{"pli":"Rūpenāsadiso buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:24.4":{"pli":"ākāse caṅkamī tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:25.1":{"pli":"Suphullo sālarājāva, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:25.2":{"pli":"vijjūvabbhaghanantare; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:25.3":{"pli":"Ñāṇenāsadiso buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:25.4":{"pli":"ākāse caṅkamī tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:26.1":{"pli":"Sīharājāvasambhīto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:26.2":{"pli":"gajarājāva dappito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:26.3":{"pli":"Lāsito byaggharājāva, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:26.4":{"pli":"ākāse caṅkamī tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:27.1":{"pli":"Siṅgīnikkhasavaṇṇābho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:27.2":{"pli":"khadiraṅgārasannibho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:27.3":{"pli":"Maṇi yathā jotiraso, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:27.4":{"pli":"ākāse caṅkamī tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:28.1":{"pli":"Visuddhakelāsanibho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:28.2":{"pli":"puṇṇamāyeva candimā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:28.3":{"pli":"Majjhanhikeva sūriyo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:28.4":{"pli":"ākāse caṅkamī tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:29.1":{"pli":"Disvā nabhe caṅkamantaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:29.2":{"pli":"evaṃ cintesahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:29.3":{"pli":"‘Devo nu kho ayaṃ satto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:29.4":{"pli":"udāhu manujo ayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:30.1":{"pli":"Na me suto vā diṭṭho vā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:30.2":{"pli":"mahiyā ediso naro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:30.3":{"pli":"Api mantapadaṃ atthi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:30.4":{"pli":"ayaṃ satthā bhavissati’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:31.1":{"pli":"Evāhaṃ cintayitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:31.2":{"pli":"sakaṃ cittaṃ pasādayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:31.3":{"pli":"Nānāpupphañca gandhañca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:31.4":{"pli":"sannipātesahaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:32.1":{"pli":"Pupphāsanaṃ paññapetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:32.2":{"pli":"sādhucittaṃ manoramaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:32.3":{"pli":"Narasārathinaṃ aggaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:32.4":{"pli":"idaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:33.1":{"pli":"‘Idaṃ me āsanaṃ vīra, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:33.2":{"pli":"paññattaṃ tavanucchavaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:33.3":{"pli":"Hāsayanto mamaṃ cittaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:33.4":{"pli":"nisīda kusumāsane’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:34.1":{"pli":"Nisīdi tattha bhagavā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:34.2":{"pli":"asambhītova kesarī; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:34.3":{"pli":"Sattarattindivaṃ buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:34.4":{"pli":"pavare kusumāsane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:35.1":{"pli":"Namassamāno aṭṭhāsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:35.2":{"pli":"sattarattindivaṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:35.3":{"pli":"Vuṭṭhahitvā samādhimhā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:35.4":{"pli":"satthā loke anuttaro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:35.5":{"pli":"Mama kammaṃ pakittento, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:35.6":{"pli":"idaṃ vacanamabravi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:36.1":{"pli":"‘Bhāvehi buddhānussatiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:36.2":{"pli":"bhāvanānamanuttaraṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:36.3":{"pli":"Imaṃ satiṃ bhāvayitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:36.4":{"pli":"pūrayissasi mānasaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:37.1":{"pli":"Tiṃsakappasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:37.2":{"pli":"devaloke ramissasi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:37.3":{"pli":"Asītikkhattuṃ devindo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:37.4":{"pli":"devarajjaṃ karissasi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:37.5":{"pli":"Sahassakkhattuṃ cakkavattī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:37.6":{"pli":"rājā raṭṭhe bhavissasi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:38.1":{"pli":"Padesarajjaṃ vipulaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:38.2":{"pli":"gaṇanāto asaṅkhiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:38.3":{"pli":"Anubhossasi taṃ sabbaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:38.4":{"pli":"buddhānussatiyā phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:39.1":{"pli":"Bhavābhave saṃsaranto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:39.2":{"pli":"mahābhogaṃ labhissasi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:39.3":{"pli":"Bhoge te ūnatā natthi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:39.4":{"pli":"buddhānussatiyā phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:40.1":{"pli":"Kappasatasahassamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:40.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:40.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:40.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:41.1":{"pli":"Asītikoṭiṃ chaḍḍetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:41.2":{"pli":"dāse kammakare bahū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:41.3":{"pli":"Gotamassa bhagavato, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:41.4":{"pli":"sāsane pabbajissasi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:42.1":{"pli":"Ārādhayitvā sambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:42.2":{"pli":"gotamaṃ sakyapuṅgavaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:42.3":{"pli":"Subhūti nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:42.4":{"pli":"hessati satthu sāvako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:43.1":{"pli":"Bhikkhusaṅghe nisīditvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:43.2":{"pli":"dakkhiṇeyyaguṇamhi taṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:43.3":{"pli":"Tathāraṇavihāre ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:43.4":{"pli":"dvīsu agge ṭhapessati’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:44.1":{"pli":"Idaṃ vatvāna sambuddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:44.2":{"pli":"jalajuttamanāmako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:44.3":{"pli":"Nabhaṃ abbhuggamī vīro, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:44.4":{"pli":"haṃsarājāva ambare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:45.1":{"pli":"Sāsito lokanāthena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:45.2":{"pli":"namassitvā tathāgataṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:45.3":{"pli":"Sadā bhāvemi mudito, ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:45.4":{"pli":"buddhānussatimuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap23:46.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap23:46.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap23:46.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span