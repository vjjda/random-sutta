,"tha-ap18:13.3":{"pli":"Gotamassa bhagavato, ","html":"<span class='verse-line'>{}</span>"},"tha-ap18:13.4":{"pli":"atrajo so bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap18:14.1":{"pli":"Sacevaseyya agāraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap18:14.2":{"pli":"cakkavattī bhaveyya so; ","html":"<span class='verse-line'>{}</span>"},"tha-ap18:14.3":{"pli":"Aṭṭhānametaṃ yaṃ tādī, ","html":"<span class='verse-line'>{}</span>"},"tha-ap18:14.4":{"pli":"agāre ratimajjhagā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap18:15.1":{"pli":"Nikkhamitvā agāramhā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap18:15.2":{"pli":"pabbajissati subbato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap18:15.3":{"pli":"Rāhulo nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap18:15.4":{"pli":"arahā so bhavissati’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap18:16.1":{"pli":"Kikīva aṇḍaṃ rakkheyya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap18:16.2":{"pli":"cāmarī viya vāladhiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap18:16.3":{"pli":"Nipako sīlasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap18:16.4":{"pli":"evaṃ rakkhiṃ mahāmuni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap18:17.1":{"pli":"Tassāhaṃ dhammamaññāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap18:17.2":{"pli":"vihāsiṃ sāsane rato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap18:17.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap18:17.4":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap18:18.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap18:18.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"tha-ap18:18.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap18:18.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap18:19.1":{"pli":"Itthaṃ sudaṃ āyasmā rāhulo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap18:20.1":{"pli":"Rāhulattherassāpadānaṃ chaṭṭhaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap180":{"tha-ap180:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap180'><header><ul><li class='division'>{}</li>"},"tha-ap180:0.2":{"pli":"Kumudavagga ","html":"<li>{}</li></ul>"},"tha-ap180:0.3":{"pli":"Maṇipūjakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap180:1.1":{"pli":"“Orena himavantassa, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap180:1.2":{"pli":"nadikā sampavattatha; ","html":"<span class='verse-line'>{}</span>"},"tha-ap180:1.3":{"pli":"Tassā cānupakhettamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap180:1.4":{"pli":"sayambhū vasate tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap180:2.1":{"pli":"Maṇiṃ paggayha pallaṅkaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap180:2.2":{"pli":"sādhucittaṃ manoramaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap180:2.3":{"pli":"Pasannacitto sumano, ","html":"<span class='verse-line'>{}</span>"},"tha-ap180:2.4":{"pli":"buddhassa abhiropayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap180:3.1":{"pli":"Catunnavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap180:3.2":{"pli":"yaṃ maṇiṃ abhiropayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap180:3.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap180:3.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap180:4.1":{"pli":"Ito ca dvādase kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap180:4.2":{"pli":"sataraṃsīsanāmakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap180:4.3":{"pli":"Aṭṭha te āsuṃ rā