{"tha-ap16":{"tha-ap16:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap16'><header><ul><li class='division'>{}</li>"},"tha-ap16:0.2":{"pli":"Sīhāsaniyavagga ","html":"<li>{}</li></ul>"},"tha-ap16:0.3":{"pli":"4 Cūḷapanthakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap16:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap16:1.2":{"pli":"āhutīnaṃ paṭiggaho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:1.3":{"pli":"Gaṇamhā vūpakaṭṭho so, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:1.4":{"pli":"himavante vasī tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:2.1":{"pli":"Ahampi himavantamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:2.2":{"pli":"vasāmi assame tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:2.3":{"pli":"Acirāgataṃ mahāvīraṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:2.4":{"pli":"upesiṃ lokanāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:3.1":{"pli":"Pupphacchattaṃ gahetvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:3.2":{"pli":"upagacchiṃ narāsabhaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:3.3":{"pli":"Samādhiṃ samāpajjantaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:3.4":{"pli":"antarāyamakāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:4.1":{"pli":"Ubho hatthehi paggayha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:4.2":{"pli":"pupphacchattaṃ adāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:4.3":{"pli":"Paṭiggahesi bhagavā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:4.4":{"pli":"padumuttaro mahāmuni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:5.1":{"pli":"Sabbe devā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:5.2":{"pli":"himavantaṃ upenti te; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:5.3":{"pli":"Sādhukāraṃ pavattesuṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:5.4":{"pli":"anumodissati cakkhumā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:6.1":{"pli":"Idaṃ vatvāna te devā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:6.2":{"pli":"upagacchuṃ naruttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:6.3":{"pli":"Ākāse dhārayantassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:6.4":{"pli":"padumacchattamuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:7.1":{"pli":"Satapattachattaṃ paggayha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:7.2":{"pli":"adāsi tāpaso mama; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:7.3":{"pli":"‘Tamahaṃ kittayissāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:7.4":{"pli":"suṇātha mama bhāsato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:8.1":{"pli":"Pañcavīsatikappāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:8.2":{"pli":"devarajjaṃ karissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:8.3":{"pli":"Catuttiṃsatikkhattuñca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:8.4":{"pli":"cakkavattī bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:9.1":{"pli":"Yaṃ yaṃ yoniṃ saṃsarati, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:9.2":{"pli":"devattaṃ atha mānusaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:9.3":{"pli":"Abbhokāse patiṭṭhantaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:9.4":{"pli":"padumaṃ dhārayissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:10.1":{"pli":"Kappasatasahassamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:10.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:10.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:10.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:11.1":{"pli":"Pakāsite pāvacane, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:11.2":{"pli":"manussattaṃ labhissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:11.3":{"pli":"Manomayamhi kāyamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:11.4":{"pli":"uttamo so bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:12.1":{"pli":"Dve bhātaro bhavissanti, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:12.2":{"pli":"ubhopi panthakavhayā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:12.3":{"pli":"Anubhotvā uttamatthaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:12.4":{"pli":"jotayissanti sāsanaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:13.1":{"pli":"Sohaṃ aṭṭhārasavasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:13.2":{"pli":"pabbajiṃ anagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:13.3":{"pli":"Visesāhaṃ na vindāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:13.4":{"pli":"sakyaputtassa sāsane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:14.1":{"pli":"Dandhā mayhaṃ gatī āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:14.2":{"pli":"paribhūto pure ahuṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:14.3":{"pli":"Bhātā ca maṃ paṇāmesi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:14.4":{"pli":"gaccha dāni sakaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:15.1":{"pli":"Sohaṃ paṇāmito santo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:15.2":{"pli":"saṅghārāmassa koṭṭhake; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:15.3":{"pli":"Dummano tattha aṭṭhāsiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:15.4":{"pli":"sāmaññasmiṃ apekkhavā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:16.1":{"pli":"Bhagavā tattha āgacchi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:16.2":{"pli":"sīsaṃ mayhaṃ parāmasi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:16.3":{"pli":"Bāhāya maṃ gahetvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:16.4":{"pli":"saṅghārāmaṃ pavesayi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:17.1":{"pli":"Anukampāya me satthā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:17.2":{"pli":"adāsi pādapuñchaniṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:17.3":{"pli":"Evaṃ suddhaṃ adhiṭṭhehi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:17.4":{"pli":"ekamantamadhiṭṭhahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:18.1":{"pli":"Hatthehi tamahaṃ gayha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:18.2":{"pli":"sariṃ kokanadaṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:18.3":{"pli":"Tattha cittaṃ vimucci me, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:18.4":{"pli":"arahattaṃ apāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:19.1":{"pli":"Manomayesu kāyesu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:19.2":{"pli":"sabbattha pāramiṃ gato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:19.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:19.4":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap16:20.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap16:20.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:20.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap16:20.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap16:21.1":{"pli":"Itthaṃ sudaṃ āyasmā cūḷapanthako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap16:22.1":{"pli":"Cūḷapanthakattherassāpadānaṃ catutthaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap160":{"tha-ap160:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap160'><header><ul><li class='division'>{}</li>"},"tha-ap160:0.2":{"pli":"Bandhujīvakavagga ","html":"<li>{}</li></ul>"},"tha-ap160:0.3":{"pli":"Nāgapupphiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap160:1.1":{"pli":"“Suvaccho nāma nāmena, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap160:1.2":{"pli":"brāhmaṇo mantapāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:1.3":{"pli":"Purakkhato sasissehi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:1.4":{"pli":"vasate pabbatantare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap160:2.1":{"pli":"Padumuttaro nāma jino, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap160:2.2":{"pli":"āhutīnaṃ paṭiggaho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:2.3":{"pli":"Mamuddharitukāmo so, ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:2.4":{"pli":"āgacchi mama santikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap160:3.1":{"pli":"Vehāsamhi caṅkamati, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap160:3.2":{"pli":"dhūpāyati jalate tathā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:3.3":{"pli":"Hāsaṃ mamaṃ viditvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:3.4":{"pli":"pakkāmi pācināmukho. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap160:4.1":{"pli":"Tañca acchariyaṃ disvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap160:4.2":{"pli":"abbhutaṃ lomahaṃsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:4.3":{"pli":"Nāgapupphaṃ gahetvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:4.4":{"pli":"gatamaggamhi okiriṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap160:5.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap160:5.2":{"pli":"yaṃ pupphaṃ okiriṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:5.3":{"pli":"Tena cittappasādena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:5.4":{"pli":"duggatiṃ nupapajjahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap160:6.1":{"pli":"Ekattiṃse kappasate, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap160:6.2":{"pli":"rājā āsi mahāraho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:6.3":{"pli":"Sattaratanasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:6.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap160:7.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap160:7.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap160:7.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap160:8.1":{"pli":"Itthaṃ sudaṃ āyasmā nāgapupphiyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap160:9.1":{"pli":"Nāgapupphiyattherassāpadānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap161":{"tha-ap161:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap161'><header><ul><li class='division'>{}</li>"},"tha-ap161:0.2":{"pli":"Bandhujīvakavagga ","html":"<li>{}</li></ul>"},"tha-ap161:0.3":{"pli":"Punnāgapupphiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap161:1.1":{"pli":"“Kānanaṃ vanamogayha, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap161:1.2":{"pli":"vasāmi luddako ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap161:1.3":{"pli":"Punnāgaṃ pupphitaṃ disvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap161:1.4":{"pli":"buddhaseṭṭhaṃ anussariṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap161:2.1":{"pli":"Taṃ pupphaṃ ocinitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap161:2.2":{"pli":"sugandhaṃ gandhitaṃ subhaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap161:2.3":{"pli":"Thūpaṃ karitvā puline, ","html":"<span class='verse-line'>{}</span>"},"tha-ap161:2.4":{"pli":"buddhassa abhiropayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap161:3.1":{"pli":"Dvenavute ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap161:3.2":{"pli":"yaṃ pupphamabhipūjayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap161:3.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap161:3.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap161:4.1":{"pli":"Ekamhi navute kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap161:4.2":{"pli":"eko āsiṃ tamonudo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap161:4.3":{"pli":"Sattaratanasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap161:4.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap161:5.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap161:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap161:5.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap161:6.1":{"pli":"Itthaṃ sudaṃ āyasmā punnāgapupphiyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap161:7.1":{"pli":"Punnāgapupphiyattherassāpadānaṃ navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap162":{"tha-ap162:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap162'><header><ul><li class='division'>{}</li>"},"tha-ap162:0.2":{"pli":"Bandhujīvakavagga ","html":"<li>{}</li></ul>"},"tha-ap162:0.3":{"pli":"Kumudadāyakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap162:1.1":{"pli":"“Himavantassāvidūre, ","html":"<blockquote class='g