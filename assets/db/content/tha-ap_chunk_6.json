},"tha-ap169:4.1":{"pli":"Sattasaṭṭhimhito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap169:4.2":{"pli":"eko sāmuddharo ahuṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap169:4.3":{"pli":"Sattaratanasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap169:4.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap169:5.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap169:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap169:5.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap169:6.1":{"pli":"Itthaṃ sudaṃ āyasmā yūthikapupphiyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap169:7.1":{"pli":"Yūthikapupphiyattherassāpadānaṃ sattamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap17":{"tha-ap17:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap17'><header><ul><li class='division'>{}</li>"},"tha-ap17:0.2":{"pli":"Sīhāsaniyavagga ","html":"<li>{}</li></ul>"},"tha-ap17:0.3":{"pli":"5 Pilindavacchattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap17:1.1":{"pli":"“Nibbute lokanāthamhi, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap17:1.2":{"pli":"sumedhe aggapuggale; ","html":"<span class='verse-line'>{}</span>"},"tha-ap17:1.3":{"pli":"Pasannacitto sumano, ","html":"<span class='verse-line'>{}</span>"},"tha-ap17:1.4":{"pli":"thūpapūjaṃ akāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap17:2.1":{"pli":"Ye ca khīṇāsavā tattha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap17:2.2":{"pli":"chaḷabhiññā mahiddhikā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap17:2.3":{"pli":"Tehaṃ tattha samānetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap17:2.4":{"pli":"saṅghabhattaṃ akāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap17:3.1":{"pli":"Su