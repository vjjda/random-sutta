{"vv34":{"vv34:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv34'><header><ul><li class='division'>{}</li>"},"vv34:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv34:0.3":{"pli":"Pāricchattakavagga ","html":"<li>{}</li></ul>"},"vv34:0.4":{"pli":"6. Daddallavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv34:1.1":{"pli":"“Daddallamānā vaṇṇena, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv34:1.2":{"pli":"yasasā ca yasassinī; ","html":"<span class='verse-line'>{}</span>"},"vv34:1.3":{"pli":"Sabbe deve tāvatiṃse, ","html":"<span class='verse-line'>{}</span>"},"vv34:1.4":{"pli":"vaṇṇena atirocasi. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:2.1":{"pli":"Dassanaṃ nābhijānāmi, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:2.2":{"pli":"idaṃ paṭhamadassanaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv34:2.3":{"pli":"Kasmā kāyā nu āgamma, ","html":"<span class='verse-line'>{}</span>"},"vv34:2.4":{"pli":"nāmena bhāsase maman”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:3.1":{"pli":"“Ahaṃ bhadde subhaddāsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:3.2":{"pli":"pubbe mānusake bhave; ","html":"<span class='verse-line'>{}</span>"},"vv34:3.3":{"pli":"Sahabhariyā ca te āsiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv34:3.4":{"pli":"bhaginī ca kaniṭṭhikā. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:4.1":{"pli":"Sā ahaṃ kāyassa bhedā, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:4.2":{"pli":"vippamuttā tato cutā; ","html":"<span class='verse-line'>{}</span>"},"vv34:4.3":{"pli":"Nimmānaratīnaṃ devānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv34:4.4":{"pli":"upapannā sahabyatan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:5.1":{"pli":"“Pahūtakatakalyāṇā, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:5.2":{"pli":"te deve yanti pāṇino; ","html":"<span class='verse-line'>{}</span>"},"vv34:5.3":{"pli":"Yesaṃ tvaṃ kittayissasi, ","html":"<span class='verse-line'>{}</span>"},"vv34:5.4":{"pli":"subhadde jātimattano. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:6.1":{"pli":"Atha tvaṃ kena vaṇṇena, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:6.2":{"pli":"kena vā anusāsitā; ","html":"<span class='verse-line'>{}</span>"},"vv34:6.3":{"pli":"Kīdiseneva dānena, ","html":"<span class='verse-line'>{}</span>"},"vv34:6.4":{"pli":"subbatena yasassinī. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:7.1":{"pli":"Yasaṃ etādisaṃ pattā, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:7.2":{"pli":"visesaṃ vipulamajjhagā; ","html":"<span class='verse-line'>{}</span>"},"vv34:7.3":{"pli":"Devate pucchitācikkha, ","html":"<span class='verse-line'>{}</span>"},"vv34:7.4":{"pli":"kissa kammassidaṃ phalan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:8.1":{"pli":"“Aṭṭheva piṇḍapātāni, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:8.2":{"pli":"yaṃ dānaṃ adadaṃ pure; ","html":"<span class='verse-line'>{}</span>"},"vv34:8.3":{"pli":"Dakkhiṇeyyassa saṅghassa, ","html":"<span class='verse-line'>{}</span>"},"vv34:8.4":{"pli":"pasannā sehi pāṇibhi. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:9.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:9.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv34:9.3":{"pli":"vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:10.1":{"pli":"“Ahaṃ tayā bahutare bhikkhū, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:10.2":{"pli":"Saññate brahmacārayo; ","html":"<span class='verse-line'>{}</span>"},"vv34:10.3":{"pli":"Tappesiṃ annapānena, ","html":"<span class='verse-line'>{}</span>"},"vv34:10.4":{"pli":"Pasannā sehi pāṇibhi. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:11.1":{"pli":"Tayā bahutaraṃ datvā, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:11.2":{"pli":"hīnakāyūpagā ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv34:11.3":{"pli":"Kathaṃ tvaṃ appataraṃ datvā, ","html":"<span class='verse-line'>{}</span>"},"vv34:11.4":{"pli":"visesaṃ vipulamajjhagā; ","html":"<span class='verse-line'>{}</span>"},"vv34:11.5":{"pli":"Devate pucchitācikkha, ","html":"<span class='verse-line'>{}</span>"},"vv34:11.6":{"pli":"kissa kammassidaṃ phalan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:12.1":{"pli":"“Manobhāvanīyo bhikkhu, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:12.2":{"pli":"sandiṭṭho me pure ahu; ","html":"<span class='verse-line'>{}</span>"},"vv34:12.3":{"pli":"Tāhaṃ bhattena nimantesiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv34:12.4":{"pli":"revataṃ attanaṭṭhamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:13.1":{"pli":"So me atthapurekkhāro, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:13.2":{"pli":"anukampāya revato; ","html":"<span class='verse-line'>{}</span>"},"vv34:13.3":{"pli":"Saṅghe dehīti maṃvoca, ","html":"<span class='verse-line'>{}</span>"},"vv34:13.4":{"pli":"tassāhaṃ vacanaṃ kariṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:14.1":{"pli":"Sā dakkhiṇā saṅghagatā, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:14.2":{"pli":"appameyye patiṭṭhitā; ","html":"<span class='verse-line'>{}</span>"},"vv34:14.3":{"pli":"Puggalesu tayā dinnaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv34:14.4":{"pli":"na taṃ tava mahapphalan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:15.1":{"pli":"“Idānevāhaṃ jānāmi, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:15.2":{"pli":"saṅghe dinnaṃ mahapphalaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv34:15.3":{"pli":"Sāhaṃ gantvā manussattaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv34:15.4":{"pli":"vadaññū vītamaccharā; ","html":"<span class='verse-line'>{}</span>"},"vv34:15.5":{"pli":"Saṅghe dānāni dassāmi, ","html":"<span class='verse-line'>{}</span>"},"vv34:15.6":{"pli":"appamattā punappunan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:16.1":{"pli":"“Kā esā devatā bhadde, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:16.2":{"pli":"tayā mantayate saha; ","html":"<span class='verse-line'>{}</span>"},"vv34:16.3":{"pli":"Sabbe deve tāvatiṃse, ","html":"<span class='verse-line'>{}</span>"},"vv34:16.4":{"pli":"vaṇṇena atirocatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:17.1":{"pli":"“Manussabhūtā devinda, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:17.2":{"pli":"pubbe mānusake bhave; ","html":"<span class='verse-line'>{}</span>"},"vv34:17.3":{"pli":"Sahabhariyā ca me āsi, ","html":"<span class='verse-line'>{}</span>"},"vv34:17.4":{"pli":"bhaginī ca kaniṭṭhikā; ","html":"<span class='verse-line'>{}</span>"},"vv34:17.5":{"pli":"Saṅghe dānāni datvāna, ","html":"<span class='verse-line'>{}</span>"},"vv34:17.6":{"pli":"katapuññā virocatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:18.1":{"pli":"“Dhammena pubbe bhaginī, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:18.2":{"pli":"tayā bhadde virocati; ","html":"<span class='verse-line'>{}</span>"},"vv34:18.3":{"pli":"Yaṃ saṅghamhi appameyye, ","html":"<span class='verse-line'>{}</span>"},"vv34:18.4":{"pli":"patiṭṭhāpesi dakkhiṇaṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:19.1":{"pli":"“Pucchito hi mayā buddho, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:19.2":{"pli":"gijjhakūṭamhi pabbate; ","html":"<span class='verse-line'>{}</span>"},"vv34:19.3":{"pli":"Vipākaṃ saṃvibhāgassa, ","html":"<span class='verse-line'>{}</span>"},"vv34:19.4":{"pli":"yattha dinnaṃ mahapphalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:20.1":{"pli":"Yajamānānaṃ manussānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:20.2":{"pli":"puññapekkhāna pāṇinaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv34:20.3":{"pli":"Karotaṃ opadhikaṃ puññaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv34:20.4":{"pli":"yattha dinnaṃ mahapphalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:21.1":{"pli":"Taṃ me buddho viyākāsi, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:21.2":{"pli":"jānaṃ kammaphalaṃ sakaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv34:21.3":{"pli":"Vipākaṃ saṃvibhāgassa, ","html":"<span class='verse-line'>{}</span>"},"vv34:21.4":{"pli":"yattha dinnaṃ mahapphalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:22.1":{"pli":"Cattāro ca paṭipannā, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:22.2":{"pli":"cattāro ca phale ṭhitā; ","html":"<span class='verse-line'>{}</span>"},"vv34:22.3":{"pli":"Esa saṅgho ujubhūto, ","html":"<span class='verse-line'>{}</span>"},"vv34:22.4":{"pli":"paññāsīlasamāhito. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:23.1":{"pli":"Yajamānānaṃ manussānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:23.2":{"pli":"puññapekkhāna pāṇinaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv34:23.3":{"pli":"Karotaṃ opadhikaṃ puññaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv34:23.4":{"pli":"saṅghe dinnaṃ mahapphalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:24.1":{"pli":"Eso hi saṅgho vipulo mahaggato, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:24.2":{"pli":"Esappameyyo udadhīva sāgaro; ","html":"<span class='verse-line'>{}</span>"},"vv34:24.3":{"pli":"Ete hi seṭṭhā naravīrasāvakā, ","html":"<span class='verse-line'>{}</span>"},"vv34:24.4":{"pli":"Pabhaṅkarā dhammamudīrayanti. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:25.1":{"pli":"Tesaṃ sudinnaṃ suhutaṃ suyiṭṭhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:25.2":{"pli":"Ye saṅghamuddissa dadanti dānaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv34:25.3":{"pli":"Sā dakkhiṇā saṅghagatā patiṭṭhitā, ","html":"<span class='verse-line'>{}</span>"},"vv34:25.4":{"pli":"Mahapphalā lokavidūna vaṇṇitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv34:26.1":{"pli":"Etādisaṃ yaññamanussarantā, ","html":"<p><span class='verse-line'>{}</span>"},"vv34:26.2":{"pli":"Ye vedajātā vicaranti loke; ","html":"<span class='verse-line'>{}</span>"},"vv34:26.3":{"pli":"Vineyya maccheramalaṃ samūlaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv34:26.4":{"pli":"Aninditā saggamupenti ṭhānan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv34:27.1":{"pli":"Daddallavimānaṃ chaṭṭhaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv35":{"vv35:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv35'><header><ul><li class='division'>{}</li>"},"vv35:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv35:0.3":{"pli":"Pāricchattakavagga ","html":"<li>{}</li></ul>"},"vv35:0.4":{"pli":"7. Pesavatīvimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv35:1.1":{"pli":"“Phalikarajatahemajālachannaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv35:1.2":{"pli":"Vividhacitratalamaddasaṃ surammaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv35:1.3":{"pli":"Byamhaṃ sunimmitaṃ toraṇūpapannaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv35:1.4":{"pli":"Rucakupakiṇṇamidaṃ subhaṃ vimānaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv35:2.1":{"pli":"Bhāti ca dasa disā nabheva suriyo, ","html":"<p><span class='verse-line'>{}</span>"},"vv35:2.2":{"pli":"Sarade tamonudo sahassaraṃsī; ","html":"<span class='verse-line'>{}</span>"},"vv35:2.3":{"pli":"Tathā tapati midaṃ tava vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv35:2.4":{"pli":"Jalamiva dhūmasikho nise nabhagge. ","html":"<span class='verse-line'>{}</span></p>"},"vv35:3.1":{"pli":"Musatīva nayanaṃ sateratāva, ","html":"<p><span class='verse-line'>{}</span>"},"vv35:3.2":{"pli":"Ākāse ṭhapitamidaṃ manuññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv35:3.3":{"pli":"Vīṇāmurajasammatāḷaghuṭṭhaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv35:3.4":{"pli":"Iddhaṃ indapuraṃ yathā tavedaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv35:4.1":{"pli":"Padumakumuduppalakuvalayaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv35:4.2":{"pli":"Yodhika bandhukanojakā ca santi; ","html":"<span class='verse-line'>{}</span>"},"vv35:4.3":{"pli":"Sālakusumitapupphitā asokā, ","html":"<span class='verse-line'>{}</span>"},"vv35:4.4":{"pli":"Vividhadumaggasugandhasevitamidaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv35:5.1":{"pli":"Saḷalalabujabhujaka saṃyuttā, ","html":"<p><span class='verse-line'>{}</span>"},"vv35:5.2":{"pli":"Kusakasuphullitalatāvalambinīhi; ","html":"<span class='verse-line'>{}</span>"},"vv35:5.3":{"pli":"Maṇijālasadisā yasassinī, ","html":"<span class='verse-line'>{}</span>"},"vv35:5.4":{"pli":"Rammā pokkharaṇī upaṭṭhitā te. ","html":"<span class='verse-line'>{}</span></p>"},"vv35:6.1":{"pli":"Udakaruhā ca yetthi pupphajātā, ","html":"<p><span class='verse-line'>{}</span>"},"vv35:6.2":{"pli":"Thalajā ye ca santi rukkhajātā; ","html":"<span class='verse-line'>{}</span>"},"vv35:6.3":{"pli":"Mānusakāmānussakā ca dibbā, ","html":"<span class='verse-line'>{}</span>"},"vv35:6.4":{"pli":"Sabbe tuyhaṃ nivesanamhi jātā. ","html":"<span class='verse-line'>{}</span></p>"},"vv35:7.1":{"pli":"Kissa saṃyamadamassayaṃ vipāko, ","html":"<p><span class='verse-line'>{}</span>"},"vv35:7.2":{"pli":"Kenāsi kammaphalenidhūpapannā; ","html":"<span class='verse-line'>{}</span>"},"vv35:7.3":{"pli":"Yathā ca te adhigatamidaṃ vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv35:7.4":{"pli":"Tadanupadaṃ avacāsiḷārapamhe”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv35:8.1":{"pli":"“Yathā ca me adhigatamidaṃ vimānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv35:8.2":{"pli":"Koñcamayūracakorasaṅghacaritaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv35:8.3":{"pli":"Dibyapilavahaṃsarājaciṇṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv35:8.4":{"pli":"Dijakāraṇḍavakokilābhinaditaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv35:9.1":{"pli":"Nānāsantānakapuppharukkhavividhā, ","html":"<p><span class='verse-line'>{}</span>"},"vv35:9.2":{"pli":"Pāṭalijambuasokarukkhavantaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv35:9.3":{"pli":"Yathā ca me adhigatamidaṃ vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv35:9.4":{"pli":"Taṃ te pavedayāmi suṇohi bhante. ","html":"<span class='verse-line'>{}</span></p>"},"vv35:10.1":{"pli":"Magadhavarapuratthimena, ","html":"<p><span class='verse-line'>{}</span>"},"vv35:10.2":{"pli":"Nāḷakagāmo nāma atthi bhante; ","html":"<span class='verse-line'>{}</span>"},"vv35:10.3":{"pli":"Tattha ahosiṃ pure suṇisā, ","html":"<span class='verse-line'>{}</span>"},"vv35:10.4":{"pli":"Pesavatīti tattha jāniṃsu mamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv35:11.1":{"pli":"Sāhamapacitatthadhammakusalaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv35:11.2":{"pli":"Devamanussapūjitaṃ mahantaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv35:11.3":{"pli":"Upatissaṃ nibbutamappameyyaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv35:11.4":{"pli":"Muditamanā kusumehi abbhukiriṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv35:12.1":{"pli":"Paramagatigatañca pūjayitvā, ","html":"<p><span class='verse-line'>{}</span>"},"vv35:12.2":{"pli":"Antimadehadharaṃ isiṃ uḷāraṃ; ","html":"<span class='verse-line'>{}</span>"},"vv35:12.3":{"pli":"Pahāya mānusakaṃ samussayaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv35:12.4":{"pli":"Tidasagatā idha māvasāmi ṭhānan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv35:13.1":{"pli":"Pesavatīvimānaṃ sattamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv36":{"vv36:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv36'><header><ul><li class='division'>{}</li>"},"vv36:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv36:0.3":{"pli":"Pāricchattakavagga ","html":"<li>{}</li></ul>"},"vv36:0.4":{"pli":"8. Mallikāvimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv36:1.1":{"pli":"“Pītavatthe pītadhaje, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv36:1.2":{"pli":"pītālaṅkārabhūsite; ","html":"<span class='verse-line'>{}</span>"},"vv36:1.3":{"pli":"Pītantarāhi vaggūhi, ","html":"<span class='verse-line'>{}</span>"},"vv36:1.4":{"pli":"apiḷandhāva sobhasi. ","html":"<span class='verse-line'>{}</span></p>"},"vv36:2.1":{"pli":"Kā kambukāyūradhare, ","html":"<p><span class='verse-line'>{}</span>"},"vv36:2.2":{"pli":"kañcanāveḷabhūsite; ","html":"<span class='verse-line'>{}</span>"},"vv36:2.3":{"pli":"Hemajālakasañchanne, ","html":"<span class='verse-line'>{}</span>"},"vv36:2.4":{"pli":"nānāratanamālinī. ","html":"<span class='verse-line'>{}</span></p>"},"vv36:3.1":{"pli":"Sovaṇṇamayā lohitaṅgamayā ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv36:3.2":{"pli":"Muttāmayā veḷuriyamayā ca; ","html":"<span class='verse-line'>{}</span>"},"vv36:3.3":{"pli":"Masāragallā sahalohitaṅgā, ","html":"<span class='verse-line'>{}</span>"},"vv36:3.4":{"pli":"Pārevatakkhīhi maṇīhi cittatā. ","html":"<span class='verse-line'>{}</span></p>"},"vv36:4.1":{"pli":"Koci koci ettha mayūrasussaro, ","html":"<p><span class='verse-line'>{}</span>"},"vv36:4.2":{"pli":"Haṃsassa rañño karavīkasussaro; ","html":"<span class='verse-line'>{}</span>"},"vv36:4.3":{"pli":"Tesaṃ saro suyyati vaggurūpo, ","html":"<span class='verse-line'>{}</span>"},"vv36:4.4":{"pli":"Pañcaṅgikaṃ tūriyamivappavāditaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv36:5.1":{"pli":"Ratho ca te subho vaggu, ","html":"<p><span class='verse-line'>{}</span>"},"vv36:5.2":{"pli":"nānāratanacittito; ","html":"<span class='verse-line'>{}</span>"},"vv36:5.3":{"pli":"Nānāvaṇṇāhi dhātūhi, ","html":"<span class='verse-line'>{}</span>"},"vv36:5.4":{"pli":"suvibhattova sobhati. ","html":"<span class='verse-line'>{}</span></p>"},"vv36:6.1":{"pli":"Tasmiṃ rathe kañcanabimbavaṇṇe, ","html":"<p><span class='verse-line'>{}</span>"},"vv36:6.2":{"pli":"Yā tvaṃ ṭhitā bhāsasimaṃ padesaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv36:6.3":{"pli":"Devate pucchitācikkha, ","html":"<span class='verse-line'>{}</span>"},"vv36:6.4":{"pli":"Kissa kammassidaṃ phalan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv36:7.1":{"pli":"“Sovaṇṇajālaṃ maṇisoṇṇacittitaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv36:7.2":{"pli":"Muttācitaṃ hemajālena channaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv36:7.3":{"pli":"Parinibbute gotame appameyye, ","html":"<span class='verse-line'>{}</span>"},"vv36:7.4":{"pli":"Pasannacittā ahamābhiropayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv36:8.1":{"pli":"Tāhaṃ kammaṃ karitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"vv36:8.2":{"pli":"kusalaṃ buddhavaṇṇitaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv36:8.3":{"pli":"Apetasokā sukhitā, ","html":"<span class='verse-line'>{}</span>"},"vv36:8.4":{"pli":"sampamodāmanāmayā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv36:9.1":{"pli":"Mallikāvimānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv37":{"vv37:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv37'><header><ul><li class='division'>{}</li>"},"vv37:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv37:0.3":{"pli":"Pāricchattakavagga ","html":"<li>{}</li></ul>"},"vv37:0.4":{"pli":"9. Visālakkhivimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv37:1.1":{"pli":"“Kā nāma tvaṃ visālakkhi, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv37:1.2":{"pli":"ramme cittalatāvane; ","html":"<span class='verse-line'>{}</span>"},"vv37:1.3":{"pli":"Samantā anupariyāsi, ","html":"<span class='verse-line'>{}</span>"},"vv37:1.4":{"pli":"nārīgaṇapurakkhatā. ","html":"<span class='verse-line'>{}</span></p>"},"vv37:2.1":{"pli":"Yadā devā tāvatiṃsā, ",