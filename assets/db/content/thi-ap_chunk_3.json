{"thi-ap18":{"thi-ap18:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap18'><header><ul><li class='division'>{}</li>"},"thi-ap18:0.2":{"pli":"Ekūposathikavagga ","html":"<li>{}</li></ul>"},"thi-ap18:0.3":{"pli":"Khemātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap18:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap18:1.2":{"pli":"sabbadhammesu cakkhumā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:1.3":{"pli":"Ito satasahassamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:1.4":{"pli":"kappe uppajji nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:2.1":{"pli":"Tadāhaṃ haṃsavatiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:2.2":{"pli":"jātā seṭṭhikule ahuṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:2.3":{"pli":"Nānāratanapajjote, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:2.4":{"pli":"mahāsukhasamappitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:3.1":{"pli":"Upetvā taṃ mahāvīraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:3.2":{"pli":"assosiṃ dhammadesanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:3.3":{"pli":"Tato jātappasādāhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:3.4":{"pli":"upemi saraṇaṃ jinaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:4.1":{"pli":"Mātaraṃ pitarañcāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:4.2":{"pli":"āyācitvā vināyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:4.3":{"pli":"Nimantayitvā sattāhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:4.4":{"pli":"bhojayiṃ sahasāvakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:5.1":{"pli":"Atikkante ca sattāhe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:5.2":{"pli":"mahāpaññānamuttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:5.3":{"pli":"Bhikkhuniṃ etadaggamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:5.4":{"pli":"ṭhapesi narasārathi. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:6.1":{"pli":"Taṃ sutvā muditā hutvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:6.2":{"pli":"puno tassa mahesino; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:6.3":{"pli":"Kāraṃ katvāna taṃ ṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:6.4":{"pli":"paṇipacca paṇīdahiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:7.1":{"pli":"Tato mama jino āha, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:7.2":{"pli":"‘sijjhataṃ paṇidhī tava; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:7.3":{"pli":"Sasaṅghe me kataṃ kāraṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:7.4":{"pli":"appameyyaphalaṃ tayā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:8.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:8.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:8.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:8.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:9.1":{"pli":"Tassa dhammesu dāyādā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:9.2":{"pli":"orasā dhammanimmitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:9.3":{"pli":"Etadaggamanuppattā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:9.4":{"pli":"khemā nāma bhavissati’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:10.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:10.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:10.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:10.4":{"pli":"tāvatiṃsūpagā ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:11.1":{"pli":"Tato cutā yāmamagaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:11.2":{"pli":"tatohaṃ tusitaṃ gatā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:11.3":{"pli":"Tato ca nimmānaratiṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:11.4":{"pli":"vasavattipuraṃ tato. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:12.1":{"pli":"Yattha yatthūpapajjāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:12.2":{"pli":"tassa kammassa vāhasā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:12.3":{"pli":"Tattha tattheva rājūnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:12.4":{"pli":"mahesittamakārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:13.1":{"pli":"Tato cutā manussatte, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:13.2":{"pli":"rājūnaṃ cakkavattinaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:13.3":{"pli":"Maṇḍalīnañca rājūnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:13.4":{"pli":"mahesittamakārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:14.1":{"pli":"Sampattiṃ anubhotvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:14.2":{"pli":"devesu manujesu ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:14.3":{"pli":"Sabbattha sukhitā hutvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:14.4":{"pli":"nekakappesu saṃsariṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:15.1":{"pli":"Ekanavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:15.2":{"pli":"vipassī lokanāyako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:15.3":{"pli":"Uppajji cārudassano, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:15.4":{"pli":"sabbadhammavipassako. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:16.1":{"pli":"Tamahaṃ lokanāyakaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:16.2":{"pli":"upetvā narasārathiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:16.3":{"pli":"Dhammaṃ bhaṇitaṃ sutvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:16.4":{"pli":"pabbajiṃ anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:17.1":{"pli":"Dasavassasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:17.2":{"pli":"tassa vīrassa sāsane; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:17.3":{"pli":"Brahmacariyaṃ caritvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:17.4":{"pli":"yuttayogā bahussutā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:18.1":{"pli":"Paccayākārakusalā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:18.2":{"pli":"catusaccavisāradā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:18.3":{"pli":"Nipuṇā cittakathikā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:18.4":{"pli":"satthusāsanakārikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:19.1":{"pli":"Tato cutāhaṃ tusitaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:19.2":{"pli":"upapannā yasassinī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:19.3":{"pli":"Abhibhomi tahiṃ aññe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:19.4":{"pli":"brahmacārīphalenahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:20.1":{"pli":"Yattha yatthūpapannāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:20.2":{"pli":"mahābhogā mahaddhanā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:20.3":{"pli":"Medhāvinī rūpavatī, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:20.4":{"pli":"vinītaparisāpi ca. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:21.1":{"pli":"Bhavāmi tena kammena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:21.2":{"pli":"yogena jinasāsane; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:21.3":{"pli":"Sabbā sampattiyo mayhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:21.4":{"pli":"sulabhā manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:22.1":{"pli":"Yopi me bhavate bhattā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:22.2":{"pli":"yattha yattha gatāyapi; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:22.3":{"pli":"Vimāneti na maṃ koci, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:22.4":{"pli":"paṭipattibalena me. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:23.1":{"pli":"Imamhi bhaddake kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:23.2":{"pli":"brahmabandhu mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:23.3":{"pli":"Nāmena koṇāgamano, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:23.4":{"pli":"uppajji vadataṃ varo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:24.1":{"pli":"Tadā hi bārāṇasiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:24.2":{"pli":"susamiddhakulappajā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:24.3":{"pli":"Dhanañjānī sumedhā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:24.4":{"pli":"ahampi ca tayo janā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:25.1":{"pli":"Saṅghārāmamadāsimha, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:25.2":{"pli":"dānasahāyikā pure; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:25.3":{"pli":"Saṅghassa ca vihārampi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:25.4":{"pli":"uddissa kārikā mayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:26.1":{"pli":"Tato cutā mayaṃ sabbā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:26.2":{"pli":"tāvatiṃsūpagā ahuṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:26.3":{"pli":"Yasasā aggataṃ pattā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:26.4":{"pli":"manussesu tatheva ca. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:27.1":{"pli":"Imasmiṃyeva kappamhi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:27.2":{"pli":"brahmabandhu mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:27.3":{"pli":"Kassapo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:27.4":{"pli":"uppajji vadataṃ varo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:28.1":{"pli":"Upaṭṭhāko mahesissa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:28.2":{"pli":"tadā āsi narissaro; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:28.3":{"pli":"Kāsirājā kikī nāma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:28.4":{"pli":"bārāṇasipuruttame. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:29.1":{"pli":"Tassāsiṃ jeṭṭhikā dhītā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:29.2":{"pli":"samaṇī iti vissutā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:29.3":{"pli":"Dhammaṃ sutvā jinaggassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:29.4":{"pli":"pabbajjaṃ samarocayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:30.1":{"pli":"Anujāni na no tāto, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:30.2":{"pli":"agāreva tadā mayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:30.3":{"pli":"Vīsavassasahassāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:30.4":{"pli":"vicarimha atanditā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:31.1":{"pli":"Komāribrahmacariyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:31.2":{"pli":"rājakaññā sukhedhitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:31.3":{"pli":"Buddhopaṭṭhānaniratā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:31.4":{"pli":"muditā satta dhītaro. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:32.1":{"pli":"Samaṇī samaṇaguttā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:32.2":{"pli":"bhikkhunī bhikkhudāyikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:32.3":{"pli":"Dhammā ceva sudhammā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:32.4":{"pli":"sattamī saṅghadāyikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:33.1":{"pli":"Ahaṃ uppalavaṇṇā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:33.2":{"pli":"paṭācārā ca kuṇḍalā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:33.3":{"pli":"Kisāgotamī dhammadinnā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:33.4":{"pli":"visākhā hoti sattamī. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:34.1":{"pli":"Kadāci so narādicco, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:34.2":{"pli":"dhammaṃ desesi abbhutaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:34.3":{"pli":"Mahānidānasuttantaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:34.4":{"pli":"sutvā taṃ pariyāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:35.1":{"pli":"Tehi kammehi sukatehi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:35.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:35.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:35.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:36.1":{"pli":"Pacchime ca bhave dāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:36.2":{"pli":"sākalāya puruttame; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:36.3":{"pli":"Rañño maddassa dhītāmhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:36.4":{"pli":"manāpā dayitā piyā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:37.1":{"pli":"Saha me jātamattamhi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:37.2":{"pli":"khemaṃ tamhi pure ahu; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:37.3":{"pli":"Tato khemāti nāmaṃ me, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:37.4":{"pli":"guṇato upapajjatha. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:38.1":{"pli":"Yadāhaṃ yobbanaṃ pattā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:38.2":{"pli":"rūpalāvaññabhūsitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:38.3":{"pli":"Tadā adāsi maṃ tāto, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:38.4":{"pli":"bimbisārassa rājino. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:39.1":{"pli":"Tassāhaṃ suppiyā āsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:39.2":{"pli":"rūpake lāyane ratā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:39.3":{"pli":"Rūpānaṃ dosavādīti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:39.4":{"pli":"na upesiṃ mahādayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:40.1":{"pli":"Bimbisāro tadā rājā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:40.2":{"pli":"mamānuggahabuddhiyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:40.3":{"pli":"Vaṇṇayitvā veḷuvanaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:40.4":{"pli":"gāyake gāpayī mamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:41.1":{"pli":"Rammaṃ veḷuvanaṃ yena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:41.2":{"pli":"na diṭṭhaṃ sugatālayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:41.3":{"pli":"Na tena nandanaṃ diṭṭhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:41.4":{"pli":"iti maññāmase mayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:42.1":{"pli":"Yena veḷuvanaṃ diṭṭhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:42.2":{"pli":"naranandananandanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:42.3":{"pli":"Sudiṭṭhaṃ nandanaṃ tena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:42.4":{"pli":"amarindasunandanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:43.1":{"pli":"Vihāya nandanaṃ devā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:43.2":{"pli":"otaritvā mahītalaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:43.3":{"pli":"Rammaṃ veḷuvanaṃ disvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:43.4":{"pli":"na tappanti suvimhitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:44.1":{"pli":"Rājapuññena nibbattaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:44.2":{"pli":"buddhapuññena bhūsitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:44.3":{"pli":"Ko vattā tassa nissesaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:44.4":{"pli":"vanassa guṇasañcayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:45.1":{"pli":"Taṃ sutvā vanasamiddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:45.2":{"pli":"mama sotamanoharaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:45.3":{"pli":"Daṭṭhukāmā tamuyyānaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:45.4":{"pli":"rañño ārocayiṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:46.1":{"pli":"Mahatā parivārena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:46.2":{"pli":"tadā ca so mahīpati; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:46.3":{"pli":"Maṃ pesesi tamuyyānaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:46.4":{"pli":"dassanāya samussukaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:47.1":{"pli":"‘Gaccha passa mahābhoge, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:47.2":{"pli":"vanaṃ nettarasāyanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:47.3":{"pli":"Yaṃ sadā bhāti siriyā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:47.4":{"pli":"sugatābhānurañjitaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:48.1":{"pli":"‘Yadā ca piṇḍāya muni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:48.2":{"pli":"giribbajapuruttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:48.3":{"pli":"Paviṭṭhohaṃ tadāyeva, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:48.4":{"pli":"vanaṃ daṭṭhumupāgamiṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:49.1":{"pli":"Tadā taṃ phullavipinaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:49.2":{"pli":"nānābhamarakūjitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:49.3":{"pli":"Kokilāgītasahitaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:49.4":{"pli":"mayūragaṇanaccitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:50.1":{"pli":"Appasaddamanākiṇṇaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:50.2":{"pli":"nānācaṅkamabhūsitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:50.3":{"pli":"Kuṭimaṇḍapasaṅkiṇṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:50.4":{"pli":"yogīvaravirājitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:51.1":{"pli":"Vicarantī amaññissaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:51.2":{"pli":"‘saphalaṃ nayanaṃ mama’; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:51.3":{"pli":"Tatthāpi taruṇaṃ bhikkhuṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:51.4":{"pli":"yuttaṃ disvā vicintayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:52.1":{"pli":"‘Īdise vipine ramme, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:52.2":{"pli":"ṭhitoyaṃ navayobbane; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:52.3":{"pli":"Vasantamiva kantena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:52.4":{"pli":"rūpena ca samanvito. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:53.1":{"pli":"Nisinno rukkhamūlamhi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:53.2":{"pli":"muṇḍo saṅghāṭipāruto; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:53.3":{"pli":"Jhāyate vatayaṃ bhikkhu, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:53.4":{"pli":"hitvā visayajaṃ ratiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:54.1":{"pli":"Nanu nāma gahaṭṭhena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:54.2":{"pli":"kāmaṃ bhutvā yathāsukhaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:54.3":{"pli":"Pacchā jiṇṇena dhammoyaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:54.4":{"pli":"caritabbo subhaddako’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:55.1":{"pli":"‘Suññakan’ti viditvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:55.2":{"pli":"‘gandhagehaṃ jinālayaṃ’; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:55.3":{"pli":"Upetvā jinamaddakkhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:55.4":{"pli":"udayantaṃva bhākaraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:56.1":{"pli":"Ekakaṃ sukhamāsīnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:56.2":{"pli":"bījamānaṃ varitthiyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:56.3":{"pli":"Disvānevaṃ vicintesiṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:56.4":{"pli":"‘nāyaṃ lūkho narāsabho. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:57.1":{"pli":"Sā kaññā kanakābhāsā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:57.2":{"pli":"padumānanalocanā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:57.3":{"pli":"Bimboṭṭhī kundadasanā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:57.4":{"pli":"manonettarasāyanā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:58.1":{"pli":"Hemadolābhasavanā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:58.2":{"pli":"kalikākārasutthanī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:58.3":{"pli":"Vedimajjhāva sussoṇī, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:58.4":{"pli":"rambhoru cārubhūsanā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:59.1":{"pli":"Rattaṃsakupasambyānā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:59.2":{"pli":"nīlamaṭṭhanivāsanā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:59.3":{"pli":"Atappaneyyarūpena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:59.4":{"pli":"hāsabhāvasamanvitā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:60.1":{"pli":"Disvā tamevaṃ cintesiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:60.2":{"pli":"‘ahoyamabhirūpinī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:60.3":{"pli":"Na mayānena nettena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:60.4":{"pli":"diṭṭhapubbā kudācanaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:61.1":{"pli":"Tato jarābhibhūtā sā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:61.2":{"pli":"vivaṇṇā vikatānanā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:61.3":{"pli":"Bhinnadantā setasirā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:61.4":{"pli":"salālā vadanāsuci. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:62.1":{"pli":"Saṅkhittakaṇṇā setakkhī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:62.2":{"pli":"lambāsubhapayodharā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:62.3":{"pli":"Valivitatasabbaṅgī, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:62.4":{"pli":"sirāvitatadehinī. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:63.1":{"pli":"Nataṅgā daṇḍadutiyā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:63.2":{"pli":"upphāsulikatā kisā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:63.3":{"pli":"Pavedhamānā patitā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:63.4":{"pli":"nissasantī muhuṃ muhuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:64.1":{"pli":"Tato me āsi saṃvego, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:64.2":{"pli":"abbhuto lomahaṃsano; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:64.3":{"pli":"‘Dhiratthu rūpaṃ asuciṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:64.4":{"pli":"ramante yattha bālisā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:65.1":{"pli":"Tadā mahākāruṇiko, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:65.2":{"pli":"disvā saṃviggamānasaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:65.3":{"pli":"Udaggacitto sugato, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:65.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:66.1":{"pli":"‘Āturaṃ asuciṃ pūtiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:66.2":{"pli":"passa kheme samussayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:66.3":{"pli":"Uggharantaṃ paggharantaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:66.4":{"pli":"bālānaṃ abhinanditaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:67.1":{"pli":"Asubhāya cittaṃ bhāvehi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:67.2":{"pli":"ekaggaṃ susamāhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:67.3":{"pli":"Sati kāyagatā tyatthu, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:67.4":{"pli":"nibbidā bahulā bhava. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:68.1":{"pli":"Yathā idaṃ tathā etaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:68.2":{"pli":"yathā etaṃ tathā idaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:68.3":{"pli":"Ajjhattañca bahiddhā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:68.4":{"pli":"kāye chandaṃ virājaya. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:69.1":{"pli":"Animittañca bhāvehi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:69.2":{"pli":"mānānusayamujjaha; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:69.3":{"pli":"Tato mānābhisamayā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:69.4":{"pli":"upasantā carissasi. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:70.1":{"pli":"Ye rāgarattānupatanti sotaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:70.2":{"pli":"Sayaṃ kataṃ makkaṭakova jālaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:70.3":{"pli":"Etampi chetvāna paribbajanti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:70.4":{"pli":"Anapekkhino kāmasukhaṃ pahāya’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:71.1":{"pli":"Tato kallitacittaṃ maṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:71.2":{"pli":"ñatvāna narasārathi; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:71.3":{"pli":"Mahānidānaṃ desesi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:71.4":{"pli":"suttantaṃ vinayāya me. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:72.1":{"pli":"Sutvā suttantaseṭṭhaṃ taṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:72.2":{"pli":"pubbasaññamanussariṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:72.3":{"pli":"Tattha ṭhitāvahaṃ santī, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:72.4":{"pli":"dhammacakkhuṃ visodhayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:73.1":{"pli":"Nipatitvā mahesissa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:73.2":{"pli":"pādamūlamhi tāvade; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:73.3":{"pli":"Accayaṃ desanatthāya, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:73.4":{"pli":"idaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:74.1":{"pli":"‘Namo te sabbadassāvī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:74.2":{"pli":"namo te karuṇākara; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:74.3":{"pli":"Namo te tiṇṇasaṃsāra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:74.4":{"pli":"namo te amataṃdada. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:75.1":{"pli":"Diṭṭhigahanapakkhandā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:75.2":{"pli":"kāmarāgavimohitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:75.3":{"pli":"Tayā sammā upāyena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:75.4":{"pli":"vinītā vinaye ratā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:76.1":{"pli":"Adassanena vibhogā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:76.2":{"pli":"tādisānaṃ mahesinaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:76.3":{"pli":"Anubhonti mahādukkhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:76.4":{"pli":"sattā saṃsārasāgare. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:77.1":{"pli":"Yadāhaṃ lokasaraṇaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:77.2":{"pli":"araṇaṃ araṇantaguṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:77.3":{"pli":"Nāddasāmi adūraṭṭhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:77.4":{"pli":"desayāmi tamaccayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:78.1":{"pli":"Mahāhitaṃ varadadaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:78.2":{"pli":"ahitoti visaṅkitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:78.3":{"pli":"Nopesiṃ rūpaniratā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:78.4":{"pli":"desayāmi tamaccayaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:79.1":{"pli":"Tadā madhuranigghoso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:79.2":{"pli":"mahākāruṇiko jino; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:79.3":{"pli":"Avoca tiṭṭha khemeti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:79.4":{"pli":"siñcanto amatena maṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:80.1":{"pli":"Tadā paṇamya sirasā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:80.2":{"pli":"katvā ca naṃ padakkhiṇaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:80.3":{"pli":"Gantvā disvā narapatiṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:80.4":{"pli":"idaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:81.1":{"pli":"‘Aho sammā upāyo te, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:81.2":{"pli":"cintitoyamarindama; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:81.3":{"pli":"Vanadassanakāmāya, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:81.4":{"pli":"diṭṭho nibbānato muni. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:82.1":{"pli":"Yadi te ruccate rāja, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:82.2":{"pli":"sāsane tassa tādino; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:82.3":{"pli":"Pabbajissāmi rūpehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:82.4":{"pli":"nibbinnā munivāṇinā’. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap18:83.1":{"pli":"Dutiyaṃ bhāṇavāraṃ. ","html":"<p class='endsutta'>{}</p>"},"thi-ap18:84.1":{"pli":"Añjaliṃ paggahetvāna, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap18:84.2":{"pli":"tadāha sa mahīpati; ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:84.3":{"pli":"‘Anujānāmi te bhadde, ","html":"<span class='verse-line'>{}</span>"},"thi-ap18:84.4":{"pli":"pabbajjā tava sijjhatu’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap18:85.1":{"pli":"Pabbajitvā tadā cāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap18:85.2":{"pli":"addhamāse upaṭṭhite; 