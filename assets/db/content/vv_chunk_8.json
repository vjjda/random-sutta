{"vv64":{"vv64:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv64'><header><ul><li class='division'>{}</li>"},"vv64:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv64:0.3":{"pli":"Mahārathavagga ","html":"<li>{}</li></ul>"},"vv64:0.4":{"pli":"14. Mahārathavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv64:1.1":{"pli":"“Sahassayuttaṃ hayavāhanaṃ subhaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv64:1.2":{"pli":"Āruyhimaṃ sandanaṃ nekacittaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv64:1.3":{"pli":"Uyyānabhūmiṃ abhito anukkamaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv64:1.4":{"pli":"Purindado bhūtapatīva vāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:2.1":{"pli":"Sovaṇṇamayā te rathakubbarā ubho, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:2.2":{"pli":"Phalehi aṃsehi atīva saṅgatā; ","html":"<span class='verse-line'>{}</span>"},"vv64:2.3":{"pli":"Sujātagumbā naravīraniṭṭhitā, ","html":"<span class='verse-line'>{}</span>"},"vv64:2.4":{"pli":"Virocatī pannaraseva cando. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:3.1":{"pli":"Suvaṇṇajālāvatato ratho ayaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:3.2":{"pli":"Bahūhi nānāratanehi cittito; ","html":"<span class='verse-line'>{}</span>"},"vv64:3.3":{"pli":"Sunandighoso ca subhassaro ca, ","html":"<span class='verse-line'>{}</span>"},"vv64:3.4":{"pli":"Virocatī cāmarahatthabāhubhi. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:4.1":{"pli":"Imā ca nābhyo manasābhinimmitā, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:4.2":{"pli":"Rathassa pādantaramajjhabhūsitā; ","html":"<span class='verse-line'>{}</span>"},"vv64:4.3":{"pli":"Imā ca nābhyo satarājicittitā, ","html":"<span class='verse-line'>{}</span>"},"vv64:4.4":{"pli":"Sateratā vijjurivappabhāsare. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:5.1":{"pli":"Anekacittāvatato ratho ayaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:5.2":{"pli":"Puthū ca nemī ca sahassaraṃsiko; ","html":"<span class='verse-line'>{}</span>"},"vv64:5.3":{"pli":"Tesaṃ saro suyyati vaggurūpo, ","html":"<span class='verse-line'>{}</span>"},"vv64:5.4":{"pli":"Pañcaṅgikaṃ tūriyamivappavāditaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:6.1":{"pli":"Sirasmiṃ cittaṃ maṇicandakappitaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:6.2":{"pli":"Sadā visuddhaṃ ruciraṃ pabhassaraṃ; ","html":"<span class='verse-line'>{}</span>"},"vv64:6.3":{"pli":"Suvaṇṇarājīhi atīva saṅgataṃ, ","html":"<span class='verse-line'>{}</span>"},"vv64:6.4":{"pli":"Veḷuriyarājīva atīva sobhati. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:7.1":{"pli":"Ime ca vāḷī maṇicandakappitā, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:7.2":{"pli":"Ārohakambū sujavā brahūpamā; ","html":"<span class='verse-line'>{}</span>"},"vv64:7.3":{"pli":"Brahā mahantā balino mahājavā, ","html":"<span class='verse-line'>{}</span>"},"vv64:7.4":{"pli":"Mano tavaññāya tatheva siṃsare. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:8.1":{"pli":"Ime ca sabbe sahitā catukkamā, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:8.2":{"pli":"Mano tavaññāya tatheva siṃsare; ","html":"<span class='verse-line'>{}</span>"},"vv64:8.3":{"pli":"Samaṃ vahantī mudukā anuddhatā, ","html":"<span class='verse-line'>{}</span>"},"vv64:8.4":{"pli":"Āmodamānā turagānamuttama. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:9.1":{"pli":"Dhunanti vagganti patanti cambare, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:9.2":{"pli":"Abbhuddhunantā sukate piḷandhane; ","html":"<span class='verse-line'>{}</span>"},"vv64:9.3":{"pli":"Tesaṃ saro suyyati vaggurūpo, ","html":"<span class='verse-line'>{}</span>"},"vv64:9.4":{"pli":"Pañcaṅgikaṃ tūriyamivappavāditaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:10.1":{"pli":"Rathassa ghoso apiḷandhanāna ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:10.2":{"pli":"Khurassa nādo abhihiṃsanāya ca; ","html":"<span class='verse-line'>{}</span>"},"vv64:10.3":{"pli":"Ghoso suvaggū samitassa suyyati, ","html":"<span class='verse-line'>{}</span>"},"vv64:10.4":{"pli":"Gandhabbatūriyāni vicitrasaṃvane. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:11.1":{"pli":"Rathe ṭhitā tā migamandalocanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:11.2":{"pli":"Āḷārapamhā hasitā piyaṃvadā; ","html":"<span class='verse-line'>{}</span>"},"vv64:11.3":{"pli":"Veḷuriyajālāvatatā tanucchavā, ","html":"<span class='verse-line'>{}</span>"},"vv64:11.4":{"pli":"Sadeva gandhabbasūraggapūjitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:12.1":{"pli":"Tā rattarattambarapītavāsasā, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:12.2":{"pli":"Visālanettā abhirattalocanā; ","html":"<span class='verse-line'>{}</span>"},"vv64:12.3":{"pli":"Kule sujātā sutanū sucimhitā, ","html":"<span class='verse-line'>{}</span>"},"vv64:12.4":{"pli":"Rathe ṭhitā pañjalikā upaṭṭhitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:13.1":{"pli":"Tā kambukeyūradharā suvāsasā, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:13.2":{"pli":"Sumajjhimā ūruthanūpapannā; ","html":"<span class='verse-line'>{}</span>"},"vv64:13.3":{"pli":"Vaṭṭaṅguliyo sumukhā sudassanā, ","html":"<span class='verse-line'>{}</span>"},"vv64:13.4":{"pli":"Rathe ṭhitā pañjalikā upaṭṭhitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:14.1":{"pli":"Aññā suveṇī susu missakesiyo, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:14.2":{"pli":"Samaṃ vibhattāhi pabhassarāhi ca; ","html":"<span class='verse-line'>{}</span>"},"vv64:14.3":{"pli":"Anubbatā tā tava mānase ratā, ","html":"<span class='verse-line'>{}</span>"},"vv64:14.4":{"pli":"Rathe ṭhitā pañjalikā upaṭṭhitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:15.1":{"pli":"Āveḷiniyo padumuppalacchadā, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:15.2":{"pli":"Alaṅkatā candanasāravāsitā; ","html":"<span class='verse-line'>{}</span>"},"vv64:15.3":{"pli":"Anubbatā tā tava mānase ratā, ","html":"<span class='verse-line'>{}</span>"},"vv64:15.4":{"pli":"Rathe ṭhitā pañjalikā upaṭṭhitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:16.1":{"pli":"Tā māliniyo padumuppalacchadā, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:16.2":{"pli":"Alaṅkatā candanasāravāsitā; ","html":"<span class='verse-line'>{}</span>"},"vv64:16.3":{"pli":"Anubbatā tā tava mānase ratā, ","html":"<span class='verse-line'>{}</span>"},"vv64:16.4":{"pli":"Rathe ṭhitā pañjalikā upaṭṭhitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:17.1":{"pli":"Kaṇṭhesu te yāni piḷandhanāni, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:17.2":{"pli":"Hatthesu pādesu tatheva sīse; ","html":"<span class='verse-line'>{}</span>"},"vv64:17.3":{"pli":"Obhāsayantī dasa sabbaso disā, ","html":"<span class='verse-line'>{}</span>"},"vv64:17.4":{"pli":"Abbhuddayaṃ sāradikova bhāṇumā. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:18.1":{"pli":"Vātassa vegena ca sampakampitā, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:18.2":{"pli":"Bhujesu mālā apiḷandhanāni ca; ","html":"<span class='verse-line'>{}</span>"},"vv64:18.3":{"pli":"Muñcanti ghosaṃ ruciraṃ suciṃ subhaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv64:18.4":{"pli":"Sabbehi viññūhi sutabbarūpaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:19.1":{"pli":"Uyyānabhūmyā ca duvaddhato ṭhitā, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:19.2":{"pli":"Rathā ca nāgā tūriyāni ca saro; ","html":"<span class='verse-line'>{}</span>"},"vv64:19.3":{"pli":"Tameva devinda pamodayanti, ","html":"<span class='verse-line'>{}</span>"},"vv64:19.4":{"pli":"Vīṇā yathā pokkharapattabāhubhi. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:20.1":{"pli":"Imāsu vīṇāsu bahūsu vaggūsu, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:20.2":{"pli":"Manuññarūpāsu hadayeritaṃ pītiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv64:20.3":{"pli":"Pavajjamānāsu atīva accharā, ","html":"<span class='verse-line'>{}</span>"},"vv64:20.4":{"pli":"Bhamanti kaññā padumesu sikkhitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:21.1":{"pli":"Yadā ca gītāni ca vāditāni ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:21.2":{"pli":"Naccāni cimāni samenti ekato; ","html":"<span class='verse-line'>{}</span>"},"vv64:21.3":{"pli":"Athettha naccanti athettha accharā, ","html":"<span class='verse-line'>{}</span>"},"vv64:21.4":{"pli":"Obhāsayantī ubhato varitthiyo. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:22.1":{"pli":"So modasi tūriyagaṇappabodhano, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:22.2":{"pli":"Mahīyamāno vajirāvudhoriva; ","html":"<span class='verse-line'>{}</span>"},"vv64:22.3":{"pli":"Imāsu vīṇāsu bahūsu vaggūsu, ","html":"<span class='verse-line'>{}</span>"},"vv64:22.4":{"pli":"Manuññarūpāsu hadayeritaṃ pītiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:23.1":{"pli":"Kiṃ tvaṃ pure kammamakāsi attanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:23.2":{"pli":"Manussabhūto purimāya jātiyā; ","html":"<span class='verse-line'>{}</span>"},"vv64:23.3":{"pli":"Uposathaṃ kaṃ vā tuvaṃ upāvasi, ","html":"<span class='verse-line'>{}</span>"},"vv64:23.4":{"pli":"Kaṃ dhammacariyaṃ vatamābhirocayi. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:24.1":{"pli":"Nayīdamappassa katassa kammuno, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:24.2":{"pli":"Pubbe suciṇṇassa uposathassa vā; ","html":"<span class='verse-line'>{}</span>"},"vv64:24.3":{"pli":"Iddhānubhāvo vipulo ayaṃ tava, ","html":"<span class='verse-line'>{}</span>"},"vv64:24.4":{"pli":"Yaṃ devasaṅghaṃ abhirocase bhusaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:25.1":{"pli":"Dānassa te idaṃ phalaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:25.2":{"pli":"atho sīlassa vā pana; ","html":"<span class='verse-line'>{}</span>"},"vv64:25.3":{"pli":"Atho añjalikammassa, ","html":"<span class='verse-line'>{}</span>"},"vv64:25.4":{"pli":"taṃ me akkhāhi pucchito”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:26.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:26.2":{"pli":"moggallānena pucchito; ","html":"<span class='verse-line'>{}</span>"},"vv64:26.3":{"pli":"Pañhaṃ puṭṭho viyākāsi, ","html":"<span class='verse-line'>{}</span>"},"vv64:26.4":{"pli":"yassa kammassidaṃ phalanti. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:27.1":{"pli":"“Jitindriyaṃ buddhamanomanikkamaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:27.2":{"pli":"Naruttamaṃ kassapamaggapuggalaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv64:27.3":{"pli":"Avāpurantaṃ amatassa dvāraṃ, ","html":"<span class='verse-line'>{}</span>"},"vv64:27.4":{"pli":"Devātidevaṃ satapuññalakkhaṇaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:28.1":{"pli":"Tamaddasaṃ kuñjaramoghatiṇṇaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:28.2":{"pli":"Suvaṇṇasiṅgīnadabimbasādisaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv64:28.3":{"pli":"Disvāna taṃ khippamahuṃ sucīmano, ","html":"<span class='verse-line'>{}</span>"},"vv64:28.4":{"pli":"Tameva disvāna subhāsitaddhajaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:29.1":{"pli":"Tamannapānaṃ athavāpi cīvaraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:29.2":{"pli":"Suciṃ paṇītaṃ rasasā upetaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv64:29.3":{"pli":"Pupphābhikkiṇamhi sake nivesane, ","html":"<span class='verse-line'>{}</span>"},"vv64:29.4":{"pli":"Patiṭṭhapesiṃ sa asaṅgamānaso. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:30.1":{"pli":"Tamannapānena ca cīvarena ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:30.2":{"pli":"Khajjena bhojjena ca sāyanena ca; ","html":"<span class='verse-line'>{}</span>"},"vv64:30.3":{"pli":"Santappayitvā dvipadānamuttamaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv64:30.4":{"pli":"So saggaso devapure ramāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:31.1":{"pli":"Etenupāyena imaṃ niraggaḷaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:31.2":{"pli":"Yaññaṃ yajitvā tividhaṃ visuddhaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv64:31.3":{"pli":"Pahāyahaṃ mānusakaṃ samussayaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv64:31.4":{"pli":"Indūpamo devapure ramāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:32.1":{"pli":"Āyuñca vaṇṇañca sukhaṃ balañca, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:32.2":{"pli":"Paṇītarūpaṃ abhikaṅkhatā muni; ","html":"<span class='verse-line'>{}</span>"},"vv64:32.3":{"pli":"Annañca pānañca bahuṃ susaṅkhataṃ, ","html":"<span class='verse-line'>{}</span>"},"vv64:32.4":{"pli":"Patiṭṭhapetabbamasaṅgamānase. ","html":"<span class='verse-line'>{}</span></p>"},"vv64:33.1":{"pli":"Nayimasmiṃ loke parasmiṃ vā pana, ","html":"<p><span class='verse-line'>{}</span>"},"vv64:33.2":{"pli":"Buddhena seṭṭho va samo va vijjati; ","html":"<span class='verse-line'>{}</span>"},"vv64:33.3":{"pli":"Āhuneyyānaṃ paramāhutiṃ gato, ","html":"<span class='verse-line'>{}</span>"},"vv64:33.4":{"pli":"Puññatthikānaṃ vipulapphalesinan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv64:34.1":{"pli":"Mahārathavimānaṃ cuddasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"vv64:34.2":{"pli":"Mahārathavaggo pañcamo. ","html":"<p class='endvagga'>{}</p>"},"vv64:35.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"vv64:36.1":{"pli":"Maṇḍūko revatī chatto, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"vv64:36.2":{"pli":"Kakkaṭo dvārapālako; ","html":"<span class='verse-line'>{}</span>"},"vv64:36.3":{"pli":"Dve karaṇīyā dve sūci, ","html":"<span class='verse-line'>{}</span>"},"vv64:36.4":{"pli":"Tayo nāgā ca dve rathā; ","html":"<span class='verse-line'>{}</span>"},"vv64:36.5":{"pli":"Purisānaṃ paṭhamo vaggo pavuccatīti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv64:37.1":{"pli":"Bhāṇavāraṃ tatiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv65":{"vv65:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv65'><header><ul><li class='division'>{}</li>"},"vv65:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv65:0.3":{"pli":"Pāyāsivagga ","html":"<li>{}</li></ul>"},"vv65:0.4":{"pli":"1. Paṭhamaagāriyavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv65:1.1":{"pli":"“Yathā vanaṃ cittalataṃ pabhāsati, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv65:1.2":{"pli":"Uyyānaseṭṭhaṃ tidasānamuttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv65:1.3":{"pli":"Tathūpamaṃ tuyhamidaṃ vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv65:1.4":{"pli":"Obhāsayaṃ tiṭṭhati antalikkhe. ","html":"<span class='verse-line'>{}</span></p>"},"vv65:2.1":{"pli":"Deviddhipattosi mahānubhāvo, ","html":"<p><span class='verse-line'>{}</span>"},"vv65:2.2":{"pli":"Manussabhūto kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv65:2.3":{"pli":"Kenāsi evaṃ jalitānubhāvo, ","html":"<span class='verse-line'>{}</span>"},"vv65:2.4":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv65:3.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv65:3.2":{"pli":"moggallānena pucchito; ","html":"<span class='verse-line'>{}</span>"},"vv65:3.3":{"pli":"Pañhaṃ puṭṭho viyākāsi, ","html":"<span class='verse-line'>{}</span>"},"vv65:3.4":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv65:4.1":{"pli":"“Ahañca bhariyā ca manussaloke, ","html":"<p><span class='verse-line'>{}</span>"},"vv65:4.2":{"pli":"Opānabhūtā gharamāvasimha; ","html":"<span class='verse-line'>{}</span>"},"vv65:4.3":{"pli":"Annañca pānañca pasannacittā, ","html":"<span class='verse-line'>{}</span>"},"vv65:4.4":{"pli":"Sakkacca dānaṃ vipulaṃ adamha. ","html":"<span class='verse-line'>{}</span></p>"},"vv65:5.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv65:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv65:5.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv65:6.1":{"pli":"Paṭhamaagāriyavimānaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv66":{"vv66:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv66'><header><ul><li class='division'>{}</li>"},"vv66:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv66:0.3":{"pli":"Pāyāsivagga ","html":"<li>{}</li></ul>"},"vv66:0.4":{"pli":"2. Dutiyaagāriyavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv66:1.1":{"pli":"“Yathā vanaṃ cittalataṃ pabhāsati, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv66:1.2":{"pli":"Uyyānaseṭṭhaṃ tidasānamuttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv66:1.3":{"pli":"Tathūpamaṃ tuyhamidaṃ vimānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv66:1.4":{"pli":"Obhāsayaṃ tiṭṭhati antalikkhe. ","html":"<span class='verse-line'>{}</span></p>"},"vv66:2.1":{"pli":"Deviddhipattosi mahānubhāvo, ","html":"<p><span class='verse-line'>{}</span>"},"vv66:2.2":{"pli":"Manussabhūto kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv66:2.3":{"pli":"Kenāsi evaṃ jalitānubhāvo, ","html":"<span class='verse-line'>{}</span>"},"vv66:2.4":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv66:3.1":{"pli":"So devaputto attamano, ","html":"<p><span class='verse-line'>{}</span>"},"vv66:3.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv66:3.3":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv66:4.1":{"pli":"“Ahañca bhariyā ca manussaloke, ","html":"<p><span class='verse-line'>{}</span>"},"vv66:4.2":{"pli":"Opānabhūtā gharamāvasimha; ","html":"<span class='verse-line'>{}</span>"},"vv66:4.3":{"pli":"Annañca pānañca pasannacittā, ","html":"<span class='verse-line'>{}</span>"},"vv66:4.4":{"pli":"Sakkacca dānaṃ vipulaṃ adamha. ","html":"<span class='verse-line'>{}</span></p>"},"vv66:5.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv66:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv66:5.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv66:6.1":{"pli":"Dutiyaagāriyavimānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv67":{"vv67:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv67'><header><ul><li class='division'>{}</li>"},"vv67:0.2":{"pli":"Purisavimāna ","html":"<li>{}</li>"},"vv67:0.3":{"pli":"Pāyāsivagga ","html":"<li>{}</li></ul>"},"vv67:0.4":{"pli":"3. Phaladāyakavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv67:1.1":{"pli":"“Uccamidaṃ maṇithūṇaṃ vimānaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv67:1.2":{"pli":"Samantato soḷasa yojanāni; ","html":"<span class='verse-line'>{}</span>"},"vv67:1.3":{"pli":"Kūṭāgārā sattasatā uḷārā, ","html":"<span class='verse-line'>{}</span>"},"vv67:1.4":{"pli":"Veḷuriyathambhā rucakatthatā subhā. ","html":"<span class='verse-line'>{}</span></p>"},"vv67:2.1":{"pli":"Tatthacchasi pivasi khādasi ca, ","html":"<p><span class='verse-line'>{}</span>"},"vv67:2.2":{"pli":"Dibbā ca vīṇā pavadanti vagguṃ; ","html":"<span class='verse-line'>{}</span>"},"vv67:2.3":{"pli":"Aṭṭhaṭṭhakā sikkhitā sādhurūpā, ","html":"<span class='verse-line'>{}</span>"},"vv67:2.4":{"pli":"Dibbā ca kaññā tidasacarā uḷārā; ","html":"<span class='verse-line'>{}</span>"},"vv67:2.5":{"pli":"Naccanti