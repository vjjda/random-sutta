{"thi-ap22":{"thi-ap22:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap22'><header><ul><li class='division'>{}</li>"},"thi-ap22:0.2":{"pli":"Kuṇḍalakesīvagga ","html":"<li>{}</li></ul>"},"thi-ap22:0.3":{"pli":"Kisāgotamītherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap22:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap22:1.2":{"pli":"sabbadhammāna pāragū; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:1.3":{"pli":"Ito satasahassamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:1.4":{"pli":"kappe uppajji nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:2.1":{"pli":"Tadāhaṃ haṃsavatiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:2.2":{"pli":"jātā aññatare kule; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:2.3":{"pli":"Upetvā taṃ naravaraṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:2.4":{"pli":"saraṇaṃ samupāgamiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:3.1":{"pli":"Dhammañca tassa assosiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:3.2":{"pli":"catusaccūpasañhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:3.3":{"pli":"Madhuraṃ paramassādaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:3.4":{"pli":"vaṭṭasantisukhāvahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:4.1":{"pli":"Tadā ca bhikkhuniṃ vīro, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:4.2":{"pli":"lūkhacīvaradhāriniṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:4.3":{"pli":"Ṭhapento etadaggamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:4.4":{"pli":"vaṇṇayī purisuttamo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:5.1":{"pli":"Janetvānappakaṃ pītiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:5.2":{"pli":"sutvā bhikkhuniyā guṇe; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:5.3":{"pli":"Kāraṃ katvāna buddhassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:5.4":{"pli":"yathāsatti yathābalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:6.1":{"pli":"Nipacca munivaraṃ taṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:6.2":{"pli":"taṃ ṭhānamabhipatthayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:6.3":{"pli":"Tadānumodi sambuddho, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:6.4":{"pli":"ṭhānalābhāya nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:7.1":{"pli":"‘Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:7.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:7.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:7.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:8.1":{"pli":"Tassa dhammesu dāyādā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:8.2":{"pli":"orasā dhammanimmitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:8.3":{"pli":"Kisāgotamī nāmena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:8.4":{"pli":"hessati satthu sāvikā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:9.1":{"pli":"Taṃ sutvā muditā hutvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:9.2":{"pli":"yāvajīvaṃ tadā jinaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:9.3":{"pli":"Mettacittā paricariṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:9.4":{"pli":"paccayehi vināyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:10.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:10.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:10.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:10.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:11.1":{"pli":"Imamhi bhaddake kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:11.2":{"pli":"brahmabandhu mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:11.3":{"pli":"Kassapo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:11.4":{"pli":"uppajji vadataṃ varo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:12.1":{"pli":"Upaṭṭhāko mahesissa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:12.2":{"pli":"tadā āsi narissaro; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:12.3":{"pli":"Kāsirājā kikī nāma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:12.4":{"pli":"bārāṇasipuruttame. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:13.1":{"pli":"Pañcamī tassa dhītāsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:13.2":{"pli":"dhammā nāmena vissutā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:13.3":{"pli":"Dhammaṃ sutvā jinaggassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:13.4":{"pli":"pabbajjaṃ samarocayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:14.1":{"pli":"Anujāni na no tāto, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:14.2":{"pli":"agāreva tadā mayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:14.3":{"pli":"Vīsavassasahassāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:14.4":{"pli":"vicarimha atanditā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:15.1":{"pli":"Komāribrahmacariyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:15.2":{"pli":"rājakaññā sukhedhitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:15.3":{"pli":"Buddhopaṭṭhānaniratā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:15.4":{"pli":"muditā satta dhītaro. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:16.1":{"pli":"Samaṇī samaṇaguttā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:16.2":{"pli":"bhikkhunī bhikkhudāyikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:16.3":{"pli":"Dhammā ceva sudhammā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:16.4":{"pli":"sattamī saṅghadāyikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:17.1":{"pli":"Khemā uppalavaṇṇā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:17.2":{"pli":"paṭācārā ca kuṇḍalā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:17.3":{"pli":"Ahañca dhammadinnā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:17.4":{"pli":"visākhā hoti sattamī. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:18.1":{"pli":"Tehi kammehi sukatehi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:18.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:18.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:18.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:19.1":{"pli":"Pacchime ca bhave dāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:19.2":{"pli":"jātā seṭṭhikule ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:19.3":{"pli":"Duggate adhane naṭṭhe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:19.4":{"pli":"gatā ca sadhanaṃ kulaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:20.1":{"pli":"Patiṃ ṭhapetvā sesā me, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:20.2":{"pli":"dessanti adhanā iti; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:20.3":{"pli":"Yadā ca pasūtā āsiṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:20.4":{"pli":"sabbesaṃ dayitā tadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:21.1":{"pli":"Yadā so taruṇo bhaddo, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:21.2":{"pli":"komalako sukhedhito; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:21.3":{"pli":"Sapāṇamiva kanto me, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:21.4":{"pli":"tadā yamavasaṃ gato. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:22.1":{"pli":"Sokaṭṭādīnavadanā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:22.2":{"pli":"assunettā rudammukhā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:22.3":{"pli":"Mataṃ kuṇapamādāya, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:22.4":{"pli":"vilapantī gamāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:23.1":{"pli":"Tadā ekena sandiṭṭhā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:23.2":{"pli":"upetvābhisakkuttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:23.3":{"pli":"Avocaṃ ‘dehi bhesajjaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:23.4":{"pli":"puttasañjīvananti bho’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:24.1":{"pli":"‘Na vijjante matā yasmiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:24.2":{"pli":"gehe siddhatthakaṃ tato; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:24.3":{"pli":"Āharā’ti jino āha, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:24.4":{"pli":"vinayopāyakovido. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:25.1":{"pli":"Tadā gamitvā sāvatthiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:25.2":{"pli":"na labhiṃ tādisaṃ gharaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:25.3":{"pli":"Kuto siddhatthakaṃ tasmā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:25.4":{"pli":"tato laddhā satiṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:26.1":{"pli":"Kuṇapaṃ chaḍḍayitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:26.2":{"pli":"upesiṃ lokanāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:26.3":{"pli":"Dūratova mamaṃ disvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:26.4":{"pli":"avoca madhurassaro. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:27.1":{"pli":"‘Yo ca vassasataṃ jīve, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:27.2":{"pli":"apassaṃ udayabbayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:27.3":{"pli":"Ekāhaṃ jīvitaṃ seyyo, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:27.4":{"pli":"passato udayabbayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:28.1":{"pli":"Na gāmadhammo nigamassa dhammo, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:28.2":{"pli":"Na cāpiyaṃ ekakulassa dhammo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:28.3":{"pli":"Sabbassa lokassa sadevakassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:28.4":{"pli":"Eseva dhammo yadidaṃ aniccatā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:29.1":{"pli":"Sāhaṃ sutvānimā gāthā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:29.2":{"pli":"dhammacakkhuṃ visodhayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:29.3":{"pli":"Tato viññātasaddhammā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:29.4":{"pli":"pabbajiṃ anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:30.1":{"pli":"Tathā pabbajitā santī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:30.2":{"pli":"yuñjantī jinasāsane; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:30.3":{"pli":"Na cireneva kālena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:30.4":{"pli":"arahattamapāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:31.1":{"pli":"Iddhīsu ca vasī homi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:31.2":{"pli":"dibbāya sotadhātuyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:31.3":{"pli":"Paracittāni jānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:31.4":{"pli":"satthusāsanakārikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:32.1":{"pli":"Pubbenivāsaṃ jānāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:32.2":{"pli":"dibbacakkhu visodhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:32.3":{"pli":"Khepetvā āsave sabbe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:32.4":{"pli":"visuddhāsiṃ sunimmalā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:33.1":{"pli":"Pariciṇṇo mayā satthā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:33.2":{"pli":"kataṃ buddhassa sāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:33.3":{"pli":"Ohito garuko bhāro, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:33.4":{"pli":"bhavanetti samūhatā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:34.1":{"pli":"Yassatthāya pabbajitā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:34.2":{"pli":"agārasmānagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:34.3":{"pli":"So me attho anuppatto, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:34.4":{"pli":"sabbasaṃyojanakkhayo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:35.1":{"pli":"Atthadhammaniruttīsu, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:35.2":{"pli":"paṭibhāne tatheva ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:35.3":{"pli":"Ñāṇaṃ me vimalaṃ suddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:35.4":{"pli":"buddhaseṭṭhassa vāhasā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:36.1":{"pli":"Saṅkārakūṭā āhitvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:36.2":{"pli":"susānā rathiyāpi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:36.3":{"pli":"Tato saṅghāṭikaṃ katvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:36.4":{"pli":"lūkhaṃ dhāremi cīvaraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:37.1":{"pli":"Jino tasmiṃ guṇe tuṭṭho, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:37.2":{"pli":"lūkhacīvaradhāraṇe; ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:37.3":{"pli":"Ṭhapesi etadaggamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:37.4":{"pli":"parisāsu vināyako. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:38.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:38.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:38.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:39.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:39.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:39.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap22:40.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap22:40.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap22:40.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap22:41.1":{"pli":"Itthaṃ sudaṃ kisāgotamī bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap22:42.1":{"pli":"Kisāgotamītheriyāpadānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"thi-ap23":{"thi-ap23:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap23'><header><ul><li class='division'>{}</li>"},"thi-ap23:0.2":{"pli":"Kuṇḍalakesīvagga ","html":"<li>{}</li></ul>"},"thi-ap23:0.3":{"pli":"Dhammadinnātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap23:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap23:1.2":{"pli":"sabbadhammāna pāragū; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:1.3":{"pli":"Ito satasahassamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:1.4":{"pli":"kappe uppajji nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:2.1":{"pli":"Tadāhaṃ haṃsavatiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:2.2":{"pli":"kule aññatare ahuṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:2.3":{"pli":"Parakammakārī āsiṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:2.4":{"pli":"nipakā sīlasaṃvutā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:3.1":{"pli":"Padumuttarabuddhassa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:3.2":{"pli":"sujāto aggasāvako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:3.3":{"pli":"Vihārā abhinikkhamma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:3.4":{"pli":"piṇḍapātāya gacchati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:4.1":{"pli":"Ghaṭaṃ gahetvā gacchantī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:4.2":{"pli":"tadā udakahārikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:4.3":{"pli":"Taṃ disvā adadaṃ pūpaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:4.4":{"pli":"pasannā sehi pāṇibhi. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:5.1":{"pli":"Paṭiggahetvā tattheva, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:5.2":{"pli":"nisinno paribhuñji so; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:5.3":{"pli":"Tato netvāna taṃ gehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:5.4":{"pli":"adāsiṃ tassa bhojanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:6.1":{"pli":"Tato me ayyako tuṭṭho, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:6.2":{"pli":"akarī suṇisaṃ sakaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:6.3":{"pli":"Sassuyā saha gantvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:6.4":{"pli":"sambuddhaṃ abhivādayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:7.1":{"pli":"Tadā so dhammakathikaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:7.2":{"pli":"bhikkhuniṃ parikittayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:7.3":{"pli":"Ṭhapesi etadaggamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:7.4":{"pli":"taṃ sutvā muditā ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:8.1":{"pli":"Nimantayitvā sugataṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:8.2":{"pli":"sasaṅghaṃ lokanāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:8.3":{"pli":"Mahādānaṃ daditvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:8.4":{"pli":"taṃ ṭhānamabhipatthayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:9.1":{"pli":"Tato maṃ sugato āha, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:9.2":{"pli":"ghananinnādasussaro; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:9.3":{"pli":"‘Mamupaṭṭhānanirate, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:9.4":{"pli":"sasaṅghaparivesike. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:10.1":{"pli":"Saddhammassavane yutte, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:10.2":{"pli":"guṇavaddhitamānase; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:10.3":{"pli":"Bhadde bhavassu muditā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:10.4":{"pli":"lacchase paṇidhīphalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:11.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:11.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:11.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:11.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:12.1":{"pli":"Tassa dhammesu dāyādā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:12.2":{"pli":"orasā dhammanimmitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:12.3":{"pli":"Dhammadinnāti nāmena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:12.4":{"pli":"hessati satthu sāvikā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:13.1":{"pli":"Taṃ sutvā muditā hutvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:13.2":{"pli":"yāvajīvaṃ mahāmuniṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:13.3":{"pli":"Mettacittā paricariṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:13.4":{"pli":"paccayehi vināyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:14.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:14.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:14.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:14.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:15.1":{"pli":"Imamhi bhaddake kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:15.2":{"pli":"brahmabandhu mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:15.3":{"pli":"Kassapo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:15.4":{"pli":"uppajji vadataṃ varo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:16.1":{"pli":"Upaṭṭhāko mahesissa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:16.2":{"pli":"tadā āsi narissaro; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:16.3":{"pli":"Kāsirājā kikī nāma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:16.4":{"pli":"bārāṇasipuruttame. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:17.1":{"pli":"Chaṭṭhā tassāsahaṃ dhītā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:17.2":{"pli":"sudhammā iti vissutā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:17.3":{"pli":"Dhammaṃ sutvā jinaggassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:17.4":{"pli":"pabbajjaṃ samarocayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:18.1":{"pli":"Anujāni na no tāto, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:18.2":{"pli":"agāreva tadā mayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:18.3":{"pli":"Vīsavassasahassāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:18.4":{"pli":"vicarimha atanditā. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap23:19.1":{"pli":"Tatiyaṃ bhāṇavāraṃ. ","html":"<p class='endsutta'>{}</p>"},"thi-ap23:20.1":{"pli":"Komāribrahmacariyaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap23:20.2":{"pli":"rājakaññā sukhedhitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:20.3":{"pli":"Buddhopaṭṭhānaniratā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:20.4":{"pli":"muditā satta dhītaro. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:21.1":{"pli":"Samaṇī samaṇaguttā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:21.2":{"pli":"bhikkhunī bhikkhudāyikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:21.3":{"pli":"Dhammā ceva sudhammā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:21.4":{"pli":"sattamī saṅghadāyikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:22.1":{"pli":"Khemā uppalavaṇṇā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:22.2":{"pli":"paṭācārā ca kuṇḍalā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:22.3":{"pli":"Gotamī ca ahañceva, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:22.4":{"pli":"visākhā hoti sattamī. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:23.1":{"pli":"Tehi kammehi sukatehi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:23.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:23.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:23.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:24.1":{"pli":"Pacchime ca bhave dāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:24.2":{"pli":"giribbajapuruttame; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:24.3":{"pli":"Jātā seṭṭhikule phīte, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:24.4":{"pli":"sabbakāmasamiddhine. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:25.1":{"pli":"Yadā rūpaguṇūpetā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:25.2":{"pli":"paṭhame yobbane ṭhitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:25.3":{"pli":"Tadā parakulaṃ gantvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:25.4":{"pli":"vasiṃ sukhasamappitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:26.1":{"pli":"Upetvā lokasaraṇaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:26.2":{"pli":"suṇitvā dhammadesanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:26.3":{"pli":"Anāgāmiphalaṃ patto, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:26.4":{"pli":"sāmiko me subuddhimā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:27.1":{"pli":"Tadāhaṃ anujānetvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:27.2":{"pli":"pabbajiṃ anagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:27.3":{"pli":"Na cireneva kālena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:27.4":{"pli":"arahattamapāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:28.1":{"pli":"Tadā upāsako so maṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:28.2":{"pli":"upagantvā apucchatha; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:28.3":{"pli":"Gambhīre nipuṇe pañhe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:28.4":{"pli":"te sabbe byākariṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:29.1":{"pli":"Jino tasmiṃ guṇe tuṭṭho, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:29.2":{"pli":"etadagge ṭhapesi maṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:29.3":{"pli":"‘Bhikkhuniṃ dhammakathikaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:29.4":{"pli":"nāññaṃ passāmi edisiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:30.1":{"pli":"Dhammadinnā yathā dhīrā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:30.2":{"pli":"evaṃ dhāretha bhikkhavo’; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:30.3":{"pli":"Evāhaṃ paṇḍitā homi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:30.4":{"pli":"nāyakenānukampitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:31.1":{"pli":"Pariciṇṇo mayā satthā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:31.2":{"pli":"kataṃ buddhassa sāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:31.3":{"pli":"Ohito garuko bhāro, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:31.4":{"pli":"bhavanetti samūhatā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:32.1":{"pli":"Yassatthāya pabbajitā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:32.2":{"pli":"agārasmānagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:32.3":{"pli":"So me attho anuppatto, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:32.4":{"pli":"sabbasaṃyojanakkhayo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:33.1":{"pli":"Iddhīsu ca vasī homi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:33.2":{"pli":"dibbāya sotadhātuyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:33.3":{"pli":"Paracittāni jānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:33.4":{"pli":"satthusāsanakārikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:34.1":{"pli":"Pubbenivāsaṃ jānāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:34.2":{"pli":"dibbacakkhu visodhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:34.3":{"pli":"Khepetvā āsave sabbe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:34.4":{"pli":"visuddhāsiṃ sunimmalā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:35.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:35.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:35.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:36.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:36.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:36.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap23:37.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap23:37.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap23:37.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap23:38.1":{"pli":"Itthaṃ sudaṃ dhammadinnā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap23:39.1":{"pli":"Dhammadinnātheriyāpadānaṃ tatiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"thi-ap24":{"thi-ap24:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap24'><header><ul><li class='division'>{}</li>"},"thi-ap24:0.2":{"pli":"Kuṇḍalakesīvagga ","html":"<li>{}</li></ul>"},"thi-ap24:0.3":{"pli":"Sakulātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap24:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap24:1.2":{"pli":"sabbadhammāna pāragū; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:1.3":{"pli":"Ito satasahassamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:1.4":{"pli":"kappe uppajji nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:2.1":{"pli":"Hitāya sabbasattānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:2.2":{"pli":"sukhāya vadataṃ varo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:2.3":{"pli":"Atthāya purisājañño, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:2.4":{"pli":"paṭipanno sadevake. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:3.1":{"pli":"Yasaggapatto sirimā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:3.2":{"pli":"kittivaṇṇagato jino; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:3.3":{"pli":"Pūjito sabbalokassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:3.4":{"pli":"disāsabbāsu vissuto. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:4.1":{"pli":"Uttiṇṇavicikiccho so, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:4.2":{"pli":"vītivattakathaṅkatho; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:4.3":{"pli":"Sampuṇṇamanasaṅkappo, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:4.4":{"pli":"patto sambodhimuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:5.1":{"pli":"Anuppannassa maggassa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:5.2":{"pli":"uppādetā naruttamo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:5.3":{"pli":"Anakkhātañca akkhāsi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:5.4":{"pli":"asañjātañca sañjanī. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:6.1":{"pli":"Maggaññū ca maggavidū, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:6.2":{"pli":"maggakkhāyī narāsabho; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:6.3":{"pli":"Maggassa kusalo satthā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:6.4":{"pli":"sārathīnaṃ varuttamo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:7.1":{"pli":"Mahākāruṇiko satthā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:7.2":{"pli":"dhammaṃ desesi nāyako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:7.3":{"pli":"Nimugge kāmapaṅkamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:7.4":{"pli":"samuddharati pāṇine. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:8.1":{"pli":"Tadāhaṃ haṃsavatiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:8.2":{"pli":"jātā khattiyanandanā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:8.3":{"pli":"Surūpā sadhanā cāpi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:8.4":{"pli":"dayitā ca sirīmatī. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:9.1":{"pli":"Ānandassa mahārañño, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:9.2":{"pli":"dhītā paramasobhaṇā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:9.3":{"pli":"Vemātā bhaginī cāpi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:9.4":{"pli":"padumuttaranāmino. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:10.1":{"pli":"Rājakaññāhi sahitā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:10.2":{"pli":"sabbābharaṇabhūsitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:10.3":{"pli":"Upāgamma mahāvīraṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:10.4":{"pli":"assosiṃ dhammadesanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:11.1":{"pli":"Tadā hi so lokagaru, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:11.2":{"pli":"bhikkhuniṃ dibbacakkhukaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:11.3":{"pli":"Kittayaṃ parisāmajjhe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:11.4":{"pli":"aggaṭṭhāne ṭhapesi taṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:12.1":{"pli":"Suṇitvā tamahaṃ haṭṭhā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:12.2":{"pli":"dānaṃ datvāna satthuno; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:12.3":{"pli":"Pūjitvāna ca sambuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:12.4":{"pli":"dibbacakkhuṃ apatthayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:13.1":{"pli":"Tato avoca maṃ satthā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:13.2":{"pli":"‘nande lacchasi patthitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:13.3":{"pli":"Padīpadhammadānānaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:13.4":{"pli":"phalametaṃ sunicchitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:14.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:14.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:14.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:14.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:15.1":{"pli":"Tassa dhammesu dāyādā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:15.2":{"pli":"orasā dhammanimmitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:15.3":{"pli":"Sakulā nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:15.4":{"pli":"hessati satthu sāvikā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:16.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:16.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:16.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:16.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:17.1":{"pli":"Imamhi bhaddake kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:17.2":{"pli":"brahmabandhu mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:17.3":{"pli":"Kassapo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:17.4":{"pli":"uppajji vadataṃ varo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:18.1":{"pli":"Paribbājakinī āsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:18.2":{"pli":"tadāhaṃ ekacārinī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:18.3":{"pli":"Bhikkhāya vicaritvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:18.4":{"pli":"alabhiṃ telamattakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:19.1":{"pli":"Tena dīpaṃ padīpetvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:19.2":{"pli":"upaṭṭhiṃ sabbasaṃvariṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:19.3":{"pli":"Cetiyaṃ dvipadaggassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:19.4":{"pli":"vippasannena cetasā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:20.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:20.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:20.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:20.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:21.1":{"pli":"Yattha yatthūpapajjāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:21.2":{"pli":"tassa kammassa vāhasā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:21.3":{"pli":"Pajjalanti mahādīpā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:21.4":{"pli":"tattha tattha gatāya me. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:22.1":{"pli":"Tirokuṭṭaṃ tiroselaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:22.2":{"pli":"samatiggayha pabbataṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:22.3":{"pli":"Passāmahaṃ yadicchāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:22.4":{"pli":"dīpadānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:23.1":{"pli":"Visuddhanayanā homi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:23.2":{"pli":"yasasā ca jalāmahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:23.3":{"pli":"Saddhāpaññāvatī ceva, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:23.4":{"pli":"dīpadānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:24.1":{"pli":"Pacchime ca bhave dāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:24.2":{"pli":"jātā vippakule ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:24.3":{"pli":"Pahūtadhanadhaññamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:24.4":{"pli":"mudite rājapūjite. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:25.1":{"pli":"Ahaṃ sabbaṅgasampannā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:25.2":{"pli":"sabbābharaṇabhūsitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:25.3":{"pli":"Purappavese sugataṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:25.4":{"pli":"vātapāne ṭhitā ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:26.1":{"pli":"Disvā jalantaṃ yasasā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:26.2":{"pli":"devamanussasakkataṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:26.3":{"pli":"Anubyañjanasampannaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:26.4":{"pli":"lakkhaṇehi vibhūsitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:27.1":{"pli":"Udaggacittā sumanā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:27.2":{"pli":"pabbajjaṃ samarocayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:27.3":{"pli":"Na cireneva kālena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:27.4":{"pli":"arahattamapāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:28.1":{"pli":"Iddhīsu ca vasī homi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:28.2":{"pli":"dibbāya sotadhātuyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:28.3":{"pli":"Paracittāni jānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:28.4":{"pli":"satthusāsanakārikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:29.1":{"pli":"Pubbenivāsaṃ jānāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:29.2":{"pli":"dibbacakkhu visodhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:29.3":{"pli":"Khepetvā āsave sabbe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:29.4":{"pli":"visuddhāsiṃ sunimmalā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:30.1":{"pli":"Pariciṇṇo mayā satthā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:30.2":{"pli":"kataṃ buddhassa sāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:30.3":{"pli":"Ohito garuko bhāro, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:30.4":{"pli":"bhavanetti samūhatā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:31.1":{"pli":"Yassatthāya pabbajitā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:31.2":{"pli":"agārasmānagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:31.3":{"pli":"So me attho anuppatto, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:31.4":{"pli":"sabbasaṃyojanakkhayo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:32.1":{"pli":"Tato mahākāruṇiko, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:32.2":{"pli":"etadagge ṭhapesi maṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:32.3":{"pli":"Dibbacakkhukānaṃ aggā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:32.4":{"pli":"sakulāti naruttamo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:33.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:33.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:33.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:34.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:34.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:34.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap24:35.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap24:35.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap24:35.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap24:36.1":{"pli":"Itthaṃ sudaṃ sakulā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap24:37.1":{"pli":"Sakulātheriyāpadānaṃ catutthaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}