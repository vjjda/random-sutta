{"tha-ap542":{"tha-ap542:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap542'><header><ul><li class='division'>{}</li>"},"tha-ap542:0.2":{"pli":"Kaccāyanavagga ","html":"<li>{}</li></ul>"},"tha-ap542:0.3":{"pli":"Mogharājattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap542:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap542:1.2":{"pli":"sabbalokavidū muni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:1.3":{"pli":"Ito satasahassamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:1.4":{"pli":"kappe uppajji cakkhumā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:2.1":{"pli":"Ovādako viññāpako, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:2.2":{"pli":"tārako sabbapāṇinaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:2.3":{"pli":"Desanākusalo buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:2.4":{"pli":"tāresi janataṃ bahuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:3.1":{"pli":"Anukampako kāruṇiko, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:3.2":{"pli":"hitesī sabbapāṇinaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:3.3":{"pli":"Sampatte titthiye sabbe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:3.4":{"pli":"pañcasīle patiṭṭhapi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:4.1":{"pli":"Evaṃ nirākulaṃ āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:4.2":{"pli":"suññataṃ titthiyehi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:4.3":{"pli":"Vicittaṃ arahantehi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:4.4":{"pli":"vasībhūtehi tādibhi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:5.1":{"pli":"Ratanānaṭṭhapaññāsaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:5.2":{"pli":"uggato so mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:5.3":{"pli":"Kañcanagghiyasaṅkāso, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:5.4":{"pli":"bāttiṃsavaralakkhaṇo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:6.1":{"pli":"Vassasatasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:6.2":{"pli":"āyu vijjati tāvade; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:6.3":{"pli":"Tāvatā tiṭṭhamāno so, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:6.4":{"pli":"tāresi janataṃ bahuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:7.1":{"pli":"Tadāhaṃ haṃsavatiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:7.2":{"pli":"kule aññatare ahuṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:7.3":{"pli":"Parakammāyane yutto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:7.4":{"pli":"natthi me kiñci sandhanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:8.1":{"pli":"Paṭikkamanasālāyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:8.2":{"pli":"vasanto katabhūmiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:8.3":{"pli":"Aggiṃ ujjālayiṃ tattha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:8.4":{"pli":"daḷhaṃ kaṇhāsi sā mahī. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:9.1":{"pli":"Tadā parisatiṃ nātho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:9.2":{"pli":"catusaccapakāsako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:9.3":{"pli":"Sāvakaṃ sampakittesi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:9.4":{"pli":"lūkhacīvaradhārakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:10.1":{"pli":"Tassa tamhi guṇe tuṭṭho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:10.2":{"pli":"paṇipacca tathāgataṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:10.3":{"pli":"Lūkhacīvaradhāraggaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:10.4":{"pli":"patthayiṃ ṭhānamuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:11.1":{"pli":"Tadā avoca bhagavā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:11.2":{"pli":"sāvake padumuttaro; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:11.3":{"pli":"‘Passathetaṃ purisakaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:11.4":{"pli":"kucelaṃ tanudehakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:12.1":{"pli":"Pītippasannavadanaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:12.2":{"pli":"saddhādhanasamanvitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:12.3":{"pli":"Udaggatanujaṃ haṭṭhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:12.4":{"pli":"acalaṃ sālapiṇḍitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:13.1":{"pli":"Eso pattheti taṃ ṭhānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:13.2":{"pli":"saccasenassa bhikkhuno; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:13.3":{"pli":"Lūkhacīvaradhārissa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:13.4":{"pli":"tassa vaṇṇasitāsayo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:14.1":{"pli":"Taṃ sutvā mudito hutvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:14.2":{"pli":"nipacca sirasā jinaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:14.3":{"pli":"Yāvajīvaṃ subhaṃ kammaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:14.4":{"pli":"karitvā jinasāsane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:15.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:15.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:15.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:15.4":{"pli":"tāvatiṃsūpago ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:16.1":{"pli":"Paṭikkamanasālāyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:16.2":{"pli":"bhūmidāhakakammunā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:16.3":{"pli":"Samasahassaṃ niraye, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:16.4":{"pli":"adayhiṃ vedanāṭṭito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:17.1":{"pli":"Tena kammāvasesena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:17.2":{"pli":"pañca jātisatānihaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:17.3":{"pli":"Manusso kulajo hutvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:17.4":{"pli":"jātiyā lakkhaṇaṅkito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:18.1":{"pli":"Pañca jātisatāneva, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:18.2":{"pli":"kuṭṭharogasamappito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:18.3":{"pli":"Mahādukkhaṃ anubhaviṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:18.4":{"pli":"tassa kammassa vāhasā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:19.1":{"pli":"Imasmiṃ bhaddake kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:19.2":{"pli":"upariṭṭhaṃ yasassinaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:19.3":{"pli":"Piṇḍapātena tappesiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:19.4":{"pli":"pasannamānaso ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:20.1":{"pli":"Tena kammavisesena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:20.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:20.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:20.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:21.1":{"pli":"Pacchime bhave sampatte, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:21.2":{"pli":"ajāyiṃ khattiye kule; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:21.3":{"pli":"Pituno accayenāhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:21.4":{"pli":"mahārajjasamappito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:22.1":{"pli":"Kuṭṭharogādhibhūtohaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:22.2":{"pli":"na ratiṃ na sukhaṃ labhe; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:22.3":{"pli":"Moghaṃ rajjaṃ sukhaṃ yasmā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:22.4":{"pli":"mogharājā tato ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:23.1":{"pli":"Kāyassa dosaṃ disvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:23.2":{"pli":"pabbajiṃ anagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:23.3":{"pli":"Bāvarissa dijaggassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:23.4":{"pli":"sissattaṃ ajjhupāgamiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:24.1":{"pli":"Mahatā parivārena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:24.2":{"pli":"upecca naranāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:24.3":{"pli":"Apucchiṃ nipuṇaṃ pañhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:24.4":{"pli":"taṃ vīraṃ vādisūdanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:25.1":{"pli":"‘Ayaṃ loko paro loko, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:25.2":{"pli":"brahmaloko sadevako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:25.3":{"pli":"Diṭṭhiṃ no nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:25.4":{"pli":"gotamassa yasassino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:26.1":{"pli":"Evābhikkantadassāviṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:26.2":{"pli":"atthi pañhena āgamaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:26.3":{"pli":"Kathaṃ lokaṃ avekkhantaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:26.4":{"pli":"maccurājā na passati’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:27.1":{"pli":"‘Suññato lokaṃ avekkhassu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:27.2":{"pli":"mogharāja sadā sato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:27.3":{"pli":"Attānudiṭṭhiṃ uhacca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:27.4":{"pli":"evaṃ maccutaro siyā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:28.1":{"pli":"Evaṃ lokaṃ avekkhantaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:28.2":{"pli":"maccurājā na passati’; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:28.3":{"pli":"Iti maṃ abhaṇi buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:28.4":{"pli":"sabbarogatikicchako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:29.1":{"pli":"Saha gāthāvasānena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:29.2":{"pli":"kesamassuvivajjito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:29.3":{"pli":"Kāsāvavatthavasano, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:29.4":{"pli":"āsiṃ bhikkhu tathārahā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:30.1":{"pli":"Saṅghikesu vihāresu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:30.2":{"pli":"na vasiṃ rogapīḷito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:30.3":{"pli":"Mā vihāro padussīti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:30.4":{"pli":"vātarogehi pīḷito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:31.1":{"pli":"Saṅkārakūṭā āhitvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:31.2":{"pli":"susānā rathikāhi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:31.3":{"pli":"Tato saṅghāṭiṃ karitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:31.4":{"pli":"dhārayiṃ lūkhacīvaraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:32.1":{"pli":"Mahābhisakko tasmiṃ me, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:32.2":{"pli":"guṇe tuṭṭho vināyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:32.3":{"pli":"Lūkhacīvaradhārīnaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:32.4":{"pli":"etadagge ṭhapesi maṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:33.1":{"pli":"Puññapāpaparikkhīṇo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:33.2":{"pli":"sabbarogavivajjito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:33.3":{"pli":"Sikhīva anupādāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:33.4":{"pli":"nibbāyissamanāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:34.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:34.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:34.3":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:35.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:35.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:35.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:36.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:36.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:36.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap542:37.1":{"pli":"Itthaṃ sudaṃ āyasmā mogharājā thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap542:38.1":{"pli":"Mogharājattherassāpadānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"tha-ap542:39.1":{"pli":"Kaccāyanavaggo catupaññāsamo. ","html":"<p class='endsection'>{}</p>"},"tha-ap542:40.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"tha-ap542:41.1":{"pli":"Kaccāno vakkalī thero, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"tha-ap542:41.2":{"pli":"mahākappinasavhayo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:41.3":{"pli":"Dabbo kumāranāmo ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:41.4":{"pli":"bāhiyo koṭṭhiko vasī. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap542:42.1":{"pli":"Uruveḷakassapo rādho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap542:42.2":{"pli":"mogharājā ca paṇḍito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:42.3":{"pli":"Tīṇi gāthāsatānettha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap542:42.4":{"pli":"bāsaṭṭhi ceva piṇḍitā. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}},"tha-ap543":{"tha-ap543:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap543'><header><ul><li class='division'>{}</li>"},"tha-ap543:0.2":{"pli":"Bhaddiyavagga ","html":"<li>{}</li></ul>"},"tha-ap543:0.3":{"pli":"1 Lakuṇḍabhaddiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap543:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap543:1.2":{"pli":"sabbadhammesu cakkhumā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:1.3":{"pli":"Ito satasahassamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:1.4":{"pli":"kappe uppajji nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:2.1":{"pli":"Tadāhaṃ haṃsavatiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:2.2":{"pli":"seṭṭhiputto mahaddhano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:2.3":{"pli":"Jaṅghāvihāraṃ vicaraṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:2.4":{"pli":"saṅghārāmaṃ agacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:3.1":{"pli":"Tadā so lokapajjoto, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:3.2":{"pli":"dhammaṃ desesi nāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:3.3":{"pli":"Mañjussarānaṃ pavaraṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:3.4":{"pli":"sāvakaṃ abhikittayi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:4.1":{"pli":"Taṃ sutvā mudito hutvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:4.2":{"pli":"kāraṃ katvā mahesino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:4.3":{"pli":"Vanditvā satthuno pāde, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:4.4":{"pli":"taṃ ṭhānamabhipatthayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:5.1":{"pli":"Tadā buddho viyākāsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:5.2":{"pli":"saṅghamajjhe vināyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:5.3":{"pli":"‘Anāgatamhi addhāne, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:5.4":{"pli":"lacchase taṃ manorathaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:6.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:6.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:6.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:6.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:7.1":{"pli":"Tassa dhammesu dāyādo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:7.2":{"pli":"oraso dhammanimmito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:7.3":{"pli":"Bhaddiyo nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:7.4":{"pli":"hessati satthu sāvako’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:8.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:8.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:8.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:8.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:9.1":{"pli":"Dvenavute ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:9.2":{"pli":"phusso uppajji nāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:9.3":{"pli":"Durāsado duppasaho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:9.4":{"pli":"sabbalokuttamo jino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:10.1":{"pli":"Caraṇena ca sampanno, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:10.2":{"pli":"brahā uju patāpavā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:10.3":{"pli":"Hitesī sabbasattānaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:10.4":{"pli":"bahuṃ mocesi bandhanā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:11.1":{"pli":"Nandārāmavane tassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:11.2":{"pli":"ahosiṃ phussakokilo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:11.3":{"pli":"Gandhakuṭisamāsanne, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:11.4":{"pli":"ambarukkhe vasāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:12.1":{"pli":"Tadā piṇḍāya gacchantaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:12.2":{"pli":"dakkhiṇeyyaṃ jinuttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:12.3":{"pli":"Disvā cittaṃ pasādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:12.4":{"pli":"mañjunābhinikūjahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:13.1":{"pli":"Rājuyyānaṃ tadā gantvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:13.2":{"pli":"supakkaṃ kanakattacaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:13.3":{"pli":"Ambapiṇḍaṃ gahetvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:13.4":{"pli":"sambuddhassopanāmayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:14.1":{"pli":"Tadā me cittamaññāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:14.2":{"pli":"mahākāruṇiko jino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:14.3":{"pli":"Upaṭṭhākassa hatthato, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:14.4":{"pli":"pattaṃ paggaṇhi nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:15.1":{"pli":"Adāsiṃ haṭṭhacittohaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:15.2":{"pli":"ambapiṇḍaṃ mahāmune; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:15.3":{"pli":"Patte pakkhippa pakkhehi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:15.4":{"pli":"pañjaliṃ katvāna mañjunā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:16.1":{"pli":"Sarena rajanīyena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:16.2":{"pli":"savanīyena vaggunā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:16.3":{"pli":"Vassanto buddhapūjatthaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:16.4":{"pli":"nīḷaṃ gantvā nipajjahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:17.1":{"pli":"Tadā muditacittaṃ maṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:17.2":{"pli":"buddhapemagatāsayaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:17.3":{"pli":"Sakuṇagghi upāgantvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:17.4":{"pli":"ghātayī duṭṭhamānaso. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:18.1":{"pli":"Tato cutohaṃ tusite, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:18.2":{"pli":"anubhotvā mahāsukhaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:18.3":{"pli":"Manussayonimāgacchiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:18.4":{"pli":"tassa kammassa vāhasā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:19.1":{"pli":"Imamhi bhaddake kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:19.2":{"pli":"brahmabandhu mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:19.3":{"pli":"Kassapo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:19.4":{"pli":"uppajji vadataṃ varo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:20.1":{"pli":"Sāsanaṃ jotayitvā so, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:20.2":{"pli":"abhibhuyya kutitthiye; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:20.3":{"pli":"Vinayitvāna veneyye, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:20.4":{"pli":"nibbuto so sasāvako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:21.1":{"pli":"Nibbute tamhi lokagge, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:21.2":{"pli":"pasannā janatā bahū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:21.3":{"pli":"Pūjanatthāya buddhassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:21.4":{"pli":"thūpaṃ kubbanti satthuno. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:22.1":{"pli":"‘Sattayojanikaṃ thūpaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:22.2":{"pli":"sattaratanabhūsitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:22.3":{"pli":"Karissāma mahesissa’, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:22.4":{"pli":"iccevaṃ mantayanti te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:23.1":{"pli":"Kikino kāsirājassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:23.2":{"pli":"tadā senāya nāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:23.3":{"pli":"Hutvāhaṃ appamāṇassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:23.4":{"pli":"pamāṇaṃ cetiye vadiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:24.1":{"pli":"Tadā te mama vākyena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:24.2":{"pli":"cetiyaṃ yojanuggataṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:24.3":{"pli":"Akaṃsu naravīrassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:24.4":{"pli":"nānāratanabhūsitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:25.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:25.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:25.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:25.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:26.1":{"pli":"Pacchime ca bhave dāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:26.2":{"pli":"jāto seṭṭhikule ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:26.3":{"pli":"Sāvatthiyaṃ puravare, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:26.4":{"pli":"iddhe phīte mahaddhane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:27.1":{"pli":"Purappavese sugataṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:27.2":{"pli":"disvā vimhitamānaso; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:27.3":{"pli":"Pabbajitvāna na ciraṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:27.4":{"pli":"arahattamapāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:28.1":{"pli":"Cetiyassa pamāṇaṃ yaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:28.2":{"pli":"akariṃ tena kammunā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:28.3":{"pli":"Lakuṇḍakasarīrohaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:28.4":{"pli":"jāto paribhavāraho. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:29.1":{"pli":"Sarena madhurenāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:29.2":{"pli":"pūjitvā isisattamaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:29.3":{"pli":"Mañjussarānaṃ bhikkhūnaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:29.4":{"pli":"aggattamanupāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:30.1":{"pli":"Phaladānena buddhassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:30.2":{"pli":"guṇānussaraṇena ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:30.3":{"pli":"Sāmaññaphalasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:30.4":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:31.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:31.2":{"pli":"bhavā sabbe samūhatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:31.3":{"pli":"Nāgova bandhanaṃ chetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:31.4":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:32.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:32.2":{"pli":"buddhaseṭṭhassa santike; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:32.3":{"pli":"Tisso vijjā anuppattā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:32.4":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap543:33.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap543:33.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:33.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap543:33.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap543:34.1":{"pli":"Itthaṃ sudaṃ āyasmā lakuṇḍabhaddiyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap543:35.1":{"pli":"Lakuṇḍabhaddiyattherassāpadānaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap544":{"tha-ap544:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap544'><header><ul><li class='division'>{}</li>"},"tha-ap544:0.2":{"pli":"Bhaddiyavagga ","html":"<li>{}</li></ul>"},"tha-ap544:0.3":{"pli":"Kaṅkhārevatattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap544:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap544:1.2":{"pli":"sabbadhammesu cakkhumā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:1.3":{"pli":"Ito satasahassamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:1.4":{"pli":"kappe uppajji nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:2.1":{"pli":"Sīhahanu brahmagiro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:2.2":{"pli":"haṃsadundubhinissano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:2.3":{"pli":"Nāgavikkantagamano, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:2.4":{"pli":"candasūrādikappabho. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:3.1":{"pli":"Mahāmatī mahāvīro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:3.2":{"pli":"mahājhāyī mahābalo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:3.3":{"pli":"Mahākāruṇiko nātho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:3.4":{"pli":"mahātamapanūdano. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:4.1":{"pli":"Sa kadāci tilokaggo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:4.2":{"pli":"veneyyaṃ vinayaṃ bahuṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:4.3":{"pli":"Dhammaṃ desesi sambuddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:4.4":{"pli":"sattāsayavidū muni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:5.1":{"pli":"Jhāyiṃ jhānarataṃ vīraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:5.2":{"pli":"upasantaṃ anāvilaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:5.3":{"pli":"Vaṇṇayanto parisatiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:5.4":{"pli":"tosesi janataṃ jino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:6.1":{"pli":"Tadāhaṃ haṃsavatiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:6.2":{"pli":"brāhmaṇo vedapāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:6.3":{"pli":"Dhammaṃ sutvāna mudito, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:6.4":{"pli":"taṃ ṭhānamabhipatthayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:7.1":{"pli":"Tadā jino viyākāsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:7.2":{"pli":"saṅghamajjhe vināyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:7.3":{"pli":"‘Mudito hohi tvaṃ brahme, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:7.4":{"pli":"lacchase taṃ manorathaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:8.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:8.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:8.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:8.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:9.1":{"pli":"Tassa dhammesu dāyādo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:9.2":{"pli":"oraso dhammanimmito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:9.3":{"pli":"Revato nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:9.4":{"pli":"hessati satthu sāvako’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:10.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:10.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:10.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:10.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:11.1":{"pli":"Pacchime ca bhave dāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:11.2":{"pli":"jātohaṃ koliye pure; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:11.3":{"pli":"Khattiye kulasampanne, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:11.4":{"pli":"iddhe phīte mahaddhane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:12.1":{"pli":"Yadā kapilavatthusmiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:12.2":{"pli":"buddho dhammamadesayi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:12.3":{"pli":"Tadā pasanno sugate, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:12.4":{"pli":"pabbajiṃ anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:13.1":{"pli":"Kaṅkhā me bahulā āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:13.2":{"pli":"kappākappe tahiṃ tahiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:13.3":{"pli":"Sabbaṃ taṃ vinayī buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:13.4":{"pli":"desetvā dhammamuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:14.1":{"pli":"Tatohaṃ tiṇṇasaṃsāro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:14.2":{"pli":"sadā jhānasukhe rato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:14.3":{"pli":"Viharāmi tadā buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:14.4":{"pli":"maṃ disvā etadabravi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:15.1":{"pli":"‘Yā kāci kaṅkhā idha vā huraṃ vā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:15.2":{"pli":"Sakavediyā vā paravediyā vā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:15.3":{"pli":"Ye jhāyino tā pajahanti sabbā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:15.4":{"pli":"Ātāpino brahmacariyaṃ carantā’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:16.1":{"pli":"Satasahasse kataṃ kammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:16.2":{"pli":"phalaṃ dassesi me idha; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:16.3":{"pli":"Sumutto saravegova, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:16.4":{"pli":"kilese jhāpayiṃ mama. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:17.1":{"pli":"Tato jhānarataṃ disvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:17.2":{"pli":"buddho lokantagū muni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:17.3":{"pli":"Jhāyīnaṃ bhikkhūnaṃ aggo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:17.4":{"pli":"paññāpeti mahāmati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:18.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:18.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:18.3":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:19.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:19.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:19.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap544:20.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap544:20.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap544:20.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap544:21.1":{"pli":"Itthaṃ sudaṃ āyasmā kaṅkhārevato thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap544:22.1":{"pli":"Kaṅkhārevatattherassāpadānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}