{"vv19":{"vv19:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv19'><header><ul><li class='division'>{}</li>"},"vv19:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv19:0.3":{"pli":"Cittalatāvagga ","html":"<li>{}</li></ul>"},"vv19:0.4":{"pli":"2. Lakhumāvimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv19:1.1":{"pli":"“Abhikkantena vaṇṇena, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv19:1.2":{"pli":"yā tvaṃ tiṭṭhasi devate; ","html":"<span class='verse-line'>{}</span>"},"vv19:1.3":{"pli":"Obhāsentī disā sabbā, ","html":"<span class='verse-line'>{}</span>"},"vv19:1.4":{"pli":"osadhī viya tārakā. ","html":"<span class='verse-line'>{}</span></p>"},"vv19:2.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv19:2.2":{"pli":"kena te idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv19:2.3":{"pli":"Uppajjanti ca te bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv19:2.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv19:3.1":{"pli":"Pucchāmi taṃ devi mahānubhāve, ","html":"<p><span class='verse-line'>{}</span>"},"vv19:3.2":{"pli":"Manussabhūtā kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv19:3.3":{"pli":"Kenāsi evaṃ jalitānubhāvā, ","html":"<span class='verse-line'>{}</span>"},"vv19:3.4":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv19:4.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv19:4.2":{"pli":"moggallānena pucchitā; ","html":"<span class='verse-line'>{}</span>"},"vv19:4.3":{"pli":"Pañhaṃ puṭṭhā viyākāsi, ","html":"<span class='verse-line'>{}</span>"},"vv19:4.4":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv19:5.1":{"pli":"“Kevaṭṭadvārā nikkhamma, ","html":"<p><span class='verse-line'>{}</span>"},"vv19:5.2":{"pli":"ahu mayhaṃ nivesanaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv19:5.3":{"pli":"Tattha sañcaramānānaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv19:5.4":{"pli":"sāvakānaṃ mahesinaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv19:6.1":{"pli":"Odanaṃ kummāsaṃ ḍākaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv19:6.2":{"pli":"loṇasovīrakañcahaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv19:6.3":{"pli":"Adāsiṃ ujubhūtesu, ","html":"<span class='verse-line'>{}</span>"},"vv19:6.4":{"pli":"vippasannena cetasā. ","html":"<span class='verse-line'>{}</span></p>"},"vv19:7.1":{"pli":"Cātuddasiṃ pañcadasiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv19:7.2":{"pli":"yā ca pakkhassa aṭṭhamī; ","html":"<span class='verse-line'>{}</span>"},"vv19:7.3":{"pli":"Pāṭihāriyapakkhañca, ","html":"<span class='verse-line'>{}</span>"},"vv19:7.4":{"pli":"aṭṭhaṅgasusamāgataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv19:8.1":{"pli":"Uposathaṃ upavasissaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv19:8.2":{"pli":"sadā sīlesu saṃvutā; ","html":"<span class='verse-line'>{}</span>"},"vv19:8.3":{"pli":"Saññamā saṃvibhāgā ca, ","html":"<span class='verse-line'>{}</span>"},"vv19:8.4":{"pli":"vimānaṃ āvasāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv19:9.1":{"pli":"Pāṇātipātā viratā, ","html":"<p><span class='verse-line'>{}</span>"},"vv19:9.2":{"pli":"musāvādā ca saññatā; ","html":"<span class='verse-line'>{}</span>"},"vv19:9.3":{"pli":"Theyyā ca aticārā ca, ","html":"<span class='verse-line'>{}</span>"},"vv19:9.4":{"pli":"majjapānā ca ārakā. ","html":"<span class='verse-line'>{}</span></p>"},"vv19:10.1":{"pli":"Pañcasikkhāpade ratā, ","html":"<p><span class='verse-line'>{}</span>"},"vv19:10.2":{"pli":"ariyasaccāna kovidā; ","html":"<span class='verse-line'>{}</span>"},"vv19:10.3":{"pli":"Upāsikā cakkhumato, ","html":"<span class='verse-line'>{}</span>"},"vv19:10.4":{"pli":"gotamassa yasassino. ","html":"<span class='verse-line'>{}</span></p>"},"vv19:11.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv19:11.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"vv19:11.3":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatīti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv19:12.1":{"pli":"Mama ca, bhante, vacanena bhagavato pāde sirasā vandeyyāsi—","html":"<p>{}"},"vv19:12.2":{"pli":"‘lakhumā nāma, bhante, upāsikā bhagavato pāde sirasā vandatī’ti. ","html":"{}"},"vv19:12.3":{"pli":"Anacchariyaṃ kho panetaṃ, bhante, yaṃ maṃ bhagavā aññatarasmiṃ sāmaññaphale byākareyya. ","html":"{}"},"vv19:12.4":{"pli":"Taṃ bhagavā sakadāgāmiphale byākāsī”ti. ","html":"{}</p>"},"vv19:13.1":{"pli":"Lakhumāvimānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv2":{"vv2:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv2'><header><ul><li class='division'>{}</li>"},"vv2:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv2:0.3":{"pli":"Pīṭhavagga ","html":"<li>{}</li></ul>"},"vv2:0.4":{"pli":"2. Dutiyapīṭhavimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv2:1.1":{"pli":"“Pīṭhaṃ te veḷuriyamayaṃ uḷāraṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv2:1.2":{"pli":"Manojavaṃ gacchati yenakāmaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv2:1.3":{"pli":"Alaṅkate malyadhare suvatthe, ","html":"<span class='verse-line'>{}</span>"},"vv2:1.4":{"pli":"Obhāsasi vijjurivabbhakūṭaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv2:2.1":{"pli":"Kena tetādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv2:2.2":{"pli":"kena te idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv2:2.3":{"pli":"Uppajjanti ca te bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv2:2.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv2:3.1":{"pli":"Pucchāmi taṃ devi mahānubhāve, ","html":"<p><span class='verse-line'>{}</span>"},"vv2:3.2":{"pli":"Manussabhūtā kimakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv2:3.3":{"pli":"Kenāsi evaṃ jalitānubhāvā, ","html":"<span class='verse-line'>{}</span>"},"vv2:3.4":{"pli":"Vaṇṇo ca te sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv2:4.1":{"pli":"Sā devatā attamanā, ","html":"<p><span class='verse-line'>{}</span>"},"vv2:4.2":{"pli":"moggallānena pucchitā; ","html":"<span class='verse-line'>{}</span>"},"vv2:4.3":{"pli":"Pañhaṃ puṭṭhā viyākāsi, ","html":"<span class='verse-line'>{}</span>"},"vv2:4.4":{"pli":"yassa kammassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv2:5.1":{"pli":"“Ahaṃ manussesu manussabhūtā, ","html":"<p><span class='verse-line'>{}</span>"},"vv2:5.2":{"pli":"Abbhāgatānāsanakaṃ adāsiṃ; ","html":"<span class='verse-line'>{}</span>"},"vv2:5.3":{"pli":"Abhivādayiṃ añjalikaṃ akāsiṃ, ","html":"<span class='verse-line'>{}</span>"},"vv2:5.4":{"pli":"Yathānubhāvañca adāsi dānaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv2:6.1":{"pli":"Tena metādiso vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"vv2:6.2":{"pli":"tena me idha mijjhati; ","html":"<span class='verse-line'>{}</span>"},"vv2:6.3":{"pli":"Uppajjanti ca me bhogā, ","html":"<span class='verse-line'>{}</span>"},"vv2:6.4":{"pli":"ye keci manaso piyā. ","html":"<span class='verse-line'>{}</span></p>"},"vv2:7.1":{"pli":"Akkhāmi te bhikkhu mahānubhāva, ","html":"<p><span class='verse-line'>{}</span>"},"vv2:7.2":{"pli":"Manussabhūtā yamakāsi puññaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv2:7.3":{"pli":"Tenamhi evaṃ jalitānubhāvā, ","html":"<span class='verse-line'>{}</span>"},"vv2:7.4":{"pli":"Vaṇṇo ca me sabbadisā pabhāsatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv2:8.1":{"pli":"Dutiyapīṭhavimānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv20":{"vv20:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv20'><header><ul><li class='division'>{}</li>"},"vv20:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv20:0.3":{"pli":"Cittalatāvagga ","html":"<li>{}</li></ul>"},"vv20:0.4":{"pli":"3. Ācāmadāyikāvimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv20:1.1":{"pli":"“Piṇḍāya te carantassa, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv20:1.2":{"pli":"tuṇhībhūtassa tiṭṭhato; ","html":"<span class='verse-line'>{}</span>"},"vv20:1.3":{"pli":"Daliddā kapaṇā nārī, ","html":"<span class='verse-line'>{}</span>"},"vv20:1.4":{"pli":"parāgāraṃ apassitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv20:2.1":{"pli":"Yā te adāsi ācāmaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv20:2.2":{"pli":"pasannā sehi pāṇibhi; ","html":"<span class='verse-line'>{}</span>"},"vv20:2.3":{"pli":"Sā hitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv20:2.4":{"pli":"kaṃ nu sā disataṃ gatā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv20:3.1":{"pli":"“Piṇḍāya me carantassa, ","html":"<p><span class='verse-line'>{}</span>"},"vv20:3.2":{"pli":"tuṇhībhūtassa tiṭṭhato; ","html":"<span class='verse-line'>{}</span>"},"vv20:3.3":{"pli":"Daliddā kapaṇā nārī, ","html":"<span class='verse-line'>{}</span>"},"vv20:3.4":{"pli":"parāgāraṃ apassitā. ","html":"<span class='verse-line'>{}</span></p>"},"vv20:4.1":{"pli":"Yā me adāsi ācāmaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv20:4.2":{"pli":"pasannā sehi pāṇibhi; ","html":"<span class='verse-line'>{}</span>"},"vv20:4.3":{"pli":"Sā hitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv20:4.4":{"pli":"vippamuttā ito cutā. ","html":"<span class='verse-line'>{}</span></p>"},"vv20:5.1":{"pli":"Nimmānaratino nāma, ","html":"<p><span class='verse-line'>{}</span>"},"vv20:5.2":{"pli":"santi devā mahiddhikā; ","html":"<span class='verse-line'>{}</span>"},"vv20:5.3":{"pli":"Tattha sā sukhitā nārī, ","html":"<span class='verse-line'>{}</span>"},"vv20:5.4":{"pli":"modatācāmadāyikā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv20:6.1":{"pli":"“Aho dānaṃ varākiyā, ","html":"<p><span class='verse-line'>{}</span>"},"vv20:6.2":{"pli":"kassape suppatiṭṭhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv20:6.3":{"pli":"Parābhatena dānena, ","html":"<span class='verse-line'>{}</span>"},"vv20:6.4":{"pli":"ijjhittha vata dakkhiṇā. ","html":"<span class='verse-line'>{}</span></p>"},"vv20:7.1":{"pli":"Yā mahesittaṃ kāreyya, ","html":"<p><span class='verse-line'>{}</span>"},"vv20:7.2":{"pli":"cakkavattissa rājino; ","html":"<span class='verse-line'>{}</span>"},"vv20:7.3":{"pli":"Nārī sabbaṅgakalyāṇī, ","html":"<span class='verse-line'>{}</span>"},"vv20:7.4":{"pli":"bhattu cānomadassikā; ","html":"<span class='verse-line'>{}</span>"},"vv20:7.5":{"pli":"Etassācāmadānassa, ","html":"<span class='verse-line'>{}</span>"},"vv20:7.6":{"pli":"kalaṃ nāgghati soḷasiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv20:8.1":{"pli":"Sataṃ nikkhā sataṃ assā, ","html":"<p><span class='verse-line'>{}</span>"},"vv20:8.2":{"pli":"sataṃ assatarīrathā; ","html":"<span class='verse-line'>{}</span>"},"vv20:8.3":{"pli":"Sataṃ kaññāsahassāni, ","html":"<span class='verse-line'>{}</span>"},"vv20:8.4":{"pli":"āmuttamaṇikuṇḍalā; ","html":"<span class='verse-line'>{}</span>"},"vv20:8.5":{"pli":"Etassācāmadānassa, ","html":"<span class='verse-line'>{}</span>"},"vv20:8.6":{"pli":"kalaṃ nāgghanti soḷasiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv20:9.1":{"pli":"Sataṃ hemavatā nāgā, ","html":"<p><span class='verse-line'>{}</span>"},"vv20:9.2":{"pli":"īsādantā urūḷhavā; ","html":"<span class='verse-line'>{}</span>"},"vv20:9.3":{"pli":"Suvaṇṇakacchā mātaṅgā, ","html":"<span class='verse-line'>{}</span>"},"vv20:9.4":{"pli":"hemakappanavāsasā; ","html":"<span class='verse-line'>{}</span>"},"vv20:9.5":{"pli":"Etassācāmadānassa, ","html":"<span class='verse-line'>{}</span>"},"vv20:9.6":{"pli":"kalaṃ nāgghati soḷasiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"vv20:10.1":{"pli":"Catunnamapi dīpānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv20:10.2":{"pli":"issaraṃ yodha kāraye; ","html":"<span class='verse-line'>{}</span>"},"vv20:10.3":{"pli":"Etassācāmadānassa, ","html":"<span class='verse-line'>{}</span>"},"vv20:10.4":{"pli":"kalaṃ nāgghati soḷasin”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"vv20:11.1":{"pli":"Ācāmadāyikāvimānaṃ tatiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"vv21":{"vv21:0.1":{"pli":"Vimānavatthu ","html":"<article id='vv21'><header><ul><li class='division'>{}</li>"},"vv21:0.2":{"pli":"Itthivimāna ","html":"<li>{}</li>"},"vv21:0.3":{"pli":"Cittalatāvagga ","html":"<li>{}</li></ul>"},"vv21:0.4":{"pli":"4. Caṇḍālivimānavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"vv21:1.1":{"pli":"“Caṇḍāli vanda pādāni, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"vv21:1.2":{"pli":"gotamassa yasassino; ","html":"<span class='verse-line'>{}</span>"},"vv21:1.3":{"pli":"Tameva anukampāya, ","html":"<span class='verse-line'>{}</span>"},"vv21:1.4":{"pli":"aṭṭhāsi isisattamo. ","html":"<span class='verse-line'>{}</span></p>"},"vv21:2.1":{"pli":"Abhippasādehi manaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv21:2.2":{"pli":"arahantamhi tādini; ","html":"<span class='verse-line'>{}</span>"},"vv21:2.3":{"pli":"Khippaṃ pañjalikā vanda, ","html":"<span class='verse-line'>{}</span>"},"vv21:2.4":{"pli":"parittaṃ tava jīvitan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv21:3.1":{"pli":"Coditā bhāvitattena, ","html":"<p><span class='verse-line'>{}</span>"},"vv21:3.2":{"pli":"sarīrantimadhārinā; ","html":"<span class='verse-line'>{}</span>"},"vv21:3.3":{"pli":"Caṇḍālī vandi pādāni, ","html":"<span class='verse-line'>{}</span>"},"vv21:3.4":{"pli":"gotamassa yasassino. ","html":"<span class='verse-line'>{}</span></p>"},"vv21:4.1":{"pli":"Tamenaṃ avadhī gāvī, ","html":"<p><span class='verse-line'>{}</span>"},"vv21:4.2":{"pli":"caṇḍāliṃ pañjaliṃ ṭhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv21:4.3":{"pli":"Namassamānaṃ sambuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv21:4.4":{"pli":"andhakāre pabhaṅkaranti. ","html":"<span class='verse-line'>{}</span></p>"},"vv21:5.1":{"pli":"“Khīṇāsavaṃ vigatarajaṃ anejaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv21:5.2":{"pli":"Ekaṃ araññamhi raho nisinnaṃ; ","html":"<span class='verse-line'>{}</span>"},"vv21:5.3":{"pli":"Deviddhipattā upasaṅkamitvā, ","html":"<span class='verse-line'>{}</span>"},"vv21:5.4":{"pli":"Vandāmi taṃ vīra mahānubhāvan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv21:6.1":{"pli":"“Suvaṇṇavaṇṇā jalitā mahāyasā, ","html":"<p><span class='verse-line'>{}</span>"},"vv21:6.2":{"pli":"Vimānamoruyha anekacittā; ","html":"<span class='verse-line'>{}</span>"},"vv21:6.3":{"pli":"Parivāritā accharāsaṅgaṇena, ","html":"<span class='verse-line'>{}</span>"},"vv21:6.4":{"pli":"Kā tvaṃ subhe devate vandase maman”ti. ","html":"<span class='verse-line'>{}</span></p>"},"vv21:7.1":{"pli":"“Ahaṃ bhaddante caṇḍālī, ","html":"<p><span class='verse-line'>{}</span>"},"vv21:7.2":{"pli":"tayā vīrena pesitā; ","html":"<span class='verse-line'>{}</span>"},"vv21:7.3":{"pli":"Vandiṃ arahato pāde, ","html":"<span class='verse-line'>{}</span>"},"vv21:7.4":{"pli":"gotamassa yasassino. ","html":"<span class='verse-line'>{}</span></p>"},"vv21:8.1":{"pli":"Sāhaṃ vanditvā pādāni, ","html":"<p><span class='verse-line'>{}</span>"},"vv21:8.2":{"pli":"cutā caṇḍālayoniyā; ","html":"<span class='verse-line'>{}</span>"},"vv21:8.3":{"pli":"Vimānaṃ sabbato bhaddaṃ, ","html":"<span class='verse-line'>{}</span>"},"vv21:8.4":{"pli":"upapannamhi nandane. ","html":"<span class='verse-line'>{}</span></p>"},"vv21:9.1":{"pli":"Accharānaṃ satasahassaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"vv21:9.2":{"pli":"purakkhatvāna tiṭṭhati; ","html":"<span class='verse-line'>{}</span>"},"vv21:9.3":{"pli":"Tāsāhaṃ pavarā seṭṭhā, ","html":"<span class='verse-line'>{}</span>"},"