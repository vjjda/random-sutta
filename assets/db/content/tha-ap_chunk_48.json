{"tha-ap519":{"tha-ap519:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap519'><header><ul><li class='division'>{}</li>"},"tha-ap519:0.2":{"pli":"Phaladāyakavagga ","html":"<li>{}</li></ul>"},"tha-ap519:0.3":{"pli":"Kuṭajapupphiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap519:1.1":{"pli":"“Himavantassāvidūre, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap519:1.2":{"pli":"vasalo nāma pabbato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap519:1.3":{"pli":"Buddho sudassano nāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap519:1.4":{"pli":"vasate pabbatantare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap519:2.1":{"pli":"Pupphaṃ hemavantaṃ gayha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap519:2.2":{"pli":"vehāsaṃ agamāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap519:2.3":{"pli":"Tatthaddasāsiṃ sambuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap519:2.4":{"pli":"oghatiṇṇamanāsavaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap519:3.1":{"pli":"Pupphaṃ kuṭajamādāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap519:3.2":{"pli":"sire katvāna añjaliṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap519:3.3":{"pli":"Buddhassa abhiropesiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap519:3.4":{"pli":"sayambhussa mahesino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap519:4.1":{"pli":"Ekatiṃse ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap519:4.2":{"pli":"yaṃ pupphamabhipūjayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap519:4.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap519:4.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap519:5.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap519:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap519:5.3":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap519:6.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap519:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap519:6.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap519:7.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap519:7.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap519:7.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap519:8.1":{"pli":"Itthaṃ sudaṃ āyasmā kuṭajapupphiyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap519:9.1":{"pli":"Kuṭajapupphiyattherassāpadānaṃ sattamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap52":{"tha-ap52:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap52'><header><ul><li class='division'>{}</li>"},"tha-ap52:0.2":{"pli":"Upālivagga ","html":"<li>{}</li></ul>"},"tha-ap52:0.3":{"pli":"Cundattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap52:1.1":{"pli":"“Siddhatthassa bhagavato, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap52:1.2":{"pli":"lokajeṭṭhassa tādino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:1.3":{"pli":"Agghiyaṃ kārayitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:1.4":{"pli":"jātipupphehi chādayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:2.1":{"pli":"Niṭṭhāpetvāna taṃ pupphaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:2.2":{"pli":"buddhassa upanāmayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:2.3":{"pli":"Pupphāvasesaṃ paggayha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:2.4":{"pli":"buddhassa abhiropayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:3.1":{"pli":"Kañcanagghiyasaṅkāsaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:3.2":{"pli":"buddhaṃ lokagganāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:3.3":{"pli":"Pasannacitto sumano, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:3.4":{"pli":"pupphagghiyamupānayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:4.1":{"pli":"Vitiṇṇakaṅkho sambuddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:4.2":{"pli":"tiṇṇoghehi purakkhato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:4.3":{"pli":"Bhikkhusaṅghe nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:4.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:5.1":{"pli":"‘Dibbagandhaṃ pavāyantaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:5.2":{"pli":"yo me pupphagghiyaṃ adā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:5.3":{"pli":"Tamahaṃ kittayissāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:5.4":{"pli":"suṇātha mama bhāsato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:6.1":{"pli":"Ito cuto ayaṃ poso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:6.2":{"pli":"devasaṅghapurakkhato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:6.3":{"pli":"Jātipupphehi parikiṇṇo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:6.4":{"pli":"devalokaṃ gamissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:7.1":{"pli":"Ubbiddhaṃ bhavanaṃ tassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:7.2":{"pli":"Sovaṇṇañca maṇīmayaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:7.3":{"pli":"Byamhaṃ pātubhavissati, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:7.4":{"pli":"Puññakammappabhāvitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:8.1":{"pli":"Catusattatikkhattuṃ so, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:8.2":{"pli":"devarajjaṃ karissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:8.3":{"pli":"Anubhossati sampattiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:8.4":{"pli":"accharāhi purakkhato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:9.1":{"pli":"Pathabyā rajjaṃ tisataṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:9.2":{"pli":"vasudhaṃ āvasissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:9.3":{"pli":"Pañcasattatikkhattuñca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:9.4":{"pli":"cakkavattī bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:10.1":{"pli":"Dujjayo nāma nāmena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:10.2":{"pli":"hessati manujādhipo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:10.3":{"pli":"Anubhotvāna taṃ puññaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:10.4":{"pli":"sakakammaṃ apassito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:11.1":{"pli":"Vinipātaṃ agantvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:11.2":{"pli":"manussattaṃ gamissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:11.3":{"pli":"Hiraññaṃ tassa nicitaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:11.4":{"pli":"koṭisatamanappakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:12.1":{"pli":"Nibbattissati yonimhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:12.2":{"pli":"brāhmaṇe so bhavissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:12.3":{"pli":"Vaṅgantassa suto dhīmā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:12.4":{"pli":"sāriyā oraso piyo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:13.1":{"pli":"So ca pacchā pabbajitvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:13.2":{"pli":"aṅgīrasassa sāsane; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:13.3":{"pli":"Cūḷacundoti nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:13.4":{"pli":"hessati satthu sāvako. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:14.1":{"pli":"Sāmaṇerova so santo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:14.2":{"pli":"khīṇāsavo bhavissati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:14.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:14.4":{"pli":"nibbāyissatināsavo’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:15.1":{"pli":"Upaṭṭhahiṃ mahāvīraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:15.2":{"pli":"aññe ca pesale bahū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:15.3":{"pli":"Bhātaraṃ me cupaṭṭhāsiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:15.4":{"pli":"uttamatthassa pattiyā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:16.1":{"pli":"Bhātaraṃ me upaṭṭhitvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:16.2":{"pli":"dhātuṃ pattamhi ohiya; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:16.3":{"pli":"Sambuddhaṃ upanāmesiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:16.4":{"pli":"lokajeṭṭhaṃ narāsabhaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:17.1":{"pli":"Ubho hatthehi paggayha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:17.2":{"pli":"buddho loke sadevake; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:17.3":{"pli":"Sandassayanto taṃ dhātuṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:17.4":{"pli":"kittayi aggasāvakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:18.1":{"pli":"Cittañca suvimuttaṃ me, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:18.2":{"pli":"saddhā mayhaṃ patiṭṭhitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:18.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:18.4":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap52:19.1":{"pli":"Paṭisambhidānuppattā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap52:19.2":{"pli":"Vimokkhāpi ca phassitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:19.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:19.4":{"pli":"Kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap52:20.1":{"pli":"Itthaṃ sudaṃ āyasmā cundo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap52:21.1":{"pli":"Cundattherassāpadānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"tha-ap52:22.1":{"pli":"Upālivaggo pañcamo. ","html":"<p class='endsection'>{}</p>"},"tha-ap52:23.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"tha-ap52:24.1":{"pli":"Upāli soṇo bhaddiyo, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"tha-ap52:24.2":{"pli":"sanniṭṭhāpakahatthiyo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:24.3":{"pli":"Chadanaṃ seyyacaṅkamaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:24.4":{"pli":"subhaddo cundasavhayo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:24.5":{"pli":"Gāthāsataṃ satālīsaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap52:24.6":{"pli":"catasso ca taduttari. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}},"tha-ap520":{"tha-ap520:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap520'><header><ul><li class='division'>{}</li>"},"tha-ap520:0.2":{"pli":"Phaladāyakavagga ","html":"<li>{}</li></ul>"},"tha-ap520:0.3":{"pli":"Ghosasaññakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap520:1.1":{"pli":"“Migaluddo pure āsiṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap520:1.2":{"pli":"araññe vipine ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap520:1.3":{"pli":"Addasaṃ virajaṃ buddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap520:1.4":{"pli":"devasaṅghapurakkhataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap520:2.1":{"pli":"Catusaccaṃ pakāsentaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap520:2.2":{"pli":"desentaṃ amataṃ padaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap520:2.3":{"pli":"Assosiṃ madhuraṃ dhammaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap520:2.4":{"pli":"sikhino lokabandhuno. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap520:3.1":{"pli":"Ghose cittaṃ pasādesiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap520:3.2":{"pli":"asamappaṭipuggale; ","html":"<span class='verse-line'>{}</span>"},"tha-ap520:3.3":{"pli":"Tattha cittaṃ pasādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap520:3.4":{"pli":"uttariṃ duttaraṃ bhavaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap520:4.1":{"pli":"Ekatiṃse ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap520:4.2":{"pli":"yaṃ saññamalabhiṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap520:4.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap520:4.4":{"pli":"ghosasaññāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap520:5.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap520:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap520:5.3":{"pli":"viharāmi anāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap520:6.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap520:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap520:6.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap520:7.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap520:7.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap520:7.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap520:8.1":{"pli":"Itthaṃ sudaṃ āyasmā ghosasaññako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap520:9.1":{"pli":"Ghosasaññakattherassāpadānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap521":{"tha-ap521:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap521'><header><ul><li class='division'>{}</li>"},"tha-ap521:0.2":{"pli":"Phaladāyakavagga ","html":"<li>{}</li></ul>"},"tha-ap521:0.3":{"pli":"Sabbaphaladāyakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap521:1.1":{"pli":"“Varuṇo nāma nāmena, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap521:1.2":{"pli":"brāhmaṇo mantapāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:1.3":{"pli":"Chaḍḍetvā dasa puttāni, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:1.4":{"pli":"vanamajjhogahiṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:2.1":{"pli":"Assamaṃ sukataṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:2.2":{"pli":"suvibhattaṃ manoramaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:2.3":{"pli":"Paṇṇasālaṃ karitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:2.4":{"pli":"vasāmi vipine ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:3.1":{"pli":"Padumuttaro lokavidū, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:3.2":{"pli":"āhutīnaṃ paṭiggaho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:3.3":{"pli":"Mamuddharitukāmo so, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:3.4":{"pli":"āgacchi mama assamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:4.1":{"pli":"Yāvatā vanasaṇḍamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:4.2":{"pli":"obhāso vipulo ahu; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:4.3":{"pli":"Buddhassa ānubhāvena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:4.4":{"pli":"pajjalī vipinaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:5.1":{"pli":"Disvāna taṃ pāṭihīraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:5.2":{"pli":"buddhaseṭṭhassa tādino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:5.3":{"pli":"Pattapuṭaṃ gahetvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:5.4":{"pli":"phalena pūjayiṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:6.1":{"pli":"Upagantvāna sambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:6.2":{"pli":"sahakhārimadāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:6.3":{"pli":"Anukampāya me buddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:6.4":{"pli":"idaṃ vacanamabravi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:7.1":{"pli":"‘Khāribhāraṃ gahetvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:7.2":{"pli":"pacchato ehi me tuvaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:7.3":{"pli":"Paribhutte ca saṅghamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:7.4":{"pli":"puññaṃ tava bhavissati’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:8.1":{"pli":"Puṭakantaṃ gahetvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:8.2":{"pli":"bhikkhusaṅghassadāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:8.3":{"pli":"Tattha cittaṃ pasādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:8.4":{"pli":"tusitaṃ upapajjahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:9.1":{"pli":"Tattha dibbehi naccehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:9.2":{"pli":"gītehi vāditehi ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:9.3":{"pli":"Puññakammena saṃyuttaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:9.4":{"pli":"anubhomi sadā sukhaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:10.1":{"pli":"Yaṃ yaṃ yonupapajjāmi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:10.2":{"pli":"devattaṃ atha mānusaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:10.3":{"pli":"Bhoge me ūnatā natthi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:10.4":{"pli":"phaladānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:11.1":{"pli":"Yāvatā caturo dīpā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:11.2":{"pli":"sasamuddā sapabbatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:11.3":{"pli":"Phalaṃ buddhassa datvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:11.4":{"pli":"issaraṃ kārayāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:12.1":{"pli":"Yāvatā me pakkhigaṇā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:12.2":{"pli":"ākāse uppatanti ce; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:12.3":{"pli":"Tepi maṃ vasamanventi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:12.4":{"pli":"phaladānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:13.1":{"pli":"Yāvatā vanasaṇḍamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:13.2":{"pli":"yakkhā bhūtā ca rakkhasā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:13.3":{"pli":"Kumbhaṇḍā garuḷā cāpi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:13.4":{"pli":"pāricariyaṃ upenti me. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:14.1":{"pli":"Kummā soṇā madhukārā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:14.2":{"pli":"ḍaṃsā ca makasā ubho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:14.3":{"pli":"Tepi maṃ vasamanventi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:14.4":{"pli":"phaladānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:15.1":{"pli":"Supaṇṇā nāma sakuṇā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:15.2":{"pli":"pakkhijātā mahabbalā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:15.3":{"pli":"Tepi maṃ saraṇaṃ yanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:15.4":{"pli":"phaladānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:16.1":{"pli":"Yepi dīghāyukā nāgā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:16.2":{"pli":"iddhimanto mahāyasā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:16.3":{"pli":"Tepi maṃ vasamanventi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:16.4":{"pli":"phaladānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:17.1":{"pli":"Sīhā byagghā ca dīpī ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:17.2":{"pli":"acchakokataracchakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:17.3":{"pli":"Tepi maṃ vasamanventi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:17.4":{"pli":"phaladānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:18.1":{"pli":"Osadhītiṇavāsī ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:18.2":{"pli":"ye ca ākāsavāsino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:18.3":{"pli":"Sabbe maṃ saraṇaṃ yanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:18.4":{"pli":"phaladānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:19.1":{"pli":"Sududdasaṃ sunipuṇaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap521:19.2":{"pli":"gambhīraṃ suppakāsitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:19.3":{"pli":"Phassayitvā viharāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap521:19.4":{"pli":"phaladānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap521:20.1":{"pli":"Vimokkhe aṭṭha phusitvā, ",