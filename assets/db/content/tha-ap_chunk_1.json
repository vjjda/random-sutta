"tha-ap112:4.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap112:5.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap112:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap112:5.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap112:6.1":{"pli":"Itthaṃ sudaṃ āyasmā reṇupūjako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap112:7.1":{"pli":"Reṇupūjakattherassāpadānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"tha-ap112:8.1":{"pli":"Bhikkhadāyivaggo ekādasamo. ","html":"<p class='endsection'>{}</p>"},"tha-ap112:9.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"tha-ap112:10.1":{"pli":"Bhikkhadāyī ñāṇasaññī, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"tha-ap112:10.2":{"pli":"hatthiyo padapūjako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap112:10.3":{"pli":"Muṭṭhipupphī udakado, ","html":"<span class='verse-line'>{}</span>"},"tha-ap112:10.4":{"pli":"naḷamāli upaṭṭhako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap112:10.5":{"pli":"Biḷālidāyī reṇu ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap112:10.6":{"pli":"gāthāyo cha ca saṭṭhi ca. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}},"tha-ap113":{"tha-ap113:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap113'><header><ul><li class='division'>{}</li>"},"tha-ap113:0.2":{"pli":"Mahāparivāravagga ","html":"<li>{}</li></ul>"},"tha-ap113:0.3":{"pli":"1 Mahāparivārakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap113:1.1":{"pli":"“Vipassī nāma bhagavā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap113:1.2":{"pli":"lokajeṭṭho narāsabho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap113:1.3":{"pli":"Aṭṭhasaṭṭhisahassehi, ","html":"<span class='verse-line'>{}</span>