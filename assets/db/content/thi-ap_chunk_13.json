{"thi-ap8":{"thi-ap8:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap8'><header><ul><li class='division'>{}</li>"},"thi-ap8:0.2":{"pli":"Sumedhāvagga ","html":"<li>{}</li></ul>"},"thi-ap8:0.3":{"pli":"Sattuppalamālikātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap8:1.1":{"pli":"“Nagare aruṇavatiyā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap8:1.2":{"pli":"aruṇo nāma khattiyo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:1.3":{"pli":"Tassa rañño ahuṃ bhariyā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:1.4":{"pli":"vāritaṃ vārayāmahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:2.1":{"pli":"Satta mālā gahetvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:2.2":{"pli":"uppalā devagandhikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:2.3":{"pli":"Nisajja pāsādavare, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:2.4":{"pli":"evaṃ cintesi tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:3.1":{"pli":"‘Kiṃ me imāhi mālāhi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:3.2":{"pli":"sirasāropitāhi me; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:3.3":{"pli":"Varaṃ me buddhaseṭṭhassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:3.4":{"pli":"ñāṇamhi abhiropitaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:4.1":{"pli":"Sambuddhaṃ paṭimānentī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:4.2":{"pli":"dvārāsanne nisīdahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:4.3":{"pli":"‘Yadā ehiti sambuddho, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:4.4":{"pli":"pūjayissaṃ mahāmuniṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:5.1":{"pli":"Kakudho vilasantova, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:5.2":{"pli":"migarājāva kesarī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:5.3":{"pli":"Bhikkhusaṅghena sahito, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:5.4":{"pli":"āgacchi vīthiyā jino. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:6.1":{"pli":"Buddhassa raṃsiṃ disvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:6.2":{"pli":"haṭṭhā saṃviggamānasā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:6.3":{"pli":"Dvāraṃ avāpuritvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:6.4":{"pli":"buddhaseṭṭhamapūjayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:7.1":{"pli":"Satta uppalapupphāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:7.2":{"pli":"parikiṇṇāni ambare; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:7.3":{"pli":"Chadiṃ karonto buddhassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:7.4":{"pli":"matthake dhārayanti te. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:8.1":{"pli":"Udaggacittā sumanā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:8.2":{"pli":"vedajātā katañjalī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:8.3":{"pli":"Tattha cittaṃ pasādetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:8.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:9.1":{"pli":"Mahānelassa chādanaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:9.2":{"pli":"dhārenti mama muddhani; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:9.3":{"pli":"Dibbagandhaṃ pavāyāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:9.4":{"pli":"sattuppalassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:10.1":{"pli":"Kadāci nīyamānāya, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:10.2":{"pli":"ñātisaṅghena me tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:10.3":{"pli":"Yāvatā parisā mayhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:10.4":{"pli":"mahānelaṃ dharīyati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:11.1":{"pli":"Sattatidevarājūnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:11.2":{"pli":"mahesittamakārayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:11.3":{"pli":"Sabbattha issarā hutvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:11.4":{"pli":"saṃsarāmi bhavābhave. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:12.1":{"pli":"Tesaṭṭhi cakkavattīnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:12.2":{"pli":"mahesittamakārayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:12.3":{"pli":"Sabbe mamanuvattanti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:12.4":{"pli":"ādeyyavacanā ahuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:13.1":{"pli":"Uppalasseva me vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:13.2":{"pli":"gandho ceva pavāyati; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:13.3":{"pli":"Dubbaṇṇiyaṃ na jānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:13.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:14.1":{"pli":"Iddhipādesu kusalā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:14.2":{"pli":"bojjhaṅgabhāvanāratā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:14.3":{"pli":"Abhiññāpāramippattā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:14.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:15.1":{"pli":"Satipaṭṭhānakusalā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:15.2":{"pli":"samādhijhānagocarā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:15.3":{"pli":"Sammappadhānamanuyuttā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:15.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:16.1":{"pli":"Vīriyaṃ me dhuradhorayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:16.2":{"pli":"yogakkhemādhivāhanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:16.3":{"pli":"Sabbāsavā parikkhīṇā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:16.4":{"pli":"natthi dāni punabbhavo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:17.1":{"pli":"Ekatiṃse ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:17.2":{"pli":"yaṃ pupphamabhipūjayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:17.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:17.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:18.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:18.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:18.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:19.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:19.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:19.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap8:20.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap8:20.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap8:20.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap8:21.1":{"pli":"Itthaṃ sudaṃ sattuppalamālikā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap8:22.1":{"pli":"Sattuppalamālikātheriyāpadānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"thi-ap9":{"thi-ap9:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap9'><header><ul><li class='division'>{}</li>"},"thi-ap9:0.2":{"pli":"Sumedhāvagga ","html":"<li>{}</li></ul>"},"thi-ap9:0.3":{"pli":"Pañcadīpikātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap9:1.1":{"pli":"“Nagare haṃsavatiyā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap9:1.2":{"pli":"cārikī āsahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:1.3":{"pli":"Ārāmena ca ārāmaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:1.4":{"pli":"carāmi kusalatthikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:2.1":{"pli":"Kāḷapakkhamhi divase, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:2.2":{"pli":"addasaṃ bodhimuttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:2.3":{"pli":"Tattha cittaṃ pasādetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:2.4":{"pli":"bodhimūle nisīdahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:3.1":{"pli":"Garucittaṃ upaṭṭhetvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:3.2":{"pli":"sire katvāna añjaliṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:3.3":{"pli":"Somanassaṃ pavedetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:3.4":{"pli":"evaṃ cintesi tāvade. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:4.1":{"pli":"‘Yadi buddho amitaguṇo, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:4.2":{"pli":"asamappaṭipuggalo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:4.3":{"pli":"Dassetu pāṭihīraṃ me, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:4.4":{"pli":"bodhi obhāsatu ayaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:5.1":{"pli":"Saha āvajjite mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:5.2":{"pli":"bodhi pajjali tāvade; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:5.3":{"pli":"Sabbasoṇṇamayā āsi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:5.4":{"pli":"disā sabbā virocati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:6.1":{"pli":"Sattarattindivaṃ tattha, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:6.2":{"pli":"bodhimūle nisīdahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:6.3":{"pli":"Sattame divase patte, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:6.4":{"pli":"dīpapūjaṃ akāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:7.1":{"pli":"Āsanaṃ parivāretvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:7.2":{"pli":"pañca dīpāni pajjaluṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:7.3":{"pli":"Yāva udeti sūriyo, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:7.4":{"pli":"dīpā me pajjaluṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:8.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:8.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:8.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:8.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:9.1":{"pli":"Tattha me sukataṃ byamhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:9.2":{"pli":"pañcadīpāti vuccati; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:9.3":{"pli":"Saṭṭhiyojanamubbedhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:9.4":{"pli":"tiṃsayojanavitthataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:10.1":{"pli":"Asaṅkhiyāni dīpāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:10.2":{"pli":"parivāre jalanti me; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:10.3":{"pli":"Yāvatā devabhavanaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:10.4":{"pli":"dīpālokena jotati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:11.1":{"pli":"Parammukhā nisīditvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:11.2":{"pli":"yadi icchāmi passituṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:11.3":{"pli":"Uddhaṃ adho ca tiriyaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:11.4":{"pli":"sabbaṃ passāmi cakkhunā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:12.1":{"pli":"Yāvatā abhikaṅkhāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:12.2":{"pli":"daṭṭhuṃ sugataduggate; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:12.3":{"pli":"Tattha āvaraṇaṃ natthi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:12.4":{"pli":"rukkhesu pabbatesu vā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:13.1":{"pli":"Asīti devarājūnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:13.2":{"pli":"mahesittamakārayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:13.3":{"pli":"Satānaṃ cakkavattīnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:13.4":{"pli":"mahesittamakārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:14.1":{"pli":"Yaṃ yaṃ yonupapajjāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:14.2":{"pli":"devattaṃ atha mānusaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:14.3":{"pli":"Dīpasatasahassāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:14.4":{"pli":"parivāre jalanti me. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:15.1":{"pli":"Devalokā cavitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:15.2":{"pli":"uppajjiṃ mātukucchiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:15.3":{"pli":"Mātukucchigatā santī, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:15.4":{"pli":"akkhi me na nimīlati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:16.1":{"pli":"Dīpasatasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:16.2":{"pli":"puññakammasamaṅgitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:16.3":{"pli":"Jalanti sūtikāgehe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:16.4":{"pli":"pañcadīpānidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:17.1":{"pli":"Pacchime bhave sampatte, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:17.2":{"pli":"mānasaṃ vinivattayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:17.3":{"pli":"Ajarāmataṃ sītibhāvaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:17.4":{"pli":"nibbānaṃ phassayiṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:18.1":{"pli":"Jātiyā sattavassāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:18.2":{"pli":"arahattamapāpuṇiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:18.3":{"pli":"Upasampādayī buddho, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:18.4":{"pli":"guṇamaññāya gotamo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:19.1":{"pli":"Maṇḍape rukkhamūle vā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:19.2":{"pli":"pāsādesu guhāsu vā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:19.3":{"pli":"Suññāgāre vasantiyā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:19.4":{"pli":"pañcadīpā jalanti me. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:20.1":{"pli":"Dibbacakkhuvisuddhaṃ me, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:20.2":{"pli":"samādhikusalā ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:20.3":{"pli":"Abhiññāpāramippattā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:20.4":{"pli":"pañcadīpānidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:21.1":{"pli":"Sabbavositavosānā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:21.2":{"pli":"katakiccā anāsavā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:21.3":{"pli":"Pañcadīpā mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:21.4":{"pli":"pāde vandāmi cakkhuma. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:22.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:22.2":{"pli":"yaṃ dīpamadadiṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:22.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:22.4":{"pli":"pañcadīpānidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:23.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:23.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:23.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:24.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:24.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:24.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap9:25.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap9:25.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap9:25.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap9:26.1":{"pli":"Itthaṃ sudaṃ pañcadīpikā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap9:27.1":{"pli":"Pañcadīpikātheriyāpadānaṃ navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}