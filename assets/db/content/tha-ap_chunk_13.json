{"tha-ap24":{"tha-ap24:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap24'><header><ul><li class='division'>{}</li>"},"tha-ap24:0.2":{"pli":"Subhūtivagga ","html":"<li>{}</li></ul>"},"tha-ap24:0.3":{"pli":"2 Upavānattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap24:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap24:1.2":{"pli":"sabbadhammāna pāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:1.3":{"pli":"Jalitvā aggikkhandhova, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:1.4":{"pli":"sambuddho parinibbuto. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:2.1":{"pli":"Mahājanā samāgamma, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:2.2":{"pli":"pūjayitvā tathāgataṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:2.3":{"pli":"Citaṃ katvāna sukataṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:2.4":{"pli":"sarīraṃ abhiropayuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:3.1":{"pli":"Sarīrakiccaṃ katvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:3.2":{"pli":"dhātuṃ tattha samānayuṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:3.3":{"pli":"Sadevamānusā sabbe, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:3.4":{"pli":"buddhathūpaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:4.1":{"pli":"Paṭhamā kañcanamayā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:4.2":{"pli":"dutiyāsi maṇīmayā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:4.3":{"pli":"Tatiyā rūpiyamayā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:4.4":{"pli":"catutthī phalikāmayā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:5.1":{"pli":"Tathā pañcamiyā bhūmi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:5.2":{"pli":"lohitaṅgamayā ahu; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:5.3":{"pli":"Chaṭṭhā masāragallassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:5.4":{"pli":"sabbaratanamayūpari. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:6.1":{"pli":"Jaṅghā maṇimayā āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:6.2":{"pli":"vedikā ratanamayā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:6.3":{"pli":"Sabbasoṇṇamayo thūpo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:6.4":{"pli":"uddhaṃ yojanamuggato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:7.1":{"pli":"Devā tattha samāgantvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:7.2":{"pli":"ekato mantayuṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:7.3":{"pli":"Mayampi thūpaṃ kassāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:7.4":{"pli":"lokanāthassa tādino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:8.1":{"pli":"Dhātu āveṇikā natthi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:8.2":{"pli":"sarīraṃ ekapiṇḍitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:8.3":{"pli":"Imamhi buddhathūpamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:8.4":{"pli":"kassāma kañcukaṃ mayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:9.1":{"pli":"Devā sattahi ratnehi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:9.2":{"pli":"aññaṃ vaḍḍhesu yojanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:9.3":{"pli":"Thūpo dviyojanubbedho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:9.4":{"pli":"timiraṃ byapahanti so. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:10.1":{"pli":"Nāgā tattha samāgantvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:10.2":{"pli":"ekato mantayuṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:10.3":{"pli":"Manussā ceva devā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:10.4":{"pli":"buddhathūpaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:11.1":{"pli":"Mā no pamattā assumha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:11.2":{"pli":"appamattā sadevakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:11.3":{"pli":"Mayampi thūpaṃ kassāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:11.4":{"pli":"lokanāthassa tādino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:12.1":{"pli":"Indanīlaṃ mahānīlaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:12.2":{"pli":"atho jotirasaṃ maṇiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:12.3":{"pli":"Ekato sannipātetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:12.4":{"pli":"buddhathūpaṃ achādayuṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:13.1":{"pli":"Sabbaṃ maṇimayaṃ āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:13.2":{"pli":"tāvatā buddhacetiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:13.3":{"pli":"Tiyojanasamubbiddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:13.4":{"pli":"ālokakaraṇaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:14.1":{"pli":"Garuḷā ca samāgantvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:14.2":{"pli":"ekato mantayuṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:14.3":{"pli":"Manussā devā nāgā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:14.4":{"pli":"buddhathūpaṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:15.1":{"pli":"‘Mā no pamattā assumha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:15.2":{"pli":"appamattā sadevakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:15.3":{"pli":"Mayampi thūpaṃ kassāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:15.4":{"pli":"lokanāthassa tādino’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:16.1":{"pli":"Sabbaṃ maṇimayaṃ thūpaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:16.2":{"pli":"akaruṃ te ca kañcukaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:16.3":{"pli":"Yojanaṃ tepi vaḍḍhesuṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:16.4":{"pli":"āyataṃ buddhacetiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:17.1":{"pli":"Catuyojanamubbiddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:17.2":{"pli":"buddhathūpo virocati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:17.3":{"pli":"Obhāseti disā sabbā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:17.4":{"pli":"sataraṃsīva uggato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:18.1":{"pli":"Kumbhaṇḍā ca samāgantvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:18.2":{"pli":"ekato mantayuṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:18.3":{"pli":"Manussā ceva devā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:18.4":{"pli":"nāgā ca garuḷā tathā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:18.5":{"pli":"Paccekaṃ buddhaseṭṭhassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:18.6":{"pli":"akaṃsu thūpamuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:19.1":{"pli":"‘Mā no pamattā assumha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:19.2":{"pli":"appamattā sadevakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:19.3":{"pli":"Mayampi thūpaṃ kassāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:19.4":{"pli":"lokanāthassa tādino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:19.5":{"pli":"Ratanehi chādessāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:19.6":{"pli":"āyataṃ buddhacetiyaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:20.1":{"pli":"Yojanaṃ tepi vaḍḍhesuṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:20.2":{"pli":"āyataṃ buddhacetiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:20.3":{"pli":"Pañcayojanamubbiddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:20.4":{"pli":"thūpo obhāsate tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:21.1":{"pli":"Yakkhā tattha samāgantvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:21.2":{"pli":"ekato mantayuṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:21.3":{"pli":"Manussā devā nāgā ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:21.4":{"pli":"garuḷā kumbhaaṇḍakā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:22.1":{"pli":"Paccekaṃ buddhaseṭṭhassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:22.2":{"pli":"akaṃsu thūpamuttamaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:22.3":{"pli":"‘Mā no pamattā assumha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:22.4":{"pli":"appamattā sadevakā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:23.1":{"pli":"Mayampi thūpaṃ kassāma, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:23.2":{"pli":"lokanāthassa tādino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:23.3":{"pli":"Phalikāhi chādessāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:23.4":{"pli":"āyataṃ buddhacetiyaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:24.1":{"pli":"Yojanaṃ tepi vaḍḍhesuṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:24.2":{"pli":"āyataṃ buddhacetiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:24.3":{"pli":"Cha yojanāni ubbiddho, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:24.4":{"pli":"thūpo obhāsate tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:25.1":{"pli":"Gandhabbā ca samāgantvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:25.2":{"pli":"ekato mantayuṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:25.3":{"pli":"‘Manujā devatā nāgā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:25.4":{"pli":"garuḷā kumbhayakkhakā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:26.1":{"pli":"Sabbekaṃsu buddhathūpaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:26.2":{"pli":"mayamettha akārakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:26.3":{"pli":"Mayampi thūpaṃ kassāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:26.4":{"pli":"lokanāthassa tādino’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:27.1":{"pli":"Vediyo satta katvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:27.2":{"pli":"chattamāropayiṃsu te; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:27.3":{"pli":"Sabbasoṇṇamayaṃ thūpaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:27.4":{"pli":"gandhabbā kārayuṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:28.1":{"pli":"Sattayojanamubbiddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:28.2":{"pli":"thūpo obhāsate tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:28.3":{"pli":"Rattindivā na ñāyanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:28.4":{"pli":"āloko hoti sabbadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:29.1":{"pli":"Abhibhonti na tassābhā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:29.2":{"pli":"candasūrā satārakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:29.3":{"pli":"Samantā yojanasate, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:29.4":{"pli":"padīpopi na pajjali. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:30.1":{"pli":"Tena kālena ye keci, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:30.2":{"pli":"thūpaṃ pūjenti mānusā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:30.3":{"pli":"Na te thūpamāruhanti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:30.4":{"pli":"ambare ukkhipanti te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:31.1":{"pli":"Devehi ṭhapito yakkho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:31.2":{"pli":"abhisammatanāmako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:31.3":{"pli":"Dhajaṃ vā pupphadāmaṃ vā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:31.4":{"pli":"abhiropeti uttari. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:32.1":{"pli":"Na te passanti taṃ yakkhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:32.2":{"pli":"dāmaṃ passanti gacchato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:32.3":{"pli":"Evaṃ passitvā gacchantā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:32.4":{"pli":"sabbe gacchanti suggatiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:33.1":{"pli":"Viruddhā ye pāvacane, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:33.2":{"pli":"pasannā ye ca sāsane; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:33.3":{"pli":"Pāṭiheraṃ daṭṭhukāmā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:33.4":{"pli":"thūpaṃ pūjenti mānusā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:34.1":{"pli":"Nagare haṃsavatiyā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:34.2":{"pli":"ahosiṃ bhatako tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:34.3":{"pli":"Āmoditaṃ janaṃ disvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:34.4":{"pli":"evaṃ cintesahaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:35.1":{"pli":"‘Uḷāro bhagavā heso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:35.2":{"pli":"yassa dhātudharedisaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:35.3":{"pli":"Imā ca janatā tuṭṭhā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:35.4":{"pli":"kāraṃ kubbaṃ na tappare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:36.1":{"pli":"Ahampi kāraṃ kassāmi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:36.2":{"pli":"lokanāthassa tādino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:36.3":{"pli":"Tassa dhammesu dāyādo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:36.4":{"pli":"bhavissāmi anāgate’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:37.1":{"pli":"Sudhotaṃ rajakenāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:37.2":{"pli":"uttareyyapaṭaṃ mama; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:37.3":{"pli":"Veḷagge ālaggetvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:37.4":{"pli":"dhajaṃ ukkhipimambare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:38.1":{"pli":"Abhisammatako gayha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:38.2":{"pli":"ambarehāsi me dhajaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:38.3":{"pli":"Vāteritaṃ dhajaṃ disvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:38.4":{"pli":"bhiyyo hāsaṃ janesahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:39.1":{"pli":"Tattha cittaṃ pasādetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:39.2":{"pli":"samaṇaṃ upasaṅkamiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:39.3":{"pli":"Taṃ bhikkhuṃ abhivādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:39.4":{"pli":"vipākaṃ pucchahaṃ dhaje. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:40.1":{"pli":"So me kathesi ānanda, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:40.2":{"pli":"pītisañjananaṃ mama; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:40.3":{"pli":"‘Tassa dhajassa vipākaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:40.4":{"pli":"anubhossasi sabbadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:41.1":{"pli":"Hatthī assā rathā pattī, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:41.2":{"pli":"senā ca caturaṅginī; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:41.3":{"pli":"Parivāressanti taṃ niccaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:41.4":{"pli":"dhajadānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:42.1":{"pli":"Saṭṭhitūriyasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:42.2":{"pli":"bheriyo samalaṅkatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:42.3":{"pli":"Parivāressanti taṃ niccaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:42.4":{"pli":"dhajadānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:43.1":{"pli":"Chaḷāsītisahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:43.2":{"pli":"nāriyo samalaṅkatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:43.3":{"pli":"Vicittavatthābharaṇā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:43.4":{"pli":"āmuttamaṇikuṇḍalā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:44.1":{"pli":"Aḷārapamhā hasulā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:44.2":{"pli":"susaññā tanumajjhimā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:44.3":{"pli":"Parivāressanti taṃ niccaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:44.4":{"pli":"dhajadānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:45.1":{"pli":"Tiṃsakappasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:45.2":{"pli":"devaloke ramissasi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:45.3":{"pli":"Asītikkhattuṃ devindo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:45.4":{"pli":"devarajjaṃ karissasi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap24:46.1":{"pli":"Sahassakkhattuṃ rājā ca, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap24:46.2":{"pli":"cakkavattī bhavissasi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap24:46.3":{"pli":"Padesarajjaṃ