{"pv24":{"pv24:0.1":{"pli":"Petavatthu ","html":"<article id='pv24'><header><ul><li class='division'>{}</li>"},"pv24:0.2":{"pli":"Ubbarivagga ","html":"<li>{}</li></ul>"},"pv24:0.3":{"pli":"12. Kaṇṇamuṇḍapetivatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv24:1.1":{"pli":"“Soṇṇasopānaphalakā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv24:1.2":{"pli":"soṇṇavālukasanthatā; ","html":"<span class='verse-line'>{}</span>"},"pv24:1.3":{"pli":"Tattha sogandhiyā vaggū, ","html":"<span class='verse-line'>{}</span>"},"pv24:1.4":{"pli":"sucigandhā manoramā. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:2.1":{"pli":"Nānārukkhehi sañchannā, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:2.2":{"pli":"nānāgandhasameritā; ","html":"<span class='verse-line'>{}</span>"},"pv24:2.3":{"pli":"Nānāpadumasañchannā, ","html":"<span class='verse-line'>{}</span>"},"pv24:2.4":{"pli":"puṇḍarīkasamotatā. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:3.1":{"pli":"Surabhiṃ sampavāyanti, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:3.2":{"pli":"manuññā māluteritā; ","html":"<span class='verse-line'>{}</span>"},"pv24:3.3":{"pli":"Haṃsakoñcābhirudā ca, ","html":"<span class='verse-line'>{}</span>"},"pv24:3.4":{"pli":"cakkavakkābhikūjitā. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:4.1":{"pli":"Nānādijagaṇākiṇṇā, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:4.2":{"pli":"nānāsaragaṇāyutā; ","html":"<span class='verse-line'>{}</span>"},"pv24:4.3":{"pli":"Nānāphaladharā rukkhā, ","html":"<span class='verse-line'>{}</span>"},"pv24:4.4":{"pli":"nānāpupphadharā vanā. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:5.1":{"pli":"Na manussesu īdisaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:5.2":{"pli":"nagaraṃ yādisaṃ idaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv24:5.3":{"pli":"Pāsādā bahukā tuyhaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv24:5.4":{"pli":"sovaṇṇarūpiyāmayā; ","html":"<span class='verse-line'>{}</span>"},"pv24:5.5":{"pli":"Daddallamānā ābhenti, ","html":"<span class='verse-line'>{}</span>"},"pv24:5.6":{"pli":"samantā caturo disā. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:6.1":{"pli":"Pañca dāsisatā tuyhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:6.2":{"pli":"yā temā paricārikā; ","html":"<span class='verse-line'>{}</span>"},"pv24:6.3":{"pli":"Tā kambukāyūradharā, ","html":"<span class='verse-line'>{}</span>"},"pv24:6.4":{"pli":"kañcanāveḷabhūsitā. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:7.1":{"pli":"Pallaṅkā bahukā tuyhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:7.2":{"pli":"sovaṇṇarūpiyāmayā; ","html":"<span class='verse-line'>{}</span>"},"pv24:7.3":{"pli":"Kadalimigasañchannā, ","html":"<span class='verse-line'>{}</span>"},"pv24:7.4":{"pli":"sajjā gonakasanthatā. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:8.1":{"pli":"Yattha tvaṃ vāsūpagatā, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:8.2":{"pli":"sabbakāmasamiddhinī; ","html":"<span class='verse-line'>{}</span>"},"pv24:8.3":{"pli":"Sampattāyaḍḍharattāya, ","html":"<span class='verse-line'>{}</span>"},"pv24:8.4":{"pli":"tato uṭṭhāya gacchasi. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:9.1":{"pli":"Uyyānabhūmiṃ gantvāna, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:9.2":{"pli":"pokkharaññā samantato; ","html":"<span class='verse-line'>{}</span>"},"pv24:9.3":{"pli":"Tassā tīre tuvaṃ ṭhāsi, ","html":"<span class='verse-line'>{}</span>"},"pv24:9.4":{"pli":"harite saddale subhe. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:10.1":{"pli":"Tato te kaṇṇamuṇḍo sunakho, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:10.2":{"pli":"Aṅgamaṅgāni khādati; ","html":"<span class='verse-line'>{}</span>"},"pv24:10.3":{"pli":"Yadā ca khāyitā āsi, ","html":"<span class='verse-line'>{}</span>"},"pv24:10.4":{"pli":"Aṭṭhisaṅkhalikā katā; ","html":"<span class='verse-line'>{}</span>"},"pv24:10.5":{"pli":"Ogāhasi pokkharaṇiṃ, ","html":"<span class='verse-line'>{}</span>"},"pv24:10.6":{"pli":"Hoti kāyo yathā pure. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:11.1":{"pli":"Tato tvaṃ aṅgapaccaṅgī, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:11.2":{"pli":"sucāru piyadassanā; ","html":"<span class='verse-line'>{}</span>"},"pv24:11.3":{"pli":"Vatthena pārupitvāna, ","html":"<span class='verse-line'>{}</span>"},"pv24:11.4":{"pli":"āyāsi mama santikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:12.1":{"pli":"Kiṃ nu kāyena vācāya, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:12.2":{"pli":"manasā dukkaṭaṃ kataṃ; ","html":"<span class='verse-line'>{}</span>"},"pv24:12.3":{"pli":"Kissa kammavipākena, ","html":"<span class='verse-line'>{}</span>"},"pv24:12.4":{"pli":"kaṇṇamuṇḍo sunakho tava; ","html":"<span class='verse-line'>{}</span>"},"pv24:12.5":{"pli":"Aṅgamaṅgāni khādatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:13.1":{"pli":"“Kimilāyaṃ gahapati, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:13.2":{"pli":"saddho āsi upāsako; ","html":"<span class='verse-line'>{}</span>"},"pv24:13.3":{"pli":"Tassāhaṃ bhariyā āsiṃ, ","html":"<span class='verse-line'>{}</span>"},"pv24:13.4":{"pli":"dussīlā aticārinī. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:14.1":{"pli":"So maṃ aticaramānāya, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:14.2":{"pli":"sāmiko etadabravi; ","html":"<span class='verse-line'>{}</span>"},"pv24:14.3":{"pli":"‘Netaṃ channaṃ patirūpaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv24:14.4":{"pli":"yaṃ tvaṃ aticarāsi maṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:15.1":{"pli":"Sāhaṃ ghorañca sapathaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:15.2":{"pli":"musāvādañca bhāsisaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv24:15.3":{"pli":"‘Nāhaṃ taṃ aticarāmi, ","html":"<span class='verse-line'>{}</span>"},"pv24:15.4":{"pli":"kāyena uda cetasā. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:16.1":{"pli":"Sacāhaṃ taṃ aticarāmi, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:16.2":{"pli":"kāyena uda cetasā; ","html":"<span class='verse-line'>{}</span>"},"pv24:16.3":{"pli":"Kaṇṇamuṇḍoyaṃ sunakho, ","html":"<span class='verse-line'>{}</span>"},"pv24:16.4":{"pli":"aṅgamaṅgāni khādatu’. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:17.1":{"pli":"Tassa kammassa vipākaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:17.2":{"pli":"musāvādassa cūbhayaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv24:17.3":{"pli":"Satteva vassasatāni, ","html":"<span class='verse-line'>{}</span>"},"pv24:17.4":{"pli":"anubhūtaṃ yato hi me; ","html":"<span class='verse-line'>{}</span>"},"pv24:17.5":{"pli":"Kaṇṇamuṇḍo ca sunakho, ","html":"<span class='verse-line'>{}</span>"},"pv24:17.6":{"pli":"aṅgamaṅgāni khādati. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:18.1":{"pli":"Tvañca deva bahukāro, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:18.2":{"pli":"atthāya me idhāgato; ","html":"<span class='verse-line'>{}</span>"},"pv24:18.3":{"pli":"Sumuttāhaṃ kaṇṇamuṇḍassa, ","html":"<span class='verse-line'>{}</span>"},"pv24:18.4":{"pli":"asokā akutobhayā. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:19.1":{"pli":"Tāhaṃ deva namassāmi, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:19.2":{"pli":"yācāmi pañjalīkatā; ","html":"<span class='verse-line'>{}</span>"},"pv24:19.3":{"pli":"Bhuñja amānuse kāme, ","html":"<span class='verse-line'>{}</span>"},"pv24:19.4":{"pli":"rama deva mayā sahā”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv24:20.1":{"pli":"“Bhuttā amānusā kāmā, ","html":"<p><span class='verse-line'>{}</span>"},"pv24:20.2":{"pli":"ramitomhi tayā saha; ","html":"<span class='verse-line'>{}</span>"},"pv24:20.3":{"pli":"Tāhaṃ subhage yācāmi, ","html":"<span class='verse-line'>{}</span>"},"pv24:20.4":{"pli":"khippaṃ paṭinayāhi man”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv24:21.1":{"pli":"Kaṇṇamuṇḍapetivatthu dvādasamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv25":{"pv25:0.1":{"pli":"Petavatthu ","html":"<article id='pv25'><header><ul><li class='division'>{}</li>"},"pv25:0.2":{"pli":"Ubbarivagga ","html":"<li>{}</li></ul>"},"pv25:0.3":{"pli":"13. Ubbaripetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv25:1.1":{"pli":"Ahu rājā brahmadatto, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv25:1.2":{"pli":"pañcālānaṃ rathesabho; ","html":"<span class='verse-line'>{}</span>"},"pv25:1.3":{"pli":"Ahorattānamaccayā, ","html":"<span class='verse-line'>{}</span>"},"pv25:1.4":{"pli":"rājā kālamakrubbatha. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:2.1":{"pli":"Tassa āḷāhanaṃ gantvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:2.2":{"pli":"bhariyā kandati ubbarī; ","html":"<span class='verse-line'>{}</span>"},"pv25:2.3":{"pli":"Brahmadattaṃ apassantī, ","html":"<span class='verse-line'>{}</span>"},"pv25:2.4":{"pli":"brahmadattāti kandati. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:3.1":{"pli":"Isi ca tattha āgacchi, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:3.2":{"pli":"sampannacaraṇo muni; ","html":"<span class='verse-line'>{}</span>"},"pv25:3.3":{"pli":"So ca tattha apucchittha, ","html":"<span class='verse-line'>{}</span>"},"pv25:3.4":{"pli":"ye tattha susamāgatā. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:4.1":{"pli":"“Kassa idaṃ āḷāhanaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:4.2":{"pli":"nānāgandhasameritaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv25:4.3":{"pli":"Kassāyaṃ kandati bhariyā, ","html":"<span class='verse-line'>{}</span>"},"pv25:4.4":{"pli":"ito dūragataṃ patiṃ; ","html":"<span class='verse-line'>{}</span>"},"pv25:4.5":{"pli":"Brahmadattaṃ apassantī, ","html":"<span class='verse-line'>{}</span>"},"pv25:4.6":{"pli":"‘brahmadattā’ti kandati”. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:5.1":{"pli":"Te ca tattha viyākaṃsu, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:5.2":{"pli":"ye tattha susamāgatā; ","html":"<span class='verse-line'>{}</span>"},"pv25:5.3":{"pli":"“Brahmadattassa bhadante, ","html":"<span class='verse-line'>{}</span>"},"pv25:5.4":{"pli":"brahmadattassa mārisa. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:6.1":{"pli":"Tassa idaṃ āḷāhanaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:6.2":{"pli":"nānāgandhasameritaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv25:6.3":{"pli":"Tassāyaṃ kandati bhariyā, ","html":"<span class='verse-line'>{}</span>"},"pv25:6.4":{"pli":"ito dūragataṃ patiṃ; ","html":"<span class='verse-line'>{}</span>"},"pv25:6.5":{"pli":"Brahmadattaṃ apassantī, ","html":"<span class='verse-line'>{}</span>"},"pv25:6.6":{"pli":"‘brahmadattā’ti kandati”. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:7.1":{"pli":"“Chaḷāsītisahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:7.2":{"pli":"brahmadattassanāmakā; ","html":"<span class='verse-line'>{}</span>"},"pv25:7.3":{"pli":"Imasmiṃ āḷāhane daḍḍhā, ","html":"<span class='verse-line'>{}</span>"},"pv25:7.4":{"pli":"tesaṃ kamanusocasī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:8.1":{"pli":"“Yo rājā cūḷanīputto, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:8.2":{"pli":"pañcālānaṃ rathesabho; ","html":"<span class='verse-line'>{}</span>"},"pv25:8.3":{"pli":"Taṃ bhante anusocāmi, ","html":"<span class='verse-line'>{}</span>"},"pv25:8.4":{"pli":"bhattāraṃ sabbakāmadan”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:9.1":{"pli":"“Sabbe vāhesuṃ rājāno, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:9.2":{"pli":"brahmadattassanāmakā; ","html":"<span class='verse-line'>{}</span>"},"pv25:9.3":{"pli":"Sabbeva cūḷanīputtā, ","html":"<span class='verse-line'>{}</span>"},"pv25:9.4":{"pli":"pañcālānaṃ rathesabhā. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:10.1":{"pli":"Sabbesaṃ anupubbena, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:10.2":{"pli":"mahesittamakārayi; ","html":"<span class='verse-line'>{}</span>"},"pv25:10.3":{"pli":"Kasmā purimake hitvā, ","html":"<span class='verse-line'>{}</span>"},"pv25:10.4":{"pli":"pacchimaṃ anusocasī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:11.1":{"pli":"“Ātume itthibhūtāya, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:11.2":{"pli":"dīgharattāya mārisa; ","html":"<span class='verse-line'>{}</span>"},"pv25:11.3":{"pli":"Yassā me itthibhūtāya, ","html":"<span class='verse-line'>{}</span>"},"pv25:11.4":{"pli":"saṃsāre bahubhāsasī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:12.1":{"pli":"“Ahu itthī ahu puriso, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:12.2":{"pli":"pasuyonimpi āgamā; ","html":"<span class='verse-line'>{}</span>"},"pv25:12.3":{"pli":"Evametaṃ atītānaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv25:12.4":{"pli":"pariyanto na dissatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:13.1":{"pli":"“Ādittaṃ vata maṃ santaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:13.2":{"pli":"ghatasittaṃva pāvakaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv25:13.3":{"pli":"Vārinā viya osiñcaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv25:13.4":{"pli":"sabbaṃ nibbāpaye daraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:14.1":{"pli":"Abbahī vata me sallaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:14.2":{"pli":"sokaṃ hadayanissitaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv25:14.3":{"pli":"Yo me sokaparetāya, ","html":"<span class='verse-line'>{}</span>"},"pv25:14.4":{"pli":"patisokaṃ apānudi. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:15.1":{"pli":"Sāhaṃ abbūḷhasallāsmi, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:15.2":{"pli":"sītibhūtāsmi nibbutā; ","html":"<span class='verse-line'>{}</span>"},"pv25:15.3":{"pli":"Na socāmi na rodāmi, ","html":"<span class='verse-line'>{}</span>"},"pv25:15.4":{"pli":"tava sutvā mahāmunī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:16.1":{"pli":"Tassa taṃ vacanaṃ sutvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:16.2":{"pli":"samaṇassa subhāsitaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv25:16.3":{"pli":"Pattacīvaramādāya, ","html":"<span class='verse-line'>{}</span>"},"pv25:16.4":{"pli":"pabbaji anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:17.1":{"pli":"Sā ca pabbajitā santā, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:17.2":{"pli":"agārasmā anagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv25:17.3":{"pli":"Mettacittaṃ abhāvesi, ","html":"<span class='verse-line'>{}</span>"},"pv25:17.4":{"pli":"brahmalokūpapattiyā. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:18.1":{"pli":"Gāmā gāmaṃ vicarantī, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:18.2":{"pli":"nigame rājadhāniyo; ","html":"<span class='verse-line'>{}</span>"},"pv25:18.3":{"pli":"Uruvelā nāma so gāmo, ","html":"<span class='verse-line'>{}</span>"},"pv25:18.4":{"pli":"yattha kālamakrubbatha. ","html":"<span class='verse-line'>{}</span></p>"},"pv25:19.1":{"pli":"Mettacittaṃ ābhāvetvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv25:19.2":{"pli":"brahmalokūpapattiyā; ","html":"<span class='verse-line'>{}</span>"},"pv25:19.3":{"pli":"Itthicittaṃ virājetvā, ","html":"<span class='verse-line'>{}</span>"},"pv25:19.4":{"pli":"brahmalokūpagā ahūti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv25:20.1":{"pli":"Ubbaripetavatthu terasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"pv25:21.1":{"pli":"Ubbarivaggo dutiyo. ","html":"<p class='endsection'>{}</p>"},"pv25:22.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"pv25:23.1":{"pli":"Mocakaṃ mātā mattā ca, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"pv25:23.2":{"pli":"Nandā kuṇḍalīnā ghaṭo; ","html":"<span class='verse-line'>{}</span>"},"pv25:23.3":{"pli":"Dve seṭṭhī tunnavāyo ca, ","html":"<span class='verse-line'>{}</span>"},"pv25:23.4":{"pli":"Uttara suttakaṇṇa ubbarīti. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}},"pv26":{"pv26:0.1":{"pli":"Petavatthu ","html":"<article id='pv26'><header><ul><li class='division'>{}</li>"},"pv26:0.2":{"pli":"Cūḷavagga ","html":"<li>{}</li></ul>"},"pv26:0.3":{"pli":"1. Abhijjamānapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv26:1.1":{"pli":"“Abhijjamāne vārimhi, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv26:1.2":{"pli":"gaṅgāya idha gacchasi; ","html":"<span class='verse-line'>{}</span>"},"pv26:1.3":{"pli":"Naggo pubbaddhapetova, ","html":"<span class='verse-line'>{}</span>"},"pv26:1.4":{"pli":"māladhārī alaṅkato; ","html":"<span class='verse-line'>{}</span>"},"pv26:1.5":{"pli":"Kuhiṃ gamissasi peta, ","html":"<span class='verse-line'>{}</span>"},"pv26:1.6":{"pli":"kattha vāso bhavissatī”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:2.1":{"pli":"“Cundaṭṭhilaṃ gamissāmi, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:2.2":{"pli":"peto so iti bhāsati; ","html":"<span class='verse-line'>{}</span>"},"pv26:2.3":{"pli":"Antare vāsabhagāmaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv26:2.4":{"pli":"bārāṇasiṃ ca santike”. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:3.1":{"pli":"Tañca disvā mahāmatto, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:3.2":{"pli":"koliyo iti vissuto; ","html":"<span class='verse-line'>{}</span>"},"pv26:3.3":{"pli":"Sattuṃ bhattañca petassa, ","html":"<span class='verse-line'>{}</span>"},"pv26:3.4":{"pli":"pītakañca yugaṃ adā. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:4.1":{"pli":"Nāvāya tiṭṭhamānāya, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:4.2":{"pli":"kappakassa adāpayi; ","html":"<span class='verse-line'>{}</span>"},"pv26:4.3":{"pli":"Kappakassa padinnamhi, ","html":"<span class='verse-line'>{}</span>"},"pv26:4.4":{"pli":"ṭhāne petassa dissatha. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:5.1":{"pli":"Tato suvatthavasano, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:5.2":{"pli":"māladhārī alaṅkato; ","html":"<span class='verse-line'>{}</span>"},"pv26:5.3":{"pli":"Ṭhāne ṭhitassa petassa, ","html":"<span class='verse-line'>{}</span>"},"pv26:5.4":{"pli":"dakkhiṇā upakappatha; ","html":"<span class='verse-line'>{}</span>"},"pv26:5.5":{"pli":"Tasmā dajjetha petānaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv26:5.6":{"pli":"anukampāya punappunaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:6.1":{"pli":"Sātunnavasanā eke, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:6.2":{"pli":"aññe kesanivāsanā; ","html":"<span class='verse-line'>{}</span>"},"pv26:6.3":{"pli":"Petā bhattāya gacchanti, ","html":"<span class='verse-line'>{}</span>"},"pv26:6.4":{"pli":"pakkamanti disodisaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:7.1":{"pli":"Dūre eke padhāvitvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:7.2":{"pli":"aladdhāva nivattare; ","html":"<span class='verse-line'>{}</span>"},"pv26:7.3":{"pli":"Chātā pamucchitā bhantā, ","html":"<span class='verse-line'>{}</span>"},"pv26:7.4":{"pli":"bhūmiyaṃ paṭisumbhitā. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:8.1":{"pli":"Te ca tattha papatitā, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:8.2":{"pli":"bhūmiyaṃ paṭisumbhitā; ","html":"<span class='verse-line'>{}</span>"},"pv26:8.3":{"pli":"Pubbe akatakalyāṇā, ","html":"<span class='verse-line'>{}</span>"},"pv26:8.4":{"pli":"aggidaḍḍhāva ātape. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:9.1":{"pli":"“Mayaṃ pubbe pāpadhammā, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:9.2":{"pli":"gharaṇī kulamātaro; ","html":"<span class='verse-line'>{}</span>"},"pv26:9.3":{"pli":"Santesu deyyadhammesu, ","html":"<span class='verse-line'>{}</span>"},"pv26:9.4":{"pli":"dīpaṃ nākamha attano. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:10.1":{"pli":"Pahūtaṃ annapānampi, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:10.2":{"pli":"apissu avakirīyati; ","html":"<span class='verse-line'>{}</span>"},"pv26:10.3":{"pli":"Sammaggate pabbajite, ","html":"<span class='verse-line'>{}</span>"},"pv26:10.4":{"pli":"na ca kiñci adamhase. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:11.1":{"pli":"Akammakāmā alasā, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:11.2":{"pli":"Sādukāmā mahagghasā; ","html":"<span class='verse-line'>{}</span>"},"pv26:11.3":{"pli":"Ālopapiṇḍadātāro, ","html":"<span class='verse-line'>{}</span>"},"pv26:11.4":{"pli":"Paṭiggahe paribhāsimhase. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:12.1":{"pli":"Te gharā tā ca dāsiyo, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:12.2":{"pli":"tānevābharaṇāni no; ","html":"<span class='verse-line'>{}</span>"},"pv26:12.3":{"pli":"Te aññe paricārenti, ","html":"<span class='verse-line'>{}</span>"},"pv26:12.4":{"pli":"mayaṃ dukkhassa bhāgino. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:13.1":{"pli":"Veṇī vā avaññā honti, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:13.2":{"pli":"rathakārī ca dubbhikā; ","html":"<span class='verse-line'>{}</span>"},"pv26:13.3":{"pli":"Caṇḍālī kapaṇā honti, ","html":"<span class='verse-line'>{}</span>"},"pv26:13.4":{"pli":"kappakā ca punappunaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:14.1":{"pli":"Yāni yāni nihīnāni, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:14.2":{"pli":"kulāni kapaṇāni ca; ","html":"<span class='verse-line'>{}</span>"},"pv26:14.3":{"pli":"Tesu tesveva jāyanti, ","html":"<span class='verse-line'>{}</span>"},"pv26:14.4":{"pli":"esā maccharino gati. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:15.1":{"pli":"Pubbe ca katakalyāṇā, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:15.2":{"pli":"dāyakā vītamaccharā; ","html":"<span class='verse-line'>{}</span>"},"pv26:15.3":{"pli":"Saggaṃ te paripūrenti, ","html":"<span class='verse-line'>{}</span>"},"pv26:15.4":{"pli":"obhāsenti ca nandanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:16.1":{"pli":"Vejayante ca pāsāde, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:16.2":{"pli":"ramitvā kāmakāmino; ","html":"<span class='verse-line'>{}</span>"},"pv26:16.3":{"pli":"Uccākulesu jāyanti, ","html":"<span class='verse-line'>{}</span>"},"pv26:16.4":{"pli":"sabhogesu tato cutā. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:17.1":{"pli":"Kūṭāgāre ca pāsāde, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:17.2":{"pli":"pallaṅke gonakatthate; ","html":"<span class='verse-line'>{}</span>"},"pv26:17.3":{"pli":"Bījitaṅgā morahatthehi, ","html":"<span class='verse-line'>{}</span>"},"pv26:17.4":{"pli":"kule jātā yasassino. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:18.1":{"pli":"Aṅkato aṅkaṃ gacchanti, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:18.2":{"pli":"māladhārī alaṅkatā; ","html":"<span class='verse-line'>{}</span>"},"pv26:18.3":{"pli":"Dhātiyo upatiṭṭhanti, ","html":"<span class='verse-line'>{}</span>"},"pv26:18.4":{"pli":"sāyaṃ pātaṃ sukhesino. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:19.1":{"pli":"Nayidaṃ akatapuññānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:19.2":{"pli":"katapuññānamevidaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv26:19.3":{"pli":"Asokaṃ nandanaṃ rammaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv26:19.4":{"pli":"tidasānaṃ mahāvanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:20.1":{"pli":"Sukhaṃ akatapuññānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:20.2":{"pli":"idha natthi parattha ca; ","html":"<span class='verse-line'>{}</span>"},"pv26:20.3":{"pli":"Sukhañca katapuññānaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv26:20.4":{"pli":"idha ceva parattha ca. ","html":"<span class='verse-line'>{}</span></p>"},"pv26:21.1":{"pli":"Tesaṃ sahabyakāmānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv26:21.2":{"pli":"kattabbaṃ kusalaṃ bahuṃ; ","html":"<span class='verse-line'>{}</span>"},"pv26:21.3":{"pli":"Katapuññā hi modanti, ","html":"<span class='verse-line'>{}</span>"},"pv26:21.4":{"pli":"sagge bhogasamaṅgino”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv26:22.1":{"pli":"Abhijjamānapetavatthu paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv27":{"pv27:0.1":{"pli":"Petavatthu ","html":"<article id='pv27'><header><ul><li class='division'>{}</li>"},"pv27:0.2":{"pli":"Cūḷavagga ","html":"<li>{}</li></ul>"},"pv27:0.3":{"pli":"2. Sāṇavāsītherapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv27:1.1":{"pli":"Kuṇḍināgariyo thero, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv27:1.2":{"pli":"sāṇavāsinivāsiko; ","html":"<span class='verse-line'>{}</span>"},"pv27:1.3":{"pli":"Poṭṭhapādoti nāmena, ","html":"<span class='verse-line'>{}</span>"},"pv27:1.4":{"pli":"samaṇo bhāvitindriyo. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:2.1":{"pli":"Tassa mātā pitā bhātā, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:2.2":{"pli":"duggatā yamalokikā; ","html":"<span class='verse-line'>{}</span>"},"pv27:2.3":{"pli":"Pāpakammaṃ karitvāna, ","html":"<span class='verse-line'>{}</span>"},"pv27:2.4":{"pli":"petalokaṃ ito gatā. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:3.1":{"pli":"Te duggatā sūcikaṭṭā, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:3.2":{"pli":"kilantā naggino kisā; ","html":"<span class='verse-line'>{}</span>"},"pv27:3.3":{"pli":"Uttasantā mahattāsā, ","html":"<span class='verse-line'>{}</span>"},"pv27:3.4":{"pli":"na dassenti kurūrino. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:4.1":{"pli":"Tassa bhātā vitaritvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:4.2":{"pli":"naggo ekapathekako; ","html":"<span class='verse-line'>{}</span>"},"pv27:4.3":{"pli":"Catukuṇḍiko bhavitvāna, ","html":"<span class='verse-line'>{}</span>"},"pv27:4.4":{"pli":"therassa dassayītumaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:5.1":{"pli":"Thero cāmanasikatvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:5.2":{"pli":"tuṇhībhūto atikkami; ","html":"<span class='verse-line'>{}</span>"},"pv27:5.3":{"pli":"So ca viññāpayī theraṃ, ","html":"<span class='verse-line'>{}</span>"},"pv27:5.4":{"pli":"“bhātā petagato ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:6.1":{"pli":"Mātā pitā ca te bhante, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:6.2":{"pli":"duggatā yamalokikā; ","html":"<span class='verse-line'>{}</span>"},"pv27:6.3":{"pli":"Pāpakammaṃ karitvāna, ","html":"<span class='verse-line'>{}</span>"},"pv27:6.4":{"pli":"petalokaṃ ito gatā. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:7.1":{"pli":"Te duggatā sūcikaṭṭā, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:7.2":{"pli":"kilantā naggino kisā; ","html":"<span class='verse-line'>{}</span>"},"pv27:7.3":{"pli":"Uttasantā mahattāsā, ","html":"<span class='verse-line'>{}</span>"},"pv27:7.4":{"pli":"na dassenti kurūrino. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:8.1":{"pli":"Anukampassu kāruṇiko, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:8.2":{"pli":"datvā anvādisāhi no; ","html":"<span class='verse-line'>{}</span>"},"pv27:8.3":{"pli":"Tava dinnena dānena, ","html":"<span class='verse-line'>{}</span>"},"pv27:8.4":{"pli":"yāpessanti kurūrino”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:9.1":{"pli":"Thero caritvā piṇḍāya, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:9.2":{"pli":"bhikkhū aññe ca dvādasa; ","html":"<span class='verse-line'>{}</span>"},"pv27:9.3":{"pli":"Ekajjhaṃ sannipatiṃsu, ","html":"<span class='verse-line'>{}</span>"},"pv27:9.4":{"pli":"bhattavissaggakāraṇā. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:10.1":{"pli":"Thero sabbeva te āha, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:10.2":{"pli":"“yathāladdhaṃ dadātha me; ","html":"<span class='verse-line'>{}</span>"},"pv27:10.3":{"pli":"Saṅghabhattaṃ karissāmi, ","html":"<span class='verse-line'>{}</span>"},"pv27:10.4":{"pli":"anukampāya ñātinaṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:11.1":{"pli":"Niyyādayiṃsu therassa, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:11.2":{"pli":"thero saṅghaṃ nimantayi; ","html":"<span class='verse-line'>{}</span>"},"pv27:11.3":{"pli":"Datvā anvādisi thero, ","html":"<span class='verse-line'>{}</span>"},"pv27:11.4":{"pli":"mātu pitu ca bhātuno; ","html":"<span class='verse-line'>{}</span>"},"pv27:11.5":{"pli":"“Idaṃ me ñātīnaṃ hotu, ","html":"<span class='verse-line'>{}</span>"},"pv27:11.6":{"pli":"sukhitā hontu ñātayo”. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:12.1":{"pli":"Samanantarānuddiṭṭhe, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:12.2":{"pli":"bhojanaṃ udapajjatha; ","html":"<span class='verse-line'>{}</span>"},"pv27:12.3":{"pli":"Suciṃ paṇītaṃ sampannaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv27:12.4":{"pli":"anekarasabyañjanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:13.1":{"pli":"Tato uddassayī bhātā, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:13.2":{"pli":"vaṇṇavā balavā sukhī; ","html":"<span class='verse-line'>{}</span>"},"pv27:13.3":{"pli":"“Pahūtaṃ bhojanaṃ bhante, ","html":"<span class='verse-line'>{}</span>"},"pv27:13.4":{"pli":"passa naggāmhase mayaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv27:13.5":{"pli":"Tathā bhante parakkama, ","html":"<span class='verse-line'>{}</span>"},"pv27:13.6":{"pli":"yathā vatthaṃ labhāmase”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:14.1":{"pli":"Thero saṅkārakūṭamhā, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:14.2":{"pli":"uccinitvāna nantake; ","html":"<span class='verse-line'>{}</span>"},"pv27:14.3":{"pli":"Pilotikaṃ paṭaṃ katvā, ","html":"<span class='verse-line'>{}</span>"},"pv27:14.4":{"pli":"saṅghe cātuddise adā. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:15.1":{"pli":"Datvā anvādisī thero, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:15.2":{"pli":"mātu pitu ca bhātuno; ","html":"<span class='verse-line'>{}</span>"},"pv27:15.3":{"pli":"“Idaṃ me ñātīnaṃ hotu, ","html":"<span class='verse-line'>{}</span>"},"pv27:15.4":{"pli":"sukhitā hontu ñātayo”. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:16.1":{"pli":"Samanantarānuddiṭṭhe, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:16.2":{"pli":"vatthāni udapajjisuṃ; ","html":"<span class='verse-line'>{}</span>"},"pv27:16.3":{"pli":"Tato suvatthavasano, ","html":"<span class='verse-line'>{}</span>"},"pv27:16.4":{"pli":"therassa dassayītumaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:17.1":{"pli":"“Yāvatā nandarājassa, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:17.2":{"pli":"vijitasmiṃ paṭicchadā; ","html":"<span class='verse-line'>{}</span>"},"pv27:17.3":{"pli":"Tato bahutarā bhante, ","html":"<span class='verse-line'>{}</span>"},"pv27:17.4":{"pli":"vatthānacchādanāni no. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:18.1":{"pli":"Koseyyakambalīyāni, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:18.2":{"pli":"khoma kappāsikāni ca; ","html":"<span class='verse-line'>{}</span>"},"pv27:18.3":{"pli":"Vipulā ca mahagghā ca, ","html":"<span class='verse-line'>{}</span>"},"pv27:18.4":{"pli":"tepākāsevalambare. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:19.1":{"pli":"Te mayaṃ paridahāma, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:19.2":{"pli":"yaṃ yaṃ hi manaso piyaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv27:19.3":{"pli":"Tathā bhante parakkama, ","html":"<span class='verse-line'>{}</span>"},"pv27:19.4":{"pli":"yathā gehaṃ labhāmase”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:20.1":{"pli":"Thero paṇṇakuṭiṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:20.2":{"pli":"saṅghe cātuddise adā; ","html":"<span class='verse-line'>{}</span>"},"pv27:20.3":{"pli":"Datvā anvādisī thero, ","html":"<span class='verse-line'>{}</span>"},"pv27:20.4":{"pli":"mātu pitu ca bhātuno; ","html":"<span class='verse-line'>{}</span>"},"pv27:20.5":{"pli":"“Idaṃ me ñātīnaṃ hotu, ","html":"<span class='verse-line'>{}</span>"},"pv27:20.6":{"pli":"sukhitā hontu ñātayo”. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:21.1":{"pli":"Samanantarānuddiṭṭhe, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:21.2":{"pli":"gharāni udapajjisuṃ; ","html":"<span class='verse-line'>{}</span>"},"pv27:21.3":{"pli":"Kūṭāgāranivesanā, ","html":"<span class='verse-line'>{}</span>"},"pv27:21.4":{"pli":"vibhattā bhāgaso mitā. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:22.1":{"pli":"“Na manussesu īdisā, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:22.2":{"pli":"yādisā no gharā idha; ","html":"<span class='verse-line'>{}</span>"},"pv27:22.3":{"pli":"Api dibbesu yādisā, ","html":"<span class='verse-line'>{}</span>"},"pv27:22.4":{"pli":"tādisā no gharā idha. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:23.1":{"pli":"Daddallamānā ābhenti, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:23.2":{"pli":"Samantā caturo disā; ","html":"<span class='verse-line'>{}</span>"},"pv27:23.3":{"pli":"Tathā bhante parakkama, ","html":"<span class='verse-line'>{}</span>"},"pv27:23.4":{"pli":"Yathā pānīyaṃ labhāmase”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:24.1":{"pli":"Thero karaṇaṃ pūretvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:24.2":{"pli":"saṅghe cātuddise adā; ","html":"<span class='verse-line'>{}</span>"},"pv27:24.3":{"pli":"Datvā anvādisī thero, ","html":"<span class='verse-line'>{}</span>"},"pv27:24.4":{"pli":"mātu pitu ca bhātuno; ","html":"<span class='verse-line'>{}</span>"},"pv27:24.5":{"pli":"“Idaṃ me ñātīnaṃ hotu, ","html":"<span class='verse-line'>{}</span>"},"pv27:24.6":{"pli":"sukhitā hontu ñātayo”. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:25.1":{"pli":"Samanantarānuddiṭṭhe, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:25.2":{"pli":"pānīyaṃ udapajjatha; ","html":"<span class='verse-line'>{}</span>"},"pv27:25.3":{"pli":"Gambhīrā caturassā ca, ","html":"<span class='verse-line'>{}</span>"},"pv27:25.4":{"pli":"pokkharañño sunimmitā. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:26.1":{"pli":"Sītodikā suppatitthā, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:26.2":{"pli":"sītā appaṭigandhiyā; ","html":"<span class='verse-line'>{}</span>"},"pv27:26.3":{"pli":"Padumuppalasañchannā, ","html":"<span class='verse-line'>{}</span>"},"pv27:26.4":{"pli":"vārikiñjakkhapūritā. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:27.1":{"pli":"Tattha nhatvā pivitvā ca, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:27.2":{"pli":"therassa paṭidassayuṃ; ","html":"<span class='verse-line'>{}</span>"},"pv27:27.3":{"pli":"“Pahūtaṃ pānīyaṃ bhante, ","html":"<span class='verse-line'>{}</span>"},"pv27:27.4":{"pli":"pādā dukkhā phalanti no”. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:28.1":{"pli":"“Āhiṇḍamānā khañjāma, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:28.2":{"pli":"sakkhare kusakaṇṭake; ","html":"<span class='verse-line'>{}</span>"},"pv27:28.3":{"pli":"‘Tathā bhante parakkama, ","html":"<span class='verse-line'>{}</span>"},"pv27:28.4":{"pli":"yathā yānaṃ labhāmase’”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:29.1":{"pli":"Thero sipāṭikaṃ laddhā, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:29.2":{"pli":"saṅghe cātuddise adā; ","html":"<span class='verse-line'>{}</span>"},"pv27:29.3":{"pli":"Datvā anvādisī thero, ","html":"<span class='verse-line'>{}</span>"},"pv27:29.4":{"pli":"mātu pitu ca bhātuno; ","html":"<span class='verse-line'>{}</span>"},"pv27:29.5":{"pli":"“Idaṃ me ñātīnaṃ hotu, ","html":"<span class='verse-line'>{}</span>"},"pv27:29.6":{"pli":"sukhitā hontu ñātayo”. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:30.1":{"pli":"Samanantarānuddiṭṭhe, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:30.2":{"pli":"petā rathena māgamuṃ; ","html":"<span class='verse-line'>{}</span>"},"pv27:30.3":{"pli":"“Anukampitamha bhadante, ","html":"<span class='verse-line'>{}</span>"},"pv27:30.4":{"pli":"bhattenacchādanena ca. ","html":"<span class='verse-line'>{}</span></p>"},"pv27:31.1":{"pli":"Gharena pānīyadānena, ","html":"<p><span class='verse-line'>{}</span>"},"pv27:31.2":{"pli":"yānadānena cūbhayaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv27:31.3":{"pli":"Muniṃ kāruṇikaṃ loke, ","html":"<span class='verse-line'>{}</span>"},"pv27:31.4":{"pli":"bhante vanditumāgatā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv27:32.1":{"pli":"Sāṇavāsītherapetavatthu dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv28":{"pv28:0.1":{"pli":"Petavatthu ","html":"<article id='pv28'><header><ul><li class='division'>{}</li>"},"pv28:0.2":{"pli":"Cūḷavagga ","html":"<li>{}</li></ul>"},"pv28:0.3":{"pli":"3. Rathakārapetivatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv28:1.1":{"pli":"“Veḷuriyathambhaṃ ruciraṃ pabhassaraṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv28:1.2":{"pli":"Vimānamāruyha anekacittaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv28:1.3":{"pli":"Tatthacchasi devi mahānubhāve, ","html":"<span class='verse-line'>{}</span>"},"pv28:1.4":{"pli":"Pathaddhani pannaraseva cando. ","html":"<span class='verse-line'>{}</span></p>"},"pv28:2.1":{"pli":"Vaṇṇo ca te kanakassa sannibho, ","html":"<p><span class='verse-line'>{}</span>"},"pv28:2.2":{"pli":"Uttattarūpo bhusa dassaneyyo; ","html":"<span class='verse-line'>{}</span>"},"pv28:2.3":{"pli":"Pallaṅkaseṭṭhe atule nisinnā, ","html":"<span class='verse-line'>{}</span>"},"pv28:2.4":{"pli":"Ekā tuvaṃ natthi ca tuyha sāmiko. ","html":"<span class='verse-line'>{}</span></p>"},"pv28:3.1":{"pli":"Imā ca te pokkharaṇī samantā, ","html":"<p><span class='verse-line'>{}</span>"},"pv28:3.2":{"pli":"Pahūtamalyā bahupuṇḍarīkā; ","html":"<span class='verse-line'>{}</span>"},"pv28:3.3":{"pli":"Suvaṇṇacuṇṇehi samantamotthatā, ","html":"<span class='verse-line'>{}</span>"},"pv28:3.4":{"pli":"Na tattha paṅko paṇako ca vijjati. ","html":"<span class='verse-line'>{}</span></p>"},"pv28:4.1":{"pli":"Haṃsā cime dassanīyā manoramā, ","html":"<p><span class='verse-line'>{}</span>"},"pv28:4.2":{"pli":"Udakasmimanupariyanti sabbadā; ","html":"<span class='verse-line'>{}</span>"},"pv28:4.3":{"pli":"Samayya vaggūpanadanti sabbe, ","html":"<span class='verse-line'>{}</span>"},"pv28:4.4":{"pli":"Bindussarā dundubhīnaṃva ghoso. ","html":"<span class='verse-line'>{}</span></p>"},"pv28:5.1":{"pli":"Daddallamānā yasasā yasassinī, ","html":"<p><span class='verse-line'>{}</span>"},"pv28:5.2":{"pli":"Nāvāya ca tvaṃ avalamba tiṭṭhasi; ","html":"<span class='verse-line'>{}</span>"},"pv28:5.3":{"pli":"Āḷārapamhe hasite piyaṃvade, ","html":"<span class='verse-line'>{}</span>"},"pv28:5.4":{"pli":"Sabbaṅgakalyāṇi bhusaṃ virocasi. ","html":"<span class='verse-line'>{}</span></p>"},"pv28:6.1":{"pli":"Idaṃ vimānaṃ virajaṃ same ṭhitaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv28:6.2":{"pli":"Uyyānavantaṃ ratinandivaḍḍhanaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv28:6.3":{"pli":"Icchāmahaṃ nāri anomadassane, ","html":"<span class='verse-line'>{}</span>"},"pv28:6.4":{"pli":"Tayā saha nandane idha moditun”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv28:7.1":{"pli":"“Karohi kammaṃ idha vedanīyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv28:7.2":{"pli":"Cittañca te idha nihitaṃ bhavatu; ","html":"<span class='verse-line'>{}</span>"},"pv28:7.3":{"pli":"Katvāna kammaṃ idha vedanīyaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv28:7.4":{"pli":"Evaṃ mamaṃ lacchasi kāmakāminin”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv28:8.1":{"pli":"“Sādhū”ti so tassā paṭissuṇitvā, ","html":"<p><span class='verse-line'>{}</span>"},"pv28:8.2":{"pli":"Akāsi kammaṃ tahiṃ vedanīyaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv28:8.3":{"pli":"Katvāna kammaṃ tahiṃ vedanīyaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv28:8.4":{"pli":"Upapajji so māṇavo tassā sahabyatanti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv28:9.1":{"pli":"Rathakārapetivatthu tatiyaṃ. ","html":"<p class='endsutta'>{}<br>"},"pv28:9.2":{"pli":"Bhāṇavāraṃ dutiyaṃ. ","html":"{}</p></article>"}},"pv29":{"pv29:0.1":{"pli":"Petavatthu ","html":"<article id='pv29'><header><ul><li class='division'>{}</li>"},"pv29:0.2":{"pli":"Cūḷavagga ","html":"<li>{}</li></ul>"},"pv29:0.3":{"pli":"4. Bhusapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv29:1.1":{"pli":"“Bhusāni eko sāliṃ punāparo, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv29:1.2":{"pli":"Ayañca nārī sakamaṃsalohitaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv29:1.3":{"pli":"Tuvañca gūthaṃ asuciṃ akantaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv29:1.4":{"pli":"Paribhuñjasi kissa ayaṃ vipāko”ti. ","html":"<span class='verse-line'>{}</span></p>"},"pv29:2.1":{"pli":"“Ayaṃ pure mātaraṃ hiṃsati, ","html":"<p><span class='verse-line'>{}</span>"},"pv29:2.2":{"pli":"Ayaṃ pana kūṭavāṇijo; ","html":"<span class='verse-line'>{}</span>"},"pv29:2.3":{"pli":"Ayaṃ maṃsāni khāditvā, ","html":"<span class='verse-line'>{}</span>"},"pv29:2.4":{"pli":"Musāvādena vañceti. ","html":"<span class='verse-line'>{}</span></p>"},"pv29:3.1":{"pli":"Ahaṃ manussesu manussabhūtā, ","html":"<p><span class='verse-line'>{}</span>"},"pv29:3.2":{"pli":"Agārinī sabbakulassa issarā; ","html":"<span class='verse-line'>{}</span>"},"pv29:3.3":{"pli":"Santesu pariguhāmi, ","html":"<span class='verse-line'>{}</span>"},"pv29:3.4":{"pli":"Mā ca kiñci ito adaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"pv29:4.1":{"pli":"Musāvādena chādemi, ","html":"<p><span class='verse-line'>{}</span>"},"pv29:4.2":{"pli":"‘natthi etaṃ mama gehe; ","html":"<span class='verse-line'>{}</span>"},"pv29:4.3":{"pli":"Sace santaṃ niguhāmi, ","html":"<span class='verse-line'>{}</span>"},"pv29:4.4":{"pli":"gūtho me hotu bhojanaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"pv29:5.1":{"pli":"Tassa kammassa vipākena, ","html":"<p><span class='verse-line'>{}</span>"},"pv29:5.2":{"pli":"musāvādassa cūbhayaṃ; ","html":"<span class='verse-line'>{}</span>"},"pv29:5.3":{"pli":"Sugandhaṃ sālino bhattaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv29:5.4":{"pli":"gūthaṃ me parivattati. ","html":"<span class='verse-line'>{}</span></p>"},"pv29:6.1":{"pli":"Avañjhāni ca kammāni, ","html":"<p><span class='verse-line'>{}</span>"},"pv29:6.2":{"pli":"Na hi kammaṃ vinassati; ","html":"<span class='verse-line'>{}</span>"},"pv29:6.3":{"pli":"Duggandhaṃ kiminaṃ mīḷaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv29:6.4":{"pli":"Bhuñjāmi ca pivāmi cā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv29:7.1":{"pli":"Bhusapetavatthu catutthaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"pv3":{"pv3:0.1":{"pli":"Petavatthu ","html":"<article id='pv3'><header><ul><li class='division'>{}</li>"},"pv3:0.2":{"pli":"Uragavagga ","html":"<li>{}</li></ul>"},"pv3:0.3":{"pli":"3. Pūtimukhapetavatthu ","html":"<h1 class='sutta-title'>{}</h1></header>"},"pv3:1.1":{"pli":"“Dibbaṃ subhaṃ dhāresi vaṇṇadhātuṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"pv3:1.2":{"pli":"Vehāyasaṃ tiṭṭhasi antalikkhe; ","html":"<span class='verse-line'>{}</span>"},"pv3:1.3":{"pli":"Mukhañca te kimayo pūtigandhaṃ, ","html":"<span class='verse-line'>{}</span>"},"pv3:1.4":{"pli":"Khādanti kiṃ kammamakāsi pubbe”. ","html":"<span class='verse-line'>{}</span></p>"},"pv3:2.1":{"pli":"“Samaṇo ahaṃ pāpotiduṭṭhavāco, ","html":"<p><span class='verse-line'>{}</span>"},"pv3:2.2":{"pli":"Tapassirūpo mukhasā asaññato; ","html":"<span class='verse-line'>{}</span>"},"pv3:2.3":{"pli":"Laddhā ca me tapasā vaṇṇadhātu, ","html":"<span class='verse-line'>{}</span>"},"pv3:2.4":{"pli":"Mukhañca me pesuṇiyena pūti. ","html":"<span class='verse-line'>{}</span></p>"},"pv3:3.1":{"pli":"Tayidaṃ tayā nārada sāmaṃ diṭṭhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"pv3:3.2":{"pli":"Anukampakā ye kusalā vadeyyuṃ; ","html":"<span class='verse-line'>{}</span>"},"pv3:3.3":{"pli":"‘Mā pesuṇaṃ mā ca musā abhāṇi, ","html":"<span class='verse-line'>{}</span>"},"pv3:3.4":{"pli":"Yakkho tuvaṃ hohisi kāmakāmī’”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"pv3:4.1":{"pli":"Pūtimukhapetavatthu tatiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}