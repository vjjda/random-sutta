{"thi-ap37":{"thi-ap37:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap37'><header><ul><li class='division'>{}</li>"},"thi-ap37:0.2":{"pli":"Khattiyāvagga ","html":"<li>{}</li></ul>"},"thi-ap37:0.3":{"pli":"7 Aḍḍhakāsitherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap37:1.1":{"pli":"“Imamhi bhaddake kappe, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap37:1.2":{"pli":"brahmabandhu mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:1.3":{"pli":"Kassapo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:1.4":{"pli":"uppajji vadataṃ varo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:2.1":{"pli":"Tadāhaṃ pabbajitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:2.2":{"pli":"tassa buddhassa sāsane; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:2.3":{"pli":"Saṃvutā pātimokkhamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:2.4":{"pli":"indriyesu ca pañcasu. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:3.1":{"pli":"Mattaññunī ca asane, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:3.2":{"pli":"yuttā jāgariyepi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:3.3":{"pli":"Vasantī yuttayogāhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:3.4":{"pli":"bhikkhuniṃ vigatāsavaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:4.1":{"pli":"Akkosiṃ duṭṭhacittāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:4.2":{"pli":"gaṇiketi bhaṇiṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:4.3":{"pli":"Tena pāpena kammena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:4.4":{"pli":"nirayamhi apaccisaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:5.1":{"pli":"Tena kammāvasesena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:5.2":{"pli":"ajāyiṃ gaṇikākule; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:5.3":{"pli":"Bahusova parādhīnā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:5.4":{"pli":"pacchimāya ca jātiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:6.1":{"pli":"Kāsīsu seṭṭhikulajā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:6.2":{"pli":"brahmacārībalenahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:6.3":{"pli":"Accharā viya devesu, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:6.4":{"pli":"ahosiṃ rūpasampadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:7.1":{"pli":"Disvāna dassanīyaṃ maṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:7.2":{"pli":"giribbajapuruttame; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:7.3":{"pli":"Gaṇikatte nivesesuṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:7.4":{"pli":"akkosanabalena me. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:8.1":{"pli":"Sāhaṃ sutvāna saddhammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:8.2":{"pli":"buddhaseṭṭhena desitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:8.3":{"pli":"Pubbavāsanasampannā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:8.4":{"pli":"pabbajiṃ anagāriyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:9.1":{"pli":"Tadūpasampadatthāya, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:9.2":{"pli":"gacchantī jinasantikaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:9.3":{"pli":"Magge dhutte ṭhite sutvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:9.4":{"pli":"labhiṃ dūtopasampadaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:10.1":{"pli":"Sabbakammaṃ parikkhīṇaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:10.2":{"pli":"puññaṃ pāpaṃ tatheva ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:10.3":{"pli":"Sabbasaṃsāramuttiṇṇā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:10.4":{"pli":"gaṇikattañca khepitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:11.1":{"pli":"Iddhīsu ca vasī homi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:11.2":{"pli":"dibbāya sotadhātuyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:11.3":{"pli":"Cetopariyañāṇassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:11.4":{"pli":"vasī homi mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:12.1":{"pli":"Pubbenivāsaṃ jānāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:12.2":{"pli":"dibbacakkhu visodhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:12.3":{"pli":"Sabbāsavā parikkhīṇā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:12.4":{"pli":"natthi dāni punabbhavo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:13.1":{"pli":"Atthadhammaniruttīsu, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:13.2":{"pli":"paṭibhāne tatheva ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:13.3":{"pli":"Ñāṇaṃ mama mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:13.4":{"pli":"uppannaṃ tava santike. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:14.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:14.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:14.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:15.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:15.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:15.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap37:16.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap37:16.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap37:16.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap37:17.1":{"pli":"Itthaṃ sudaṃ aḍḍhakāsi bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap37:18.1":{"pli":"Aḍḍhakāsitheriyāpadānaṃ sattamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"thi-ap38":{"thi-ap38:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap38'><header><ul><li class='division'>{}</li>"},"thi-ap38:0.2":{"pli":"Khattiyāvagga ","html":"<li>{}</li></ul>"},"thi-ap38:0.3":{"pli":"8 Puṇṇikātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap38:1.1":{"pli":"“Vipassino bhagavato, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap38:1.2":{"pli":"sikhino vessabhussa ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:1.3":{"pli":"Kakusandhassa munino, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:1.4":{"pli":"koṇāgamanatādino. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:2.1":{"pli":"Kassapassa ca buddhassa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:2.2":{"pli":"pabbajitvāna sāsane; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:2.3":{"pli":"Bhikkhunī sīlasampannā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:2.4":{"pli":"nipakā saṃvutindriyā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:3.1":{"pli":"Bahussutā dhammadharā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:3.2":{"pli":"dhammatthapaṭipucchikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:3.3":{"pli":"Uggahetā ca dhammānaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:3.4":{"pli":"sotā payirupāsitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:4.1":{"pli":"Desentī janamajjhehaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:4.2":{"pli":"ahosiṃ jinasāsane; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:4.3":{"pli":"Bāhusaccena tenāhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:4.4":{"pli":"pesalā atimaññisaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:5.1":{"pli":"Pacchime ca bhave dāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:5.2":{"pli":"sāvatthiyaṃ puruttame; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:5.3":{"pli":"Anāthapiṇḍino gehe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:5.4":{"pli":"jātāhaṃ kumbhadāsiyā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:6.1":{"pli":"Gatā udakahāriyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:6.2":{"pli":"sotthiyaṃ dijamaddasaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:6.3":{"pli":"Sītaṭṭaṃ toyamajjhamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:6.4":{"pli":"taṃ disvā idamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:7.1":{"pli":"‘Udahārī ahaṃ sīte, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:7.2":{"pli":"sadā udakamotariṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:7.3":{"pli":"Ayyānaṃ daṇḍabhayabhītā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:7.4":{"pli":"vācādosabhayaṭṭitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:8.1":{"pli":"Kassa brāhmaṇa tvaṃ bhīto, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:8.2":{"pli":"sadā udakamotari; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:8.3":{"pli":"Vedhamānehi gattehi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:8.4":{"pli":"sītaṃ vedayase bhusaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:9.1":{"pli":"‘Jānantī vata maṃ bhoti, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:9.2":{"pli":"puṇṇike paripucchasi; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:9.3":{"pli":"Karontaṃ kusalaṃ kammaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:9.4":{"pli":"rundhantaṃ katapāpakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:10.1":{"pli":"Yo ce vuḍḍho daharo vā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:10.2":{"pli":"pāpakammaṃ pakubbati; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:10.3":{"pli":"Dakābhisiñcanā sopi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:10.4":{"pli":"pāpakammā pamuccati’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:11.1":{"pli":"Uttarantassa akkhāsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:11.2":{"pli":"dhammatthasaṃhitaṃ padaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:11.3":{"pli":"Tañca sutvā sa saṃviggo, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:11.4":{"pli":"pabbajitvārahā ahu. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:12.1":{"pli":"Pūrentī ūnakasataṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:12.2":{"pli":"jātā dāsikule yato; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:12.3":{"pli":"Tato puṇṇāti nāmaṃ me, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:12.4":{"pli":"bhujissaṃ maṃ akaṃsu te. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:13.1":{"pli":"Seṭṭhiṃ tatonujānetvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:13.2":{"pli":"pabbajiṃ anagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:13.3":{"pli":"Na cireneva kālena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:13.4":{"pli":"arahattamapāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:14.1":{"pli":"Iddhīsu ca vasī homi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:14.2":{"pli":"dibbāya sotadhātuyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:14.3":{"pli":"Cetopariyañāṇassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:14.4":{"pli":"vasī homi mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:15.1":{"pli":"Pubbenivāsaṃ jānāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:15.2":{"pli":"dibbacakkhu visodhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:15.3":{"pli":"Sabbāsavā parikkhīṇā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:15.4":{"pli":"natthi dāni punabbhavo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:16.1":{"pli":"Atthadhammaniruttīsu, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:16.2":{"pli":"paṭibhāne tatheva ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:16.3":{"pli":"Ñāṇaṃ me vimalaṃ suddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:16.4":{"pli":"buddhaseṭṭhassa vāhasā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:17.1":{"pli":"Bhāvanāya mahāpaññā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:17.2":{"pli":"suteneva sutāvinī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:17.3":{"pli":"Mānena nīcakulajā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:17.4":{"pli":"na hi kammaṃ vinassati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:18.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:18.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:18.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:19.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:19.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:19.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap38:20.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap38:20.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap38:20.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap38:21.1":{"pli":"Itthaṃ sudaṃ puṇṇikā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap38:22.1":{"pli":"Puṇṇikātheriyāpadānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"thi-ap39":{"thi-ap39:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap39'><header><ul><li class='division'>{}</li>"},"thi-ap39:0.2":{"pli":"Khattiyāvagga ","html":"<li>{}</li></ul>"},"thi-ap39:0.3":{"pli":"9 Ambapālitherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap39:1.1":{"pli":"“Yo raṃsiphusitāveḷo, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap39:1.2":{"pli":"phusso nāma mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:1.3":{"pli":"Tassāhaṃ bhaginī āsiṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:1.4":{"pli":"ajāyiṃ khattiye kule. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap39:2.1":{"pli":"Tassa dhammaṃ suṇitvāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap39:2.2":{"pli":"vippasannena cetasā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:2.3":{"pli":"Mahādānaṃ daditvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:2.4":{"pli":"patthayiṃ rūpasampadaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap39:3.1":{"pli":"Ekattiṃse ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap39:3.2":{"pli":"sikhī lokagganāyako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:3.3":{"pli":"Uppanno lokapajjoto, ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:3.4":{"pli":"tilokasaraṇo jino. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap39:4.1":{"pli":"Tadāruṇapure ramme, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap39:4.2":{"pli":"brāhmaññakulasambhavā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:4.3":{"pli":"Vimuttacittaṃ kupitā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:4.4":{"pli":"bhikkhuniṃ abhisāpayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap39:5.1":{"pli":"‘Vesikāva anācārā’, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap39:5.2":{"pli":"jinasāsanadūsikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:5.3":{"pli":"Evaṃ akkosayitvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:5.4":{"pli":"tena pāpena kammunā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap39:6.1":{"pli":"Dāruṇaṃ nirayaṃ gantvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap39:6.2":{"pli":"mahādukkhasamappitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:6.3":{"pli":"Tato cutā manussesu, ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:6.4":{"pli":"upapannā tapassinī. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap39:7.1":{"pli":"Dasajātisahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap39:7.2":{"pli":"gaṇikattamakārayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:7.3":{"pli":"Tamhā pāpā na muccissaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:7.4":{"pli":"bhutvā duṭṭhavisaṃ yathā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap39:8.1":{"pli":"Brahmacariyamasevissaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap39:8.2":{"pli":"kassape jinasāsane; ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:8.3":{"pli":"Tena kammavipākena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:8.4":{"pli":"ajāyiṃ tidase pure. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap39:9.1":{"pli":"Pacchime bhave sampatte, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap39:9.2":{"pli":"ahosiṃ opapātikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:9.3":{"pli":"Ambasākhantare jātā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:9.4":{"pli":"ambapālīti tenahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap39:10.1":{"pli":"Parivutā pāṇakoṭīhi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap39:10.2":{"pli":"pabbajiṃ jinasāsane; ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:10.3":{"pli":"Pattāhaṃ acalaṃ ṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap39:10.4":{"pli":"dhītā budd