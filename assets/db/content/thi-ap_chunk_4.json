{"thi-ap19":{"thi-ap19:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap19'><header><ul><li class='division'>{}</li>"},"thi-ap19:0.2":{"pli":"Ekūposathikavagga ","html":"<li>{}</li></ul>"},"thi-ap19:0.3":{"pli":"Uppalavaṇṇātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap19:1.1":{"pli":"“Bhikkhunī uppalavaṇṇā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap19:1.2":{"pli":"iddhiyā pāramiṃ gatā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:1.3":{"pli":"Vanditvā satthuno pāde, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:1.4":{"pli":"idaṃ vacanamabravi. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:2.1":{"pli":"‘Nitthiṇṇā jātisaṃsāraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:2.2":{"pli":"pattāhaṃ acalaṃ padaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:2.3":{"pli":"Sabbadukkhaṃ mayā khīṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:2.4":{"pli":"ārocemi mahāmuni. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:3.1":{"pli":"Yāvatā parisā atthi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:3.2":{"pli":"pasannā jinasāsane; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:3.3":{"pli":"Yassā ca meparādhotthi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:3.4":{"pli":"khamantu jinasammukhā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:4.1":{"pli":"Saṃsāre saṃsarantiyā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:4.2":{"pli":"khalitaṃ me sace bhave; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:4.3":{"pli":"Ārocemi mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:4.4":{"pli":"aparādhaṃ khamassu taṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:5.1":{"pli":"‘Iddhiñcāpi nidassehi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:5.2":{"pli":"mama sāsanakārike; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:5.3":{"pli":"Catasso parisā ajja, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:5.4":{"pli":"kaṅkhaṃ chindāhi yāvatā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:6.1":{"pli":"‘Dhītā tuyhaṃ mahāvīra, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:6.2":{"pli":"paññavanta jutindhara; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:6.3":{"pli":"Bahuñca dukkaraṃ kammaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:6.4":{"pli":"kataṃ me atidukkaraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:7.1":{"pli":"Uppalasseva me vaṇṇo, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:7.2":{"pli":"nāmenuppalanāmikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:7.3":{"pli":"Sāvikā te mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:7.4":{"pli":"pāde vandāmi cakkhuma. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:8.1":{"pli":"Rāhulo ca ahañceva, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:8.2":{"pli":"nekajātisate bahū; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:8.3":{"pli":"Ekasmiṃ sambhave jātā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:8.4":{"pli":"samānachandamānasā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:9.1":{"pli":"Nibbatti ekato hoti, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:9.2":{"pli":"jātiyāpi ca ekato; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:9.3":{"pli":"Pacchime bhave sampatte, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:9.4":{"pli":"ubhopi nānāsambhavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:10.1":{"pli":"Putto ca rāhulo nāma, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:10.2":{"pli":"dhītā uppalasavhayā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:10.3":{"pli":"Passa vīra mamaṃ iddhiṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:10.4":{"pli":"balaṃ dassemi satthuno’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:11.1":{"pli":"Mahāsamudde caturo, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:11.2":{"pli":"pakkhipi hatthapātiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:11.3":{"pli":"Telaṃ hatthagatañceva, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:11.4":{"pli":"khiḍḍo komārako yathā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:12.1":{"pli":"Ubbattayitvā pathaviṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:12.2":{"pli":"pakkhipi hatthapātiyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:12.3":{"pli":"Cittaṃ muñjaṃ yathā nāma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:12.4":{"pli":"luñci komārako yuvā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:13.1":{"pli":"Cakkavāḷasamaṃ pāṇiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:13.2":{"pli":"chādayitvāna matthake; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:13.3":{"pli":"Vassāpetvāna phusitaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:13.4":{"pli":"nānāvaṇṇaṃ punappunaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:14.1":{"pli":"Bhūmiṃ udukkhalaṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:14.2":{"pli":"dhaññaṃ katvāna sakkharaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:14.3":{"pli":"Sineruṃ musalaṃ katvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:14.4":{"pli":"maddi komārikā yathā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:15.1":{"pli":"‘Dhītāhaṃ buddhaseṭṭhassa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:15.2":{"pli":"nāmenuppalasavhayā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:15.3":{"pli":"Abhiññāsu vasībhūtā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:15.4":{"pli":"tava sāsanakārikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:16.1":{"pli":"Nānāvikubbanaṃ katvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:16.2":{"pli":"dassetvā lokanāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:16.3":{"pli":"Nāmagottañca sāvetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:16.4":{"pli":"pāde vandāmi cakkhuma. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:17.1":{"pli":"Iddhīsu ca vasī homi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:17.2":{"pli":"dibbāya sotadhātuyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:17.3":{"pli":"Cetopariyañāṇassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:17.4":{"pli":"vasī homi mahāmune. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:18.1":{"pli":"Pubbenivāsaṃ jānāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:18.2":{"pli":"dibbacakkhu visodhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:18.3":{"pli":"Sabbāsavā parikkhīṇā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:18.4":{"pli":"natthi dāni punabbhavo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:19.1":{"pli":"Atthadhammaniruttīsu, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:19.2":{"pli":"Paṭibhāne tatheva ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:19.3":{"pli":"Ñāṇaṃ me vimalaṃ suddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:19.4":{"pli":"Sabhāvena mahesino. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:20.1":{"pli":"Purimānaṃ jinaggānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:20.2":{"pli":"saṅgamaṃ te nidassitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:20.3":{"pli":"Adhikāraṃ bahuṃ mayhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:20.4":{"pli":"tuyhatthāya mahāmuni. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:21.1":{"pli":"Yaṃ mayā pūritaṃ kammaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:21.2":{"pli":"Kusalaṃ sara me muni; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:21.3":{"pli":"Tavatthāya mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:21.4":{"pli":"Puññaṃ upacitaṃ mayā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:22.1":{"pli":"Abhabbaṭṭhāne vajjetvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:22.2":{"pli":"vārayantī anācaraṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:22.3":{"pli":"Tavatthāya mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:22.4":{"pli":"cattaṃ me jīvituttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:23.1":{"pli":"Dasakoṭisahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:23.2":{"pli":"adāsiṃ mama jīvitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:23.3":{"pli":"Pariccattā ca me homi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:23.4":{"pli":"tavatthāya mahāmuni’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:24.1":{"pli":"Tadātivimhitā sabbā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:24.2":{"pli":"sirasāva katañjalī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:24.3":{"pli":"‘Avocayye kathaṃ āsi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:24.4":{"pli":"atuliddhiparakkamā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:25.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:25.2":{"pli":"nāgakaññā ahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:25.3":{"pli":"Vimalā nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:25.4":{"pli":"kaññānaṃ sādhusammatā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:26.1":{"pli":"Mahorago mahānāgo, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:26.2":{"pli":"pasanno jinasāsane; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:26.3":{"pli":"Padumuttaraṃ mahātejaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:26.4":{"pli":"nimantesi sasāvakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:27.1":{"pli":"Ratanamayaṃ maṇḍapaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:27.2":{"pli":"pallaṅkaṃ ratanāmayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:27.3":{"pli":"Ratanaṃ vālukākiṇṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:27.4":{"pli":"upabhogaṃ ratanāmayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:28.1":{"pli":"Maggañca paṭiyādesi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:28.2":{"pli":"ratanaddhajabhūsitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:28.3":{"pli":"Paccuggantvāna sambuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:28.4":{"pli":"vajjanto tūriyehi so. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:29.1":{"pli":"Parisāhi ca catūhi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:29.2":{"pli":"parivuto lokanāyako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:29.3":{"pli":"Mahoragassa bhavane, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:29.4":{"pli":"nisīdi paramāsane. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:30.1":{"pli":"Annaṃ pānaṃ khādanīyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:30.2":{"pli":"bhojanañca mahārahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:30.3":{"pli":"Varaṃ varañca pādāsi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:30.4":{"pli":"nāgarājā mahāyasaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:31.1":{"pli":"Bhuñjitvāna sambuddho, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:31.2":{"pli":"pattaṃ dhovitvā yoniso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:31.3":{"pli":"Anumodanīyaṅkāsi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:31.4":{"pli":"nāgakaññā mahiddhikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:32.1":{"pli":"Sabbaññuṃ phullitaṃ disvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:32.2":{"pli":"nāgakaññā mahāyasaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:32.3":{"pli":"Pasannaṃ satthuno cittaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:32.4":{"pli":"sunibandhañca mānasaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:33.1":{"pli":"Mamañca cittamaññāya, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:33.2":{"pli":"jalajuttamanāmako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:33.3":{"pli":"Tasmiṃ khaṇe mahāvīro, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:33.4":{"pli":"bhikkhuniṃ dassayiddhiyā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:34.1":{"pli":"Iddhī anekā dassesi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:34.2":{"pli":"bhikkhunī sā visāradā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:34.3":{"pli":"Pamoditā vedajātā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:34.4":{"pli":"satthāraṃ idamabravi. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:35.1":{"pli":"‘Addasāhaṃ imaṃ iddhiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:35.2":{"pli":"sumanaṃ itarāyapi; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:35.3":{"pli":"Kathaṃ ahosi sā vīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:35.4":{"pli":"iddhiyā suvisāradā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:36.1":{"pli":"‘Orasā mukhato jātā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:36.2":{"pli":"dhītā mama mahiddhikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:36.3":{"pli":"Mamānusāsanikarā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:36.4":{"pli":"iddhiyā suvisāradā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:37.1":{"pli":"Buddhassa vacanaṃ sutvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:37.2":{"pli":"evaṃ patthesahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:37.3":{"pli":"‘Ahampi tādisā homi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:37.4":{"pli":"iddhiyā suvisāradā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:38.1":{"pli":"Pamoditāhaṃ sumanā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:38.2":{"pli":"patthe uttamamānasā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:38.3":{"pli":"Anāgatamhi addhāne, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:38.4":{"pli":"īdisā homi nāyaka’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:39.1":{"pli":"Maṇimayamhi pallaṅke, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:39.2":{"pli":"maṇḍapamhi pabhassare; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:39.3":{"pli":"Annapānena tappetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:39.4":{"pli":"sasaṅghaṃ lokanāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:40.1":{"pli":"Nāgānaṃ pavaraṃ pupphaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:40.2":{"pli":"aruṇaṃ nāma uppalaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:40.3":{"pli":"‘Vaṇṇaṃ me īdisaṃ hotu’, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:40.4":{"pli":"pūjesiṃ lokanāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:41.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:41.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:41.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:41.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:42.1":{"pli":"Tato cutāhaṃ manuje, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:42.2":{"pli":"upapannā sayambhuno; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:42.3":{"pli":"Uppalehi paṭicchannaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:42.4":{"pli":"piṇḍapātamadāsahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:43.1":{"pli":"Ekanavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:43.2":{"pli":"vipassī nāma nāyako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:43.3":{"pli":"Uppajji cārudassano, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:43.4":{"pli":"sabbadhammesu cakkhumā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:44.1":{"pli":"Seṭṭhidhītā tadā hutvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:44.2":{"pli":"bārāṇasipuruttame; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:44.3":{"pli":"Nimantetvāna sambuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:44.4":{"pli":"sasaṅghaṃ lokanāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:45.1":{"pli":"Mahādānaṃ daditvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:45.2":{"pli":"uppalehi vināyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:45.3":{"pli":"Pūjayitvā cetasāva, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:45.4":{"pli":"vaṇṇasobhaṃ apatthayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:46.1":{"pli":"Imamhi bhaddake kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:46.2":{"pli":"brahmabandhu mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:46.3":{"pli":"Kassapo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:46.4":{"pli":"uppajji vadataṃ varo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:47.1":{"pli":"Upaṭṭhāko mahesissa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:47.2":{"pli":"tadā āsi narissaro; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:47.3":{"pli":"Kāsirājā kikī nāma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:47.4":{"pli":"bārāṇasipuruttame. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:48.1":{"pli":"Tassāsiṃ dutiyā dhītā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:48.2":{"pli":"samaṇaguttasavhayā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:48.3":{"pli":"Dhammaṃ sutvā jinaggassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:48.4":{"pli":"pabbajjaṃ samarocayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:49.1":{"pli":"Anujāni na no tāto, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:49.2":{"pli":"agāreva tadā mayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:49.3":{"pli":"Vīsavassasahassāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:49.4":{"pli":"vicarimha atanditā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:50.1":{"pli":"Komāribrahmacariyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:50.2":{"pli":"rājakaññā sukhedhitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:50.3":{"pli":"Buddhopaṭṭhānaniratā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:50.4":{"pli":"muditā sattadhītaro. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:51.1":{"pli":"Samaṇī samaṇaguttā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:51.2":{"pli":"bhikkhunī bhikkhudāyikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:51.3":{"pli":"Dhammā ceva sudhammā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:51.4":{"pli":"sattamī saṅghadāyikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:52.1":{"pli":"Ahaṃ khemā ca sappaññā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:52.2":{"pli":"paṭācārā ca kuṇḍalā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:52.3":{"pli":"Kisāgotamī dhammadinnā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:52.4":{"pli":"visākhā hoti sattamī. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:53.1":{"pli":"Tehi kammehi sukatehi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:53.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:53.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:53.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:54.1":{"pli":"Tato cutā manussesu, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:54.2":{"pli":"upapannā mahākule; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:54.3":{"pli":"Pītaṃ maṭṭhaṃ varaṃ dussaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:54.4":{"pli":"adaṃ arahato ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:55.1":{"pli":"Tato cutāriṭṭhapure, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:55.2":{"pli":"jātā vippakule ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:55.3":{"pli":"Dhītā tiriṭivacchassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:55.4":{"pli":"ummādantī manoharā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:56.1":{"pli":"Tato cutā janapade, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:56.2":{"pli":"kule aññatare ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:56.3":{"pli":"Pasūtā nātiphītamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:56.4":{"pli":"sāliṃ gopemahaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:57.1":{"pli":"Disvā paccekasambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:57.2":{"pli":"pañcalājāsatānihaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:57.3":{"pli":"Datvā padumacchannāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:57.4":{"pli":"pañca puttasatānihaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:58.1":{"pli":"Patthayiṃ tepi patthesuṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:58.2":{"pli":"madhuṃ datvā sayambhuno; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:58.3":{"pli":"Tato cutā araññehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:58.4":{"pli":"ajāyiṃ padumodare. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:59.1":{"pli":"Kāsirañño mahesīhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:59.2":{"pli":"hutvā sakkatapūjitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:59.3":{"pli":"Ajaniṃ rājaputtānaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:59.4":{"pli":"anūnaṃ satapañcakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:60.1":{"pli":"Yadā te yobbanappattā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:60.2":{"pli":"kīḷantā jalakīḷitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:60.3":{"pli":"Disvā opattapadumaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:60.4":{"pli":"āsuṃ paccekanāyakā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:61.1":{"pli":"Sāhaṃ tehi vinābhūtā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:61.2":{"pli":"sutavīrehi sokinī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:61.3":{"pli":"Cutā isigilipasse, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:61.4":{"pli":"gāmakamhi ajāyihaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:62.1":{"pli":"Yadā buddhā sutamatī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:62.2":{"pli":"sutānaṃ bhattunopi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:62.3":{"pli":"Yāguṃ ādāya gacchantī, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:62.4":{"pli":"aṭṭha paccekanāyake. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:63.1":{"pli":"Bhikkhāya gāmaṃ gacchante, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:63.2":{"pli":"disvā putte anussariṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:63.3":{"pli":"Khīradhārā viniggacchi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:63.4":{"pli":"tadā me puttapemasā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:64.1":{"pli":"Tato tesaṃ adaṃ yāguṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:64.2":{"pli":"pasannā sehi pāṇibhi; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:64.3":{"pli":"Tato cutāhaṃ tidasaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:64.4":{"pli":"nandanaṃ upapajjahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:65.1":{"pli":"Anubhotvā sukhaṃ dukkhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:65.2":{"pli":"saṃsaritvā bhavābhave; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:65.3":{"pli":"Tavatthāya mahāvīra, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:65.4":{"pli":"pariccattañca jīvitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:66.1":{"pli":"Evaṃ bahuvidhaṃ dukkhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:66.2":{"pli":"sampattī ca bahubbidhā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:66.3":{"pli":"Pacchime bhave sampatte, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:66.4":{"pli":"jātā sāvatthiyaṃ pure. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:67.1":{"pli":"Mahādhanaseṭṭhikule, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:67.2":{"pli":"sukhite sajjite tathā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:67.3":{"pli":"Nānāratanapajjote, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:67.4":{"pli":"sabbakāmasamiddhine. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:68.1":{"pli":"Sakkatā pūjitā ceva, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:68.2":{"pli":"mānitāpacitā tathā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:68.3":{"pli":"Rūpasobhaggasampannā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:68.4":{"pli":"kulesu atisakkatā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:69.1":{"pli":"Atīva patthitā cāsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:69.2":{"pli":"rūpabhogasirīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:69.3":{"pli":"Patthitā seṭṭhiputtehi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:69.4":{"pli":"anekehi satehipi. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:70.1":{"pli":"Agāraṃ pajahitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:70.2":{"pli":"pabbajiṃ anagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:70.3":{"pli":"Aḍḍhamāse asampatte, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:70.4":{"pli":"catusaccamapāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:71.1":{"pli":"Iddhiyā abhinimmitvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:71.2":{"pli":"caturassaṃ rathaṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:71.3":{"pli":"Buddhassa pāde vandissaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:71.4":{"pli":"lokanāthassa tādino. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:72.1":{"pli":"‘Supupphitaggaṃ upagamma pādapaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:72.2":{"pli":"Ekā tuvaṃ tiṭṭhasi sālamūle; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:72.3":{"pli":"Na cāpi te dutiyo atthi koci, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:72.4":{"pli":"Bāle na tvaṃ bhāyasi dhuttakānaṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:73.1":{"pli":"‘Sataṃ sahassānipi dhuttakānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:73.2":{"pli":"Samāgatā edisakā bhaveyyuṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:73.3":{"pli":"Lomaṃ na iñje na sampavedhe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:73.4":{"pli":"Kiṃ me tuvaṃ māra karissaseko. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:74.1":{"pli":"Esā antaradhāyāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:74.2":{"pli":"kucchiṃ vā pavisāmi te; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:74.3":{"pli":"Bhamukantarikāyampi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:74.4":{"pli":"tiṭṭhantiṃ maṃ na dakkhasi. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:75.1":{"pli":"Cittasmiṃ vasībhūtāmhi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:75.2":{"pli":"iddhipādā subhāvitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:75.3":{"pli":"Sabbabandhanamuttāmhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:75.4":{"pli":"na taṃ bhāyāmi āvuso. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:76.1":{"pli":"Sattisūlūpamā kāmā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:76.2":{"pli":"khandhāsaṃ adhikuṭṭanā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:76.3":{"pli":"Yaṃ tvaṃ kāmaratiṃ brūsi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:76.4":{"pli":"aratī dāni sā mama. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:77.1":{"pli":"Sabbattha vihatā nandī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:77.2":{"pli":"tamokhandho padālito; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:77.3":{"pli":"Evaṃ jānāhi pāpima, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:77.4":{"pli":"nihato tvamasi antaka’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:78.1":{"pli":"Jino tamhi guṇe tuṭṭho, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:78.2":{"pli":"etadagge ṭhapesi maṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:78.3":{"pli":"Aggā iddhimatīnanti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:78.4":{"pli":"parisāsu vināyako. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:79.1":{"pli":"Pariciṇṇo mayā satthā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:79.2":{"pli":"kataṃ buddhassa sāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:79.3":{"pli":"Ohito garuko bhāro, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:79.4":{"pli":"bhavanettisamūhatā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:80.1":{"pli":"Yassatthāya pabbajitā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:80.2":{"pli":"agārasmānagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:80.3":{"pli":"So me attho anuppatto, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:80.4":{"pli":"sabbasaṃyojanakkhayo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:81.1":{"pli":"Cīvaraṃ piṇḍapātañca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:81.2":{"pli":"paccayaṃ sayanāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:81.3":{"pli":"Khaṇena upanāmenti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:81.4":{"pli":"sahassāni samantato. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:82.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:82.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:82.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:83.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:83.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:83.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap19:84.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap19:84.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap19:84.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap19:85.1":{"pli":"Itthaṃ sudaṃ uppalavaṇṇā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap19:86.1":{"pli":"Uppalavaṇṇātheriyāpadānaṃ navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"thi-ap2":{"thi-ap2:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap2'><header><ul><li class='division'>{}</li>"},"thi-ap2:0.2":{"pli":"Sumedhāvagga ","html":"<li>{}</li></ul>"},"thi-ap2:0.3":{"pli":"2 Mekhalādāyikātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap2:1.1":{"pli":"“Siddhatthassa bhagavato, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap2:1.2":{"pli":"thūpakārāpikā ahuṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap2:1.3":{"pli":"Mekhalikā mayā dinnā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap2:1.4":{"pli":"navakammāya satthuno. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap2:2.1":{"pli":"Niṭṭhite ca mahāthūpe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap2:2.2":{"pli":"mekhalaṃ punadāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap2:2.3":{"pli":"Lokanāthassa munino, ","html":"<span class='verse-line'>{}</span>"},"thi-ap2:2.4":{"pli":"pasannā sehi pāṇibhi. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap2:3.1":{"pli":"Catunnavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap2:3.2":{"pli":"yaṃ mekhalamadaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap2:3.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap2:3.4":{"pli":"thūpakārassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap2:4.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap2:4.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap2:4.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap2:5.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap2:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap2:5.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap2:6.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap2:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap2:6.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap2:7.1":{"pli":"Itthaṃ sudaṃ mekhalādāyikā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap2:8.1":{"pli":"Mekhalādāyikātheriyāpadānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}