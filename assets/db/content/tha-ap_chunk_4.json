{"tha-ap132":{"tha-ap132:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap132'><header><ul><li class='division'>{}</li>"},"tha-ap132:0.2":{"pli":"Sereyyavagga ","html":"<li>{}</li></ul>"},"tha-ap132:0.3":{"pli":"Padumapūjakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap132:1.1":{"pli":"“Himavantassāvidūre, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap132:1.2":{"pli":"gotamo nāma pabbato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:1.3":{"pli":"Nānārukkhehi sañchanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:1.4":{"pli":"mahābhūtagaṇālayo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap132:2.1":{"pli":"Vemajjhamhi ca tassāsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap132:2.2":{"pli":"assamo abhinimmito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:2.3":{"pli":"Purakkhato sasissehi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:2.4":{"pli":"vasāmi assame ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap132:3.1":{"pli":"Āyantu me sissagaṇā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap132:3.2":{"pli":"padumaṃ āharantu me; ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:3.3":{"pli":"Buddhapūjaṃ karissāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:3.4":{"pli":"dvipadindassa tādino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap132:4.1":{"pli":"Evanti te paṭissutvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap132:4.2":{"pli":"padumaṃ āhariṃsu me; ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:4.3":{"pli":"Tathā nimittaṃ katvāhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:4.4":{"pli":"buddhassa abhiropayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap132:5.1":{"pli":"Sisse tadā samānetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap132:5.2":{"pli":"sādhukaṃ anusāsahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:5.3":{"pli":"Mā kho tumhe pamajjittha, ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:5.4":{"pli":"appamādo sukhāvaho. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap132:6.1":{"pli":"Evaṃ samanusāsitvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap132:6.2":{"pli":"te sisse vacanakkhame; ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:6.3":{"pli":"Appamādaguṇe yutto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:6.4":{"pli":"tadā kālaṅkato ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap132:7.1":{"pli":"Ekanavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap132:7.2":{"pli":"yaṃ pupphamabhiropayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:7.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:7.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap132:8.1":{"pli":"Ekapaññāsakappamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap132:8.2":{"pli":"rājā āsiṃ jaluttamo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:8.3":{"pli":"Sattaratanasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:8.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap132:9.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap132:9.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:9.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap132:10.1":{"pli":"Itthaṃ sudaṃ āyasmā padumapūjako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap132:11.1":{"pli":"Padumapūjakattherassāpadānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"tha-ap132:12.1":{"pli":"Sereyyavaggo terasamo. ","html":"<p class='endsection'>{}</p>"},"tha-ap132:13.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"tha-ap132:14.1":{"pli":"Sereyyako pupphathūpi, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"tha-ap132:14.2":{"pli":"pāyaso gandhathomako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:14.3":{"pli":"Āsani phalasaññī ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:14.4":{"pli":"gaṇṭhipadumapupphiyo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:14.5":{"pli":"Pañcuttarasatā gāthā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap132:14.6":{"pli":"gaṇitā atthadassibhi. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}},"tha-ap133":{"tha-ap133:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap133'><header><ul><li class='division'>{}</li>"},"tha-ap133:0.2":{"pli":"Sobhitavagga ","html":"<li>{}</li></ul>"},"tha-ap133:0.3":{"pli":"Sobhitattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap133:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap133:1.2":{"pli":"lokajeṭṭho narāsabho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:1.3":{"pli":"Mahato janakāyassa, ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:1.4":{"pli":"deseti amataṃ padaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap133:2.1":{"pli":"Tassāhaṃ vacanaṃ sutvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap133:2.2":{"pli":"vācāsabhimudīritaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:2.3":{"pli":"Añjaliṃ paggahetvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:2.4":{"pli":"ekaggo āsahaṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap133:3.1":{"pli":"‘Yathā samuddo udadhīnamaggo, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap133:3.2":{"pli":"Nerū nagānaṃ pavaro siluccayo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:3.3":{"pli":"Tatheva ye cittavasena vattare’, ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:3.4":{"pli":"Na buddhañāṇassa kalaṃ upenti te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap133:4.1":{"pli":"Dhammavidhiṃ ṭhapetvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap133:4.2":{"pli":"buddho kāruṇiko isi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:4.3":{"pli":"Bhikkhusaṅghe nisīditvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:4.4":{"pli":"imā gāthā abhāsatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap133:5.1":{"pli":"‘Yo so ñāṇaṃ pakittesi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap133:5.2":{"pli":"buddhamhi lokanāyake; ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:5.3":{"pli":"Kappānaṃ satasahassaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:5.4":{"pli":"duggatiṃ na gamissati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap133:6.1":{"pli":"Kilese jhāpayitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap133:6.2":{"pli":"ekaggo susamāhito; ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:6.3":{"pli":"Sobhito nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:6.4":{"pli":"hessati satthu sāvako’. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap133:7.1":{"pli":"Paññāse kappasahasse, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap133:7.2":{"pli":"sattevāsuṃ yasuggatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:7.3":{"pli":"Sattaratanasampannā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:7.4":{"pli":"cakkavattī mahabbalā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap133:8.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap133:8.2":{"pli":"bhavā sabbe samūhatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:8.3":{"pli":"Tisso vijjā anuppattā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:8.4":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap133:9.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap133:9.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:9.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap133:9.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap133:10.1":{"pli":"Itthaṃ sudaṃ āyasmā sobhito thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap133:11.1":{"pli":"Sobhitattherassāpadānaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap134":{"tha-ap134:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap134'><header><ul><li class='division'>{}</li>"},"tha-ap134:0.2":{"pli":"Sobhitavagga ","html":"<li>{}</li></ul>"},"tha-ap134:0.3":{"pli":"Sudassanattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap134:1.1":{"pli":"“Vinatā nadiyā tīre, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap134:1.2":{"pli":"pilakkhu phalito ahu; ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:1.3":{"pli":"Tāhaṃ rukkhaṃ gavesanto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:1.4":{"pli":"addasaṃ lokanāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap134:2.1":{"pli":"Ketakaṃ pupphitaṃ disvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap134:2.2":{"pli":"vaṇṭe chetvānahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:2.3":{"pli":"Buddhassa abhiropesiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:2.4":{"pli":"sikhino lokabandhuno. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap134:3.1":{"pli":"Yena ñāṇena pattosi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap134:3.2":{"pli":"accutaṃ amataṃ padaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:3.3":{"pli":"Taṃ ñāṇaṃ abhipūjemi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:3.4":{"pli":"buddhaseṭṭha mahāmuni. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap134:4.1":{"pli":"Ñāṇamhi pūjaṃ katvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap134:4.2":{"pli":"pilakkhumaddasaṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:4.3":{"pli":"Paṭiladdhomhi taṃ paññaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:4.4":{"pli":"ñāṇapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap134:5.1":{"pli":"Ekattiṃse ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap134:5.2":{"pli":"yaṃ pupphamabhiropayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:5.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:5.4":{"pli":"ñāṇapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap134:6.1":{"pli":"Ito terasakappamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap134:6.2":{"pli":"dvādasāsuṃ phaluggatā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:6.3":{"pli":"Sattaratanasampannā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:6.4":{"pli":"cakkavattī mahapphalā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap134:7.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap134:7.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap134:7.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap134:8.1":{"pli":"Itthaṃ sudaṃ āyasmā sudassano thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap134:9.1":{"pli":"Sudassanattherassāpadānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap135":{"tha-ap135:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap135'><header><ul><li class='division'>{}</li>"},"tha-ap135:0.2":{"pli":"Sobhitavagga ","html":"<li>{}</li></ul>"},"tha-ap135:0.3":{"pli":"3 Candanapūjanakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap135:1.1":{"pli":"“Candabhāgānadītīre, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap135:1.2":{"pli":"ahosiṃ kinnaro tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:1.3":{"pli":"Pupphabhakkho cahaṃ āsiṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:1.4":{"pli":"pupphānivasano tathā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap135:2.1":{"pli":"Atthadassī tu bhagavā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap135:2.2":{"pli":"lokajeṭṭho narāsabho; ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:2.3":{"pli":"Vipinaggena niyyāsi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:2.4":{"pli":"haṃsarājāva ambare. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap135:3.1":{"pli":"Namo te purisājañña, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap135:3.2":{"pli":"cittaṃ te suvisodhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:3.3":{"pli":"Pasannamukhavaṇṇosi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:3.4":{"pli":"vippasannamukhindriyo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap135:4.1":{"pli":"Orohitvāna ākāsā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap135:4.2":{"pli":"bhūripañño sumedhaso; ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:4.3":{"pli":"Saṅghāṭiṃ pattharitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:4.4":{"pli":"pallaṅkena upāvisi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap135:5.1":{"pli":"Vilīnaṃ candanādāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap135:5.2":{"pli":"agamāsiṃ jinantikaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:5.3":{"pli":"Pasannacitto sumano, ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:5.4":{"pli":"buddhassa abhiropayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap135:6.1":{"pli":"Abhivādetvāna sambuddhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap135:6.2":{"pli":"lokajeṭṭhaṃ narāsabhaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:6.3":{"pli":"Pāmojjaṃ janayitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:6.4":{"pli":"pakkāmiṃ uttarāmukho. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap135:7.1":{"pli":"Aṭṭhārase kappasate, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap135:7.2":{"pli":"candanaṃ yaṃ apūjayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:7.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:7.4":{"pli":"buddhapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap135:8.1":{"pli":"Catuddase kappasate, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap135:8.2":{"pli":"ito āsiṃsu te tayo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:8.3":{"pli":"Rohaṇī nāma nāmena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:8.4":{"pli":"cakkavattī mahabbalā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap135:9.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap135:9.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap135:9.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap135:10.1":{"pli":"Itthaṃ sudaṃ āyasmā candanapūjanako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap135:11.1":{"pli":"Candanapūjanakattherassāpadānaṃ tatiyaṃ. ","html":"<p class='endsutta'>{}<br>"},"tha-ap135:11.2":{"pli":"Aṭṭhamabhāṇavāraṃ. ","html":"{}</p></article>"}},"tha-ap136":{"tha-ap136:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap136'><header><ul><li class='division'>{}</li>"},"tha-ap136:0.2":{"pli":"Sobhitavagga ","html":"<li>{}</li></ul>"},"tha-ap136:0.3":{"pli":"4 Pupphacchadaniyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap136:1.1":{"pli":"“Sunando nāma nāmena, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap136:1.2":{"pli":"brāhmaṇo mantapāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:1.3":{"pli":"Ajjhāyako yācayogo, ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:1.4":{"pli":"vājapeyyaṃ ayājayi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap136:2.1":{"pli":"Padumuttaro lokavidū, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap136:2.2":{"pli":"aggo kāruṇiko isi; ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:2.3":{"pli":"Janataṃ anukampanto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:2.4":{"pli":"ambare caṅkamī tadā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap136:3.1":{"pli":"Caṅkamitvāna sambuddho, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap136:3.2":{"pli":"sabbaññū lokanāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:3.3":{"pli":"Mettāya aphari satte, ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:3.4":{"pli":"appamāṇe nirūpadhi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap136:4.1":{"pli":"Vaṇṭe chetvāna pupphāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap136:4.2":{"pli":"brāhmaṇo mantapāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:4.3":{"pli":"Sabbe sisse samānetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:4.4":{"pli":"ākāse ukkhipāpayi. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap136:5.1":{"pli":"Yāvatā nagaraṃ āsi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap136:5.2":{"pli":"pupphānaṃ chadanaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:5.3":{"pli":"Buddhassa ānubhāvena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:5.4":{"pli":"sattāhaṃ na vigacchatha. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap136:6.1":{"pli":"Teneva sukkamūlena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap136:6.2":{"pli":"anubhotvāna sampadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:6.3":{"pli":"Sabbāsave pariññāya, ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:6.4":{"pli":"tiṇṇo loke visattikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap136:7.1":{"pli":"Ekārase kappasate, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap136:7.2":{"pli":"pañcatiṃsāsu khattiyā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:7.3":{"pli":"Ambaraṃsasanāmā te, ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:7.4":{"pli":"cakkavattī mahabbalā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap136:8.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap136:8.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap136:8.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap136:9.1":{"pli":"Itthaṃ sudaṃ āyasmā pupphacchadaniyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap136:10.1":{"pli":"Pupphacchadaniyattherassāpadānaṃ catutthaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap137":{"tha-ap137:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap137'><header><ul><li class='division'>{}</li>"},"tha-ap137:0.2":{"pli":"Sobhitavagga ","html":"<li>{}</li></ul>"},"tha-ap137:0.3":{"pli":"5 Rahosaññakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap137:1.1":{"pli":"“Himavantassāvidūre, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap137:1.2":{"pli":"vasabho nāma pabbato; ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:1.3":{"pli":"Tasmiṃ pabbatapādamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:1.4":{"pli":"assamo āsi māpito. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap137:2.1":{"pli":"Tīṇi sissasahassāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap137:2.2":{"pli":"vācesiṃ brāhmaṇo tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:2.3":{"pli":"Saṃharitvāna te sisse, ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:2.4":{"pli":"ekamantaṃ upāvisiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap137:3.1":{"pli":"Ekamantaṃ nisīditvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap137:3.2":{"pli":"brāhmaṇo mantapāragū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:3.3":{"pli":"Buddhavedaṃ gavesanto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:3.4":{"pli":"ñāṇe cittaṃ pasādayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap137:4.1":{"pli":"Tattha cittaṃ pasādetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap137:4.2":{"pli":"nisīdiṃ paṇṇasanthare; ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:4.3":{"pli":"Pallaṅkaṃ ābhujitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:4.4":{"pli":"tattha kālaṅkato ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap137:5.1":{"pli":"Ekattiṃse ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap137:5.2":{"pli":"yaṃ saññamalabhiṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:5.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:5.4":{"pli":"ñāṇasaññāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap137:6.1":{"pli":"Sattavīsatikappamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap137:6.2":{"pli":"rājā siridharo ahu; ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:6.3":{"pli":"Sattaratanasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:6.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap137:7.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap137:7.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap137:7.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap137:8.1":{"pli":"Itthaṃ sudaṃ āyasmā rahosaññako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap137:9.1":{"pli":"Rahosaññakattherassāpadānaṃ pañcamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap138":{"tha-ap138:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap138'><header><ul><li class='division'>{}</li>"},"tha-ap138:0.2":{"pli":"Sobhitavagga ","html":"<li>{}</li></ul>"},"tha-ap138:0.3":{"pli":"Campakapupphiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap138:1.1":{"pli":"“Kaṇikāraṃva jotantaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap138:1.2":{"pli":"nisinnaṃ pabbatantare; ","html":"<span class='verse-line'>{}</span>"},"tha-ap138:1.3":{"pli":"Obhāsentaṃ disā sabbā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap138:1.4":{"pli":"osadhiṃ viya tārakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap138:2.1":{"pli":"Tayo māṇavakā āsuṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap138:2.2":{"pli":"sake sippe susikkhitā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap138:2.3":{"pli":"Khāribhāraṃ gahetvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap138:2.4":{"pli":"anventi mama pacchato. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap138:3.1":{"pli":"Puṭake satta pupphāni, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap138:3.2":{"pli":"nikkhittāni tapassinā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap138:3.3":{"pli":"Gahetvā tāni ñāṇamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap138:3.4":{"pli":"vessabhussābhiropayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap138:4.1":{"pli":"Ekattiṃse ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap138:4.2":{"pli":"yaṃ pupphamabhiropayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap138:4.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap138:4.4":{"pli":"ñāṇapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap138:5.1":{"pli":"Ekūnatiṃsakappamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap138:5.2":{"pli":"vipulābhasanāmako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap138:5.3":{"pli":"Sattaratanasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap138:5.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap138:6.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap138:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap138:6.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap138:7.1":{"pli":"Itthaṃ sudaṃ āyasmā campakapupphiyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap138:8.1":{"pli":"Campakapupphiyattherassāpadānaṃ chaṭṭhaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap139":{"tha-ap139:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap139'><header><ul><li class='division'>{}</li>"},"tha-ap139:0.2":{"pli":"Sobhitavagga ","html":"<li>{}</li></ul>"},"tha-ap139:0.3":{"pli":"Atthasandassakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap139:1.1":{"pli":"“Visālamāḷe āsīno, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap139:1.2":{"pli":"addasaṃ lokanāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:1.3":{"pli":"Khīṇāsavaṃ balappattaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:1.4":{"pli":"bhikkhusaṅghapurakkhataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap139:2.1":{"pli":"Satasahassā tevijjā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap139:2.2":{"pli":"chaḷabhiññā mahiddhikā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:2.3":{"pli":"Parivārenti sambuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:2.4":{"pli":"ko disvā nappasīdati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap139:3.1":{"pli":"Ñāṇe upanidhā yassa, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap139:3.2":{"pli":"na vijjati sadevake; ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:3.3":{"pli":"Anantañāṇaṃ sambuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:3.4":{"pli":"ko disvā nappasīdati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap139:4.1":{"pli":"Dhammakāyañca dīpentaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap139:4.2":{"pli":"kevalaṃ ratanākaraṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:4.3":{"pli":"Vikappetuṃ na sakkonti, ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:4.4":{"pli":"ko disvā nappasīdati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap139:5.1":{"pli":"Imāhi tīhi gāthāhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap139:5.2":{"pli":"nāradovhayavacchalo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:5.3":{"pli":"Padumuttaraṃ thavitvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:5.4":{"pli":"sambuddhaṃ aparājitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap139:6.1":{"pli":"Tena cittappasādena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap139:6.2":{"pli":"buddhasanthavanena ca; ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:6.3":{"pli":"Kappānaṃ satasahassaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:6.4":{"pli":"duggatiṃ nupapajjahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap139:7.1":{"pli":"Ito tiṃsakappasate, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap139:7.2":{"pli":"sumitto nāma khattiyo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:7.3":{"pli":"Sattaratanasampanno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:7.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap139:8.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap139:8.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap139:8.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap139:9.1":{"pli":"Itthaṃ sudaṃ āyasmā atthasandassako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap139:10.1":{"pli":"Atthasandassakattherassāpadānaṃ sattamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap14":{"tha-ap14:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap14'><header><ul><li class='division'>{}</li>"},"tha-ap14:0.2":{"pli":"Sīhāsaniyavagga ","html":"<li>{}</li></ul>"},"tha-ap14:0.3":{"pli":"Ekatthambhikattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap14:1.1":{"pli":"“Siddhatthassa bhagavato, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap14:1.2":{"pli":"mahāpūgagaṇo ahu; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:1.3":{"pli":"Saraṇaṃ gatā ca te buddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:1.4":{"pli":"saddahanti tathāgataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:2.1":{"pli":"Sabbe saṅgamma mantetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:2.2":{"pli":"māḷaṃ kubbanti satthuno; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:2.3":{"pli":"Ekatthambhaṃ alabhantā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:2.4":{"pli":"vicinanti brahāvane. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:3.1":{"pli":"Tehaṃ araññe disvāna, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:3.2":{"pli":"upagamma gaṇaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:3.3":{"pli":"Añjaliṃ paggahetvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:3.4":{"pli":"paripucchiṃ gaṇaṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:4.1":{"pli":"Te me puṭṭhā viyākaṃsu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:4.2":{"pli":"sīlavanto upāsakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:4.3":{"pli":"Māḷaṃ mayaṃ kattukāmā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:4.4":{"pli":"ekatthambho na labbhati. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:5.1":{"pli":"Ekatthambhaṃ mamaṃ detha, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:5.2":{"pli":"ahaṃ dassāmi satthuno; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:5.3":{"pli":"Āharissāmahaṃ thambhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:5.4":{"pli":"appossukkā bhavantu te. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:6.1":{"pli":"Te me thambhaṃ pavecchiṃsu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:6.2":{"pli":"pasannā tuṭṭhamānasā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:6.3":{"pli":"Tato paṭinivattitvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:6.4":{"pli":"agamaṃsu sakaṃ gharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:7.1":{"pli":"Aciraṃ gate pūgagaṇe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:7.2":{"pli":"thambhaṃ ahāsahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:7.3":{"pli":"Haṭṭho haṭṭhena cittena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:7.4":{"pli":"paṭhamaṃ ussapesahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:8.1":{"pli":"Tena cittappasādena, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:8.2":{"pli":"Vimānaṃ upapajjahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:8.3":{"pli":"Ubbiddhaṃ bhavanaṃ mayhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:8.4":{"pli":"Sattabhūmaṃ samuggataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:9.1":{"pli":"Vajjamānāsu bherīsu, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:9.2":{"pli":"paricāremahaṃ sadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:9.3":{"pli":"Pañcapaññāsakappamhi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:9.4":{"pli":"rājā āsiṃ yasodharo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:10.1":{"pli":"Tatthāpi bhavanaṃ mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:10.2":{"pli":"sattabhūmaṃ samuggataṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:10.3":{"pli":"Kūṭāgāravarūpetaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:10.4":{"pli":"ekatthambhaṃ manoramaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:11.1":{"pli":"Ekavīsatikappamhi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:11.2":{"pli":"udeno nāma khattiyo; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:11.3":{"pli":"Tatrāpi bhavanaṃ mayhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:11.4":{"pli":"sattabhūmaṃ samuggataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:12.1":{"pli":"Yaṃ yaṃ yonupapajjāmi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:12.2":{"pli":"devattaṃ atha mānusaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:12.3":{"pli":"Anubhomi sukhaṃ sabbaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:12.4":{"pli":"ekatthambhassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:13.1":{"pli":"Catunnavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:13.2":{"pli":"yaṃ thambhamadadaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:13.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:13.4":{"pli":"ekatthambhassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap14:14.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap14:14.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:14.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap14:14.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap14:15.1":{"pli":"Itthaṃ sudaṃ āyasmā ekatthambhiko thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap14:16.1":{"pli":"Ekatthambhikattherassāpadānaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap140":{"tha-ap140:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap140'><header><ul><li class='division'>{}</li>"},"tha-ap140:0.2":{"pli":"Sobhitavagga ","html":"<li>{}</li></ul>"},"tha-ap140:0.3":{"pli":"8 Ekapasādaniyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap140:1.1":{"pli":"“Nārado iti me nāmaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap140:1.2":{"pli":"kesavo iti maṃ vidū; ","html":"<span class='verse-line'>{}</span>"},"tha-ap140:1.3":{"pli":"Kusalākusalaṃ esaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap140:1.4":{"pli":"agamaṃ buddhasantikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap140:2.1":{"pli":"Mettacitto kāruṇiko, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap140:2.2":{"pli":"atthadassī mahāmuni; ","html":"<span class='verse-line'>{}</span>"},"tha-ap140:2.3":{"pli":"Assāsayanto satte so, ","html":"<span class='verse-line'>{}</span>"},"tha-ap140:2.4":{"pli":"dhammaṃ deseti cakkhumā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap140:3.1":{"pli":"Sakaṃ cittaṃ pasādetvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap140:3.2":{"pli":"sire katvāna añjaliṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap140:3.3":{"pli":"Satthāraṃ abhivādetvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap140:3.4":{"pli":"pakkāmiṃ pācināmukho. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap140:4.1":{"pli":"Sattarase kappasate, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap140:4.2":{"pli":"rājā āsi mahīpati; ","html":"<span class='verse-line'>{}</span>"},"tha-ap140:4.3":{"pli":"Amittatāpano nāma, ","html":"<span class='verse-line'>{}</span>"},"tha-ap140:4.4":{"pli":"cakkavattī mahabbalo. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap140:5.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap140:5.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap140:5.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap140:6.1":{"pli":"Itthaṃ sudaṃ āyasmā ekapasādaniyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap140:7.1":{"pli":"Ekapasādaniyattherassāpadānaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap141":{"tha-ap141:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap141'><header><ul><li class='division'>{}</li>"},"tha-ap141:0.2":{"pli":"Sobhitavagga ","html":"<li>{}</li></ul>"},"tha-ap141:0.3":{"pli":"9 Sālapupphadāyakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap141:1.1":{"pli":"“Migarājā tadā āsiṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap141:1.2":{"pli":"abhijāto sukesarī; ","html":"<span class='verse-line'>{}</span>"},"tha-ap141:1.3":{"pli":"Giriduggaṃ gavesanto, ","html":"<span class='verse-line'>{}</span>"},"tha-ap141:1.4":{"pli":"addasaṃ lokanāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap141:2.1":{"pli":"Ayaṃ nu kho mahāvīro, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap141:2.2":{"pli":"nibbāpeti mahājanaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap141:2.3":{"pli":"Yannūnāhaṃ upāseyyaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap141:2.4":{"pli":"devadevaṃ narāsabhaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap141:3.1":{"pli":"Sākhaṃ sālassa bhañjitvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap141:3.2":{"pli":"sakosaṃ pupphamāhariṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap141:3.3":{"pli":"Upagantvāna sambuddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"tha-ap141:3.4":{"pli":"adāsiṃ pupphamuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap141:4.1":{"pli":"Ekanavutito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap141:4.2":{"pli":"yaṃ pupphamabhiropayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap141:4.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap141:4.4":{"pli":"pupphadānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap141:5.1":{"pli":"Ito ca navame kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap141:5.2":{"pli":"virocanasanāmakā; ","html":"<span class='verse-line'>{}</span>"},"tha-ap141:5.3":{"pli":"Tayo āsiṃsu rājāno, ","html":"<span class='verse-line'>{}</span>"},"tha-ap141:5.4":{"pli":"cakkavattī mahabbalā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap141:6.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap141:6.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap141:6.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap141:7.1":{"pli":"Itthaṃ sudaṃ āyasmā sālapupphadāyako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap141:8.1":{"pli":"Sālapupphadāyakattherassāpadānaṃ navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"tha-ap142":{"tha-ap142:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap142'><header><ul><li class='division'>{}</li>"},"tha-ap142:0.2":{"pli":"Sobhitavagga ","html":"<li>{}</li></ul>"},"tha-ap142:0.3":{"pli":"10 Piyālaphaladāyakattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap142:1.1":{"pli":"“Pārāvato tadā āsiṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap142:1.2":{"pli":"paraṃ anuparodhako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:1.3":{"pli":"Pabbhāre seyyaṃ kappemi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:1.4":{"pli":"avidūre sikhisatthuno. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap142:2.1":{"pli":"Sāyaṃ pātañca passāmi, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap142:2.2":{"pli":"buddhaṃ lokagganāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:2.3":{"pli":"Deyyadhammo ca me natthi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:2.4":{"pli":"dvipadindassa tādino. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap142:3.1":{"pli":"Piyālaphalamādāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap142:3.2":{"pli":"agamaṃ buddhasantikaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:3.3":{"pli":"Paṭiggahesi bhagavā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:3.4":{"pli":"lokajeṭṭho narāsabho. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap142:4.1":{"pli":"Tato paraṃ upādāya, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap142:4.2":{"pli":"paricāriṃ vināyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:4.3":{"pli":"Tena cittappasādena, ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:4.4":{"pli":"tattha kālaṅkato ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap142:5.1":{"pli":"Ekattiṃse ito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap142:5.2":{"pli":"yaṃ phalaṃ adadiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:5.3":{"pli":"Duggatiṃ nābhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:5.4":{"pli":"phaladānassidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap142:6.1":{"pli":"Ito pannarase kappe, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap142:6.2":{"pli":"tayo āsuṃ piyālino; ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:6.3":{"pli":"Sattaratanasampannā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:6.4":{"pli":"cakkavattī mahabbalā. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap142:7.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap142:7.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:7.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap142:8.1":{"pli":"Itthaṃ sudaṃ āyasmā piyālaphaladāyako thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap142:9.1":{"pli":"Piyālaphaladāyakattherassāpadānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"tha-ap142:10.1":{"pli":"Sobhitavaggo cuddasamo. ","html":"<p class='endsection'>{}</p>"},"tha-ap142:11.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"tha-ap142:12.1":{"pli":"Sobhitasudassano ca, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"tha-ap142:12.2":{"pli":"candano pupphachadano; ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:12.3":{"pli":"Raho campakapupphī ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:12.4":{"pli":"atthasandassakena ca. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap142:13.1":{"pli":"Ekapasādī sāladado, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap142:13.2":{"pli":"dasamo phaladāyako; ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:13.3":{"pli":"Gāthāyo sattati dve ca, ","html":"<span class='verse-line'>{}</span>"},"tha-ap142:13.4":{"pli":"gaṇitāyo vibhāvibhi. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}},"tha-ap143":{"tha-ap143:0.1":{"pli":"Therāpadāna ","html":"<article id='tha-ap143'><header><ul><li class='division'>{}</li>"},"tha-ap143:0.2":{"pli":"Chattavagga ","html":"<li>{}</li></ul>"},"tha-ap143:0.3":{"pli":"1 Atichattiyattheraapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"tha-ap143:1.1":{"pli":"“Parinibbute bhagavati, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"tha-ap143:1.2":{"pli":"atthadassīnaruttame; ","html":"<span class='verse-line'>{}</span>"},"tha-ap143:1.3":{"pli":"Chattātichattaṃ kāretvā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap143:1.4":{"pli":"thūpamhi abhiropayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap143:2.1":{"pli":"Kālena kālamāgantvā, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap143:2.2":{"pli":"namassiṃ lokanāyakaṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap143:2.3":{"pli":"Pupphacchadanaṃ katvāna, ","html":"<span class='verse-line'>{}</span>"},"tha-ap143:2.4":{"pli":"chattamhi abhiropayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap143:3.1":{"pli":"Sattarase kappasate, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap143:3.2":{"pli":"devarajjamakārayiṃ; ","html":"<span class='verse-line'>{}</span>"},"tha-ap143:3.3":{"pli":"Manussattaṃ na gacchāmi, ","html":"<span class='verse-line'>{}</span>"},"tha-ap143:3.4":{"pli":"thūpapūjāyidaṃ phalaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"tha-ap143:4.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"tha-ap143:4.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"tha-ap143:4.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"tha-ap143:4.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"tha-ap143:5.1":{"pli":"Itthaṃ sudaṃ āyasmā atichattiyo thero imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"tha-ap143:6.1":{"pli":"Atichattiyattherassāpadānaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}