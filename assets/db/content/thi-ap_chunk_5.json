{"thi-ap20":{"thi-ap20:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap20'><header><ul><li class='division'>{}</li>"},"thi-ap20:0.2":{"pli":"Ekūposathikavagga ","html":"<li>{}</li></ul>"},"thi-ap20:0.3":{"pli":"Paṭācārātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap20:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap20:1.2":{"pli":"sabbadhammāna pāragū; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:1.3":{"pli":"Ito satasahassamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:1.4":{"pli":"kappe uppajji nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:2.1":{"pli":"Tadāhaṃ haṃsavatiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:2.2":{"pli":"jātā seṭṭhikule ahuṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:2.3":{"pli":"Nānāratanapajjote, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:2.4":{"pli":"mahāsukhasamappitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:3.1":{"pli":"Upetvā taṃ mahāvīraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:3.2":{"pli":"assosiṃ dhammadesanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:3.3":{"pli":"Tato jātapasādāhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:3.4":{"pli":"upesiṃ saraṇaṃ jinaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:4.1":{"pli":"Tato vinayadhārīnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:4.2":{"pli":"aggaṃ vaṇṇesi nāyako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:4.3":{"pli":"Bhikkhuniṃ lajjiniṃ tādiṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:4.4":{"pli":"kappākappavisāradaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:5.1":{"pli":"Tadā muditacittāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:5.2":{"pli":"taṃ ṭhānamabhikaṅkhinī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:5.3":{"pli":"Nimantetvā dasabalaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:5.4":{"pli":"sasaṅghaṃ lokanāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:6.1":{"pli":"Bhojayitvāna sattāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:6.2":{"pli":"daditvāva ticīvaraṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:6.3":{"pli":"Nipacca sirasā pāde, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:6.4":{"pli":"idaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:7.1":{"pli":"‘Yā tayā vaṇṇitā vīra, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:7.2":{"pli":"ito aṭṭhamake muni; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:7.3":{"pli":"Tādisāhaṃ bhavissāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:7.4":{"pli":"yadi sijjhati nāyaka’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:8.1":{"pli":"Tadā avoca maṃ satthā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:8.2":{"pli":"‘bhadde mā bhāyi assasa; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:8.3":{"pli":"Anāgatamhi addhāne, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:8.4":{"pli":"lacchase taṃ manorathaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:9.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:9.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:9.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:9.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:10.1":{"pli":"Tassa dhammesu dāyādā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:10.2":{"pli":"orasā dhammanimmitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:10.3":{"pli":"Paṭācārāti nāmena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:10.4":{"pli":"hessati satthu sāvikā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:11.1":{"pli":"Tadāhaṃ muditā hutvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:11.2":{"pli":"yāvajīvaṃ tadā jinaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:11.3":{"pli":"Mettacittā paricariṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:11.4":{"pli":"sasaṅghaṃ lokanāyakaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:12.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:12.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:12.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:12.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:13.1":{"pli":"Imamhi bhaddake kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:13.2":{"pli":"brahmabandhu mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:13.3":{"pli":"Kassapo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:13.4":{"pli":"uppajji vadataṃ varo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:14.1":{"pli":"Upaṭṭhāko mahesissa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:14.2":{"pli":"tadā āsi narissaro; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:14.3":{"pli":"Kāsirājā kikī nāma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:14.4":{"pli":"bārāṇasipuruttame. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:15.1":{"pli":"Tassāsiṃ tatiyā dhītā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:15.2":{"pli":"bhikkhunī iti vissutā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:15.3":{"pli":"Dhammaṃ sutvā jinaggassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:15.4":{"pli":"pabbajjaṃ samarocayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:16.1":{"pli":"Anujāni na no tāto, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:16.2":{"pli":"agāreva tadā mayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:16.3":{"pli":"Vīsavassasahassāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:16.4":{"pli":"vicarimha atanditā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:17.1":{"pli":"Komāribrahmacariyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:17.2":{"pli":"rājakaññā sukhedhitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:17.3":{"pli":"Buddhopaṭṭhānaniratā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:17.4":{"pli":"muditā sattadhītaro. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:18.1":{"pli":"Samaṇī samaṇaguttā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:18.2":{"pli":"bhikkhunī bhikkhudāyikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:18.3":{"pli":"Dhammā ceva sudhammā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:18.4":{"pli":"sattamī saṅghadāyikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:19.1":{"pli":"Ahaṃ uppalavaṇṇā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:19.2":{"pli":"khemā bhaddā ca bhikkhunī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:19.3":{"pli":"Kisāgotamī dhammadinnā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:19.4":{"pli":"visākhā hoti sattamī. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:20.1":{"pli":"Tehi kammehi sukatehi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:20.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:20.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:20.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:21.1":{"pli":"Pacchime ca bhave dāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:21.2":{"pli":"jātā seṭṭhikule ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:21.3":{"pli":"Sāvatthiyaṃ puravare, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:21.4":{"pli":"iddhe phīte mahaddhane. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:22.1":{"pli":"Yadā ca yobbanūpetā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:22.2":{"pli":"vitakkavasagā ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:22.3":{"pli":"Naraṃ jārapatiṃ disvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:22.4":{"pli":"tena saddhiṃ agacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:23.1":{"pli":"Ekaputtapasūtāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:23.2":{"pli":"dutiyo kucchiyā mamaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:23.3":{"pli":"Tadāhaṃ mātāpitaro, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:23.4":{"pli":"okkhāmīti sunicchitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:24.1":{"pli":"Nārocesiṃ patiṃ mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:24.2":{"pli":"tadā tamhi pavāsite; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:24.3":{"pli":"Ekikā niggatā gehā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:24.4":{"pli":"gantuṃ sāvatthimuttamaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:25.1":{"pli":"Tato me sāmi āgantvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:25.2":{"pli":"sambhāvesi pathe mamaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:25.3":{"pli":"Tadā me kammajā vātā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:25.4":{"pli":"uppannā atidāruṇā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:26.1":{"pli":"Uṭṭhito ca mahāmegho, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:26.2":{"pli":"pasūtisamaye mama; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:26.3":{"pli":"Dabbatthāya tadā gantvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:26.4":{"pli":"sāmi sappena mārito. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:27.1":{"pli":"Tadā vijātadukkhena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:27.2":{"pli":"anāthā kapaṇā ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:27.3":{"pli":"Kunnadiṃ pūritaṃ disvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:27.4":{"pli":"gacchantī sakulālayaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:28.1":{"pli":"Bālaṃ ādāya atariṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:28.2":{"pli":"pārakūle ca ekakaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:28.3":{"pli":"Sāyetvā bālakaṃ puttaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:28.4":{"pli":"itaraṃ taraṇāyahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:29.1":{"pli":"Nivattā ukkuso hāsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:29.2":{"pli":"taruṇaṃ vilapantakaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:29.3":{"pli":"Itarañca vahī soto, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:29.4":{"pli":"sāhaṃ sokasamappitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:30.1":{"pli":"Sāvatthinagaraṃ gantvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:30.2":{"pli":"assosiṃ sajane mate; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:30.3":{"pli":"Tadā avocaṃ sokaṭṭā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:30.4":{"pli":"mahāsokasamappitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:31.1":{"pli":"‘Ubho puttā kālaṅkatā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:31.2":{"pli":"panthe mayhaṃ patī mato; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:31.3":{"pli":"Mātā pitā ca bhātā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:31.4":{"pli":"ekacitamhi ḍayhare’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:32.1":{"pli":"Tadā kisā ca paṇḍu ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:32.2":{"pli":"anāthā dīnamānasā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:32.3":{"pli":"Ito tato bhamantīhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:32.4":{"pli":"addasaṃ narasārathiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:33.1":{"pli":"Tato avoca maṃ satthā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:33.2":{"pli":"‘putte mā soci assasa; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:33.3":{"pli":"Attānaṃ te gavesassu, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:33.4":{"pli":"kiṃ niratthaṃ vihaññasi. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:34.1":{"pli":"Na santi puttā tāṇāya, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:34.2":{"pli":"na ñātī napi bandhavā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:34.3":{"pli":"Antakenādhipannassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:34.4":{"pli":"natthi ñātīsu tāṇatā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:35.1":{"pli":"Taṃ sutvā munino vākyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:35.2":{"pli":"paṭhamaṃ phalamajjhagaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:35.3":{"pli":"Pabbajitvāna naciraṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:35.4":{"pli":"arahattamapāpuṇiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:36.1":{"pli":"Iddhīsu ca vasī homi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:36.2":{"pli":"dibbāya sotadhātuyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:36.3":{"pli":"Paracittāni jānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:36.4":{"pli":"satthusāsanakārikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:37.1":{"pli":"Pubbenivāsaṃ jānāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:37.2":{"pli":"dibbacakkhu visodhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:37.3":{"pli":"Khepetvā āsave sabbe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:37.4":{"pli":"visuddhāsiṃ sunimmalā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:38.1":{"pli":"Tatohaṃ vinayaṃ sabbaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:38.2":{"pli":"santike sabbadassino; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:38.3":{"pli":"Uggahiṃ sabbavitthāraṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:38.4":{"pli":"byāhariñca yathātathaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:39.1":{"pli":"Jino tasmiṃ guṇe tuṭṭho, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:39.2":{"pli":"etadagge ṭhapesi maṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:39.3":{"pli":"‘Aggā vinayadhārīnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:39.4":{"pli":"paṭācārāva ekikā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:40.1":{"pli":"Pariciṇṇo mayā satthā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:40.2":{"pli":"kataṃ buddhassa sāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:40.3":{"pli":"Ohito garuko bhāro, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:40.4":{"pli":"bhavanetti samūhatā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:41.1":{"pli":"Yassatthāya pabbajitā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:41.2":{"pli":"agārasmānagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:41.3":{"pli":"So me attho anuppatto, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:41.4":{"pli":"sabbasaṃyojanakkhayo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:42.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:42.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:42.3":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:43.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:43.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:43.3":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:44.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:44.2":{"pli":"…pe… ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:44.3":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap20:45.1":{"pli":"Itthaṃ sudaṃ paṭācārā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap20:46.1":{"pli":"Paṭācārātheriyāpadānaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"thi-ap20:47.1":{"pli":"Ekūposathikavaggo dutiyo. ","html":"<p class='endsection'>{}</p>"},"thi-ap20:48.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"thi-ap20:49.1":{"pli":"Ekūposathikā ceva, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"thi-ap20:49.2":{"pli":"saḷalā cātha modakā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:49.3":{"pli":"Ekāsanā pañcadīpā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:49.4":{"pli":"naḷamālī ca gotamī. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap20:50.1":{"pli":"Khemā uppalavaṇṇā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap20:50.2":{"pli":"paṭācārā ca bhikkhunī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:50.3":{"pli":"Gāthā satāni pañceva, ","html":"<span class='verse-line'>{}</span>"},"thi-ap20:50.4":{"pli":"nava cāpi taduttari. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}},"thi-ap21":{"thi-ap21:0.1":{"pli":"Therīapadāna ","html":"<article id='thi-ap21'><header><ul><li class='division'>{}</li>"},"thi-ap21:0.2":{"pli":"Kuṇḍalakesīvagga ","html":"<li>{}</li></ul>"},"thi-ap21:0.3":{"pli":"1 Kuṇḍalakesātherīapadāna ","html":"<h1 class='sutta-title'>{}</h1></header>"},"thi-ap21:1.1":{"pli":"“Padumuttaro nāma jino, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"thi-ap21:1.2":{"pli":"sabbadhammāna pāragū; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:1.3":{"pli":"Ito satasahassamhi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:1.4":{"pli":"kappe uppajji nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:2.1":{"pli":"Tadāhaṃ haṃsavatiyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:2.2":{"pli":"jātā seṭṭhikule ahuṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:2.3":{"pli":"Nānāratanapajjote, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:2.4":{"pli":"mahāsukhasamappitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:3.1":{"pli":"Upetvā taṃ mahāvīraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:3.2":{"pli":"assosiṃ dhammadesanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:3.3":{"pli":"Tato jātappasādāhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:3.4":{"pli":"upesiṃ saraṇaṃ jinaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:4.1":{"pli":"Tadā mahākāruṇiko, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:4.2":{"pli":"padumuttaranāmako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:4.3":{"pli":"Khippābhiññānamagganti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:4.4":{"pli":"ṭhapesi bhikkhuniṃ subhaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:5.1":{"pli":"Taṃ sutvā muditā hutvā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:5.2":{"pli":"dānaṃ datvā mahesino; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:5.3":{"pli":"Nipacca sirasā pāde, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:5.4":{"pli":"taṃ ṭhānamabhipatthayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:6.1":{"pli":"Anumodi mahāvīro, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:6.2":{"pli":"‘bhadde yaṃ tebhipatthitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:6.3":{"pli":"Samijjhissati taṃ sabbaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:6.4":{"pli":"sukhinī hohi nibbutā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:7.1":{"pli":"Satasahassito kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:7.2":{"pli":"okkākakulasambhavo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:7.3":{"pli":"Gotamo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:7.4":{"pli":"satthā loke bhavissati. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:8.1":{"pli":"Tassa dhammesu dāyādā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:8.2":{"pli":"orasā dhammanimmitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:8.3":{"pli":"Bhaddākuṇḍalakesāti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:8.4":{"pli":"hessati satthu sāvikā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:9.1":{"pli":"Tena kammena sukatena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:9.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:9.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:9.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:10.1":{"pli":"Tato cutā yāmamagaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:10.2":{"pli":"tatohaṃ tusitaṃ gatā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:10.3":{"pli":"Tato ca nimmānaratiṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:10.4":{"pli":"vasavattipuraṃ tato. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:11.1":{"pli":"Yattha yatthūpapajjāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:11.2":{"pli":"tassa kammassa vāhasā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:11.3":{"pli":"Tattha tattheva rājūnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:11.4":{"pli":"mahesittamakārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:12.1":{"pli":"Tato cutā manussesu, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:12.2":{"pli":"rājūnaṃ cakkavattinaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:12.3":{"pli":"Maṇḍalīnañca rājūnaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:12.4":{"pli":"mahesittamakārayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:13.1":{"pli":"Sampattiṃ anubhotvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:13.2":{"pli":"devesu mānusesu ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:13.3":{"pli":"Sabbattha sukhitā hutvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:13.4":{"pli":"nekakappesu saṃsariṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:14.1":{"pli":"Imamhi bhaddake kappe, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:14.2":{"pli":"brahmabandhu mahāyaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:14.3":{"pli":"Kassapo nāma gottena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:14.4":{"pli":"uppajji vadataṃ varo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:15.1":{"pli":"Upaṭṭhāko mahesissa, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:15.2":{"pli":"tadā āsi narissaro; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:15.3":{"pli":"Kāsirājā kikī nāma, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:15.4":{"pli":"bārāṇasipuruttame. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:16.1":{"pli":"Tassa dhītā catutthāsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:16.2":{"pli":"bhikkhudāyīti vissutā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:16.3":{"pli":"Dhammaṃ sutvā jinaggassa, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:16.4":{"pli":"pabbajjaṃ samarocayiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:17.1":{"pli":"Anujāni na no tāto, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:17.2":{"pli":"agāreva tadā mayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:17.3":{"pli":"Vīsavassasahassāni, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:17.4":{"pli":"vicarimha atanditā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:18.1":{"pli":"Komāribrahmacariyaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:18.2":{"pli":"rājakaññā sukhedhitā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:18.3":{"pli":"Buddhopaṭṭhānaniratā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:18.4":{"pli":"muditā satta dhītaro. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:19.1":{"pli":"Samaṇī samaṇaguttā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:19.2":{"pli":"bhikkhunī bhikkhudāyikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:19.3":{"pli":"Dhammā ceva sudhammā ca, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:19.4":{"pli":"sattamī saṅghadāyikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:20.1":{"pli":"Khemā uppalavaṇṇā ca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:20.2":{"pli":"paṭācārā ahaṃ tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:20.3":{"pli":"Kisāgotamī dhammadinnā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:20.4":{"pli":"visākhā hoti sattamī. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:21.1":{"pli":"Tehi kammehi sukatehi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:21.2":{"pli":"cetanāpaṇidhīhi ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:21.3":{"pli":"Jahitvā mānusaṃ dehaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:21.4":{"pli":"tāvatiṃsamagacchahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:22.1":{"pli":"Pacchime ca bhave dāni, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:22.2":{"pli":"giribbajapuruttame; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:22.3":{"pli":"Jātā seṭṭhikule phīte, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:22.4":{"pli":"yadāhaṃ yobbane ṭhitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:23.1":{"pli":"Coraṃ vadhatthaṃ nīyantaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:23.2":{"pli":"disvā rattā tahiṃ ahaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:23.3":{"pli":"Pitā me taṃ sahassena, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:23.4":{"pli":"mocayitvā vadhā tato. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:24.1":{"pli":"Adāsi tassa maṃ tāto, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:24.2":{"pli":"viditvāna manaṃ mama; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:24.3":{"pli":"Tassāhamāsiṃ visaṭṭhā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:24.4":{"pli":"atīva dayitā hitā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:25.1":{"pli":"So me bhūsanalobhena, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:25.2":{"pli":"balimajjhāsayo diso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:25.3":{"pli":"Corappapātaṃ netvāna, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:25.4":{"pli":"pabbataṃ cetayī vadhaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:26.1":{"pli":"Tadāhaṃ paṇamitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:26.2":{"pli":"sattukaṃ sukatañjalī; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:26.3":{"pli":"Rakkhantī attano pāṇaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:26.4":{"pli":"idaṃ vacanamabraviṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:27.1":{"pli":"‘Idaṃ suvaṇṇakeyūraṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:27.2":{"pli":"muttā veḷuriyā bahū; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:27.3":{"pli":"Sabbaṃ harassu bhaddante, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:27.4":{"pli":"mañca dāsīti sāvaya’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:28.1":{"pli":"‘Oropayassu kalyāṇī, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:28.2":{"pli":"mā bāḷhaṃ paridevasi; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:28.3":{"pli":"Na cāhaṃ abhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:28.4":{"pli":"ahantvā dhanamābhataṃ’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:29.1":{"pli":"‘Yato sarāmi attānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:29.2":{"pli":"yato pattosmi viññutaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:29.3":{"pli":"Na cāhaṃ abhijānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:29.4":{"pli":"aññaṃ piyataraṃ tayā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:30.1":{"pli":"Ehi taṃ upagūhissaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:30.2":{"pli":"katvāna taṃ padakkhiṇaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:30.3":{"pli":"Na ca dāni puno atthi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:30.4":{"pli":"mama tuyhañca saṅgamo’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:31.1":{"pli":"‘Na hi sabbesu ṭhānesu, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:31.2":{"pli":"puriso hoti paṇḍito; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:31.3":{"pli":"Itthīpi paṇḍitā hoti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:31.4":{"pli":"tattha tattha vicakkhaṇā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:32.1":{"pli":"Na hi sabbesu ṭhānesu, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:32.2":{"pli":"puriso hoti paṇḍito; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:32.3":{"pli":"Itthīpi paṇḍitā hoti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:32.4":{"pli":"lahuṃ atthavicintikā’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:33.1":{"pli":"Lahuñca vata khippañca, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:33.2":{"pli":"nikaṭṭhe samacetayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:33.3":{"pli":"Migaṃ uṇṇā yathā evaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:33.4":{"pli":"tadāhaṃ sattukaṃ vadhiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:34.1":{"pli":"Yo ca uppatitaṃ atthaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:34.2":{"pli":"na khippamanubujjhati; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:34.3":{"pli":"So haññate mandamati, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:34.4":{"pli":"corova girigabbhare. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:35.1":{"pli":"Yo ca uppatitaṃ atthaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:35.2":{"pli":"khippameva nibodhati; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:35.3":{"pli":"Muccate sattusambādhā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:35.4":{"pli":"tadāhaṃ sattukā yathā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:36.1":{"pli":"Tadāhaṃ pātayitvāna, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:36.2":{"pli":"giriduggamhi sattukaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:36.3":{"pli":"Santikaṃ setavatthānaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:36.4":{"pli":"upetvā pabbajiṃ ahaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:37.1":{"pli":"Saṇḍāsena ca kese me, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:37.2":{"pli":"luñcitvā sabbaso tadā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:37.3":{"pli":"Pabbajitvāna samayaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:37.4":{"pli":"ācikkhiṃsu nirantaraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:38.1":{"pli":"Tato taṃ uggahetvāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:38.2":{"pli":"nisīditvāna ekikā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:38.3":{"pli":"Samayaṃ taṃ vicintesiṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:38.4":{"pli":"suvāno mānusaṃ karaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:39.1":{"pli":"Chinnaṃ gayha samīpe me, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:39.2":{"pli":"pātayitvā apakkami; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:39.3":{"pli":"Disvā nimittamalabhiṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:39.4":{"pli":"hatthaṃ taṃ puḷavākulaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:40.1":{"pli":"Tato uṭṭhāya saṃviggā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:40.2":{"pli":"apucchiṃ sahadhammike; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:40.3":{"pli":"Te avocuṃ ‘vijānanti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:40.4":{"pli":"taṃ atthaṃ sakyabhikkhavo’. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:41.1":{"pli":"Sāhaṃ tamatthaṃ pucchissaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:41.2":{"pli":"upetvā buddhasāvake; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:41.3":{"pli":"Te mamādāya gacchiṃsu, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:41.4":{"pli":"buddhaseṭṭhassa santikaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:42.1":{"pli":"So me dhammamadesesi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:42.2":{"pli":"khandhāyatanadhātuyo; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:42.3":{"pli":"Asubhāniccadukkhāti, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:42.4":{"pli":"anattāti ca nāyako. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:43.1":{"pli":"Tassa dhammaṃ suṇitvāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:43.2":{"pli":"dhammacakkhuṃ visodhayiṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:43.3":{"pli":"Tato viññātasaddhammā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:43.4":{"pli":"pabbajjaṃ upasampadaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:44.1":{"pli":"Āyācito tadā āha, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:44.2":{"pli":"‘ehi bhadde’ti nāyako; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:44.3":{"pli":"Tadāhaṃ upasampannā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:44.4":{"pli":"parittaṃ toyamaddasaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:45.1":{"pli":"Pādapakkhālanenāhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:45.2":{"pli":"ñatvā saudayabbayaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:45.3":{"pli":"Tathā sabbepi saṅkhāre, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:45.4":{"pli":"īdisaṃ cintayiṃ tadā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:46.1":{"pli":"Tato cittaṃ vimucci me, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:46.2":{"pli":"anupādāya sabbaso; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:46.3":{"pli":"Khippābhiññānamaggaṃ me, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:46.4":{"pli":"tadā paññāpayī jino. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:47.1":{"pli":"Iddhīsu ca vasī homi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:47.2":{"pli":"dibbāya sotadhātuyā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:47.3":{"pli":"Paracittāni jānāmi, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:47.4":{"pli":"satthusāsanakārikā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:48.1":{"pli":"Pubbenivāsaṃ jānāmi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:48.2":{"pli":"dibbacakkhu visodhitaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:48.3":{"pli":"Khepetvā āsave sabbe, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:48.4":{"pli":"visuddhāsiṃ sunimmalā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:49.1":{"pli":"Pariciṇṇo mayā satthā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:49.2":{"pli":"kataṃ buddhassa sāsanaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:49.3":{"pli":"Ohito garuko bhāro, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:49.4":{"pli":"bhavanetti samūhatā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:50.1":{"pli":"Yassatthāya pabbajitā, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:50.2":{"pli":"agārasmānagāriyaṃ; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:50.3":{"pli":"So me attho anuppatto, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:50.4":{"pli":"sabbasaṃyojanakkhayo. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:51.1":{"pli":"Atthadhammaniruttīsu, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:51.2":{"pli":"paṭibhāne tatheva ca; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:51.3":{"pli":"Ñāṇaṃ me vimalaṃ suddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:51.4":{"pli":"buddhaseṭṭhassa sāsane. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:52.1":{"pli":"Kilesā jhāpitā mayhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:52.2":{"pli":"bhavā sabbe samūhatā; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:52.3":{"pli":"Nāgīva bandhanaṃ chetvā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:52.4":{"pli":"viharāmi anāsavā. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:53.1":{"pli":"Svāgataṃ vata me āsi, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:53.2":{"pli":"mama buddhassa santike; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:53.3":{"pli":"Tisso vijjā anuppattā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:53.4":{"pli":"kataṃ buddhassa sāsanaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"thi-ap21:54.1":{"pli":"Paṭisambhidā catasso, ","html":"<p><span class='verse-line'>{}</span>"},"thi-ap21:54.2":{"pli":"vimokkhāpi ca aṭṭhime; ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:54.3":{"pli":"Chaḷabhiññā sacchikatā, ","html":"<span class='verse-line'>{}</span>"},"thi-ap21:54.4":{"pli":"kataṃ buddhassa sāsanaṃ”. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"thi-ap21:55.1":{"pli":"Itthaṃ sudaṃ bhaddākuṇḍalakesā bhikkhunī imā gāthāyo abhāsitthāti. ","html":"<p>{}</p>"},"thi-ap21:56.1":{"pli":"Kuṇḍalakesātheriyāpadānaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}}}