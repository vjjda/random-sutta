{"ja394":{"ja394:0.1":{"pli":"Jātaka ","html":"<article id='ja394'><header><ul><li class='division'>{}</li>"},"ja394:0.2":{"pli":"Chakkanipāta ","html":"<li>{}</li>"},"ja394:0.3":{"pli":"Kharaputtavagga ","html":"<li>{}</li></ul>"},"ja394:0.4":{"pli":"9. Vaṭṭakajātaka ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja394:1.1":{"pli":"“Paṇītaṃ bhuñjase bhattaṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"ja394:1.2":{"pli":"sappitelañca mātula; ","html":"<span class='verse-line'>{}</span>"},"ja394:1.3":{"pli":"Atha kena nu vaṇṇena, ","html":"<span class='verse-line'>{}</span>"},"ja394:1.4":{"pli":"kiso tvamasi vāyasa”. ","html":"<span class='verse-line'>{}</span></p>"},"ja394:2.1":{"pli":"“Amittamajjhe vasato, ","html":"<p><span class='verse-line'>{}</span>"},"ja394:2.2":{"pli":"tesu āmisamesato; ","html":"<span class='verse-line'>{}</span>"},"ja394:2.3":{"pli":"Niccaṃ ubbiggahadayassa, ","html":"<span class='verse-line'>{}</span>"},"ja394:2.4":{"pli":"kuto kākassa daḷhiyaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"ja394:3.1":{"pli":"Niccaṃ ubbegino kākā, ","html":"<p><span class='verse-line'>{}</span>"},"ja394:3.2":{"pli":"dhaṅkā pāpena kammunā; ","html":"<span class='verse-line'>{}</span>"},"ja394:3.3":{"pli":"Laddho piṇḍo na pīṇeti, ","html":"<span class='verse-line'>{}</span>"},"ja394:3.4":{"pli":"kiso tenasmi vaṭṭaka. ","html":"<span class='verse-line'>{}</span></p>"},"ja394:4.1":{"pli":"Lūkhāni tiṇabījāni, ","html":"<p><span class='verse-line'>{}</span>"},"ja394:4.2":{"pli":"appasnehāni bhuñjasi; ","html":"<span class='verse-line'>{}</span>"},"ja394:4.3":{"pli":"Atha kena nu vaṇṇena, ","html":"<span class='verse-line'>{}</span>"},"ja394:4.4":{"pli":"thūlo tvamasi vaṭṭaka”. ","html":"<span class='verse-line'>{}</span></p>"},"ja394:5.1":{"pli":"“Appicchā appacintāya, ","html":"<p><span class='verse-line'>{}</span>"},"ja394:5.2":{"pli":"adūragamanena ca; ","html":"<span class='verse-line'>{}</span>"},"ja394:5.3":{"pli":"Laddhāladdhena yāpento, ","html":"<span class='verse-line'>{}</span>"},"ja394:5.4":{"pli":"thūlo tenasmi vāyasa. ","html":"<span class='verse-line'>{}</span></p>"},"ja394:6.1":{"pli":"Appicchassa hi posassa, ","html":"<p><span class='verse-line'>{}</span>"},"ja394:6.2":{"pli":"appacintasukhassa ca; ","html":"<span class='verse-line'>{}</span>"},"ja394:6.3":{"pli":"Susaṅgahitamānassa, ","html":"<span class='verse-line'>{}</span>"},"ja394:6.4":{"pli":"vuttī susamudānayā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja394:7.1":{"pli":"Vaṭṭakajātakaṃ navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"ja395":{"ja395:0.1":{"pli":"Jātaka ","html":"<article id='ja395'><header><ul><li class='division'>{}</li>"},"ja395:0.2":{"pli":"Chakkanipāta ","html":"<li>{}</li>"},"ja395:0.3":{"pli":"Kharaputtavagga ","html":"<li>{}</li></ul>"},"ja395:0.4":{"pli":"10. Pārāvatajātaka ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja395:1.1":{"pli":"“Cirassaṃ vata passāmi, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"ja395:1.2":{"pli":"sahāyaṃ maṇidhārinaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja395:1.3":{"pli":"Sukatā massukuttiyā, ","html":"<span class='verse-line'>{}</span>"},"ja395:1.4":{"pli":"sobhate vata me sakhā”. ","html":"<span class='verse-line'>{}</span></p>"},"ja395:2.1":{"pli":"“Parūḷhakacchanakhalomo, ","html":"<p><span class='verse-line'>{}</span>"},"ja395:2.2":{"pli":"ahaṃ kammesu byāvaṭo; ","html":"<span class='verse-line'>{}</span>"},"ja395:2.3":{"pli":"Cirassaṃ nhāpitaṃ laddhā, ","html":"<span class='verse-line'>{}</span>"},"ja395:2.4":{"pli":"lomaṃ taṃ ajja hārayiṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"ja395:3.1":{"pli":"“Yannu lomaṃ ahāresi, ","html":"<p><span class='verse-line'>{}</span>"},"ja395:3.2":{"pli":"dullabhaṃ laddha kappakaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja395:3.3":{"pli":"Atha kiñcarahi te samma, ","html":"<span class='verse-line'>{}</span>"},"ja395:3.4":{"pli":"kaṇṭhe kiṇikiṇāyati”. ","html":"<span class='verse-line'>{}</span></p>"},"ja395:4.1":{"pli":"“Manussasukhumālānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja395:4.2":{"pli":"maṇi kaṇṭhesu lambati; ","html":"<span class='verse-line'>{}</span>"},"ja395:4.3":{"pli":"Tesāhaṃ anusikkhāmi, ","html":"<span class='verse-line'>{}</span>"},"ja395:4.4":{"pli":"mā tvaṃ maññi davā kataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"ja395:5.1":{"pli":"Sacepimaṃ pihayasi, ","html":"<p><span class='verse-line'>{}</span>"},"ja395:5.2":{"pli":"massukuttiṃ sukāritaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja395:5.3":{"pli":"Kārayissāmi te samma, ","html":"<span class='verse-line'>{}</span>"},"ja395:5.4":{"pli":"maṇiñcāpi dadāmi te”. ","html":"<span class='verse-line'>{}</span></p>"},"ja395:6.1":{"pli":"“Tvaññeva maṇinā channo, ","html":"<p><span class='verse-line'>{}</span>"},"ja395:6.2":{"pli":"Sukatāya ca massuyā; ","html":"<span class='verse-line'>{}</span>"},"ja395:6.3":{"pli":"Āmanta kho taṃ gacchāmi, ","html":"<span class='verse-line'>{}</span>"},"ja395:6.4":{"pli":"Piyaṃ me tavadassanan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja395:7.1":{"pli":"Pārāvatajātakaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"ja395:7.2":{"pli":"Kharaputtavaggo dutiyo. ","html":"<p class='endvagga'>{}</p>"},"ja395:8.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"ja395:9.1":{"pli":"Atha passa sasūci ca tuṇḍilako, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"ja395:9.2":{"pli":"Miga mayhakapañcamapakkhivaro; ","html":"<span class='verse-line'>{}</span>"},"ja395:9.3":{"pli":"Atha pañjali vārija mejjha puna, ","html":"<span class='verse-line'>{}</span>"},"ja395:9.4":{"pli":"Atha vaṭṭa kapotavarena dasāti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja395:10.1":{"pli":"Atha vagguddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"ja395:11.1":{"pli":"Atha vaggaṃ pakittissaṃ, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"ja395:11.2":{"pli":"chanipātaṃ varuttame; ","html":"<span class='verse-line'>{}</span>"},"ja395:11.3":{"pli":"Avāriyā ca kharo ca, ","html":"<span class='verse-line'>{}</span>"},"ja395:11.4":{"pli":"dve ca vuttā subyañjanāti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja395:12.1":{"pli":"Chakkanipātaṃ niṭṭhitaṃ. ","html":"<p class='endsection'>{}</p></article>"}},"ja396":{"ja396:0.1":{"pli":"Jātaka ","html":"<article id='ja396'><header><ul><li class='division'>{}</li>"},"ja396:0.2":{"pli":"Sattakanipāta ","html":"<li>{}</li>"},"ja396:0.3":{"pli":"Kukkuvagga ","html":"<li>{}</li></ul>"},"ja396:0.4":{"pli":"1. Kukkujātaka ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja396:1.1":{"pli":"“Diyaḍḍhakukkū udayena kaṇṇikā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"ja396:1.2":{"pli":"Vidatthiyo aṭṭha parikkhipanti naṃ; ","html":"<span class='verse-line'>{}</span>"},"ja396:1.3":{"pli":"Sā siṃsapā sāramayā apheggukā, ","html":"<span class='verse-line'>{}</span>"},"ja396:1.4":{"pli":"Kuhiṃ ṭhitā upparito na dhaṃsati”. ","html":"<span class='verse-line'>{}</span></p>"},"ja396:2.1":{"pli":"“Yā tiṃsati sāramayā anujjukā, ","html":"<p><span class='verse-line'>{}</span>"},"ja396:2.2":{"pli":"Parikiriya gopānasiyo samaṃ ṭhitā; ","html":"<span class='verse-line'>{}</span>"},"ja396:2.3":{"pli":"Tāhi susaṅgahitā balasā pīḷitā, ","html":"<span class='verse-line'>{}</span>"},"ja396:2.4":{"pli":"Samaṃ ṭhitā upparito na dhaṃsati. ","html":"<span class='verse-line'>{}</span></p>"},"ja396:3.1":{"pli":"Evampi mittehi daḷhehi paṇḍito, ","html":"<p><span class='verse-line'>{}</span>"},"ja396:3.2":{"pli":"Abhejjarūpehi sucīhi mantibhi; ","html":"<span class='verse-line'>{}</span>"},"ja396:3.3":{"pli":"Susaṅgahīto siriyā na dhaṃsati, ","html":"<span class='verse-line'>{}</span>"},"ja396:3.4":{"pli":"Gopānasī bhāravahāva kaṇṇikā. ","html":"<span class='verse-line'>{}</span></p>"},"ja396:4.1":{"pli":"Kharattacaṃ bellaṃ yathāpi satthavā, ","html":"<p><span class='verse-line'>{}</span>"},"ja396:4.2":{"pli":"Anāmasantopi karoti tittakaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja396:4.3":{"pli":"Samāharaṃ sāduṃ karoti patthiva, ","html":"<span class='verse-line'>{}</span>"},"ja396:4.4":{"pli":"Asāduṃ kayirā tanubandhamuddharaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"ja396:5.1":{"pli":"Evampi gāmanigamesu paṇḍito, ","html":"<p><span class='verse-line'>{}</span>"},"ja396:5.2":{"pli":"Asāhasaṃ rājadhanāni saṅgharaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja396:5.3":{"pli":"Dhammānuvattī paṭipajjamāno, ","html":"<span class='verse-line'>{}</span>"},"ja396:5.4":{"pli":"Sa phāti kayirā aviheṭhayaṃ paraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"ja396:6.1":{"pli":"Odātamūlaṃ sucivārisambhavaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja396:6.2":{"pli":"Jātaṃ yathā pokkharaṇīsu ambujaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja396:6.3":{"pli":"Padumaṃ yathā agginikāsiphālimaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja396:6.4":{"pli":"Na kaddamo na rajo na vāri limpati. ","html":"<span class='verse-line'>{}</span></p>"},"ja396:7.1":{"pli":"Evampi vohārasuciṃ asāhasaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja396:7.2":{"pli":"Visuddhakammantamapetapāpakaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja396:7.3":{"pli":"Na limpati kammakilesa tādiso, ","html":"<span class='verse-line'>{}</span>"},"ja396:7.4":{"pli":"Jātaṃ yathā pokkharaṇīsu ambujan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja396:8.1":{"pli":"Kukkujātakaṃ paṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"ja397":{"ja397:0.1":{"pli":"Jātaka ","html":"<article id='ja397'><header><ul><li class='division'>{}</li>"},"ja397:0.2":{"pli":"Sattakanipāta ","html":"<li>{}</li>"},"ja397:0.3":{"pli":"Kukkuvagga ","html":"<li>{}</li></ul>"},"ja397:0.4":{"pli":"2. Manojajātaka ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja397:1.1":{"pli":"“Yathā cāpo ninnamati, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"ja397:1.2":{"pli":"jiyā cāpi nikūjati; ","html":"<span class='verse-line'>{}</span>"},"ja397:1.3":{"pli":"Haññate nūna manojo, ","html":"<span class='verse-line'>{}</span>"},"ja397:1.4":{"pli":"migarājā sakhā mama. ","html":"<span class='verse-line'>{}</span></p>"},"ja397:2.1":{"pli":"Handa dāni vanantāni, ","html":"<p><span class='verse-line'>{}</span>"},"ja397:2.2":{"pli":"pakkamāmi yathāsukhaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja397:2.3":{"pli":"Netādisā sakhā honti, ","html":"<span class='verse-line'>{}</span>"},"ja397:2.4":{"pli":"labbhā me jīvato sakhā”. ","html":"<span class='verse-line'>{}</span></p>"},"ja397:3.1":{"pli":"“Na pāpajanasaṃsevī, ","html":"<p><span class='verse-line'>{}</span>"},"ja397:3.2":{"pli":"accantaṃ sukhamedhati; ","html":"<span class='verse-line'>{}</span>"},"ja397:3.3":{"pli":"Manojaṃ passa semānaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja397:3.4":{"pli":"giriyassānusāsanī”. ","html":"<span class='verse-line'>{}</span></p>"},"ja397:4.1":{"pli":"“Na pāpasampavaṅkena, ","html":"<p><span class='verse-line'>{}</span>"},"ja397:4.2":{"pli":"mātā puttena nandati; ","html":"<span class='verse-line'>{}</span>"},"ja397:4.3":{"pli":"Manojaṃ passa semānaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja397:4.4":{"pli":"acchannaṃ samhi lohite”. ","html":"<span class='verse-line'>{}</span></p>"},"ja397:5.1":{"pli":"“Evamāpajjate poso, ","html":"<p><span class='verse-line'>{}</span>"},"ja397:5.2":{"pli":"pāpiyo ca nigacchati; ","html":"<span class='verse-line'>{}</span>"},"ja397:5.3":{"pli":"Yo ve hitānaṃ vacanaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja397:5.4":{"pli":"na karoti atthadassinaṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"ja397:6.1":{"pli":"“Evañca so hoti tato ca pāpiyo, ","html":"<p><span class='verse-line'>{}</span>"},"ja397:6.2":{"pli":"Yo uttamo adhamajanūpasevī; ","html":"<span class='verse-line'>{}</span>"},"ja397:6.3":{"pli":"Passuttamaṃ adhamajanūpasevitaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja397:6.4":{"pli":"Migādhipaṃ saravaraveganiddhutaṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"ja397:7.1":{"pli":"“Nihīyati puriso nihīnasevī, ","html":"<p><span class='verse-line'>{}</span>"},"ja397:7.2":{"pli":"Na ca hāyetha kadāci tulyasevī; ","html":"<span class='verse-line'>{}</span>"},"ja397:7.3":{"pli":"Seṭṭhamupagamaṃ udeti khippaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja397:7.4":{"pli":"Tasmāttanā uttaritaraṃ bhajethā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja397:8.1":{"pli":"Manojajātakaṃ dutiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"ja398":{"ja398:0.1":{"pli":"Jātaka ","html":"<article id='ja398'><header><ul><li class='division'>{}</li>"},"ja398:0.2":{"pli":"Sattakanipāta ","html":"<li>{}</li>"},"ja398:0.3":{"pli":"Kukkuvagga ","html":"<li>{}</li></ul>"},"ja398:0.4":{"pli":"3. Sutanujātaka ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja398:1.1":{"pli":"“Rājā te bhattaṃ pāhesi, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"ja398:1.2":{"pli":"Suciṃ maṃsūpasecanaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja398:1.3":{"pli":"Maghadevasmiṃ adhivatthe, ","html":"<span class='verse-line'>{}</span>"},"ja398:1.4":{"pli":"Ehi nikkhamma bhuñjasu”. ","html":"<span class='verse-line'>{}</span></p>"},"ja398:2.1":{"pli":"“Ehi māṇava orena, ","html":"<p><span class='verse-line'>{}</span>"},"ja398:2.2":{"pli":"bhikkhamādāya sūpitaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja398:2.3":{"pli":"Tvañca māṇava bhikkhā ca, ","html":"<span class='verse-line'>{}</span>"},"ja398:2.4":{"pli":"ubho bhakkhā bhavissatha”. ","html":"<span class='verse-line'>{}</span></p>"},"ja398:3.1":{"pli":"“Appakena tuvaṃ yakkha, ","html":"<p><span class='verse-line'>{}</span>"},"ja398:3.2":{"pli":"thullamatthaṃ jahissasi; ","html":"<span class='verse-line'>{}</span>"},"ja398:3.3":{"pli":"Bhikkhaṃ te nāharissanti, ","html":"<span class='verse-line'>{}</span>"},"ja398:3.4":{"pli":"janā maraṇasaññino. ","html":"<span class='verse-line'>{}</span></p>"},"ja398:4.1":{"pli":"Laddhāya yakkhā tava niccabhikkhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja398:4.2":{"pli":"Suciṃ paṇītaṃ rasasā upetaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja398:4.3":{"pli":"Bhikkhañca te āhariyo naro idha, ","html":"<span class='verse-line'>{}</span>"},"ja398:4.4":{"pli":"Sudullabho hehiti bhakkhite mayi”. ","html":"<span class='verse-line'>{}</span></p>"},"ja398:5.1":{"pli":"“Mameva sutano attho, ","html":"<p><span class='verse-line'>{}</span>"},"ja398:5.2":{"pli":"yathā bhāsasi māṇava; ","html":"<span class='verse-line'>{}</span>"},"ja398:5.3":{"pli":"Mayā tvaṃ samanuññāto, ","html":"<span class='verse-line'>{}</span>"},"ja398:5.4":{"pli":"sotthiṃ passāhi mātaraṃ. ","html":"<span class='verse-line'>{}</span></p>"},"ja398:6.1":{"pli":"Khaggaṃ chattañca pātiñca, ","html":"<p><span class='verse-line'>{}</span>"},"ja398:6.2":{"pli":"gacchamādāya māṇava; ","html":"<span class='verse-line'>{}</span>"},"ja398:6.3":{"pli":"Sotthiṃ passatu te mātā, ","html":"<span class='verse-line'>{}</span>"},"ja398:6.4":{"pli":"tvañca passāhi mātaraṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"ja398:7.1":{"pli":"“Evaṃ yakkha sukhī hohi, ","html":"<p><span class='verse-line'>{}</span>"},"ja398:7.2":{"pli":"saha sabbehi ñātibhi; ","html":"<span class='verse-line'>{}</span>"},"ja398:7.3":{"pli":"Dhanañca me adhigataṃ, ","html":"<span class='verse-line'>{}</span>"},"ja398:7.4":{"pli":"rañño ca vacanaṃ katan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja398:8.1":{"pli":"Sutanujātakaṃ tatiyaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"ja399":{"ja399:0.1":{"pli":"Jātaka ","html":"<article id='ja399'><header><ul><li class='division'>{}</li>"},"ja399:0.2":{"pli":"Sattakanipāta ","html":"<li>{}</li>"},"ja399:0.3":{"pli":"Kukkuvagga ","html":"<li>{}</li></ul>"},"ja399:0.4":{"pli":"4. Mātuposakagijjhajātaka ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja399:1.1":{"pli":"“Te kathaṃ nu karissanti, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"ja399:1.2":{"pli":"vuddhā giridarīsayā; ","html":"<span class='verse-line'>{}</span>"},"ja399:1.3":{"pli":"Ahaṃ baddhosmi pāsena, ","html":"<span class='verse-line'>{}</span>"},"ja399:1.4":{"pli":"nilīyassa vasaṃ gato”. ","html":"<span class='verse-line'>{}</span></p>"},"ja399:2.1":{"pli":"“Kiṃ gijjha paridevasi, ","html":"<p><span class='verse-line'>{}</span>"},"ja399:2.2":{"pli":"kā nu te paridevanā; ","html":"<span class='verse-line'>{}</span>"},"ja399:2.3":{"pli":"Na me suto vā diṭṭho vā, ","html":"<span class='verse-line'>{}</span>"},"ja399:2.4":{"pli":"bhāsanto mānusiṃ dijo”. ","html":"<span class='verse-line'>{}</span></p>"},"ja399:3.1":{"pli":"“Bharāmi mātāpitaro, ","html":"<p><span class='verse-line'>{}</span>"},"ja399:3.2":{"pli":"vuddhe giridarīsaye; ","html":"<span class='verse-line'>{}</span>"},"ja399:3.3":{"pli":"Te kathaṃ nu karissanti, ","html":"<span class='verse-line'>{}</span>"},"ja399:3.4":{"pli":"ahaṃ vasaṃ gato tava”. ","html":"<span class='verse-line'>{}</span></p>"},"ja399:4.1":{"pli":"“Yannu gijjho yojanasataṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja399:4.2":{"pli":"kuṇapāni avekkhati; ","html":"<span class='verse-line'>{}</span>"},"ja399:4.3":{"pli":"Kasmā jālañca pāsañca, ","html":"<span class='verse-line'>{}</span>"},"ja399:4.4":{"pli":"āsajjāpi na bujjhasi”. ","html":"<span class='verse-line'>{}</span></p>"},"ja399:5.1":{"pli":"“Yadā parābhavo hoti, ","html":"<p><span class='verse-line'>{}</span>"},"ja399:5.2":{"pli":"poso jīvitasaṅkhaye; ","html":"<span class='verse-line'>{}</span>"},"ja399:5.3":{"pli":"Atha jālañca pāsañca, ","html":"<span class='verse-line'>{}</span>"},"ja399:5.4":{"pli":"āsajjāpi na bujjhati”. ","html":"<span class='verse-line'>{}</span></p>"},"ja399:6.1":{"pli":"“Bharassu mātāpitaro, ","html":"<p><span class='verse-line'>{}</span>"},"ja399:6.2":{"pli":"vuddhe giridarīsaye; ","html":"<span class='verse-line'>{}</span>"},"ja399:6.3":{"pli":"Mayā tvaṃ samanuññāto, ","html":"<span class='verse-line'>{}</span>"},"ja399:6.4":{"pli":"sotthiṃ passāhi ñātake”. ","html":"<span class='verse-line'>{}</span></p>"},"ja399:7.1":{"pli":"“Evaṃ luddaka nandassu, ","html":"<p><span class='verse-line'>{}</span>"},"ja399:7.2":{"pli":"saha sabbehi ñātibhi; ","html":"<span class='verse-line'>{}</span>"},"ja399:7.3":{"pli":"Bharissaṃ mātāpitaro, ","html":"<span class='verse-line'>{}</span>"},"ja399:7.4":{"pli":"vuddhe giridarīsaye”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja399:8.1":{"pli":"Mātuposakagijjhajātakaṃ catutthaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"ja4":{"ja4:0.1":{"pli":"Jātaka ","eng":"Stories of Past Lives 4 ","html":"<article id='ja4'><header><ul><li class='division'>{}</li>"},"ja4:0.2":{"pli":"Ekakanipāta ","eng":"The Book of the Ones ","html":"<li>{}</li>"},"ja4:0.3":{"pli":"Apaṇṇakavagga ","eng":"Unfailing ","html":"<li>{}</li></ul>"},"ja4:0.4":{"pli":"4. Cūḷaseṭṭhijātaka ","eng":"The Little Financier: a past life story ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja4:1.1":{"pli":"“Appakenapi medhāvī, ","eng":"Even with a little capital ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>","comm":"The prose story tells of an enterprising young man who started his business by trading a dead mouse for a penny as cat food. This concept can be taken as a precursor for the modern concept of microfinance. "},"ja4:1.2":{"pli":"pābhatena vicakkhaṇo; ","eng":"the intelligent and clear-seeing man ","html":"<span class='verse-line'>{}</span>"},"ja4:1.3":{"pli":"Samuṭṭhāpeti attānaṃ, ","eng":"uplifts himself, ","html":"<span class='verse-line'>{}</span>"},"ja4:1.4":{"pli":"aṇuṃ aggiṃva sandhaman”ti. ","eng":"like a tiny flame when fanned. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja4:2.1":{"pli":"Cūḷaseṭṭhijātakaṃ catutthaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"ja40":{"ja40:0.1":{"pli":"Jātaka ","eng":"Stories of Past Lives 40 ","html":"<article id='ja40'><header><ul><li class='division'>{}</li>"},"ja40:0.2":{"pli":"Ekakanipāta ","eng":"The Book of the Ones ","html":"<li>{}</li>"},"ja40:0.3":{"pli":"Kulāvakavagga ","eng":"Nests ","html":"<li>{}</li></ul>"},"ja40:0.4":{"pli":"10. Khadiraṅgārajātaka ","eng":"The Pit of Acacia Coals: a past life story ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja40:1.1":{"pli":"“Kāmaṃ patāmi nirayaṃ, ","eng":"Fine, let me fall ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>","comm":"Māra, in order to prevent the Bodhisatta from giving alms to a Paccekabuddha, creates an illusion of a vast pit burning with coals of acacia wood. But the Bodhisatta is not intimidated. "},"ja40:1.2":{"pli":"uddhampādo avaṃsiro; ","eng":"head over heels into hell, ","html":"<span class='verse-line'>{}</span>"},"ja40:1.3":{"pli":"Nānariyaṃ karissāmi, ","eng":"but I shall not do what is ignoble. ","html":"<span class='verse-line'>{}</span>"},"ja40:1.4":{"pli":"handa piṇḍaṃ paṭiggahā”ti. ","eng":"Please accept my alms offering. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja40:2.1":{"pli":"Khadiraṅgārajātakaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"ja40:2.2":{"pli":"Kulāvakavaggo catuttho. ","html":"<p class='endvagga'>{}</p>"},"ja40:3.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"ja40:4.1":{"pli":"Sirimātali dhītara pakkhivaro, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"ja40:4.2":{"pli":"Ratiyāgato mātāpitā ca puna; ","html":"<span class='verse-line'>{}</span>"},"ja40:4.3":{"pli":"Jagatīruha vuḍḍha sukakkaṭako, ","html":"<span class='verse-line'>{}</span>"},"ja40:4.4":{"pli":"Tathā nandakapiṇḍavarena dasāti. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}},"ja400":{"ja400:0.1":{"pli":"Jātaka ","html":"<article id='ja400'><header><ul><li class='division'>{}</li>"},"ja400:0.2":{"pli":"Sattakanipāta ","html":"<li>{}</li>"},"ja400:0.3":{"pli":"Kukkuvagga ","html":"<li>{}</li></ul>"},"ja400:0.4":{"pli":"5. Dabbhapupphajātaka ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja400:1.1":{"pli":"“Anutīracārī bhaddante, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"ja400:1.2":{"pli":"sahāyamanudhāva maṃ; ","html":"<span class='verse-line'>{}</span>"},"ja400:1.3":{"pli":"Mahā me gahito maccho, ","html":"<span class='verse-line'>{}</span>"},"ja400:1.4":{"pli":"so maṃ harati vegasā”. ","html":"<span class='verse-line'>{}</span></p>"},"ja400:2.1":{"pli":"“Gambhīracārī bhaddante, ","html":"<p><span class='verse-line'>{}</span>"},"ja400:2.2":{"pli":"daḷhaṃ gaṇhāhi thāmasā; ","html":"<span class='verse-line'>{}</span>"},"ja400:2.3":{"pli":"Ahaṃ taṃ uddharissāmi, ","html":"<span class='verse-line'>{}</span>"},"ja400:2.4":{"pli":"supaṇṇo uragāmiva”. ","html":"<span class='verse-line'>{}</span></p>"},"ja400:3.1":{"pli":"“Vivādo no samuppanno, ","html":"<p><span class='verse-line'>{}</span>"},"ja400:3.2":{"pli":"dabbhapuppha suṇohi me; ","html":"<span class='verse-line'>{}</span>"},"ja400:3.3":{"pli":"Samehi medhagaṃ samma, ","html":"<span class='verse-line'>{}</span>"},"ja400:3.4":{"pli":"vivādo vūpasammataṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"ja400:4.1":{"pli":"“Dhammaṭṭhohaṃ pure āsiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja400:4.2":{"pli":"bahū aḍḍā me tīritā; ","html":"<span class='verse-line'>{}</span>"},"ja400:4.3":{"pli":"Samemi medhagaṃ samma, ","html":"<span class='verse-line'>{}</span>"},"ja400:4.4":{"pli":"vivādo vūpasammataṃ. ","html":"<span class='verse-line'>{}</span></p>"},"ja400:5.1":{"pli":"Anutīracāri naṅguṭṭhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja400:5.2":{"pli":"sīsaṃ gambhīracārino; ","html":"<span class='verse-line'>{}</span>"},"ja400:5.3":{"pli":"Accāyaṃ majjhimo khaṇḍo, ","html":"<span class='verse-line'>{}</span>"},"ja400:5.4":{"pli":"dhammaṭṭhassa bhavissati”. ","html":"<span class='verse-line'>{}</span></p>"},"ja400:6.1":{"pli":"“Cirampi bhakkho abhavissa, ","html":"<p><span class='verse-line'>{}</span>"},"ja400:6.2":{"pli":"sace na vivademase; ","html":"<span class='verse-line'>{}</span>"},"ja400:6.3":{"pli":"Asīsakaṃ anaṅguṭṭhaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja400:6.4":{"pli":"siṅgālo harati rohitaṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"ja400:7.1":{"pli":"“Yathāpi rājā nandeyya, ","html":"<p><span class='verse-line'>{}</span>"},"ja400:7.2":{"pli":"rajjaṃ laddhāna khattiyo; ","html":"<span class='verse-line'>{}</span>"},"ja400:7.3":{"pli":"Evāhamajja nandāmi, ","html":"<span class='verse-line'>{}</span>"},"ja400:7.4":{"pli":"disvā puṇṇamukhaṃ patiṃ. ","html":"<span class='verse-line'>{}</span></p>"},"ja400:8.1":{"pli":"Kathaṃ nu thalajo santo, ","html":"<p><span class='verse-line'>{}</span>"},"ja400:8.2":{"pli":"udake macchaṃ parāmasi; ","html":"<span class='verse-line'>{}</span>"},"ja400:8.3":{"pli":"Puṭṭho me samma akkhāhi, ","html":"<span class='verse-line'>{}</span>"},"ja400:8.4":{"pli":"kathaṃ adhigataṃ tayā”. ","html":"<span class='verse-line'>{}</span></p>"},"ja400:9.1":{"pli":"“Vivādena kisā honti, ","html":"<p><span class='verse-line'>{}</span>"},"ja400:9.2":{"pli":"vivādena dhanakkhayā; ","html":"<span class='verse-line'>{}</span>"},"ja400:9.3":{"pli":"Jīnā uddā vivādena, ","html":"<span class='verse-line'>{}</span>"},"ja400:9.4":{"pli":"bhuñja māyāvi rohitaṃ. ","html":"<span class='verse-line'>{}</span></p>"},"ja400:10.1":{"pli":"Evameva manussesu, ","html":"<p><span class='verse-line'>{}</span>"},"ja400:10.2":{"pli":"vivādo yattha jāyati; ","html":"<span class='verse-line'>{}</span>"},"ja400:10.3":{"pli":"Dhammaṭṭhaṃ paṭidhāvanti, ","html":"<span class='verse-line'>{}</span>"},"ja400:10.4":{"pli":"so hi nesaṃ vināyako; ","html":"<span class='verse-line'>{}</span>"},"ja400:10.5":{"pli":"Dhanāpi tattha jīyanti, ","html":"<span class='verse-line'>{}</span>"},"ja400:10.6":{"pli":"rājakoso pavaḍḍhatī”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja400:11.1":{"pli":"Dabbhapupphajātakaṃ pañcamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"ja401":{"ja401:0.1":{"pli":"Jātaka ","html":"<article id='ja401'><header><ul><li class='division'>{}</li>"},"ja401:0.2":{"pli":"Sattakanipāta ","html":"<li>{}</li>"},"ja401:0.3":{"pli":"Kukkuvagga ","html":"<li>{}</li></ul>"},"ja401:0.4":{"pli":"6. Paṇṇakajātaka ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja401:1.1":{"pli":"“Paṇṇakaṃ tikhiṇadhāraṃ, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"ja401:1.2":{"pli":"Asiṃ sampannapāyinaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja401:1.3":{"pli":"Parisāyaṃ puriso gilati, ","html":"<span class='verse-line'>{}</span>"},"ja401:1.4":{"pli":"Kiṃ dukkarataraṃ tato; ","html":"<span class='verse-line'>{}</span>"},"ja401:1.5":{"pli":"Yadaññaṃ dukkaraṃ ṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja401:1.6":{"pli":"Taṃ me akkhāhi pucchito”. ","html":"<span class='verse-line'>{}</span></p>"},"ja401:2.1":{"pli":"“Gileyya puriso lobhā, ","html":"<p><span class='verse-line'>{}</span>"},"ja401:2.2":{"pli":"asiṃ sampannapāyinaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja401:2.3":{"pli":"Yo ca vajjā dadāmīti, ","html":"<span class='verse-line'>{}</span>"},"ja401:2.4":{"pli":"taṃ dukkarataraṃ tato; ","html":"<span class='verse-line'>{}</span>"},"ja401:2.5":{"pli":"Sabbaññaṃ sukaraṃ ṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja401:2.6":{"pli":"evaṃ jānāhi maddava”. ","html":"<span class='verse-line'>{}</span></p>"},"ja401:3.1":{"pli":"“Byākāsi āyuro pañhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja401:3.2":{"pli":"atthaṃ dhammassa kovido; ","html":"<span class='verse-line'>{}</span>"},"ja401:3.3":{"pli":"Pukkusaṃ dāni pucchāmi, ","html":"<span class='verse-line'>{}</span>"},"ja401:3.4":{"pli":"kiṃ dukkarataraṃ tato; ","html":"<span class='verse-line'>{}</span>"},"ja401:3.5":{"pli":"Yadaññaṃ dukkaraṃ ṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja401:3.6":{"pli":"taṃ me akkhāhi pucchito”. ","html":"<span class='verse-line'>{}</span></p>"},"ja401:4.1":{"pli":"“Na vācamupajīvanti, ","html":"<p><span class='verse-line'>{}</span>"},"ja401:4.2":{"pli":"aphalaṃ giramudīritaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja401:4.3":{"pli":"Yo ca datvā avākayirā, ","html":"<span class='verse-line'>{}</span>"},"ja401:4.4":{"pli":"taṃ dukkarataraṃ tato; ","html":"<span class='verse-line'>{}</span>"},"ja401:4.5":{"pli":"Sabbaññaṃ sukaraṃ ṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja401:4.6":{"pli":"evaṃ jānāhi maddava”. ","html":"<span class='verse-line'>{}</span></p>"},"ja401:5.1":{"pli":"“Byākāsi pukkuso pañhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja401:5.2":{"pli":"atthaṃ dhammassa kovido; ","html":"<span class='verse-line'>{}</span>"},"ja401:5.3":{"pli":"Senakaṃ dāni pucchāmi, ","html":"<span class='verse-line'>{}</span>"},"ja401:5.4":{"pli":"kiṃ dukkarataraṃ tato; ","html":"<span class='verse-line'>{}</span>"},"ja401:5.5":{"pli":"Yadaññaṃ dukkaraṃ ṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja401:5.6":{"pli":"taṃ me akkhāhi pucchito”. ","html":"<span class='verse-line'>{}</span></p>"},"ja401:6.1":{"pli":"“Dadeyya puriso dānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja401:6.2":{"pli":"appaṃ vā yadi vā bahuṃ; ","html":"<span class='verse-line'>{}</span>"},"ja401:6.3":{"pli":"Yo ca datvā nānutappe, ","html":"<span class='verse-line'>{}</span>"},"ja401:6.4":{"pli":"taṃ dukkarataraṃ tato; ","html":"<span class='verse-line'>{}</span>"},"ja401:6.5":{"pli":"Sabbaññaṃ sukaraṃ ṭhānaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja401:6.6":{"pli":"evaṃ jānāhi maddava”. ","html":"<span class='verse-line'>{}</span></p>"},"ja401:7.1":{"pli":"“Byākāsi āyuro pañhaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja401:7.2":{"pli":"atho pukkusaporiso; ","html":"<span class='verse-line'>{}</span>"},"ja401:7.3":{"pli":"Sabbe pañhe atibhoti, ","html":"<span class='verse-line'>{}</span>"},"ja401:7.4":{"pli":"yathā bhāsati senako”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja401:8.1":{"pli":"Paṇṇakajātakaṃ chaṭṭhaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"ja402":{"ja402:0.1":{"pli":"Jātaka ","html":"<article id='ja402'><header><ul><li class='division'>{}</li>"},"ja402:0.2":{"pli":"Sattakanipāta ","html":"<li>{}</li>"},"ja402:0.3":{"pli":"Kukkuvagga ","html":"<li>{}</li></ul>"},"ja402:0.4":{"pli":"7. Sattubhastajātaka ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja402:1.1":{"pli":"“Vibbhantacitto kupitindriyosi, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"ja402:1.2":{"pli":"Nettehi te vārigaṇā savanti; ","html":"<span class='verse-line'>{}</span>"},"ja402:1.3":{"pli":"Kiṃ te naṭṭhaṃ kiṃ pana patthayāno, ","html":"<span class='verse-line'>{}</span>"},"ja402:1.4":{"pli":"Idhāgamā brahme tadiṅgha brūhi”. ","html":"<span class='verse-line'>{}</span></p>"},"ja402:2.1":{"pli":"“Miyyetha bhariyā vajato mamajja, ","html":"<p><span class='verse-line'>{}</span>"},"ja402:2.2":{"pli":"Agacchato maraṇamāha yakkho; ","html":"<span class='verse-line'>{}</span>"},"ja402:2.3":{"pli":"Etena dukkhena pavedhitosmi, ","html":"<span class='verse-line'>{}</span>"},"ja402:2.4":{"pli":"Akkhāhi me senaka etamatthaṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"ja402:3.1":{"pli":"“Bahūni ṭhānāni vicintayitvā, ","html":"<p><span class='verse-line'>{}</span>"},"ja402:3.2":{"pli":"Yamettha vakkhāmi tadeva saccaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja402:3.3":{"pli":"Maññāmi te brāhmaṇa sattubhastaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja402:3.4":{"pli":"Ajānato kaṇhasappo paviṭṭho. ","html":"<span class='verse-line'>{}</span></p>"},"ja402:4.1":{"pli":"Ādāya daṇḍaṃ parisumbha bhastaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja402:4.2":{"pli":"Passeḷamūgaṃ uragaṃ dujivhaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja402:4.3":{"pli":"Chindajja kaṅkhaṃ vicikicchitāni, ","html":"<span class='verse-line'>{}</span>"},"ja402:4.4":{"pli":"Bhujaṅgamaṃ passa pamuñca bhastaṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"ja402:5.1":{"pli":"Saṃviggarūpo parisāya majjhe, ","html":"<p><span class='verse-line'>{}</span>"},"ja402:5.2":{"pli":"So brāhmaṇo sattubhastaṃ pamuñci; ","html":"<span class='verse-line'>{}</span>"},"ja402:5.3":{"pli":"Atha nikkhami urago uggatejo, ","html":"<span class='verse-line'>{}</span>"},"ja402:5.4":{"pli":"Āsīviso sappo phaṇaṃ karitvā. ","html":"<span class='verse-line'>{}</span></p>"},"ja402:6.1":{"pli":"“Suladdhalābhā janakassa rañño, ","html":"<p><span class='verse-line'>{}</span>"},"ja402:6.2":{"pli":"Yo passatī senakaṃ sādhupaññaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja402:6.3":{"pli":"Vivaṭṭachaddo nusi sabbadassī, ","html":"<span class='verse-line'>{}</span>"},"ja402:6.4":{"pli":"Ñāṇaṃ nu te brāhmaṇa bhiṃsarūpaṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"ja402:7.1":{"pli":"“Imāni me sattasatāni atthi, ","html":"<p><span class='verse-line'>{}</span>"},"ja402:7.2":{"pli":"Gaṇhāhi sabbāni dadāmi tuyhaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja402:7.3":{"pli":"Tayā hi me jīvitamajja laddhaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja402:7.4":{"pli":"Athopi bhariyāya makāsi sotthiṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"ja402:8.1":{"pli":"“Na paṇḍitā vetanamādiyanti, ","html":"<p><span class='verse-line'>{}</span>"},"ja402:8.2":{"pli":"Citrāhi gāthāhi subhāsitāhi; ","html":"<span class='verse-line'>{}</span>"},"ja402:8.3":{"pli":"Itopi te brahme dadantu vittaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja402:8.4":{"pli":"Ādāya tvaṃ gaccha sakaṃ niketan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja402:9.1":{"pli":"Sattubhastajātakaṃ sattamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"ja403":{"ja403:0.1":{"pli":"Jātaka ","html":"<article id='ja403'><header><ul><li class='division'>{}</li>"},"ja403:0.2":{"pli":"Sattakanipāta ","html":"<li>{}</li>"},"ja403:0.3":{"pli":"Kukkuvagga ","html":"<li>{}</li></ul>"},"ja403:0.4":{"pli":"8. Aṭṭhisenakajātaka ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja403:1.1":{"pli":"“Yeme ahaṃ na jānāmi, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"ja403:1.2":{"pli":"aṭṭhisena vanibbake; ","html":"<span class='verse-line'>{}</span>"},"ja403:1.3":{"pli":"Te maṃ saṅgamma yācanti, ","html":"<span class='verse-line'>{}</span>"},"ja403:1.4":{"pli":"kasmā maṃ tvaṃ na yācasi”. ","html":"<span class='verse-line'>{}</span></p>"},"ja403:2.1":{"pli":"“Yācako appiyo hoti, ","html":"<p><span class='verse-line'>{}</span>"},"ja403:2.2":{"pli":"yācaṃ adadamappiyo; ","html":"<span class='verse-line'>{}</span>"},"ja403:2.3":{"pli":"Tasmāhaṃ taṃ na yācāmi, ","html":"<span class='verse-line'>{}</span>"},"ja403:2.4":{"pli":"mā me videssanā ahu”. ","html":"<span class='verse-line'>{}</span></p>"},"ja403:3.1":{"pli":"“Yo ve yācanajīvāno, ","html":"<p><span class='verse-line'>{}</span>"},"ja403:3.2":{"pli":"kāle yācaṃ na yācati; ","html":"<span class='verse-line'>{}</span>"},"ja403:3.3":{"pli":"Parañca puññā dhaṃseti, ","html":"<span class='verse-line'>{}</span>"},"ja403:3.4":{"pli":"attanāpi na jīvati. ","html":"<span class='verse-line'>{}</span></p>"},"ja403:4.1":{"pli":"Yo ca yācanajīvāno, ","html":"<p><span class='verse-line'>{}</span>"},"ja403:4.2":{"pli":"kāle yācañhi yācati; ","html":"<span class='verse-line'>{}</span>"},"ja403:4.3":{"pli":"Parañca puññaṃ labbheti, ","html":"<span class='verse-line'>{}</span>"},"ja403:4.4":{"pli":"attanāpi ca jīvati. ","html":"<span class='verse-line'>{}</span></p>"},"ja403:5.1":{"pli":"Na ve dessanti sappaññā, ","html":"<p><span class='verse-line'>{}</span>"},"ja403:5.2":{"pli":"Disvā yācakamāgate; ","html":"<span class='verse-line'>{}</span>"},"ja403:5.3":{"pli":"Brahmacāri piyo mesi, ","html":"<span class='verse-line'>{}</span>"},"ja403:5.4":{"pli":"Vada tvaṃ bhaññamicchasi”. ","html":"<span class='verse-line'>{}</span></p>"},"ja403:6.1":{"pli":"“Na ve yācanti sappaññā, ","html":"<p><span class='verse-line'>{}</span>"},"ja403:6.2":{"pli":"Dhīro ca veditumarahati; ","html":"<span class='verse-line'>{}</span>"},"ja403:6.3":{"pli":"Uddissa ariyā tiṭṭhanti, ","html":"<span class='verse-line'>{}</span>"},"ja403:6.4":{"pli":"Esā ariyāna yācanā”. ","html":"<span class='verse-line'>{}</span></p>"},"ja403:7.1":{"pli":"“Dadāmi te brāhmaṇa rohiṇīnaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja403:7.2":{"pli":"Gavaṃ sahassaṃ saha puṅgavena; ","html":"<span class='verse-line'>{}</span>"},"ja403:7.3":{"pli":"Ariyo hi ariyassa kathaṃ na dajjā, ","html":"<span class='verse-line'>{}</span>"},"ja403:7.4":{"pli":"Sutvāna gāthā tava dhammayuttā”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja403:8.1":{"pli":"Aṭṭhisenakajātakaṃ aṭṭhamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"ja404":{"ja404:0.1":{"pli":"Jātaka ","html":"<article id='ja404'><header><ul><li class='division'>{}</li>"},"ja404:0.2":{"pli":"Sattakanipāta ","html":"<li>{}</li>"},"ja404:0.3":{"pli":"Kukkuvagga ","html":"<li>{}</li></ul>"},"ja404:0.4":{"pli":"9. Kapijātaka ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja404:1.1":{"pli":"“Yattha verī nivasati, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"ja404:1.2":{"pli":"na vase tattha paṇḍito; ","html":"<span class='verse-line'>{}</span>"},"ja404:1.3":{"pli":"Ekarattaṃ dirattaṃ vā, ","html":"<span class='verse-line'>{}</span>"},"ja404:1.4":{"pli":"dukkhaṃ vasati verisu. ","html":"<span class='verse-line'>{}</span></p>"},"ja404:2.1":{"pli":"Diso ve lahucittassa, ","html":"<p><span class='verse-line'>{}</span>"},"ja404:2.2":{"pli":"posassānuvidhīyato; ","html":"<span class='verse-line'>{}</span>"},"ja404:2.3":{"pli":"Ekassa kapino hetu, ","html":"<span class='verse-line'>{}</span>"},"ja404:2.4":{"pli":"yūthassa anayo kato. ","html":"<span class='verse-line'>{}</span></p>"},"ja404:3.1":{"pli":"Bālova paṇḍitamānī, ","html":"<p><span class='verse-line'>{}</span>"},"ja404:3.2":{"pli":"yūthassa parihārako; ","html":"<span class='verse-line'>{}</span>"},"ja404:3.3":{"pli":"Sacittassa vasaṃ gantvā, ","html":"<span class='verse-line'>{}</span>"},"ja404:3.4":{"pli":"sayethāyaṃ yathā kapi. ","html":"<span class='verse-line'>{}</span></p>"},"ja404:4.1":{"pli":"Na sādhu balavā bālo, ","html":"<p><span class='verse-line'>{}</span>"},"ja404:4.2":{"pli":"yūthassa parihārako; ","html":"<span class='verse-line'>{}</span>"},"ja404:4.3":{"pli":"Ahito bhavati ñātīnaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja404:4.4":{"pli":"sakuṇānaṃva cetako. ","html":"<span class='verse-line'>{}</span></p>"},"ja404:5.1":{"pli":"Dhīrova balavā sādhu, ","html":"<p><span class='verse-line'>{}</span>"},"ja404:5.2":{"pli":"yūthassa parihārako; ","html":"<span class='verse-line'>{}</span>"},"ja404:5.3":{"pli":"Hito bhavati ñātīnaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja404:5.4":{"pli":"tidasānaṃva vāsavo. ","html":"<span class='verse-line'>{}</span></p>"},"ja404:6.1":{"pli":"Yo ca sīlañca paññañca, ","html":"<p><span class='verse-line'>{}</span>"},"ja404:6.2":{"pli":"sutañcattani passati; ","html":"<span class='verse-line'>{}</span>"},"ja404:6.3":{"pli":"Ubhinnamatthaṃ carati, ","html":"<span class='verse-line'>{}</span>"},"ja404:6.4":{"pli":"attano ca parassa ca. ","html":"<span class='verse-line'>{}</span></p>"},"ja404:7.1":{"pli":"Tasmā tuleyya mattānaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja404:7.2":{"pli":"sīlapaññāsutāmiva; ","html":"<span class='verse-line'>{}</span>"},"ja404:7.3":{"pli":"Gaṇaṃ vā parihare dhīro, ","html":"<span class='verse-line'>{}</span>"},"ja404:7.4":{"pli":"eko vāpi paribbaje”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja404:8.1":{"pli":"Kapijātakaṃ navamaṃ. ","html":"<p class='endsutta'>{}</p></article>"}},"ja405":{"ja405:0.1":{"pli":"Jātaka ","html":"<article id='ja405'><header><ul><li class='division'>{}</li>"},"ja405:0.2":{"pli":"Sattakanipāta ","html":"<li>{}</li>"},"ja405:0.3":{"pli":"Kukkuvagga ","html":"<li>{}</li></ul>"},"ja405:0.4":{"pli":"10. Bakajātaka ","html":"<h1 class='sutta-title'>{}</h1></header>"},"ja405:1.1":{"pli":"“Dvāsattati gotama puññakammā, ","html":"<blockquote class='gatha'><p><span class='verse-line'>{}</span>"},"ja405:1.2":{"pli":"Vasavattino jātijaraṃ atītā; ","html":"<span class='verse-line'>{}</span>"},"ja405:1.3":{"pli":"Ayamantimā vedagū brahmapatti, ","html":"<span class='verse-line'>{}</span>"},"ja405:1.4":{"pli":"Asmābhijappanti janā anekā”. ","html":"<span class='verse-line'>{}</span></p>"},"ja405:2.1":{"pli":"“Appañhi etaṃ na hi dīghamāyu, ","html":"<p><span class='verse-line'>{}</span>"},"ja405:2.2":{"pli":"Yaṃ tvaṃ baka maññasi dīghamāyuṃ; ","html":"<span class='verse-line'>{}</span>"},"ja405:2.3":{"pli":"Sataṃ sahassāni nirabbudānaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja405:2.4":{"pli":"Āyuṃ pajānāmi tavāha brahme”. ","html":"<span class='verse-line'>{}</span></p>"},"ja405:3.1":{"pli":"“Anantadassī bhagavāhamasmi, ","html":"<p><span class='verse-line'>{}</span>"},"ja405:3.2":{"pli":"Jātijjaraṃ sokamupātivatto; ","html":"<span class='verse-line'>{}</span>"},"ja405:3.3":{"pli":"Kiṃ me purāṇaṃ vatasīlavattaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja405:3.4":{"pli":"Ācikkha me taṃ yamahaṃ vijaññaṃ”. ","html":"<span class='verse-line'>{}</span></p>"},"ja405:4.1":{"pli":"“Yaṃ tvaṃ apāyesi bahū manusse, ","html":"<p><span class='verse-line'>{}</span>"},"ja405:4.2":{"pli":"Pipāsite ghammani samparete; ","html":"<span class='verse-line'>{}</span>"},"ja405:4.3":{"pli":"Taṃ te purāṇaṃ vatasīlavattaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja405:4.4":{"pli":"Suttappabuddhova anussarāmi. ","html":"<span class='verse-line'>{}</span></p>"},"ja405:5.1":{"pli":"Yaṃ eṇikūlasmi janaṃ gahītaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja405:5.2":{"pli":"Amocayī gayhaka niyyamānaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja405:5.3":{"pli":"Taṃ te purāṇaṃ vatasīlavattaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja405:5.4":{"pli":"Suttappabuddhova anussarāmi. ","html":"<span class='verse-line'>{}</span></p>"},"ja405:6.1":{"pli":"Gaṅgāya sotasmiṃ gahītanāvaṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja405:6.2":{"pli":"Luddena nāgena manussakappā; ","html":"<span class='verse-line'>{}</span>"},"ja405:6.3":{"pli":"Amocayi tvaṃ balasā pasayha, ","html":"<span class='verse-line'>{}</span>"},"ja405:6.4":{"pli":"Taṃ te purāṇaṃ vatasīlavattaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja405:6.5":{"pli":"Suttappabuddhova anussarāmi. ","html":"<span class='verse-line'>{}</span></p>"},"ja405:7.1":{"pli":"Kappo ca te baddhacaro ahosiṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja405:7.2":{"pli":"Sambuddhimantaṃ vatinaṃ amaññaṃ; ","html":"<span class='verse-line'>{}</span>"},"ja405:7.3":{"pli":"Taṃ te purāṇaṃ vatasīlavattaṃ, ","html":"<span class='verse-line'>{}</span>"},"ja405:7.4":{"pli":"Suttappabuddhova anussarāmi”. ","html":"<span class='verse-line'>{}</span></p>"},"ja405:8.1":{"pli":"“Addhā pajānāsi mametamāyuṃ, ","html":"<p><span class='verse-line'>{}</span>"},"ja405:8.2":{"pli":"Aññampi jānāsi tathā hi buddho; ","html":"<span class='verse-line'>{}</span>"},"ja405:8.3":{"pli":"Tathā hi tāyaṃ jalitānubhāvo, ","html":"<span class='verse-line'>{}</span>"},"ja405:8.4":{"pli":"Obhāsayaṃ tiṭṭhati brahmalokan”ti. ","html":"<span class='verse-line'>{}</span></p></blockquote>"},"ja405:9.1":{"pli":"Bakajātakaṃ dasamaṃ. ","html":"<p class='endsutta'>{}</p>"},"ja405:9.2":{"pli":"Kukkuvaggo paṭhamo. ","html":"<p class='endvagga'>{}</p>"},"ja405:10.1":{"pli":"Tassuddānaṃ ","html":"<p class='uddana-intro'>{}</p>"},"ja405:11.1":{"pli":"Varakaṇṇika cāpavaro sutano, ","html":"<blockquote class='uddanagatha'><p><span class='verse-line'>{}</span>"},"ja405:11.2":{"pli":"Atha gijjha sarohitamacchavaro; ","html":"<span class='verse-line'>{}</span>"},"ja405:11.3":{"pli":"Puna paṇṇaka senaka yācanako, ","html":"<span class='verse-line'>{}</span>"},"ja405:11.4":{"pli":"Atha veri sabrahmabakena dasāti. ","html":"<span class='verse-line'>{}</span></p></blockquote></article>"}}}